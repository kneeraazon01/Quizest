pan>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p>The number of people who must be exposed to a treatment to cause harm to 1 person who otherwise would not have been harmed is known as the <strong>number needed to harm (NNH)</strong>.&amp;nbsp; It is calculated in a manner similar to the number needed to treat (NNT) but using the <strong>absolute risk increase (ARI)</strong> instead of the absolute risk reduction:</p><div style=&quot;margin-left:3em;&quot;><p><strong>NNH</strong> = (1 / ARI)</p></div><p>ARI is the <strong>difference in the rate</strong> (risk) of the <strong>adverse event</strong> (AE) between the treatment group (eg, cariprazine) and the control group (eg, placebo):</p><div style=&quot;margin-left:3em;&quot;><p><strong>ARI</strong> = (Rate AE<font size=&quot;2&quot;><sub>cariprazine</sub></font> &amp;#8722; Rate AE<font size=&quot;2&quot;><sub>placebo</sub></font>)</p></div><p>In this study, the rate of any AE in the treatment group (ie, cariprazine) is 0.07 (ie, 7%) and in the control (ie, placebo) group is 0.05 (5%).&amp;nbsp; Therefore, the ARI in adverse events of cariprazine versus placebo is:</p><div style=&quot;margin-left:3em;&quot;><p>ARI = (Rate AE<font size=&quot;2&quot;><sub>cariprazine</sub></font> &amp;#8722; Rate AE<font size=&quot;2&quot;><sub>placebo</sub></font>) = 0.07 &amp;#8722; 0.05 = 0.02 (or 2%) <strong>(Choice A)</strong></p></div><p>Consequently, the NNH is:</p><div style=&quot;margin-left:3em;&quot;><p>NNH = (1 / ARI) = 1 / 0.02 = 50</p></div><p>Therefore, <strong>approximately 50 patients</strong> would need to be treated with cariprazine to cause harm to 1 person who otherwise would not have been harmed.&amp;nbsp; The lower the NNH, the more risk of harm; an NNH of 1 means that every patient treated is harmed.</p><p><strong>(Choices B, C, and D)</strong>&amp;nbsp; The absolute risk reduction (ARR) is the difference in risk of an unfavorable event (eg, treatment nonresponse) between the cariprazine and placebo groups: ARR = (Rate<font size=&quot;2&quot;><sub>placebo</sub></font> &amp;#8722; Rate<font size=&quot;2&quot;><sub>cariprazine</sub></font>).&amp;nbsp; In this case, the rate of no response in the cariprazine group is 100% &amp;#8722; 46% = 54% and in the placebo group is 100% &amp;#8722; 32% = 68%, so ARR = 0.68 &amp;#8722; 0.54 = 0.14 (ie, 14%).&amp;nbsp; The relative risk reduction (RRR) is (Rate<font size=&quot;2&quot;><sub>placebo</sub></font> &amp;#8722; Rate<font size=&quot;2&quot;><sub>cariprazine</sub></font> ) / Rate<font size=&quot;2&quot;><sub>placebo</sub></font>, so RRR = (0.68 &amp;#8722; 0.54) / 0.68 &amp;#8776; 0.21 (ie, 21%).&amp;nbsp; The number needed to treat (NNT) to avoid 1 additional negative event (ie, no treatment response) compared to placebo is the inverse of the absolute risk reduction (ARR): NNT = 1 / ARR = 1 / 0.14 &amp;#8776; 8.</p><p><strong>Educational objective:</strong><br>The number needed to harm (NNH) is the number of people who must be exposed to a treatment to cause harm to 1 person who otherwise would not have been harmed.&amp;nbsp; To calculate NNH, the absolute risk increase (ARI) between the treatment and control groups must be known: NNH = 1 / ARI.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\">The number of people who must be exposed to a treatment to cause harm to 1 person who otherwise would not have been harmed is known as the <strong>number needed to harm (NNH)</strong>.&nbsp; It is calculated in a manner similar to the number needed to treat (NNT) but using the <strong>absolute risk increase (ARI)</strong> instead of the absolute risk reduction:</p><div style=\"margin-left:3em;\" class=\"ng-scope\"><p><strong>NNH</strong> = (1 / ARI)</p></div><p class=\"ng-scope\">ARI is the <strong>difference in the rate</strong> (risk) of the <strong>adverse event</strong> (AE) between the treatment group (eg, cariprazine) and the control group (eg, placebo):</p><div style=\"margin-left:3em;\" class=\"ng-scope\"><p><strong>ARI</strong> = (Rate AE<font size=\"2\"><sub>cariprazine</sub></font> \u2212 Rate AE<font size=\"2\"><sub>placebo</sub></font>)</p></div><p class=\"ng-scope\">In this study, the rate of any AE in the treatment group (ie, cariprazine) is 0.07 (ie, 7%) and in the control (ie, placebo) group is 0.05 (5%).&nbsp; Therefore, the ARI in adverse events of cariprazine versus placebo is:</p><div style=\"margin-left:3em;\" class=\"ng-scope\"><p>ARI = (Rate AE<font size=\"2\"><sub>cariprazine</sub></font> \u2212 Rate AE<font size=\"2\"><sub>placebo</sub></font>) = 0.07 \u2212 0.05 = 0.02 (or 2%) <strong>(Choice A)</strong></p></div><p class=\"ng-scope\">Consequently, the NNH is:</p><div style=\"margin-left:3em;\" class=\"ng-scope\"><p>NNH = (1 / ARI) = 1 / 0.02 = 50</p></div><p class=\"ng-scope\">Therefore, <strong>approximately 50 patients</strong> would need to be treated with cariprazine to cause harm to 1 person who otherwise would not have been harmed.&nbsp; The lower the NNH, the more risk of harm; an NNH of 1 means that every patient treated is harmed.</p><p class=\"ng-scope\"><strong>(Choices B, C, and D)</strong>&nbsp; The absolute risk reduction (ARR) is the difference in risk of an unfavorable event (eg, treatment nonresponse) between the cariprazine and placebo groups: ARR = (Rate<font size=\"2\"><sub>placebo</sub></font> \u2212 Rate<font size=\"2\"><sub>cariprazine</sub></font>).&nbsp; In this case, the rate of no response in the cariprazine group is 100% \u2212 46% = 54% and in the placebo group is 100% \u2212 32% = 68%, so ARR = 0.68 \u2212 0.54 = 0.14 (ie, 14%).&nbsp; The relative risk reduction (RRR) is (Rate<font size=\"2\"><sub>placebo</sub></font> \u2212 Rate<font size=\"2\"><sub>cariprazine</sub></font> ) / Rate<font size=\"2\"><sub>placebo</sub></font>, so RRR = (0.68 \u2212 0.54) / 0.68 \u2248 0.21 (ie, 21%).&nbsp; The number needed to treat (NNT) to avoid 1 additional negative event (ie, no treatment response) compared to placebo is the inverse of the absolute risk reduction (ARR): NNT = 1 / ARR = 1 / 0.14 \u2248 8.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>The number needed to harm (NNH) is the number of people who must be exposed to a treatment to cause harm to 1 person who otherwise would not have been harmed.&nbsp; To calculate NNH, the absolute risk increase (ARI) between the treatment and control groups must be known: NNH = 1 / ARI.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Biostatistics</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Biostatistics &amp; Epidemiology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Number needed to harm</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>