stInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;20674&quot; src=&quot;https://www.uworld.com/media/L20325.jpg&quot; ></p><p>Microscopy of this patient&amp;#39;s penis ulcer reveals a <strong>multinucleated giant cell</strong> with ground-glass intranuclear inclusions, raising strong suspicion for <strong>herpes simplex virus (HSV) infection</strong>.&amp;nbsp; Genital HSV is usually caused by HSV type 2, which is spread primarily via direct skin contact with herpetic sores or infected genital secretions during sexual activity.&amp;nbsp; Initial infection is often characterized by <strong>painful genital ulcers</strong>, fever, lymphadenopathy, and dysuria.&amp;nbsp; Symptoms typically resolve within a few weeks, but latent infection develops in the sacral dorsal root ganglia and can periodically reactivate, leading to recurrence.</p><p>HSV is generally diagnosed by cell culture or PCR, but <strong>Tzanck smear</strong> (as seen above) can reveal the cytopathic effects of the virus (eg, <a ng-click=&quot;showexhibit('38380',true);&quot;>multinucleated giant cells</a>, acantholytic cells, keratinocyte ballooning, nuclear molding).&amp;nbsp; Oral antiviral agents such as <strong>acyclovir</strong>, a competitive inhibitor of viral DNA polymerase, help speed symptom resolution.</p><p><strong>(Choice B)</strong>&amp;nbsp; Azithromycin can be used to treat chancroid, which is characterized by the formation of a papule that evolves into a pustule and then an ulcer.&amp;nbsp; However, it is very rare in the United States and is caused by the gram-negative organism <em>Haemophilus ducreyi</em>; multinucleated giant cells would not be seen.</p><p><strong>(Choice C)</strong>&amp;nbsp; Doxycycline can treat lymphogranuloma venereum, a genital infection caused by certain serotypes of <em>Chlamydia trachomatis</em>.&amp;nbsp; This disease is marked by the formation of a small genital ulcer, followed by significant lymph node inflammation and pain in the inguinal or femoral region.&amp;nbsp; <em>C trachomatis</em> appears as intravacuolar organisms in macrophages.</p><p><strong>(Choice D)</strong>&amp;nbsp; Fluconazole can treat <a ng-click=&quot;showexhibit('41412',true);&quot;><em>Candida</em></a> balanitis, which is typically marked by a painful, pruritic, erythematous rash.&amp;nbsp; Yeasts and pseudohyphae can often be seen on microscopy.</p><p><strong>(Choice E)</strong>&amp;nbsp; Penicillin can be used to treat syphilis, a spirochetal infection caused by <em>Treponema pallidium</em> that begins with the formation of a chancre (painless ulcer with heaped-up borders and a clean base) at the site of inoculation.&amp;nbsp; Diagnosis is generally made with serologic testing because <em>T pallidium</em> cannot be cultured; multiple ulcers and multinucleated giant cells are not typically seen.</p><p><strong>(Choice F)</strong>&amp;nbsp; Permethrin can be used to treat pubic lice, which are associated with significant pruritus of hairy genital areas.&amp;nbsp; Lice can generally be visualized on the hair shaft.</p><p><strong>Educational objective:</strong><br>Herpes simplex virus is a common, sexually transmitted infection marked by the formation of painful genital ulcers.&amp;nbsp; Tzanck smear can reveal the characteristic cytopathic effects of the virus, including multinucleated giant cells, ground-glass intranuclear inclusions, acantholytic cells, keratinocyte ballooning, and nuclear molding.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"20674\" src=\"image/L20325.jpg\"></p><p class=\"ng-scope\">Microscopy of this patient's penis ulcer reveals a <strong>multinucleated giant cell</strong> with ground-glass intranuclear inclusions, raising strong suspicion for <strong>herpes simplex virus (HSV) infection</strong>.&nbsp; Genital HSV is usually caused by HSV type 2, which is spread primarily via direct skin contact with herpetic sores or infected genital secretions during sexual activity.&nbsp; Initial infection is often characterized by <strong>painful genital ulcers</strong>, fever, lymphadenopathy, and dysuria.&nbsp; Symptoms typically resolve within a few weeks, but latent infection develops in the sacral dorsal root ganglia and can periodically reactivate, leading to recurrence.</p><p class=\"ng-scope\">HSV is generally diagnosed by cell culture or PCR, but <strong>Tzanck smear</strong> (as seen above) can reveal the cytopathic effects of the virus (eg, <a ng-click=\"showexhibit('38380',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L63691.jpg&quot;,&quot;title&quot;:&quot;L63691&quot;}]\">multinucleated giant cells</a>, acantholytic cells, keratinocyte ballooning, nuclear molding).&nbsp; Oral antiviral agents such as <strong>acyclovir</strong>, a competitive inhibitor of viral DNA polymerase, help speed symptom resolution.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Azithromycin can be used to treat chancroid, which is characterized by the formation of a papule that evolves into a pustule and then an ulcer.&nbsp; However, it is very rare in the United States and is caused by the gram-negative organism <em>Haemophilus ducreyi</em>; multinucleated giant cells would not be seen.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Doxycycline can treat lymphogranuloma venereum, a genital infection caused by certain serotypes of <em>Chlamydia trachomatis</em>.&nbsp; This disease is marked by the formation of a small genital ulcer, followed by significant lymph node inflammation and pain in the inguinal or femoral region.&nbsp; <em>C trachomatis</em> appears as intravacuolar organisms in macrophages.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Fluconazole can treat <a ng-click=\"showexhibit('41412',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L67063.png&quot;,&quot;title&quot;:&quot;L67063&quot;}]\"><em>Candida</em></a> balanitis, which is typically marked by a painful, pruritic, erythematous rash.&nbsp; Yeasts and pseudohyphae can often be seen on microscopy.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Penicillin can be used to treat syphilis, a spirochetal infection caused by <em>Treponema pallidium</em> that begins with the formation of a chancre (painless ulcer with heaped-up borders and a clean base) at the site of inoculation.&nbsp; Diagnosis is generally made with serologic testing because <em>T pallidium</em> cannot be cultured; multiple ulcers and multinucleated giant cells are not typically seen.</p><p class=\"ng-scope\"><strong>(Choice F)</strong>&nbsp; Permethrin can be used to treat pubic lice, which are associated with significant pruritus of hairy genital areas.&nbsp; Lice can generally be visualized on the hair shaft.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Herpes simplex virus is a common, sexually transmitted infection marked by the formation of painful genital ulcers.&nbsp; Tzanck smear can reveal the characteristic cytopathic effects of the virus, including multinucleated giant cells, ground-glass intranuclear inclusions, acantholytic cells, keratinocyte ballooning, and nuclear molding.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Microbiology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Infectious Diseases</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">HSV infection</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
