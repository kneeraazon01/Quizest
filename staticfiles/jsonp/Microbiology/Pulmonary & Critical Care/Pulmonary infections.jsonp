n.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p>These patients have &amp;#x22;<strong>walking pneumonia</strong>,&amp;#x22; an infection generally caused by <em><strong>Mycoplasma pneumoniae</strong></em>.&amp;nbsp; Patients experience a low-grade fever, malaise, and a chronic, dry nagging cough.&amp;nbsp; A classic sign is a chest x-ray revealing findings that are much <strong>worse</strong> than the clinical appearance of the patient.&amp;nbsp; Another clue for <em>M pneumoniae</em> infection is that the organisms require <strong>cholesterol</strong> to grow because their cell membrane is composed of a single cholesterol-rich phospholipid bilayer.&amp;nbsp; They completely lack a peptidoglycan cell wall, cell envelope, or capsule.&amp;nbsp; (Fungi also incorporate sterols into their cell membranes; however, fungi use ergosterol, a target for antifungal therapy.)</p><p><strong>(Choices A and D)</strong>&amp;nbsp; The fungus <em>Coccidioides immitis</em> causes San Joaquin Valley fever (known as &amp;#x22;valley fever&amp;#x22;).&amp;nbsp; It is most common in the Southwest United States and Northern Mexico.&amp;nbsp; <em>Histoplasma capsulatum</em> causes a similar illness, but it is typically localized to the Mississippi and Ohio River Valleys.&amp;nbsp; Both organisms grow on standard fungal culture media.</p><p><strong>(Choice B)</strong>&amp;nbsp; <em>Coxiella burnetii</em> causes Q-fever, a pneumonia-like illness resulting from inhalation of <em>C burnetii</em> spores that commonly contaminate animal hides.&amp;nbsp; <em>C burnetii</em> is an obligate intracellular parasite that must be provided a cell culture to grow.</p><p><strong>(Choice C)</strong>&amp;nbsp; <em>Haemophilus influenzae</em> requires chocolate agar (heat lysed blood agar), supplemented with factor X (hematin) and factor V (NAD+).</p><p><strong>(Choice E)</strong>&amp;nbsp; <em>Klebsiella pneumoniae</em> pneumonia classically presents with cough productive of red jelly-like sputum (&amp;#x22;currant jelly sputum&amp;#x22;) in patients with alcohol use disorder.&amp;nbsp; <em>K pneumoniae</em> can be grown on standard agar, but MacConkey agar is often preferred because it contains bile, which will inhibit the growth of contaminant organisms.</p><p><strong>(Choice F)</strong>&amp;nbsp; <em>Legionella pneumophila</em> requires an L-cysteine-supplemented agar to grow.</p><p><strong>(Choice H)</strong>&amp;nbsp; The fungus <em>Pneumocystis jirovecii</em> can cause severe pneumonia in HIV+ patients with CD4 cell counts &amp;lt;200/mm<font size=&quot;2&quot;><sup>3</sup></font>.&amp;nbsp; Patients often have characteristic &amp;#x22;ground glass&amp;#x22; infiltrates on chest x-ray.&amp;nbsp; <em>P jirovecii</em> pneumonia is diagnosed by silver staining the fluid collected during bronchoscopy.</p><p><strong>(Choice I)</strong>&amp;nbsp; <em>Streptococcus pneumoniae</em> would cause lobar consolidation on chest x-ray.&amp;nbsp; This organism can grow well on standard unenriched blood agar, but it is bile-soluble and cannot grow in the presence of optochin.</p><p><strong>Educational objective:</strong><br><em>Mycoplasma pneumoniae</em> is the causative agent of &amp;#x22;walking pneumonia,&amp;#x22; an infection typically characterized by a nagging nonproductive cough, low-grade fever, and malaise.&amp;nbsp; Often, the chest x-ray suggests a severe pneumonia even though the patient appears relatively well.&amp;nbsp; <em>Mycoplasma</em> species require cholesterol supplementation to grow on artificial media.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\">These patients have \"<strong>walking pneumonia</strong>,\" an infection generally caused by <em><strong>Mycoplasma pneumoniae</strong></em>.&nbsp; Patients experience a low-grade fever, malaise, and a chronic, dry nagging cough.&nbsp; A classic sign is a chest x-ray revealing findings that are much <strong>worse</strong> than the clinical appearance of the patient.&nbsp; Another clue for <em>M pneumoniae</em> infection is that the organisms require <strong>cholesterol</strong> to grow because their cell membrane is composed of a single cholesterol-rich phospholipid bilayer.&nbsp; They completely lack a peptidoglycan cell wall, cell envelope, or capsule.&nbsp; (Fungi also incorporate sterols into their cell membranes; however, fungi use ergosterol, a target for antifungal therapy.)</p><p class=\"ng-scope\"><strong>(Choices A and D)</strong>&nbsp; The fungus <em>Coccidioides immitis</em> causes San Joaquin Valley fever (known as \"valley fever\").&nbsp; It is most common in the Southwest United States and Northern Mexico.&nbsp; <em>Histoplasma capsulatum</em> causes a similar illness, but it is typically localized to the Mississippi and Ohio River Valleys.&nbsp; Both organisms grow on standard fungal culture media.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; <em>Coxiella burnetii</em> causes Q-fever, a pneumonia-like illness resulting from inhalation of <em>C burnetii</em> spores that commonly contaminate animal hides.&nbsp; <em>C burnetii</em> is an obligate intracellular parasite that must be provided a cell culture to grow.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; <em>Haemophilus influenzae</em> requires chocolate agar (heat lysed blood agar), supplemented with factor X (hematin) and factor V (NAD+).</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; <em>Klebsiella pneumoniae</em> pneumonia classically presents with cough productive of red jelly-like sputum (\"currant jelly sputum\") in patients with alcohol use disorder.&nbsp; <em>K pneumoniae</em> can be grown on standard agar, but MacConkey agar is often preferred because it contains bile, which will inhibit the growth of contaminant organisms.</p><p class=\"ng-scope\"><strong>(Choice F)</strong>&nbsp; <em>Legionella pneumophila</em> requires an L-cysteine-supplemented agar to grow.</p><p class=\"ng-scope\"><strong>(Choice H)</strong>&nbsp; The fungus <em>Pneumocystis jirovecii</em> can cause severe pneumonia in HIV+ patients with CD4 cell counts &lt;200/mm<font size=\"2\"><sup>3</sup></font>.&nbsp; Patients often have characteristic \"ground glass\" infiltrates on chest x-ray.&nbsp; <em>P jirovecii</em> pneumonia is diagnosed by silver staining the fluid collected during bronchoscopy.</p><p class=\"ng-scope\"><strong>(Choice I)</strong>&nbsp; <em>Streptococcus pneumoniae</em> would cause lobar consolidation on chest x-ray.&nbsp; This organism can grow well on standard unenriched blood agar, but it is bile-soluble and cannot grow in the presence of optochin.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br><em>Mycoplasma pneumoniae</em> is the causative agent of \"walking pneumonia,\" an infection typically characterized by a nagging nonproductive cough, low-grade fever, and malaise.&nbsp; Often, the chest x-ray suggests a severe pneumonia even though the patient appears relatively well.&nbsp; <em>Mycoplasma</em> species require cholesterol supplementation to grow on artificial media.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Microbiology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pulmonary &amp; Critical Care</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Community acquired pneumonia</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
