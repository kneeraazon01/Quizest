io\" type=\"radio\">\n                            <strong>F.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight6\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Optochin resistance</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; id=&quot;3163&quot;  src=&quot;https://www.uworld.com/media/L22217.jpg&quot; draggable=&quot;false&quot; ></p><p>The presence of lancet-shaped, gram-positive <a ng-click=&quot;showexhibit('1983',true);&quot;>diplococci</a> in the sputum sample of a patient with fever and cough is suggestive of infection with <strong><em>Streptococcus pneumoniae</em></strong>.&amp;nbsp; All streptococci are <strong>catalase-negative</strong>.&amp;nbsp; The degree of hemolysis on blood agar distinguishes hemolytic (partial or complete) from nonhemolytic streptococci.&amp;nbsp; The bile solubility and optochin tests are used to differentiate between <a ng-click=&quot;showexhibit('1984',true);&quot;>partial (green) hemolytic species (&amp;#945;-hemolysis)</a>, namely <em>S pneumoniae</em> and viridans streptococci group.&amp;nbsp; Autolysin produced by <em>S pneumoniae</em> is activated by bile, resulting in cell lysis.&amp;nbsp; Therefore, addition of bile salts into a tube with <em>S pneumoniae</em> growth (turbid) leads to cell lysis (clearing of turbidity); this is known as <strong>bile solubility</strong>.&amp;nbsp; <em>S pneumoniae</em> is bile-soluble and <a ng-click=&quot;showexhibit('1985',true);&quot;>optochin-sensitive</a>; viridans group streptococci are bile-insoluble and optochin-resistant <strong>(Choice F)</strong>.</p><p><strong>(Choice A)</strong>&amp;nbsp; Bacitracin susceptibility testing is used to differentiate <em>Streptococcus pyogenes</em> (bacitracin- susceptible) from other &amp;#946;-hemolytic streptococci.&amp;nbsp; Many laboratories have replaced this test (which is not specific to <em>S pyogenes</em>) with the pyrrolidonyl arylamidase (PYR) test.&amp;nbsp; Both <em>S pyogenes</em> and enterococci are PYR-positive.&amp;nbsp; <em>Streptococcus agalactiae</em> is PYR-negative and enhances the area of <em>Staphylococcus aureus</em> hemolysis when grown in the same plate, resulting in a positive CAMP test (named after Christie, Atkins, and Munch-Petersen).</p><p><strong>(Choice C)</strong>&amp;nbsp; Staphylococci are catalase-positive; streptococci are catalase-negative.</p><p><strong>(Choice D)</strong>&amp;nbsp; Complete (or &amp;#946;) hemolysis is characteristic of Group A streptococcus (<em>S pyogenes</em>) and Group B streptococcus (<em>S agalactiae</em>).&amp;nbsp; <em>S aureus</em> (catalase-positive) also demonstrates complete hemolysis, as noted by the <a ng-click=&quot;showexhibit('1986',true);&quot;>arrows</a>.</p><p><strong>(Choice E)</strong>&amp;nbsp; &amp;#947;-hemolytic (no hemolysis) catalase-negative organisms include enterococci and non-enterococci (<em>S bovis</em>).&amp;nbsp; Enterococci are able to grow in hypertonic saline (6.5% NaCl) and bile.&amp;nbsp; <em>S bovis</em> can grow in the presence of bile but not 6.5% NaCl.&amp;nbsp; <em>S bovis</em> bacteremia is associated with colon cancer.</p><p><strong>Educational objective:</strong><br><em>Streptococcus pneumoniae</em> are gram-positive, &amp;#945;-hemolytic, optochin-sensitive, bile-soluble diplococci.&amp;nbsp; Viridans group streptococci are also &amp;#945;-hemolytic, but they are optochin-resistant and bile insoluble.&amp;nbsp; <em>Streptococcus pyogenes</em> (Group A <em>Streptococcus</em>) appear as gram-positive cocci in chains with bacitracin susceptibility.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" id=\"3163\" src=\"image/L22217.jpg\" draggable=\"false\"></p><p class=\"ng-scope\">The presence of lancet-shaped, gram-positive <a ng-click=\"showexhibit('1983',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L13321.jpg&quot;,&quot;title&quot;:&quot;L13321&quot;}]\">diplococci</a> in the sputum sample of a patient with fever and cough is suggestive of infection with <strong><em>Streptococcus pneumoniae</em></strong>.&nbsp; All streptococci are <strong>catalase-negative</strong>.&nbsp; The degree of hemolysis on blood agar distinguishes hemolytic (partial or complete) from nonhemolytic streptococci.&nbsp; The bile solubility and optochin tests are used to differentiate between <a ng-click=\"showexhibit('1984',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L13320.jpg&quot;,&quot;title&quot;:&quot;L13320&quot;}]\">partial (green) hemolytic species (\u03b1-hemolysis)</a>, namely <em>S pneumoniae</em> and viridans streptococci group.&nbsp; Autolysin produced by <em>S pneumoniae</em> is activated by bile, resulting in cell lysis.&nbsp; Therefore, addition of bile salts into a tube with <em>S pneumoniae</em> growth (turbid) leads to cell lysis (clearing of turbidity); this is known as <strong>bile solubility</strong>.&nbsp; <em>S pneumoniae</em> is bile-soluble and <a ng-click=\"showexhibit('1985',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L13322.png&quot;,&quot;title&quot;:&quot;L13322&quot;}]\">optochin-sensitive</a>; viridans group streptococci are bile-insoluble and optochin-resistant <strong>(Choice F)</strong>.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Bacitracin susceptibility testing is used to differentiate <em>Streptococcus pyogenes</em> (bacitracin- susceptible) from other \u03b2-hemolytic streptococci.&nbsp; Many laboratories have replaced this test (which is not specific to <em>S pyogenes</em>) with the pyrrolidonyl arylamidase (PYR) test.&nbsp; Both <em>S pyogenes</em> and enterococci are PYR-positive.&nbsp; <em>Streptococcus agalactiae</em> is PYR-negative and enhances the area of <em>Staphylococcus aureus</em> hemolysis when grown in the same plate, resulting in a positive CAMP test (named after Christie, Atkins, and Munch-Petersen).</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Staphylococci are catalase-positive; streptococci are catalase-negative.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Complete (or \u03b2) hemolysis is characteristic of Group A streptococcus (<em>S pyogenes</em>) and Group B streptococcus (<em>S agalactiae</em>).&nbsp; <em>S aureus</em> (catalase-positive) also demonstrates complete hemolysis, as noted by the <a ng-click=\"showexhibit('1986',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L13316.jpg&quot;,&quot;title&quot;:&quot;L13316&quot;}]\">arrows</a>.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; \u03b3-hemolytic (no hemolysis) catalase-negative organisms include enterococci and non-enterococci (<em>S bovis</em>).&nbsp; Enterococci are able to grow in hypertonic saline (6.5% NaCl) and bile.&nbsp; <em>S bovis</em> can grow in the presence of bile but not 6.5% NaCl.&nbsp; <em>S bovis</em> bacteremia is associated with colon cancer.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br><em>Streptococcus pneumoniae</em> are gram-positive, \u03b1-hemolytic, optochin-sensitive, bile-soluble diplococci.&nbsp; Viridans group streptococci are also \u03b1-hemolytic, but they are optochin-resistant and bile insoluble.&nbsp; <em>Streptococcus pyogenes</em> (Group A <em>Streptococcus</em>) appear as gram-positive cocci in chains with bacitracin susceptibility.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Microbiology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pulmonary &amp; Critical Care</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Community acquired pneumonia</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
