         <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; id=&quot;1396&quot;  src=&quot;https://www.uworld.com/media/U10732.png&quot; draggable=&quot;false&quot; ></p><p><strong>Erythema infectiosum</strong>, or fifth disease, is a <strong>benign</strong> childhood illness caused by <strong>parvovirus B19</strong>, a single-stranded DNA virus.&amp;nbsp; After a 1-2 week incubation period, a <strong>nonspecific prodrome</strong> (eg, malaise, congestion, headache, fever) is followed by an erythematous rash on the cheeks.&amp;nbsp; This &amp;#x22;<strong>slapped-cheek</strong>&amp;#x22; rash, as shown in the image above, usually spares the nasolabial folds and develops after the initial symptoms have resolved.&amp;nbsp; A <a ng-click=&quot;showexhibit('326',true);&quot;>lacy, reticular rash</a> then follows, spreading over the trunk and extremities.&amp;nbsp; The rash may result from immune complex deposition, the timing of which coincides with increasing levels of serum virus-specific IgM and IgG.</p><p>Parvovirus B19 replicates in <strong>erythrocyte</strong> precursors in the <strong>bone marrow</strong>.&amp;nbsp; These cells express blood group P antigen (also known as globoside), which is the cellular receptor for parvovirus B19.&amp;nbsp; Viral replication in the nucleus leads to cell lysis (ie, decreased reticulocytes and red blood cells).&amp;nbsp; Acute and chronic sequelae of parvovirus B19 infection can occur in specific populations (eg, chronic anemia in immunosuppressed patients, aplastic crisis in those with underlying hematologic abnormalities such as sickle cell disease).</p><p><strong>(Choice B)</strong>&amp;nbsp; Hepatitis A-E viruses replicate within mature hepatocytes and cause inflammation and cell necrosis.&amp;nbsp; Hepatitis B and C are chronic infections and the most common causes of cirrhosis and liver cancer.</p><p><strong>(Choice C)</strong>&amp;nbsp; Rotavirus replicates in fully differentiated intestinal epithelial cells.&amp;nbsp; The infected and damaged enterocytes slough into the intestinal lumen, leading to profuse watery diarrhea.</p><p><strong>(Choice D)</strong>&amp;nbsp; Mature lymphocytes are a common site of viral replication (eg, human herpesvirus-6 [HHV-6], HIV).&amp;nbsp; HHV-6 causes roseola infantum, which presents as high fever followed by a diffuse maculopapular rash in children age &amp;lt;2.</p><p><strong>(Choice E)</strong>&amp;nbsp; Many common viruses (eg, influenza, parainfluenza, rhinovirus) replicate within respiratory epithelial cells (but not their precursors).&amp;nbsp; Parainfluenza replication, for example, can be localized to only nasal and pharyngeal epithelium (eg, rhinorrhea, sore throat) or can spread more extensively, causing laryngotracheitis (croup), bronchiolitis, or pneumonia.&amp;nbsp;</p><p><strong>Educational objective:</strong><br>Erythema infectiosum (fifth disease) is caused by parvovirus B19 and presents with a nonspecific prodrome (eg, malaise, fever, congestion) followed by a classic &amp;#x22;slapped-cheek&amp;#x22; facial rash and a lacy, reticular body rash.&amp;nbsp; Parvovirus is highly tropic for erythroid precursor cells and replicates predominantly in the bone marrow.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" id=\"1396\" src=\"image/U10732.png\" draggable=\"false\"></p><p class=\"ng-scope\"><strong>Erythema infectiosum</strong>, or fifth disease, is a <strong>benign</strong> childhood illness caused by <strong>parvovirus B19</strong>, a single-stranded DNA virus.&nbsp; After a 1-2 week incubation period, a <strong>nonspecific prodrome</strong> (eg, malaise, congestion, headache, fever) is followed by an erythematous rash on the cheeks.&nbsp; This \"<strong>slapped-cheek</strong>\" rash, as shown in the image above, usually spares the nasolabial folds and develops after the initial symptoms have resolved.&nbsp; A <a ng-click=\"showexhibit('326',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/U10916.png&quot;,&quot;title&quot;:&quot;U10916&quot;}]\">lacy, reticular rash</a> then follows, spreading over the trunk and extremities.&nbsp; The rash may result from immune complex deposition, the timing of which coincides with increasing levels of serum virus-specific IgM and IgG.</p><p class=\"ng-scope\">Parvovirus B19 replicates in <strong>erythrocyte</strong> precursors in the <strong>bone marrow</strong>.&nbsp; These cells express blood group P antigen (also known as globoside), which is the cellular receptor for parvovirus B19.&nbsp; Viral replication in the nucleus leads to cell lysis (ie, decreased reticulocytes and red blood cells).&nbsp; Acute and chronic sequelae of parvovirus B19 infection can occur in specific populations (eg, chronic anemia in immunosuppressed patients, aplastic crisis in those with underlying hematologic abnormalities such as sickle cell disease).</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Hepatitis A-E viruses replicate within mature hepatocytes and cause inflammation and cell necrosis.&nbsp; Hepatitis B and C are chronic infections and the most common causes of cirrhosis and liver cancer.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Rotavirus replicates in fully differentiated intestinal epithelial cells.&nbsp; The infected and damaged enterocytes slough into the intestinal lumen, leading to profuse watery diarrhea.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Mature lymphocytes are a common site of viral replication (eg, human herpesvirus-6 [HHV-6], HIV).&nbsp; HHV-6 causes roseola infantum, which presents as high fever followed by a diffuse maculopapular rash in children age &lt;2.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Many common viruses (eg, influenza, parainfluenza, rhinovirus) replicate within respiratory epithelial cells (but not their precursors).&nbsp; Parainfluenza replication, for example, can be localized to only nasal and pharyngeal epithelium (eg, rhinorrhea, sore throat) or can spread more extensively, causing laryngotracheitis (croup), bronchiolitis, or pneumonia.&nbsp;</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Erythema infectiosum (fifth disease) is caused by parvovirus B19 and presents with a nonspecific prodrome (eg, malaise, fever, congestion) followed by a classic \"slapped-cheek\" facial rash and a lacy, reticular body rash.&nbsp; Parvovirus is highly tropic for erythroid precursor cells and replicates predominantly in the bone marrow.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.st