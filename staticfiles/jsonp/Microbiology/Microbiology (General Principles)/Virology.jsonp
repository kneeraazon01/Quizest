stInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;3855&quot; src=&quot;https://www.uworld.com/media/L12431.jpg&quot; ></p><p><strong>Acute hepatitis B virus</strong> (HBV) infection is marked by the presence of HBsAg, HBeAg, and anti-HBc, and a lack of anti-HBs.&amp;nbsp; Infections are often asymptomatic, but many patients have a few months of jaundice, fatigue, nausea, and right upper&amp;ndash;quadrant discomfort.&amp;nbsp; Perinatal transmission is most common in high-prevalence countries (eg, Sub-Saharan Africa); in low-prevalence regions (eg, United States), most cases occur due to unprotected sex or intravenous drug use.</p><p>HBV is a hepadnavirus composed of:</p><ul><li><p>an <strong>outer lipid envelope</strong> that contains viral-encoded proteins (HBsAg) and host lipid components.</p></li><li><p>an <strong>icosahedral nucleocapsid core</strong> that contains a circular, partially double-stranded DNA genome and a DNA polymerase with reverse transcriptase activity.</p></li></ul><p>HBV binds to a bile salt transporter on the surface of hepatocytes and enters the cell.&amp;nbsp; After the virus is uncoated in the host cytoplasm, the single-stranded DNA portion of the viral genome is completed (repaired) by cellular DNA polymerases.&amp;nbsp; This generates <strong>double-stranded viral DNA</strong>, which is subsequently transcribed by host RNA polymerase into a <strong>+single-stranded RNA pregenome</strong>.&amp;nbsp; The +RNA template is then translated into the proteins that compose the virus (eg, envelope, core, polymerase); it is also converted by viral DNA polymerase/reverse transcriptase into the <strong>partially double-stranded DNA progeny</strong> of developing viral particles.</p><p><strong>(Choice A)</strong>&amp;nbsp; This replicative sequence characterizes adenovirus, herpesvirus, and poxvirus.</p><p><strong>(Choice C)</strong>&amp;nbsp; This replicative sequence characterizes parvovirus B19.</p><p><strong>(Choice D)</strong>&amp;nbsp; This replicative sequence characterizes the retroviruses (eg, HIV).</p><p><strong>(Choice E)</strong>&amp;nbsp; This replicative sequence characterizes poliovirus.</p><p><strong>(Choice F)</strong>&amp;nbsp; This replicative sequence characterizes influenza virus, measles virus, and rabies virus.</p><p><strong>Educational objective:</strong><br>The hepatitis B virus (HBV) replicates via the following sequence: double-stranded DNA &amp;#8594; +RNA template &amp;#8594; partially double-stranded DNA progeny.&amp;nbsp; Although it is a DNA virus, HBV uses reverse transcription to generate new viral DNA from a positive-sense RNA template.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"3855\" src=\"image/L12431.jpg\"></p><p class=\"ng-scope\"><strong>Acute hepatitis B virus</strong> (HBV) infection is marked by the presence of HBsAg, HBeAg, and anti-HBc, and a lack of anti-HBs.&nbsp; Infections are often asymptomatic, but many patients have a few months of jaundice, fatigue, nausea, and right upper\u2013quadrant discomfort.&nbsp; Perinatal transmission is most common in high-prevalence countries (eg, Sub-Saharan Africa); in low-prevalence regions (eg, United States), most cases occur due to unprotected sex or intravenous drug use.</p><p class=\"ng-scope\">HBV is a hepadnavirus composed of:</p><ul class=\"ng-scope\"><li><p>an <strong>outer lipid envelope</strong> that contains viral-encoded proteins (HBsAg) and host lipid components.</p></li><li><p>an <strong>icosahedral nucleocapsid core</strong> that contains a circular, partially double-stranded DNA genome and a DNA polymerase with reverse transcriptase activity.</p></li></ul><p class=\"ng-scope\">HBV binds to a bile salt transporter on the surface of hepatocytes and enters the cell.&nbsp; After the virus is uncoated in the host cytoplasm, the single-stranded DNA portion of the viral genome is completed (repaired) by cellular DNA polymerases.&nbsp; This generates <strong>double-stranded viral DNA</strong>, which is subsequently transcribed by host RNA polymerase into a <strong>+single-stranded RNA pregenome</strong>.&nbsp; The +RNA template is then translated into the proteins that compose the virus (eg, envelope, core, polymerase); it is also converted by viral DNA polymerase/reverse transcriptase into the <strong>partially double-stranded DNA progeny</strong> of developing viral particles.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; This replicative sequence characterizes adenovirus, herpesvirus, and poxvirus.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; This replicative sequence characterizes parvovirus B19.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; This replicative sequence characterizes the retroviruses (eg, HIV).</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; This replicative sequence characterizes poliovirus.</p><p class=\"ng-scope\"><strong>(Choice F)</strong>&nbsp; This replicative sequence characterizes influenza virus, measles virus, and rabies virus.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>The hepatitis B virus (HBV) replicates via the following sequence: double-stranded DNA \u2192 +RNA template \u2192 partially double-stranded DNA progeny.&nbsp; Although it is a DNA virus, HBV uses reverse transcription to generate new viral DNA from a positive-sense RNA template.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Microbiology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Microbiology (General Principles)</div>\n                                    <div sty