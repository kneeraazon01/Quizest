If: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; id=&quot;6857&quot;  src=&quot;https://www.uworld.com/media/L12267.jpg&quot; draggable=&quot;false&quot; ></p><p>The graph illustrates the pO<font size=&quot;2&quot;><sub>2</sub></font> of blood as it moves through the lungs and into the systemic circulation.&amp;nbsp; Blood in the pulmonary arteries comes from the systemic venous system and is normally deoxygenated.&amp;nbsp; As blood moves through the pulmonary capillaries, it becomes progressively more oxygenated until it equilibrates with the alveolar pO<font size=&quot;2&quot;><sub>2</sub></font> (~104 mm Hg when breathing room air).&amp;nbsp; This is represented by the curve&amp;#39;s first upward deflection and subsequent plateau.&amp;nbsp; However, blood in the left atrium and ventricle has a slightly lower pO<font size=&quot;2&quot;><sub>2</sub></font> (~100 mm Hg) than blood in the pulmonary capillaries.&amp;nbsp; This is due to the <strong>mixing of deoxygenated blood</strong> with oxygenated blood from the pulmonary veins and is represented by the downward deflection identified by the arrow.&amp;nbsp; This deoxygenated blood comes from the following sources:</p><ol><li><p>The <strong>bronchial arteries</strong> carry blood to the bronchi and bronchioles and, together with the pulmonary artery, form the dual blood supply to the lungs.&amp;nbsp; The <strong>bronchial veins</strong> return only a portion of this blood to the right heart via the azygos and hemi-azygos veins; most of the blood supplied by the bronchial arteries returns to the left heart via the <strong>pulmonary veins</strong>.</p></li><li><p>Small cardiac (<strong>thebesian</strong>) veins that drain into the left atrium and ventricle also contribute to normal anatomic shunting.</p></li></ol><p><strong>(Choice A)</strong>&amp;nbsp; Hypoxia due to a diffusion limitation frequently results from pulmonary fibrosis, acute respiratory distress syndrome, emphysema, or hyaline membrane disease of the infant.&amp;nbsp; In diffusion-limited gas exchange, the blood pO<font size=&quot;2&quot;><sub>2</sub></font> does not equilibrate with the alveolar pO<font size=&quot;2&quot;><sub>2</sub></font> by the end of its traversal through the alveolar capillaries.</p><p><strong>(Choice B)</strong>&amp;nbsp; Dead space ventilation (wasted ventilation) refers to the volume of inspired air that does not participate in gas exchange.&amp;nbsp; Anatomic dead space refers to the volume of air that remains in the conducting parts of the airways, such as the bronchi and bronchioles.&amp;nbsp; Alveolar dead space refers to the volume of air in the alveoli that cannot participate in gas exchange due to poor perfusion.</p><p><strong>(Choice D)</strong>&amp;nbsp; Perfusion-limited gas exchange occurs in the normal physiologic resting state, as equilibration of the blood with the alveolar gas is complete by the time the blood traverses one-third of the total pulmonary capillary length.</p><p><strong>(Choice E)</strong>&amp;nbsp; The <a ng-click=&quot;showexhibit('1588',true);&quot;>hemoglobin dissociation curve</a> shifts to the right in the systemic capillaries to increase oxygen delivery to the body tissues.&amp;nbsp; Increases in pCO<font size=&quot;2&quot;><sub>2</sub></font>, temperature, and 2,3-diphosphoglycerate concentration cause right-shifting of the curve; the opposite conditions cause a left shift and favor oxygen loading in the lungs.</p><p><strong>Educational objective:</strong><br>The pO<font size=&quot;2&quot;><sub>2</sub></font> in the left atrium and ventricle is lower than that in the pulmonary capillaries due to mixing of oxygenated blood from the pulmonary veins with deoxygenated blood from the bronchial circulation and thebesian veins.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" id=\"6857\" src=\"image/L12267.jpg\" draggable=\"false\"></p><p class=\"ng-scope\">The graph illustrates the pO<font size=\"2\"><sub>2</sub></font> of blood as it moves through the lungs and into the systemic circulation.&nbsp; Blood in the pulmonary arteries comes from the systemic venous system and is normally deoxygenated.&nbsp; As blood moves through the pulmonary capillaries, it becomes progressively more oxygenated until it equilibrates with the alveolar pO<font size=\"2\"><sub>2</sub></font> (~104 mm Hg when breathing room air).&nbsp; This is represented by the curve's first upward deflection and subsequent plateau.&nbsp; However, blood in the left atrium and ventricle has a slightly lower pO<font size=\"2\"><sub>2</sub></font> (~100 mm Hg) than blood in the pulmonary capillaries.&nbsp; This is due to the <strong>mixing of deoxygenated blood</strong> with oxygenated blood from the pulmonary veins and is represented by the downward deflection identified by the arrow.&nbsp; This deoxygenated blood comes from the following sources:</p><ol class=\"ng-scope\"><li><p>The <strong>bronchial arteries</strong> carry blood to the bronchi and bronchioles and, together with the pulmonary artery, form the dual blood supply to the lungs.&nbsp; The <strong>bronchial veins</strong> return only a portion of this blood to the right heart via the azygos and hemi-azygos veins; most of the blood supplied by the bronchial arteries returns to the left heart via the <strong>pulmonary veins</strong>.</p></li><li><p>Small cardiac (<strong>thebesian</strong>) veins that drain into the left atrium and ventricle also contribute to normal anatomic shunting.</p></li></ol><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Hypoxia due to a diffusion limitation frequently results from pulmonary fibrosis, acute respiratory distress syndrome, emphysema, or hyaline membrane disease of the infant.&nbsp; In diffusion-limited gas exchange, the blood pO<font size=\"2\"><sub>2</sub></font> does not equilibrate with the alveolar pO<font size=\"2\"><sub>2</sub></font> by the end of its traversal through the alveolar capillaries.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Dead space ventilation (wasted ventilation) refers to the volume of inspired air that does not participate in gas exchange.&nbsp; Anatomic dead space refers to the volume of air that remains in the conducting parts of the airways, such as the bronchi and bronchioles.&nbsp; Alveolar dead space refers to the volume of air in the alveoli that cannot participate in gas exchange due to poor perfusion.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Perfusion-limited gas exchange occurs in the normal physiologic resting state, as equilibration of the blood with the alveolar gas is complete by the time the blood traverses one-third of the total pulmonary capillary length.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; The <a ng-click=\"showexhibit('1588',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L14561.jpg&quot;,&quot;title&quot;:&quot;L14561&quot;}]\">hemoglobin dissociation curve</a> shifts to the right in the systemic capillaries to increase oxygen delivery to the body tissues.&nbsp; Increases in pCO<font size=\"2\"><sub>2</sub></font>, temperature, and 2,3-diphosphoglycerate concentration cause right-shifting of the curve; the opposite conditions cause a left shift and favor oxygen loading in the lungs.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>The pO<font size=\"2\"><sub>2</sub></font> in the left atrium and ventricle is lower than that in the pulmonary capillaries due to mixing of oxygenated blood from the pulmonary veins with deoxygenated blood from the bronchial circulation and thebesian veins.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Physiology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pulmonary &amp; Critical Care</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Respiratory physiology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
