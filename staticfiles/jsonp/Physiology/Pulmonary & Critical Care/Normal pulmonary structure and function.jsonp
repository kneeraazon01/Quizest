(currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>E.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight5\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">-10 cm H<font size=\"2\"><sub>2</sub></font>O</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;18596&quot; src=&quot;https://www.uworld.com/media/L43530.jpg&quot; ></p><p>On the above pressure-volume curve, collapsing pressure is positive and expanding pressure is negative.&amp;nbsp; The <strong>lungs </strong>have a <strong>tendency to collapse</strong> at all lung volumes; therefore, the entire lung curve is located in the positive region.&amp;nbsp; In contrast, the resting <strong>chest wall</strong> has a <strong>tendency to expand</strong> at all but very high lung volumes; therefore, most of the chest wall curve is located in the negative region.&amp;nbsp; The point where the collapsing force of the lungs is equivalent to the expanding force of the chest wall is the <strong>resting equilibrium</strong> of the respiratory system (ie, <a ng-click=&quot;showexhibit('1156',true);&quot;>end-tidal expiration</a>), represented by the black dot); the alveolar pressure at this point is equivalent to atmospheric pressure (ie, 0 cm H<font size=&quot;2&quot;><sub>2</sub></font>O) and the lung volume is the <strong>functional residual capacity</strong> (FRC).</p><p>Compliance is defined as change in volume per change in pressure and is represented by the slope of the pressure-volume curve.&amp;nbsp; A highly compliant container is able to stretch to accommodate large increases in volume with little change in pressure, demonstrating a steep compliance curve.&amp;nbsp; Generally, lung compliance is greatest around the FRC and decreases at very high and very low lung volumes (note that surface tension causes compliance to <a ng-click=&quot;showexhibit('38238',true);&quot;>differ</a> during inspiration and expiration).&amp;nbsp; The chest wall has low compliance at low lung volumes and becomes more compliant as lung volume increases with inspiration.&amp;nbsp; The compliance of the respiratory system as a whole is the combined compliance of the lungs and chest wall (represented by the blue curve).</p><p><strong>Intrapleural pressures</strong> are not represented on the lung pressure-volume curve.&amp;nbsp; The expanding force of the chest wall working in opposition to the collapsing force of the lungs creates <strong>negative intrapleural pressure</strong> throughout the <a ng-click=&quot;showexhibit('38239',true);&quot;>respiratory cycle</a>.&amp;nbsp; Inspiration is driven by active expansion of the chest wall, which generates even greater intrapleural negative pressure and pulls the lungs outward.&amp;nbsp; Intrapleural negative pressure peaks at maximal inspiration at a value of approximately -8 cm H<font size=&quot;2&quot;><sub>2</sub></font>O.&amp;nbsp; During passive expiration the chest wall relaxes and the respiratory system returns to its equilibrium position, in which intrapleural pressure is approximately <strong>-5 cm H<font size=&quot;2&quot;><sub>2</sub></font>O</strong>.</p><p><strong>Educational objective:</strong><br>The lungs generate a collapsing force and the chest wall generates an expanding force; the point at which these opposing forces are equivalent is the resting equilibrium of the respiratory system, where alveolar pressure is equal to atmospheric pressure (ie, 0 cm H<font size=&quot;2&quot;><sub>2</sub></font>O) and lung volume is the functional residual capacity.&amp;nbsp; The opposing forces create negative intrapleural pressure throughout the respiratory cycle; intrapleural pressure at resting equilibrium (ie, end-tidal expiration) is approximately -5 cm H<font size=&quot;2&quot;><sub>2</sub></font>O.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"18596\" src=\"image/L43530.jpg\"></p><p class=\"ng-scope\">On the above pressure-volume curve, collapsing pressure is positive and expanding pressure is negative.&nbsp; The <strong>lungs </strong>have a <strong>tendency to collapse</strong> at all lung volumes; therefore, the entire lung curve is located in the positive region.&nbsp; In contrast, the resting <strong>chest wall</strong> has a <strong>tendency to expand</strong> at all but very high lung volumes; therefore, most of the chest wall curve is located in the negative region.&nbsp; The point where the collapsing force of the lungs is equivalent to the expanding force of the chest wall is the <strong>resting equilibrium</strong> of the respiratory system (ie, <a ng-click=\"showexhibit('1156',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L16332.jpg&quot;,&quot;title&quot;:&quot;L16332&quot;}]\">end-tidal expiration</a>), represented by the black dot); the alveolar pressure at this point is equivalent to atmospheric pressure (ie, 0 cm H<font size=\"2\"><sub>2</sub></font>O) and the lung volume is the <strong>functional residual capacity</strong> (FRC).</p><p class=\"ng-scope\">Compliance is defined as change in volume per change in pressure and is represented by the slope of the pressure-volume curve.&nbsp; A highly compliant container is able to stretch to accommodate large increases in volume with little change in pressure, demonstrating a steep compliance curve.&nbsp; Generally, lung compliance is greatest around the FRC and decreases at very high and very low lung volumes (note that surface tension causes compliance to <a ng-click=\"showexhibit('38238',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L63706.jpg&quot;,&quot;title&quot;:&quot;L63706&quot;}]\">differ</a> during inspiration and expiration).&nbsp; The chest wall has low compliance at low lung volumes and becomes more compliant as lung volume increases with inspiration.&nbsp; The compliance of the respiratory system as a whole is the combined compliance of the lungs and chest wall (represented by the blue curve).</p><p class=\"ng-scope\"><strong>Intrapleural pressures</strong> are not represented on the lung pressure-volume curve.&nbsp; The expanding force of the chest wall working in opposition to the collapsing force of the lungs creates <strong>negative intrapleural pressure</strong> throughout the <a ng-click=\"showexhibit('38239',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L62395.jpg&quot;,&quot;title&quot;:&quot;L62395&quot;}]\">respiratory cycle</a>.&nbsp; Inspiration is driven by active expansion of the chest wall, which generates even greater intrapleural negative pressure and pulls the lungs outward.&nbsp; Intrapleural negative pressure peaks at maximal inspiration at a value of approximately -8 cm H<font size=\"2\"><sub>2</sub></font>O.&nbsp; During passive expiration the chest wall relaxes and the respiratory system returns to its equilibrium position, in which intrapleural pressure is approximately <strong>-5 cm H<font size=\"2\"><sub>2</sub></font>O</strong>.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>The lungs generate a collapsing force and the chest wall generates an expanding force; the point at which these opposing forces are equivalent is the resting equilibrium of the respiratory system, where alveolar pressure is equal to atmospheric pressure (ie, 0 cm H<font size=\"2\"><sub>2</sub></font>O) and lung volume is the functional residual capacity.&nbsp; The opposing forces create negative intrapleural pressure throughout the respiratory cycle; intrapleural pressure at resting equilibrium (ie, end-tidal expiration) is approximately -5 cm H<font size=\"2\"><sub>2</sub></font>O.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/32119404\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/32119404\"><span compiledata=\"\" htmlcontent=\"Physiology, lung compliance.\" class=\"ng-scope\"><p class=\"ng-scope\">Physiology, lung compliance.</p></span></a><!-- end ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal -->\n                                    <!-- ngIf: