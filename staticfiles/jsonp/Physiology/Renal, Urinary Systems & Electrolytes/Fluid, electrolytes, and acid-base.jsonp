heme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;1124&quot; src=&quot;https://www.uworld.com/media/L17487.jpg&quot; ></p><p>This patient&amp;#39;s elevated pH and low PaCO<font size=&quot;2&quot;><sub>2</sub></font> (&amp;lt;40 mm Hg) are suggestive of <strong>primary respiratory alkalosis</strong>.&amp;nbsp; Normally, serum bicarbonate is expected to drop in response.&amp;nbsp; However, a bicarbonate of 13 mEq/L is much lower than the <a ng-click=&quot;showexhibit('1216',true);&quot;>expected secondary compensation</a>, which indicates a second primary acid-base disorder.&amp;nbsp; The lower-than-expected serum bicarbonate with a high <a ng-click=&quot;showexhibit('3915',true);&quot;>anion gap</a> [139 &amp;#8722; (98 + 13) = 28] is consistent with a concomitant <strong>primary anion gap metabolic acidosis</strong>.&amp;nbsp; This mixed respiratory alkalosis and anion gap metabolic acidosis is most likely the result of <strong>salicylate poisoning</strong>:</p><ul><li>Primary respiratory alkalosis is due to direct stimulation of the medullary respiratory center by excess salicylate, resulting in both an <strong>increased respiratory rate</strong> and <strong>tidal volume</strong> (ie, increased minute ventilation).</li><li>Primary metabolic acidosis occurs mostly because salicylate uncouples oxidative phosphorylation, which results in <strong>increased production of lactic acid</strong> and ketoacids in peripheral tissues.&amp;nbsp; Excess salicylic acid in the serum also makes a minor contribution.&amp;nbsp; The ionized form of these compounds also increases the anion gap (unmeasured anions).</li></ul><p>The pH is typically near or within normal range in patients with salicylate poisoning because of the opposing influences of the respiratory alkalosis and metabolic acidosis.</p><p><strong>(Choice A)</strong>&amp;nbsp; Primary metabolic acidosis and primary metabolic alkalosis can simultaneously develop in the setting of 2 disparate pathologic processes (eg, renal failure and vomiting).&amp;nbsp; The pH and bicarbonate levels can be increased, normal, or decreased, depending on the severity of the metabolic acidosis compared with the metabolic alkalosis.</p><p><strong>(Choice B)</strong>&amp;nbsp; Primary metabolic acidosis with primary respiratory acidosis (eg, sepsis and hypoventilatory respiratory failure) typically leads to significant acidemia (very low pH) with low bicarbonate and high PaCO<font size=&quot;2&quot;><sub>2</sub></font>.</p><p><strong>(Choice D)</strong>&amp;nbsp; Primary metabolic alkalosis with primary respiratory acidosis (eg, vomiting and hypoventilation due to opioid overdose) is expected to show elevated bicarbonate and PaCO<font size=&quot;2&quot;><sub>2</sub></font>; the pH can reflect alkalemia or acidemia, depending on the relative severity of the 2 disturbances.</p><p><strong>(Choice E)</strong>&amp;nbsp; Primary metabolic alkalosis with primary respiratory alkalosis (eg, vomiting and hyperventilation due to anxiety) is expected to cause significant alkalemia (very high pH) with high bicarbonate and low PaCO<font size=&quot;2&quot;><sub>2</sub></font>.</p><p><strong>Educational objective:</strong><br>Salicylate poisoning causes mixed primary respiratory alkalosis and primary anion gap metabolic acidosis.&amp;nbsp; Mixed acid-base disturbances can be recognized by inappropriate secondary compensation for one of the primary disturbances, indicating that an additional primary disturbance must be present.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"1124\" src=\"image/L17487.jpg\"></p><p class=\"ng-scope\">This patient's elevated pH and low PaCO<font size=\"2\"><sub>2</sub></font> (&lt;40 mm Hg) are suggestive of <strong>primary respiratory alkalosis</strong>.&nbsp; Normally, serum bicarbonate is expected to drop in response.&nbsp; However, a bicarbonate of 13 mEq/L is much lower than the <a ng-click=\"showexhibit('1216',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/46385.html&quot;,&quot;title&quot;:&quot;46385&quot;}]\">expected secondary compensation</a>, which indicates a second primary acid-base disorder.&nbsp; The lower-than-expected serum bicarbonate with a high <a ng-click=\"showexhibit('3915',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/42521.html&quot;,&quot;title&quot;:&quot;42521&quot;}]\">anion gap</a> [139 \u2212 (98 + 13) = 28] is consistent with a concomitant <strong>primary anion gap metabolic acidosis</strong>.&nbsp; This mixed respiratory alkalosis and anion gap metabolic acidosis is most likely the result of <strong>salicylate poisoning</strong>:</p><ul class=\"ng-scope\"><li>Primary respiratory alkalosis is due to direct stimulation of the medullary respiratory center by excess salicylate, resulting in both an <strong>increased respiratory rate</strong> and <strong>tidal volume</strong> (ie, increased minute ventilation).</li><li>Primary metabolic acidosis occurs mostly because salicylate uncouples oxidative phosphorylation, which results in <strong>increased production of lactic acid</strong> and ketoacids in peripheral tissues.&nbsp; Excess salicylic acid in the serum also makes a minor contribution.&nbsp; The ionized form of these compounds also increases the anion gap (unmeasured anions).</li></ul><p class=\"ng-scope\">The pH is typically near or within normal range in patients with salicylate poisoning because of the opposing influences of the respiratory alkalosis and metabolic acidosis.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Primary metabolic acidosis and primary metabolic alkalosis can simultaneously develop in the setting of 2 disparate pathologic processes (eg, renal failure and vomiting).&nbsp; The pH and bicarbonate levels can be increased, normal, or decreased, depending on the severity of the metabolic acidosis compared with the metabolic alkalosis.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Primary metabolic acidosis with primary respiratory acidosis (eg, sepsis and hypoventilatory respiratory failure) typically leads to significant acidemia (very low pH) with low bicarbonate and high PaCO<font size=\"2\"><sub>2</sub></font>.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Primary metabolic alkalosis with primary respiratory acidosis (eg, vomiting and hypoventilation due to opioid overdose) is expected to show elevated bicarbonate and PaCO<font size=\"2\"><sub>2</sub></font>; the pH can reflect alkalemia or acidemia, depending on the relative severity of the 2 disturbances.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Primary metabolic alkalosis with primary respiratory alkalosis (eg, vomiting and hyperventilation due to anxiety) is expected to cause significant alkalemia (very high pH) with high bicarbonate and low PaCO<font size=\"2\"><sub>2</sub></font>.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Salicylate poisoning causes mixed primary respiratory alkalosis and primary anion gap metabolic acidosis.&nbsp; Mixed acid-base disturbances can be recognized by inappropriate secondary compensation for one of the primary disturbances, indicating that an additional primary disturbance must be present.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Physiology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Renal, Urinary Systems &amp; Electrolytes</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Salicylate poisoning</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
