onstants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p>Humans consume approximately 2 L of fluid each day and secrete an additional 7 L of fluid from upper digestive organs (eg, salivary glands, stomach, pancreas, gallbladder).&amp;nbsp; Nearly all of this fluid is reabsorbed from the gastrointestinal lumen by the small intestine epithelium.&amp;nbsp; The <strong>osmotic gradient</strong> for water reabsorption is largely established by the <strong>active cotransport of sodium</strong> into the apical end of enterocytes coupled with chloride, glucose, or amino acids.&amp;nbsp; Sodium is then transported out of the basolateral end of the enterocyte (sodium/potassium ATPase), which pulls water down its osmotic gradient from the gastrointestinal lumen through the tight junctions between enterocytes and into the lamina propria (and ultimately the bloodstream).</p><p>This young patient who recently began going to day care likely has <strong>acute infectious diarrhea</strong> of a viral etiology (eg, rotavirus, norovirus).&amp;nbsp; Infections that target the small intestine cause watery diarrhea by blocking the active transport of sodium chloride into the enterocyte or by increasing chloride excretion into the gastrointestinal lumen (mediated by elevated intracellular cyclic AMP, cyclic GMP, or calcium).&amp;nbsp; In this setting, oral rehydration with a hypotonic, <strong>equimolar sodium-glucose solution</strong> maximizes water and sodium reabsorption by the remaining functional enterocytes, and is the most effective oral therapy for maintaining volume and electrolyte status in children with acute infectious diarrhea.</p><p><strong>(Choice A)</strong>&amp;nbsp; Chicken broth with added salt is a hyperosmolar solution that can cause hypernatremia and is, therefore, not recommended.</p><p><strong>(Choice B)</strong>&amp;nbsp; Diluted or undiluted cow&amp;#39;s milk is not an effective rehydration solution during acute diarrhea due to impaired digestion of milk components, which can promote osmotic diarrhea.</p><p><strong>(Choice C)</strong>&amp;nbsp; Filtered water alone is not usually sufficient to prevent dehydration in patients with acute infectious diarrhea due to the increased solute load in the gastrointestinal lumen, which causes most of the free water to be retained in the intestine and excreted in the feces.</p><p><strong>(Choices D and F)</strong>&amp;nbsp; Although sports drinks and sugary fruit juices contain glucose, they often have significantly lower sodium concentrations and high osmolality.&amp;nbsp; This ultimately reduces water reabsorption and may promote osmotic diarrhea.</p><p><strong>Educational objective:</strong><br>Children with watery diarrhea should be treated with oral rehydration solutions containing hypotonic, equimolar concentrations of sodium and glucose to help prevent dehydration and electrolyte abnormalities.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\">Humans consume approximately 2 L of fluid each day and secrete an additional 7 L of fluid from upper digestive organs (eg, salivary glands, stomach, pancreas, gallbladder).&nbsp; Nearly all of this fluid is reabsorbed from the gastrointestinal lumen by the small intestine epithelium.&nbsp; The <strong>osmotic gradient</strong> for water reabsorption is largely established by the <strong>active cotransport of sodium</strong> into the apical end of enterocytes coupled with chloride, glucose, or amino acids.&nbsp; Sodium is then transported out of the basolateral end of the enterocyte (sodium/potassium ATPase), which pulls water down its osmotic gradient from the gastrointestinal lumen through the tight junctions between enterocytes and into the lamina propria (and ultimately the bloodstream).</p><p class=\"ng-scope\">This young patient who recently began going to day care likely has <strong>acute infectious diarrhea</strong> of a viral etiology (eg, rotavirus, norovirus).&nbsp; Infections that target the small intestine cause watery diarrhea by blocking the active transport of sodium chloride into the enterocyte or by increasing chloride excretion into the gastrointestinal lumen (mediated by elevated intracellular cyclic AMP, cyclic GMP, or calcium).&nbsp; In this setting, oral rehydration with a hypotonic, <strong>equimolar sodium-glucose solution</strong> maximizes water and sodium reabsorption by the remaining functional enterocytes, and is the most effective oral therapy for maintaining volume and electrolyte status in children with acute infectious diarrhea.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Chicken broth with added salt is a hyperosmolar solution that can cause hypernatremia and is, therefore, not recommended.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Diluted or undiluted cow's milk is not an effective rehydration solution during acute diarrhea due to impaired digestion of milk components, which can promote osmotic diarrhea.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Filtered water alone is not usually sufficient to prevent dehydration in patients with acute infectious diarrhea due to the increased solute load in the gastrointestinal lumen, which causes most of the free water to be retained in the intestine and excreted in the feces.</p><p class=\"ng-scope\"><strong>(Choices D and F)</strong>&nbsp; Although sports drinks and sugary fruit juices contain glucose, they often have significantly lower sodium concentrations and high osmolality.&nbsp; This ultimately reduces water reabsorption and may promote osmotic diarrhea.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Children with watery diarrhea should be treated with oral rehydration solutions containing hypotonic, equimolar concentrations of sodium and glucose to help prevent dehydration and electrolyte abnormalities.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/14627948\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/1462