bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Viral infection\u2013induced cardiac injury</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p>This patient most likely has <strong>early disseminated Lyme disease</strong>.&amp;nbsp; Lyme disease is caused by the bacterial spirochete <em>Borrelia burgdorferi</em>, which is transmitted by the <em>Ixodes</em> tick that is endemic to the northeastern and parts of the midwestern <a ng-click=&quot;showexhibit('514',true);&quot;>United States</a>.&amp;nbsp; Most cases initially present with a non-itchy, bull&amp;#39;s-eye-shaped rash, known as <a ng-click=&quot;showexhibit('21263',true);&quot;>erythema migrans</a>, at the site of infection.&amp;nbsp; Left untreated for several weeks or more, a small percentage of those infected will develop cardiac involvement <strong>(Lyme carditis)</strong>.</p><p>Lyme carditis most commonly manifests with varying degrees of <strong>atrioventricular (AV) conduction block</strong>.&amp;nbsp; Many patients remain asymptomatic; however, patients who develop complete AV block are likely to have symptoms of dyspnea, <strong>lightheadedness</strong>, or <strong>syncope</strong>.&amp;nbsp; Treatment for symptomatic AV block due to Lyme carditis typically requires hospitalization with temporary pacing and the administration of intravenous antibiotics (ceftriaxone is the drug of choice).</p><p><strong>(Choice B)</strong>&amp;nbsp; Granulomatous myocardial inflammation occurs in cardiac sarcoidosis, and AV conduction block is the most common disease manifestation.&amp;nbsp; However, this patient&amp;#39;s recent rash and residence in an endemic area make Lyme carditis more likely.</p><p><strong>(Choice C)</strong>&amp;nbsp; A missense mutation affecting the cardiac sarcomere protein beta-myosin heavy chain is a common cause of hypertrophic cardiomyopathy.&amp;nbsp; Myocardial disarray creates a propensity for ventricular arrhythmias, but AV conduction block is not typical.</p><p><strong>(Choice D)</strong>&amp;nbsp; Congenital long QT syndrome results from a mutation affecting cardiomyocyte potassium channels.&amp;nbsp; It can cause syncope or sudden cardiac death, typically via the triggering of <a ng-click=&quot;showexhibit('1740',true);&quot;>torsades de pointes</a> (polymorphic ventricular tachycardia).&amp;nbsp; AV conduction block would be an unusual manifestation.</p><p><strong>(Choice E)</strong>&amp;nbsp; Viral infection of the heart typically manifests as myocarditis.&amp;nbsp; The most common symptomatic presentation is decompensated heart failure due to dilated cardiomyopathy.&amp;nbsp; AV conduction block is not typical.</p><p><strong>Educational objective:</strong><br>Early disseminated Lyme disease can have cardiac involvement (Lyme carditis) that most commonly manifests with varying degrees of atrioventricular (AV) conduction block.&amp;nbsp; Patients may be asymptomatic, but those with complete AV conduction block are likely to have dyspnea, lightheadedness, or syncope.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\">This patient most likely has <strong>early disseminated Lyme disease</strong>.&nbsp; Lyme disease is caused by the bacterial spirochete <em>Borrelia burgdorferi</em>, which is transmitted by the <em>Ixodes</em> tick that is endemic to the northeastern and parts of the midwestern <a ng-click=\"showexhibit('514',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L11968.jpg&quot;,&quot;title&quot;:&quot;L11968&quot;}]\">United States</a>.&nbsp; Most cases initially present with a non-itchy, bull's-eye-shaped rash, known as <a ng-click=\"showexhibit('21263',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/U44732.jpg&quot;,&quot;title&quot;:&quot;U44732&quot;}]\">erythema migrans</a>, at the site of infection.&nbsp; Left untreated for several weeks or more, a small percentage of those infected will develop cardiac involvement <strong>(Lyme carditis)</strong>.</p><p class=\"ng-scope\">Lyme carditis most commonly manifests with varying degrees of <strong>atrioventricular (AV) conduction block</strong>.&nbsp; Many patients remain asymptomatic; however, patients who develop complete AV block are likely to have symptoms of dyspnea, <strong>lightheadedness</strong>, or <strong>syncope</strong>.&nbsp; Treatment for symptomatic AV block due to Lyme carditis typically requires hospitalization with temporary pacing and the administration of intravenous antibiotics (ceftriaxone is the drug of choice).</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Granulomatous myocardial inflammation occurs in cardiac sarcoidosis, and AV conduction block is the most common disease manifestation.&nbsp; However, this patient's recent rash and residence in an endemic area make Lyme carditis more likely.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; A missense mutation affecting the cardiac sarcomere protein beta-myosin heavy chain is a common cause of hypertrophic cardiomyopathy.&nbsp; Myocardial disarray creates a propensity for ventricular arrhythmias, but AV conduction block is not typical.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Congenital long QT syndrome results from a mutation affecting cardiomyocyte potassium channels.&nbsp; It can cause syncope or sudden cardiac death, typically via the triggering of <a ng-click=\"showexhibit('1740',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/U21069.jpg&quot;,&quot;title&quot;:&quot;U21069&quot;}]\">torsades de pointes</a> (polymorphic ventricular tachycardia).&nbsp; AV conduction block would be an unusual manifestation.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Viral infection of the heart typically manifests as myocarditis.&nbsp; The most common symptomatic presentation is decompensated heart failure due to dilated cardiomyopathy.&nbsp; AV conduction block is not typical.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Early disseminated Lyme disease can have cardiac involvement (Lyme carditis) that most commonly manifests with varying degrees of atrioventricular (AV) conduction block.&nbsp; Patients may be asymptomatic, but those with complete AV conduction block are likely to have dyspnea, lightheadedness, or syncope.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pathology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Cardiovascular System</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Lyme disease</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
