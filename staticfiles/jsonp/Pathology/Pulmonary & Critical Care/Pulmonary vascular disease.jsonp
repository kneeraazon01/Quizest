 -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot;  draggable=&quot;false&quot; id=&quot;21640&quot; src=&quot;https://www.uworld.com/media/highresdefault/L47469.jpg&quot;  ></p><p>This patient displays Raynaud phenomenon (cold-induced painful digital vasospasm), sclerodactyly (skin tightening of the hands/ feet), telangiectasias, gastroesophageal dysfunction.&amp;nbsp; These findings suggest <strong>systemic sclerosis</strong> (ie, scleroderma), specifically the <a ng-click=&quot;showexhibit('32640',true);&quot;>limited cutaneous</a> variant, or <strong>CREST</strong> (Calcinosis cutis, Raynaud phenomenon, Esophageal dysmotility, Sclerodactyly, Telangiectasia) syndrome.</p><p>Systemic sclerosis frequently involves multisystem vasculopathy affecting several vessel beds, including distal extremities (digital ischemia), kidneys (renal crisis), and pulmonary arteries.&amp;nbsp; <strong>Pulmonary arterial hypertension</strong> (PAH), a leading cause of mortality in patients with systemic sclerosis, is driven by abnormal inflammatory and fibroproliferative signaling (eg, transforming growth factor beta), leading to neointimal/medial thickening and luminal narrowing.&amp;nbsp; Histologically, vessels have a concentric onion-skin, sclerotic appearance.</p><p>Pulmonary vascular remodeling is progressive and relentless.&amp;nbsp; Over time, the right ventricle cannot pump against the increased afterload, leading to <strong>right ventricular failure</strong>.&amp;nbsp; In this patient, PAH is suggested by a prominent pulmonic component of S2 (ie, <strong>loud P2</strong>, reflecting high pressure in pulmonary artery) with signs of right-sided venous congestion (eg, hepatomegaly, lower extremity edema).</p><p><strong>(Choice A)</strong>&amp;nbsp; Systemic sclerosis can induce extrapulmonary restriction via chest wall skin tightening that limits thoracic expansion.&amp;nbsp; Clear lungs are expected on auscultation and imaging; however, pulmonary function testing would show a restrictive pattern (ie, low FVC).</p><p><strong>(Choice B)</strong>&amp;nbsp; Fibrotic interstitial lung disease is another <a ng-click=&quot;showexhibit('55273',true);&quot;>pulmonary complication</a> of systemic sclerosis (particularly the diffuse cutaneous variant), characterized by lung parenchymal inflammation and collagen deposition.&amp;nbsp; However, inspiratory crackles, reticular interstitial thickening on chest x-ray, and a restrictive pattern on pulmonary function testing are expected.</p><p><strong>(Choice C)</strong>&amp;nbsp; Pericardial fibrosis, occasionally a complication of systemic sclerosis, typically manifests as right-sided heart failure due to impaired diastolic filling of the right ventricle.&amp;nbsp; However, this impaired filling prevents elevation of pulmonary artery pressure; therefore, a loud P2 is not expected.</p><p><strong>(Choice E)</strong>&amp;nbsp; Pulmonic stenosis can lead to right-sided heart failure.&amp;nbsp; However, right ventricular outflow obstruction leads to low pulmonary artery pressures, resulting in a soft P2.&amp;nbsp; In addition, pulmonic stenosis (almost always a congenital lesion) does not explain the additional findings of CREST syndrome.</p><p><strong>Educational objective:</strong><br>Pulmonary arterial hypertension, a vascular complication of systemic sclerosis, involves fibroproliferative remodeling of the vessel wall, leading to increased pulmonary vascular resistance and right-sided heart failure.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"21640\" src=\"image/L47469.jpg\"></p><p class=\"ng-scope\">This patient displays Raynaud phenomenon (cold-induced painful digital vasospasm), sclerodactyly (skin tightening of the hands/ feet), telangiectasias, gastroesophageal dysfunction.&nbsp; These findings suggest <strong>systemic sclerosis</strong> (ie, scleroderma), specifically the <a ng-click=\"showexhibit('32640',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/58635.html&quot;,&quot;title&quot;:&quot;58635&quot;}]\">limited cutaneous</a> variant, or <strong>CREST</strong> (Calcinosis cutis, Raynaud phenomenon, Esophageal dysmotility, Sclerodactyly, Telangiectasia) syndrome.</p><p class=\"ng-scope\">Systemic sclerosis frequently involves multisystem vasculopathy affecting several vessel beds, including distal extremities (digital ischemia), kidneys (renal crisis), and pulmonary arteries.&nbsp; <strong>Pulmonary arterial hypertension</strong> (PAH), a leading cause of mortality in patients with systemic sclerosis, is driven by abnormal inflammatory and fibroproliferative signaling (eg, transforming growth factor beta), leading to neointimal/medial thickening and luminal narrowing.&nbsp; Histologically, vessels have a concentric onion-skin, sclerotic appearance.</p><p class=\"ng-scope\">Pulmonary vascular remodeling is progressive and relentless.&nbsp; Over time, the right ventricle cannot pump against the increased afterload, leading to <strong>right ventricular failure</strong>.&nbsp; In this patient, PAH is suggested by a prominent pulmonic component of S2 (ie, <strong>loud P2</strong>, reflecting high pressure in pulmonary artery) with signs of right-sided venous congestion (eg, hepatomegaly, lower extremity edema).</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Systemic sclerosis can induce extrapulmonary restriction via chest wall skin tightening that limits thoracic expansion.&nbsp; Clear lungs are expected on auscultation and imaging; however, pulmonary function testing would show a restrictive pattern (ie, low FVC).</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Fibrotic interstitial lung disease is another <a ng-click=\"showexhibit('55273',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/31117.html&quot;,&quot;title&quot;:&quot;31117&quot;}]\">pulmonary complication</a> of systemic sclerosis (particularly the diffuse cutaneous variant), characterized by lung parenchymal inflammation and collagen deposition.&nbsp; However, inspiratory crackles, reticular interstitial thickening on chest x-ray, and a restrictive pattern on pulmonary function testing are expected.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Pericardial fibrosis, occasionally a complication of systemic sclerosis, typically manifests as right-sided heart failure due to impaired diastolic filling of the right ventricle.&nbsp; However, this impaired filling prevents elevation of pulmonary artery pressure; therefore, a loud P2 is not expected.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Pulmonic stenosis can lead to right-sided heart failure.&nbsp; However, right ventricular outflow obstruction leads to low pulmonary artery pressures, resulting in a soft P2.&nbsp; In addition, pulmonic stenosis (almost always a congenital lesion) does not explain the additional findings of CREST syndrome.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Pulmonary arterial hypertension, a vascular complication of systemic sclerosis, involves fibroproliferative remodeling of the vessel wall, leading to increased pulmonary vascular resistance and right-sided heart failure.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/31920409\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/31920409\"><span compiledata=\"\" htmlcontent=\"Pulmonary arterial hypertension in systemic sclerosis: challenges in diagnosis, screening, and treatment.\" class=\"ng-scope\"><p class=\"ng-scope\">Pulmonary arterial hypertension in systemic sclerosis: challenges in diagnosis, screening, and treatment.</p></span></a><!-- end ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal -->\n                                    <!-- ngIf: launchTestOptions.topLevelProductId == const