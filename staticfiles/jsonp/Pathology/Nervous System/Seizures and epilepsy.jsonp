eTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><div id=&quot;div_T59554&quot;><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; class=&quot;table-default-style table-header-footer-style&quot;><tbody><tr><td colspan=&quot;2&quot;><p align=&quot;center&quot;><strong>Temporal lobe epilepsy</strong></p></td></tr><tr><td><p align=&quot;center&quot;><strong>Etiology</strong></p></td><td><ul><li>Hippocampal sclerosis (most common)</li><li>Other: infection, head trauma, tumors, congenital malformations</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Manifestations</strong></p></td><td><ul><li>Focal seizures with impaired awareness</li><li>Secondary generalized seizures may occur</li><li>Aura and automatisms are common</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Diagnostic studies</strong></p></td><td><ul><li>EEG: epileptiform waves in the temporal region</li><li>MRI: Hippocampal atrophy</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Histopathology</strong></p></td><td><ul><li>Atrophy of the hippocampal neurons with reactive gliosis</li></ul></td></tr><tr><td colspan=&quot;2&quot;><p><strong>EEG</strong> = electroencephalography.</p></td></tr></tbody></table></div><p>This patient with recurrent seizures has an epileptogenic focus in the temporal lobe, <strong>suggesting temporal lobe epilepsy</strong> (TLE), the most common type of localization-related (ie, focal) epilepsy.&amp;nbsp; Localization epilepsies occur when partial seizures originate from one area of the brain and are usually due to an underlying structural disorder which serves as an epileptogenic focus.&amp;nbsp; In the case of TLE, this is most commonly due to <strong>hippocampal sclerosis</strong>, which is usually visible as <a ng-click=&quot;showexhibit('32714',true);&quot;>hippocampal atrophy</a> on MRI.</p><p>The pathogenesis of hippocampal sclerosis is incompletely understood but may in some cases be related to prior prolonged febrile seizures, particularly in patients with an underlying genetic predisposition.&amp;nbsp; This results in the characteristic histopathologic findings of <strong>atrophy of the hippocampal neurons</strong> with marked reactive gliosis (<strong>astrocyte proliferation</strong> in response to injury).&amp;nbsp; Surgical resection may reduce or cure seizures associated with TLE.</p><p><strong>(Choice B)</strong>&amp;nbsp; Hemosiderin-laden macrophages are associated with prior intracerebral hemorrhage, which may cause seizures but typically presents acutely with altered mental status and focal neurologic deficits.&amp;nbsp; Neuroimaging would demonstrate expanding blood, not a focal abnormality in the hippocampus.</p><p><strong>(Choice C)</strong>&amp;nbsp; Microglial nodules form when cells of microglia surround a neuron and phagocytize it.&amp;nbsp; The nodules are most commonly seen in viral CNS infection (particularly HIV) and are not typically associated with TLE.</p><p><strong>(Choice D)</strong>&amp;nbsp; Neuronal inclusion bodies are often seen in frontotemporal dementia (characterized by prominent personality/behavioral changes and atrophy of the frontal and temporal lobes on neuroimaging) and Parkinson disease (characterized by bradykinesia, rigidity, pill-rolling tremor, and dementia).&amp;nbsp; These disorders occur in elderly patients and are not associated with seizures.</p><p><strong>(Choice E)</strong>&amp;nbsp; Spongiform changes occur in prion diseases (eg, Creutzfeldt-Jacob, kuru).&amp;nbsp; Patients have progressive tremors, ataxia, and dementia.&amp;nbsp; Electroencephalography reveals periodic sharp-wave complexes, not focal epileptiform discharges, and hippocampal abnormalities are unexpected.</p><p><strong>Educational objective:</strong><br>Temporal lobe epilepsy is most commonly due to hippocampal sclerosis.&amp;nbsp; Histopathology of hippocampal sclerosis demonstrates atrophy of the hippocampal neurons with marked reactive gliosis (astrocyte proliferation in response to injury).</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><div id=\"div_T59554\" class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" class=\"table-default-style table-header-footer-style\"><tbody><tr><td colspan=\"2\"><p align=\"center\"><strong>Temporal lobe epilepsy</strong></p></td></tr><tr><td><p align=\"center\"><strong>Etiology</strong></p></td><td><ul><li>Hippocampal sclerosis (most common)</li><li>Other: infection, head trauma, tumors, congenital malformations</li></ul></td></tr><tr><td><p align=\"center\"><strong>Manifestations</strong></p></td><td><ul><li>Focal seizures with impaired awareness</li><li>Secondary generalized seizures may occur</li><li>Aura and automatisms are common</li></ul></td></tr><tr><td><p align=\"center\"><strong>Diagnostic studies</strong></p></td><td><ul><li>EEG: epileptiform waves in the temporal region</li><li>MRI: Hippocampal atrophy</li></ul></td></tr><tr><td><p align=\"center\"><strong>Histopathology</strong></p></td><td><ul><li>Atrophy of the hippocampal neurons with reactive gliosis</li></ul></td></tr><tr><td colspan=\"2\"><p><strong>EEG</strong> = electroencephalography.</p></td></tr></tbody></table></div><p class=\"ng-scope\">This patient with recurrent seizures has an epileptogenic focus in the temporal lobe, <strong>suggesting temporal lobe epilepsy</strong> (TLE), the most common type of localization-related (ie, focal) epilepsy.&nbsp; Localization epilepsies occur when partial seizures originate from one area of the brain and are usually due to an underlying structural disorder which serves as an epileptogenic focus.&nbsp; In the case of TLE, this is most commonly due to <strong>hippocampal sclerosis</strong>, which is usually visible as <a ng-click=\"showexhibit('32714',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L50110.jpg&quot;,&quot;title&quot;:&quot;L50110&quot;}]\">hippocampal atrophy</a> on MRI.</p><p class=\"ng-scope\">The pathogenesis of hippocampal sclerosis is incompletely understood but may in some cases be related to prior prolonged febrile seizures, particularly in patients with an underlying genetic predisposition.&nbsp; This results in the characteristic histopathologic findings of <strong>atrophy of the hippocampal neurons</strong> with marked reactive gliosis (<strong>astrocyte proliferation</strong> in response to injury).&nbsp; Surgical resection may reduce or cure seizures associated with TLE.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Hemosiderin-laden macrophages are associated with prior intracerebral hemorrhage, which may cause seizures but typically presents acutely with altered mental status and focal neurologic deficits.&nbsp; Neuroimaging would demonstrate expanding blood, not a focal abnormality in the hippocampus.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Microglial nodules form when cells of microglia surround a neuron and phagocytize it.&nbsp; The nodules are most commonly seen in viral CNS infection (particularly HIV) and are not typically associated with TLE.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Neuronal inclusion bodies are often seen in frontotemporal dementia (characterized by prominent personality/behavioral changes and atrophy of the frontal and temporal lobes on neuroimaging) and Parkinson disease (characterized by bradykinesia, rigidity, pill-rolling tremor, and dementia).&nbsp; These disorders occur in elderly patients and are not associated with seizures.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Spongiform changes occur in prion diseases (eg, Creutzfeldt-Jacob, kuru).&nbsp; Patients have progressive tremors, ataxia, and dementia.&nbsp; Electroencephalography reveals periodic sharp-wave complexes, not focal epileptiform discharges, and hippocampal abnormalities are unexpected.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Temporal lobe epilepsy is most commonly due to hippocampal sclerosis.&nbsp; Histopathology of hippocampal sclerosis demonstrates atrophy of the hippocampal neurons with marked reactive gliosis (astrocyte proliferation in response to injury).</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pathology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Nervous System</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Seizures</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
