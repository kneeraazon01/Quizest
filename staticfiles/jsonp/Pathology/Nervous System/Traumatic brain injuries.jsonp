& testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; id=&quot;6642&quot;  src=&quot;https://www.uworld.com/media/L25923.jpg&quot; draggable=&quot;false&quot; ></p><p><strong>Abusive head trauma (AHT)</strong>, formerly known as &amp;#x22;shaken baby syndrome,&amp;#x22; is inflicted intracranial injury due to blunt force trauma or <strong>vigorous shaking</strong> in a to-and-fro fashion.&amp;nbsp; The resultant injuries are due to infants&amp;#39; unique anatomic features.&amp;nbsp; Infants have larger heads, enlarged subarachnoid spaces, higher brain water content, and decreased cervical muscle tone compared to older children.&amp;nbsp; These factors lead to increased movement of the immature brain in relation to the skull, resulting in <strong>tearing of the bridging veins</strong> and subdural hemorrhages.&amp;nbsp; Because AHT may occur repeatedly over weeks to months, <a ng-click=&quot;showexhibit('1308',true);&quot;>acute on chronic subdural hemorrhages</a> are often seen.</p><p>The shaking motion also results in <strong>retinal hemorrhages</strong> due to rupture of congested retinal veins.&amp;nbsp; Retinal hemorrhages are highly suggestive of AHT.&amp;nbsp; Infants&amp;#39; ribs are flexible and pliable, and <strong>posterior rib fractures</strong> are also alarming for trauma by a perpetrator&amp;#39;s grasp on the torso.&amp;nbsp; In addition, the history provided by this patient&amp;#39;s parent does not match his developmental age, as rolling over is not typical before age 4 months.&amp;nbsp; Furthermore, an accidental fall <strong>(Choice F)</strong> from a bed would generally not be sufficient to cause a large intracranial hemorrhage resulting in altered mental status and would not explain retinal hemorrhages.</p><p><strong>(Choice B)</strong>&amp;nbsp; Although bacterial meningitis can cause lethargy, patients are typically febrile.</p><p><strong>(Choice C)</strong>&amp;nbsp; Idiopathic thrombocytopenic purpura, caused by platelet autoantibody formation, presents with petechiae, ecchymoses, and bleeding after minor trauma.&amp;nbsp; Intracranial hemorrhage is rare and retinal hemorrhages are not seen.</p><p><strong>(Choice D)</strong>&amp;nbsp; All newborns are vitamin K-deficient and are given prophylactic vitamin K injections at birth.&amp;nbsp; Although vitamin K deficiency can present with intracranial, umbilical, and gastrointestinal bleeding, retinal hemorrhages are not seen.</p><p><strong>(Choice E)</strong>&amp;nbsp; <a ng-click=&quot;showexhibit('1059',true);&quot;>Retinoblastoma</a> is the most common ocular tumor of infancy and presents with absence of the &amp;#x22;red reflex&amp;#x22; on physical examination.&amp;nbsp; Lethargy and retinal hemorrhage are not seen.</p><p><strong>Educational objective: </strong><br>Abusive head trauma can be caused by vigorous shaking of an infant and results in <strong>subdural hemorrhage</strong> (due to tearing of bridging veins) and <strong>retinal hemorrhages</strong>.&amp;nbsp; Additional red flags are injuries inconsistent with history or developmental age.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" id=\"6642\" src=\"image/L25923.jpg\" draggable=\"false\"></p><p class=\"ng-scope\"><strong>Abusive head trauma (AHT)</strong>, formerly known as \"shaken baby syndrome,\" is inflicted intracranial injury due to blunt force trauma or <strong>vigorous shaking</strong> in a to-and-fro fashion.&nbsp; The resultant injuries are due to infants' unique anatomic features.&nbsp; Infants have larger heads, enlarged subarachnoid spaces, higher brain water content, and decreased cervical muscle tone compared to older children.&nbsp; These factors lead to increased movement of the immature brain in relation to the skull, resulting in <strong>tearing of the bridging veins</strong> and subdural hemorrhages.&nbsp; Because AHT may occur repeatedly over weeks to months, <a ng-click=\"showexhibit('1308',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L12263.png&quot;,&quot;title&quot;:&quot;L12263&quot;}]\">acute on chronic subdural hemorrhages</a> are often seen.</p><p class=\"ng-scope\">The shaking motion also results in <strong>retinal hemorrhages</strong> due to rupture of congested retinal veins.&nbsp; Retinal hemorrhages are highly suggestive of AHT.&nbsp; Infants' ribs are flexible and pliable, and <strong>posterior rib fractures</strong> are also alarming for trauma by a perpetrator's grasp on the torso.&nbsp; In addition, the history provided by this patient's parent does not match his developmental age, as rolling over is not typical before age 4 months.&nbsp; Furthermore, an accidental fall <strong>(Choice F)</strong> from a bed would generally not be sufficient to cause a large intracranial hemorrhage resulting in altered mental status and would not explain retinal hemorrhages.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Although bacterial meningitis can cause lethargy, patients are typically febrile.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Idiopathic thrombocytopenic purpura, caused by platelet autoantibody formation, presents with petechiae, ecchymoses, and bleeding after minor trauma.&nbsp; Intracranial hemorrhage is rare and retinal hemorrhages are not seen.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; All newborns are vitamin K-deficient and are given prophylactic vitamin K injections at birth.&nbsp; Although vitamin K deficiency can present with intracranial, umbilical, and gastrointestinal bleeding, retinal hemorrhages are not seen.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; <a ng-click=\"showexhibit('1059',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L17506.jpg&quot;,&quot;title&quot;:&quot;L17506&quot;}]\">Retinoblastoma</a> is the most common ocular tumor of infancy and presents with absence of the \"red reflex\" on physical examination.&nbsp; Lethargy and retinal hemorrhage are not seen.</p><p class=\"ng-scope\"><strong>Educational objective: </strong><br>Abusive head trauma can be caused by vigorous shaking of an infant and results in <strong>subdural hemorrhage</strong> (due to tearing of bridging veins) and <strong>retinal hemorrhages</strong>.&nbsp; Additional red flags are injuries inconsistent with history or developmental age.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"refer