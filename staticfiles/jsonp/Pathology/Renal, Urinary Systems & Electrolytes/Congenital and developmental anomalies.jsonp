ions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><div id=&quot;div_T57772&quot;><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; class=&quot;table-default-style&quot;><tbody><tr><td colspan=&quot;4&quot;><p align=&quot;center&quot;><strong>Polyuria &amp;amp; polydipsia</strong></p></td></tr><tr><td rowspan=&quot;2&quot;></td><td rowspan=&quot;2&quot;><p align=&quot;center&quot;><strong>Baseline<br>serum<br>osmolality</strong></p></td><td colspan=&quot;2&quot;><p align=&quot;center&quot;><strong>Water deprivation test</strong></p></td></tr><tr><td><p align=&quot;center&quot;><strong>Urine osmolality<br>after water<br>deprivation</strong></p></td><td><p align=&quot;center&quot;><strong>Urine osmolality<br>with vasopressin<br>injection</strong></p></td></tr><tr><td><p align=&quot;center&quot;><strong>Normal</strong></p></td><td><p align=&quot;center&quot;>Normal</p></td><td><p align=&quot;center&quot;>Increased</p></td><td><p align=&quot;center&quot;>No change</p></td></tr><tr><td><p align=&quot;center&quot;><strong>Central diabetes<br>insipidus</strong></p></td><td><p align=&quot;center&quot;>High</p></td><td><p align=&quot;center&quot;>No change</p></td><td><p align=&quot;center&quot;>Increased</p></td></tr><tr><td><p align=&quot;center&quot;><strong>Nephrogenic<br>diabetes insipidus</strong></p></td><td><p align=&quot;center&quot;>High</p></td><td><p align=&quot;center&quot;>No change</p></td><td><p align=&quot;center&quot;>No change</p></td></tr><tr><td><p align=&quot;center&quot;><strong>Primary polydipsia</strong></p></td><td><p align=&quot;center&quot;>Low</p></td><td><p align=&quot;center&quot;>Increased</p></td><td><p align=&quot;center&quot;>No change</p></td></tr></tbody></table></div><p>This patient with a vasopressin-2 (V2) receptor mutation has polyuria and hypernatremia, findings which are consistent with congenital <strong>nephrogenic diabetes insipidus</strong> (DI).</p><p><strong>V2 receptors</strong> are located in the renal cortical collecting ducts.&amp;nbsp; When serum osmolality rises (eg, water deprivation, dehydration), there is increased release of antidiuretic hormone (ADH, vasopressin) which activates V2 receptors to reabsorb water into the systemic circulation.&amp;nbsp; Water reabsorption leads to concentrated urine (low urinary volume, high urine osmolality) and lower serum osmolality.</p><p><strong>Mutations </strong>that impair normal V2 receptor function result in nephrogenic DI (<strong>ADH resistance</strong>).&amp;nbsp; Following water deprivation, the collecting ducts are unable to reabsorb water despite high circulating ADH levels, leading to ongoing urinary water losses.&amp;nbsp; Therefore, nephrogenic DI manifests with large volumes of abnormally <strong>dilute urine</strong> (ie, polyuria with low urine osmolality), <strong>high serum osmolality</strong> (ie, hypernatremia), and dehydration.</p><p>High serum osmolality, polyuria, and dilute urine also occur in central DI, which is caused by deficient ADH production by the hypothalamus.&amp;nbsp; As with nephrogenic DI, urine remains abnormally dilute after water deprivation.&amp;nbsp; However, <strong>challenge with desmopressin</strong> (an analogue of ADH) can differentiate between central and nephrogenic DI:</p><ul><li>In nephrogenic DI, desmopressin cannot correct the underlying ADH resistance.&amp;nbsp; Therefore, <strong>urine osmolality remains unchanged</strong> (eg, low).</li><li>In <strong>central DI</strong>, desmopressin corrects the underlying ADH deficiency.&amp;nbsp; Therefore, <strong>urine osmolality increases</strong> following administration <strong>(Choice C)</strong>.</li></ul><p><strong>(Choice B)</strong>&amp;nbsp; High baseline serum osmolality and high urine osmolality can be seen in states of marked hyperglycemia with glucosuria (ie, solute diuresis).&amp;nbsp; Although polyuria can occur, the ADH response remains intact, so urine osmolality will be high with water deprivation.</p><p><strong>(Choice D)&amp;nbsp;</strong> Primary polydipsia can also result in polyuria with dilute urine.&amp;nbsp; However, unlike DI, serum osmolality and sodium are low (due to excess water intake), and dilute urine represents an appropriate physiologic response.&amp;nbsp; ADH response is intact, so urine osmolality is appropriately high following water deprivation.&amp;nbsp; No additional change is seen with desmopressin because maximum ADH levels are reached with water deprivation.</p><p><strong>(Choices E and F)</strong>&amp;nbsp; This patient has visible signs of dehydration and hypernatremia; because sodium is a major determinant of serum osmolality, a high serum osmolality is expected.</p><p><strong>Educational objective:</strong><br>Vasopressin-2 receptor mutations are a cause of congenital nephrogenic diabetes insipidus, a condition in which renal resistance to antidiuretic hormone results in excessive urinary water losses.&amp;nbsp; Expected findings include high baseline serum osmolality (typically with hypernatremia), persistently dilute urine after water deprivation (low urine osmolality), and lack of response to desmopressin.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><div id=\"div_T57772\" class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" class=\"table-default-style\"><tbody><tr><td colspan=\"4\"><p align=\"center\"><strong>Polyuria &amp; polydipsia</strong></p></td></tr><tr><td rowspan=\"2\"></td><td rowspan=\"2\"><p align=\"center\"><strong>Baseline<br>serum<br>osmolality</strong></p></td><td colspan=\"2\"><p align=\"center\"><strong>Water deprivation test</strong></p></td></tr><tr><td><p align=\"center\"><strong>Urine osmolality<br>after water<br>deprivation</strong></p></td><td><p align=\"center\"><strong>Urine osmolality<br>with vasopressin<br>injection</strong></p></td></tr><tr><td><p align=\"center\"><strong>Normal</strong></p></td><td><p align=\"center\">Normal</p></td><td><p align=\"center\">Increased</p></td><td><p align=\"center\">No change</p></td></tr><tr><td><p align=\"center\"><strong>Central diabetes<br>insipidus</strong></p></td><td><p align=\"center\">High</p></td><td><p align=\"center\">No change</p></td><td><p align=\"center\">Increased</p></td></tr><tr><td><p align=\"center\"><strong>Nephrogenic<br>diabetes insipidus</strong></p></td><td><p align=\"center\">High</p></td><td><p align=\"center\">No change</p></td><td><p align=\"center\">No change</p></td></tr><tr><td><p align=\"center\"><strong>Primary polydipsia</strong></p></td><td><p align=\"center\">Low</p></td><td><p align=\"center\">Increased</p></td><td><p align=\"center\">No change</p></td></tr></tbody></table></div><p class=\"ng-scope\">This patient with a vasopressin-2 (V2) receptor mutation has polyuria and hypernatremia, findings which are consistent with congenital <strong>nephrogenic diabetes insipidus</strong> (DI).</p><p class=\"ng-scope\"><strong>V2 receptors</strong> are located in the renal cortical collecting ducts.&nbsp; When serum osmolality rises (eg, water deprivation, dehydration), there is increased release of antidiuretic hormone (ADH, vasopressin) which activates V2 receptors to reabsorb water into the systemic circulation.&nbsp; Water reabsorption leads to concentrated urine (low urinary volume, high urine osmolality) and lower serum osmolality.</p><p class=\"ng-scope\"><strong>Mutations </strong>that impair normal V2 receptor function result in nephrogenic DI (<strong>ADH resistance</strong>).&nbsp; Following water deprivation, the collecting ducts are unable to reabsorb water despite high circulating ADH levels, leading to ongoing urinary water losses.&nbsp; Therefore, nephrogenic DI manifests with large volumes of abnormally <strong>dilute urine</strong> (ie, polyuria with low urine osmolality), <strong>high serum osmolality</strong> (ie, hypernatremia), and dehydration.</p><p class=\"ng-scope\">High serum osmolality, polyuria, and dilute urine also occur in central DI, which is caused by deficient ADH production by the hypothalamus.&nbsp; As with nephrogenic DI, urine remains abnormally dilute after water deprivation.&nbsp; However, <strong>challenge with desmopressin</strong> (an analogue of ADH) can differentiate between central and nephrogenic DI:</p><ul class=\"ng-scope\"><li>In nephrogenic DI, desmopressin cannot correct the underlying ADH resistance.&nbsp; Therefore, <strong>urine osmolality remains unchanged</strong> (eg, low).</li><li>In <strong>central DI</strong>, desmopressin corrects the underlying ADH deficiency.&nbsp; Therefore, <strong>urine osmolality increases</strong> following administration <strong>(Choice C)</strong>.</li></ul><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; High baseline serum osmolality and high urine osmolality can be seen in states of marked hyperglycemia with glucosuria (ie, solute diuresis).&nbsp; Although polyuria can occur, the ADH response remains intact, so urine osmolality will be high with water deprivation.</p><p class=\"ng-scope\"><strong>(Choice D)&nbsp;</strong> Primary polydipsia can also result in polyuria with dilute urine.&nbsp; However, unlike DI, serum osmolality and sodium are low (due to excess water intake), and dilute urine represents an appropriate physiologic response.&nbsp; ADH response is intact, so urine osmolality is appropriately high following water deprivation.&nbsp; No additional change is seen with desmopressin because maximum ADH levels are reached with water deprivation.</p><p class=\"ng-scope\"><strong>(Choices E and F)</strong>&nbsp; This patient has visible signs of dehydration and hypernatremia; because sodium is a major determinant of serum osmolality, a high serum osmolality is expected.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Vasopressin-2 receptor mutations are a cause of congenital nephrogenic diabetes insipidus, a condition in which renal resistance to antidiuretic hormone results in excessive urinary water losses.&nbsp; Expected findings include high baseline serum osmolality (typically with hypernatremia), persistently dilute urine after water deprivation (low urine osmolality), and lack of response to desmopressin.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pathology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Renal, Urinary Systems &amp; Electrolytes</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Diabetes insipidus</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
        "correctAnswerIndex": 0,
        "category": "Congenital and developmental anomalies"
