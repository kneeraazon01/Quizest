Id == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;19153&quot; src=&quot;https://www.uworld.com/media/L43848.jpg&quot; ></p><p>This pediatric patient with microscopic hematuria, red blood cell casts, mild pyuria, and proteinuria has a nephritic syndrome (<a ng-click=&quot;showexhibit('19104',true);&quot;>glomerulonephritis</a>).&amp;nbsp; The biopsy sample demonstrates <strong>enlarged, hypercellular glomeruli</strong> consistent with <strong>poststreptococcal glomerulonephritis</strong> (PSGN), which is the most common cause of <strong>nephritic syndrome</strong> in children.&amp;nbsp; PSGN is caused by immune complex deposition in the glomerulus following group A beta-hemolytic <em>Streptococcus</em> (eg, <em>Streptococcus pyogenes</em>) infection.&amp;nbsp; Hypercellularity, which involves all lobules of all glomeruli, is the result of leukocyte infiltration and endothelial and mesangial cell proliferation in response to secreted cytokines.</p><p>Other classic findings include elevated titers of antistreptococcal antibodies (eg, antistreptolysin O, anti&amp;ndash;DNase B) and <strong>low C3 concentrations</strong> (due to consumption).&amp;nbsp; Electron microscopy typically shows electron-dense deposits composed of immune complexes on the epithelial side of the glomerular basement membrane.&amp;nbsp; On immunofluorescent microscopy, there are coarse, granular deposits of IgG and C3 with a characteristic &amp;#x22;starry sky&amp;#x22; appearance.</p><p><strong>(Choices A and B)</strong>&amp;nbsp; Serum antiglomerular basement membrane antibodies are found in Goodpasture syndrome.&amp;nbsp; Serum antineutrophil cytoplasmic antibodies are associated with certain vasculitides (eg, granulomatosis with polyangiitis, microscopic polyangiitis).&amp;nbsp; These conditions can cause both pulmonary-renal syndrome with hemoptysis and renal failure.&amp;nbsp; However, the typical renal manifestation is rapidly progressive glomerulonephritis, which is characterized by <a ng-click=&quot;showexhibit('6578',true);&quot;>crescent formation</a> on light microscopy.&amp;nbsp; In addition, these conditions are rare in children.</p><p><strong>(Choice D)</strong>&amp;nbsp; Although C3 levels are decreased in almost all patients with PSGN, C4 levels are usually normal as complement activation in PSGN occurs predominantly via the alternative pathway.</p><p><strong>(Choice E)</strong>&amp;nbsp; Eosinophilia can occur in acute interstitial nephritis, which causes renal failure but is typically associated with fever, rash, sterile pyuria, and white blood cell casts; significant hematuria and red blood cell casts are unexpected.</p><p><strong>Educational objective:</strong><br>Poststreptococcal glomerulonephritis is the most common cause of nephritic syndrome in children.&amp;nbsp; Light microscopy demonstrates enlarged, hypercellular glomeruli.&amp;nbsp; Laboratory findings in poststreptococcal glomerulonephritis include elevated antistreptococcal antibodies (eg, antistreptolysin O, anti&amp;ndash;DNase B) and decreased C3 and total complement levels.&amp;nbsp; C4 levels are usually normal.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"19153\" src=\"image/L43848.jpg\"></p><p class=\"ng-scope\">This pediatric patient with microscopic hematuria, red blood cell casts, mild pyuria, and proteinuria has a nephritic syndrome (<a ng-click=\"showexhibit('19104',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/43278.html&quot;,&quot;title&quot;:&quot;43278&quot;}]\">glomerulonephritis</a>).&nbsp; The biopsy sample demonstrates <strong>enlarged, hypercellular glomeruli</strong> consistent with <strong>poststreptococcal glomerulonephritis</strong> (PSGN), which is the most common cause of <strong>nephritic syndrome</strong> in children.&nbsp; PSGN is caused by immune complex deposition in the glomerulus following group A beta-hemolytic <em>Streptococcus</em> (eg, <em>Streptococcus pyogenes</em>) infection.&nbsp; Hypercellularity, which involves all lobules of all glomeruli, is the result of leukocyte infiltration and endothelial and mesangial cell proliferation in response to secreted cytokines.</p><p class=\"ng-scope\">Other classic findings include elevated titers of antistreptococcal antibodies (eg, antistreptolysin O, anti\u2013DNase B) and <strong>low C3 concentrations</strong> (due to consumption).&nbsp; Electron microscopy typically shows electron-dense deposits composed of immune complexes on the epithelial side of the glomerular basement membrane.&nbsp; On immunofluorescent microscopy, there are coarse, granular deposits of IgG and C3 with a characteristic \"starry sky\" appearance.</p><p class=\"ng-scope\"><strong>(Choices A and B)</strong>&nbsp; Serum antiglomerular basement membrane antibodies are found in Goodpasture syndrome.&nbsp; Serum antineutrophil cytoplasmic antibodies are associated with certain vasculitides (eg, granulomatosis with polyangiitis, microscopic polyangiitis).&nbsp; These conditions can cause both pulmonary-renal syndrome with hemoptysis and renal failure.&nbsp; However, the typical renal manifestation is rapidly progressive glomerulonephritis, which is characterized by <a ng-click=\"showexhibit('6578',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L25420.JPG&quot;,&quot;title&quot;:&quot;L25420&quot;}]\">crescent formation</a> on light microscopy.&nbsp; In addition, these conditions are rare in children.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Although C3 levels are decreased in almost all patients with PSGN, C4 levels are usually normal as complement activation in PSGN occurs predominantly via the alternative pathway.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Eosinophilia can occur in acute interstitial nephritis, which causes renal failure but is typically associated with fever, rash, sterile pyuria, and white blood cell casts; significant hematuria and red blood cell casts are unexpected.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Poststreptococcal glomerulonephritis is the most common cause of nephritic syndrome in children.&nbsp; Light microscopy demonstrates enlarged, hypercellular glomeruli.&nbsp; Laboratory findings in poststreptococcal glomerulonephritis include elevated antistreptococcal antibodies (eg, antistreptolysin O, anti\u2013DNase B) and decreased C3 and total complement levels.&nbsp; C4 levels are usually normal.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pathology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Renal, Urinary Systems &amp; Electrolytes</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Poststreptococcal Glomerulonephritis</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n        