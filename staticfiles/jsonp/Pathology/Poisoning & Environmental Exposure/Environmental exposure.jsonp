tMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p>Exposure to <strong>ionizing radiation</strong> causes cell injury directly through <strong>DNA double-strand breakage</strong> and indirectly through the generation of <strong>free radicals</strong> that can damage DNA and other cellular components.&amp;nbsp; Significant DNA damage results in the <strong>upregulation of p53</strong>, which inhibits replication and induces <strong>apoptosis</strong> of the affected cell.</p><p>The effects of ionizing radiation vary based on the rate of cellular division.&amp;nbsp; <strong>Highly proliferative cells</strong> (eg, stem cells) have less time to repair radiation-induced DNA damage before cell death occurs.&amp;nbsp; Therefore, these cells are highly sensitive to ionizing radiation and most likely to be <strong>affected first</strong> following exposure.</p><p><strong>Intestinal crypt stem cells</strong> divide continuously to produce new intestinal epithelial cells, which have a short life span and are completely renewed every 3-5 days.&amp;nbsp; Rapid denudation of the gastrointestinal mucosa can occur if the radiation dose is significant, resulting in nausea/vomiting, diarrhea, and gastrointestinal bleeding.</p><p>Other cells that are highly sensitive to the effects of ionizing radiation include the following:</p><ul><li>Bone marrow:&amp;nbsp; <strong>hematopoietic progenitors</strong> (pancytopenia)</li><li>Gonads:&amp;nbsp; spermatogonia (infertility)</li><li>Skin:&amp;nbsp; basal keratinocytes, hair follicle stem cells (desquamation, hair loss)</li></ul><p><strong>(Choices A and D)</strong>&amp;nbsp; Bladder epithelial cells and renal proximal tubular epithelial cells have a much slower turnover rate (eg, weeks to months) compared with intestinal epithelial cells and are less likely to be affected first by exposure to ionizing radiation.</p><p><strong>(Choices B and E)</strong>&amp;nbsp; Cardiac and skeletal myocytes are long-lived, differentiated cells that do not rapidly divide and can more effectively repair radiation-induced DNA damage before cell death occurs.</p><p><strong>(Choice F)</strong>&amp;nbsp; Although type II pneumocytes can dedifferentiate and proliferate in response to injury to replace type I pneumocytes, they have a low rate of proliferation at rest and are less likely to be affected acutely by exposure to ionizing radiation.</p><p><strong>Educational objective:</strong><br>Ionizing radiation induces DNA damage (eg, double-strand breakage, free radical generation) that predominantly affects highly proliferative cells (eg, skin stem cells, hematogenous progenitors, intestinal crypt cells).&amp;nbsp; These rapidly dividing cells are the first to be lost following significant radiation exposure, resulting in hair loss, pancytopenia, diarrhea, and nausea/vomiting.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\">Exposure to <strong>ionizing radiation</strong> causes cell injury directly through <strong>DNA double-strand breakage</strong> and indirectly through the generation of <strong>free radicals</strong> that can damage DNA and other cellular components.&nbsp; Significant DNA damage results in the <strong>upregulation of p53</strong>, which inhibits replication and induces <strong>apoptosis</strong> of the affected cell.</p><p class=\"ng-scope\">The effects of ionizing radiation vary based on the rate of cellular division.&nbsp; <strong>Highly proliferative cells</strong> (eg, stem cells) have less time to repair radiation-induced DNA damage before cell death occurs.&nbsp; Therefore, these cells are highly sensitive to ionizing radiation and most likely to be <strong>affected first</strong> following exposure.</p><p class=\"ng-scope\"><strong>Intestinal crypt stem cells</strong> divide continuously to produce new intestinal epithelial cells, which have a short life span and are completely renewed every 3-5 days.&nbsp; Rapid denudation of the gastrointestinal mucosa can occur if the radiation dose is significant, resulting in nausea/vomiting, diarrhea, and gastrointestinal bleeding.</p><p class=\"ng-scope\">Other cells that are highly sensitive to the effects of ionizing radiation include the following:</p><ul class=\"ng-scope\"><li>Bone marrow:&nbsp; <strong>hematopoietic progenitors</strong> (pancytopenia)</li><li>Gonads:&nbsp; spermatogonia (infertility)</li><li>Skin:&nbsp; basal keratinocytes, hair follicle stem cells (desquamation, hair loss)</li></ul><p class=\"ng-scope\"><strong>(Choices A and D)</strong>&nbsp; Bladder epithelial cells and renal proximal tubular epithelial cells have a much slower turnover rate (eg, weeks to months) compared with intestinal epithelial cells and are less likely to be affected first by exposure to ionizing radiation.</p><p class=\"ng-scope\"><strong>(Choices B and E)</strong>&nbsp; Cardiac and skeletal myocytes are long-lived, differentiated cells that do not rapidly divide and can more effectively repair radiation-induced DNA damage before cell death occurs.</p><p class=\"ng-scope\"><strong>(Choice F)</strong>&nbsp; Although type II pneumocytes can dedifferentiate and proliferate in response to injury to replace type I pneumocytes, they have a low rate of proliferation at rest and are less likely to be affected acutely by exposure to ionizing radiation.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Ionizing radiation induces DNA damage (eg, double-strand breakage, free radical generation) that predominantly affects highly proliferative cells (eg, skin stem cells, hematogenous progenitors, intestinal crypt cells).&nbsp; These rapidly dividing cells are the first to be lost following significant radiation exposure, resulting in hair loss, pancytopenia, diarrhea, and nausea/vomiting.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pathology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Poisoning &amp; Environmental Exposure</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Radiation injury</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
