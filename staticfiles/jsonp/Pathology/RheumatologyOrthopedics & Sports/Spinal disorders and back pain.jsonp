         <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><div id=&quot;div_T58706&quot;><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; class=&quot;table-default-style&quot;><tbody><tr><td colspan=&quot;2&quot;><p align=&quot;center&quot;><strong>Differential diagnosis of back pain</strong></p></td></tr><tr><td><p align=&quot;center&quot;><strong>Condition</strong></p></td><td><p align=&quot;center&quot;><strong>Clinical clues</strong></p></td></tr><tr><td><p align=&quot;center&quot;><strong>Degenerative<br>(osteoarthritis)</strong></p></td><td><ul><li>Positional</li><li><strong>Relieved with rest</strong></li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Radiculopathy</strong><br>(eg, disc herniation)</p></td><td><ul><li><strong>Radiates</strong> to leg</li><li>Sensory &amp;amp; motor findings</li><li>Positive <strong>straight-leg raising test</strong></li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Spinal stenosis</strong></p></td><td><ul><li>Pain with standing (spinal extension)</li><li>Relieved by spinal flexion</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Spondyloarthropathy</strong></p></td><td><ul><li>Young men</li><li>HLA-B27</li><li><strong>Relieved with exercise</strong></li><li>Prolonged morning stiffness</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Spinal metastasis</strong></p></td><td><ul><li>Constant pain</li><li>Worse at night</li><li>Not responsive to position changes</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Vertebral<br>osteomyelitis</strong></p></td><td><ul><li>Focal tenderness</li><li>Fevers &amp;amp; night sweats</li><li>Recent <strong>infection</strong>, <strong>intravenous</strong> drug abuse, or immune compromise</li></ul></td></tr></tbody></table></div><p><strong>Back pain</strong> is common, and the majority of cases are due to benign skeletal (eg, osteoarthritis, spinal stenosis, disc herniation) or muscular (eg, muscle strain) conditions.&amp;nbsp; The most common causes of back pain generally worsen with activity and improve with rest, postural modifications (eg, spinal stenosis worsens with back extension and is relieved with flexion), and analgesic therapy.</p><p>However, this patient&amp;#39;s <strong>constant</strong> back pain that fails to improve with positional changes, rest, or analgesics is concerning for a more serious condition (infection, malignancy).&amp;nbsp; This pattern of pain in an <strong>older patient</strong> (age &amp;gt;50) without a recent history of intravenous drug use should raise suspicion for a <strong>neoplastic process</strong>, particularly if the pain is worse at night (<strong>nocturnal pain</strong>).&amp;nbsp; Other features that raise concern for malignancy include a personal history of cancer, weight loss, neurologic deficits (eg, weakness), and point tenderness on the spinous processes.</p><p>Malignancies with a propensity for bony metastasis include <strong>P</strong>rostate, <strong>B</strong>reast, <strong>K</strong>idney, <strong>T</strong>hyroid, and <strong>L</strong>ung (mnemonic: <strong>Pb KTL</strong>, or <strong>&amp;#x22;lead kettle&amp;#x22;</strong>).&amp;nbsp; Prostate cancer is the most common malignancy in older men and frequently metastasizes to the axial skeleton and femur.</p><p><strong>(Choice A)</strong>&amp;nbsp; Degenerative disorders in the spine (eg, facet osteoarthritis) are typically relieved with rest and while in bed overnight; this patient&amp;#39;s constant pain that persists overnight is more worrisome for malignancy.</p><p><strong>(Choice B)</strong>&amp;nbsp; Vertebral osteomyelitis (pain at rest, focal tenderness) and epidural abscess (nerve root compression, radicular symptoms) can be associated with intravenous drug use but typically occur following recent use, not decades later, and are usually associated with systemic symptoms (eg, fever, night sweats).</p><p><strong>(Choice C)</strong>&amp;nbsp; Inflammatory back pain (eg, spondyloarthropathy) is characterized by slowly progressive pain that is often worse at night.&amp;nbsp; However, it is associated with prolonged morning stiffness (eg, hours) and improves with exercise.&amp;nbsp; In addition, onset almost always occurs at age &amp;lt;45.</p><p><strong>(Choice E)</strong>&amp;nbsp; Back pain may be psychogenic (eg, factitious, malingering).&amp;nbsp; Evidence of secondary gain (eg, litigation/compensation) and an unusual pattern of signs and symptoms can help identify these patients.</p><p><strong>Educational objective:</strong><br>Clinical features that suggest a malignant cause of back pain include persistent pain at night, no relief with rest, onset at age &amp;gt;50, and systemic symptoms.&amp;nbsp; Common malignancies with a propensity for bony metastasis include prostate, breast, kidney, thyroid, and lung.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><div id=\"div_T58706\" class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" class=\"table-default-style\"><tbody><tr><td colspan=\"2\"><p align=\"center\"><strong>Differential diagnosis of back pain</strong></p></td></tr><tr><td><p align=\"center\"><strong>Condition</strong></p></td><td><p align=\"center\"><strong>Clinical clues</strong></p></td></tr><tr><td><p align=\"center\"><strong>Degenerative<br>(osteoarthritis)</strong></p></td><td><ul><li>Positional</li><li><strong>Relieved with rest</strong></li></ul></td></tr><tr><td><p align=\"center\"><strong>Radiculopathy</strong><br>(eg, disc herniation)</p></td><td><ul><li><strong>Radiates</strong> to leg</li><li>Sensory &amp; motor findings</li><li>Positive <strong>straight-leg raising test</strong></li></ul></td></tr><tr><td><p align=\"center\"><strong>Spinal stenosis</strong></p></td><td><ul><li>Pain with standing (spinal extension)</li><li>Relieved by spinal flexion</li></ul></td></tr><tr><td><p align=\"center\"><strong>Spondyloarthropathy</strong></p></td><td><ul><li>Young men</li><li>HLA-B27</li><li><strong>Relieved with exercise</strong></li><li>Prolonged morning stiffness</li></ul></td></tr><tr><td><p align=\"center\"><strong>Spinal metastasis</strong></p></td><td><ul><li>Constant pain</li><li>Worse at night</li><li>Not responsive to position changes</li></ul></td></tr><tr><td><p align=\"center\"><strong>Vertebral<br>osteomyelitis</strong></p></td><td><ul><li>Focal tenderness</li><li>Fevers &amp; night sweats</li><li>Recent <strong>infection</strong>, <strong>intravenous</strong> drug abuse, or immune compromise</li></ul></td></tr></tbody></table></div><p class=\"ng-scope\"><strong>Back pain</strong> is common, and the majority of cases are due to benign skeletal (eg, osteoarthritis, spinal stenosis, disc herniation) or muscular (eg, muscle strain) conditions.&nbsp; The most common causes of back pain generally worsen with activity and improve with rest, postural modifications (eg, spinal stenosis worsens with back extension and is relieved with flexion), and analgesic therapy.</p><p class=\"ng-scope\">However, this patient's <strong>constant</strong> back pain that fails to improve with positional changes, rest, or analgesics is concerning for a more serious condition (infection, malignancy).&nbsp; This pattern of pain in an <strong>older patient</strong> (age &gt;50) without a recent history of intravenous drug use should raise suspicion for a <strong>neoplastic process</strong>, particularly if the pain is worse at night (<strong>nocturnal pain</strong>).&nbsp; Other features that raise concern for malignancy include a personal history of cancer, weight loss, neurologic deficits (eg, weakness), and point tenderness on the spinous processes.</p><p class=\"ng-scope\">Malignancies with a propensity for bony metastasis include <strong>P</strong>rostate, <strong>B</strong>reast, <strong>K</strong>idney, <strong>T</strong>hyroid, and <strong>L</strong>ung (mnemonic: <strong>Pb KTL</strong>, or <strong>\"lead kettle\"</strong>).&nbsp; Prostate cancer is the most common malignancy in older men and frequently metastasizes to the axial skeleton and femur.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Degenerative disorders in the spine (eg, facet osteoarthritis) are typically relieved with rest and while in bed overnight; this patient's constant pain that persists overnight is more worrisome for malignancy.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Vertebral osteomyelitis (pain at rest, focal tenderness) and epidural abscess (nerve root compression, radicular symptoms) can be associated with intravenous drug use but typically occur following recent use, not decades later, and are usually associated with systemic symptoms (eg, fever, night sweats).</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Inflammatory back pain (eg, spondyloarthropathy) is characterized by slowly progressive pain that is often worse at night.&nbsp; However, it is associated with prolonged morning stiffness (eg, hours) and improves with exercise.&nbsp; In addition, onset almost always occurs at age &lt;45.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Back pain may be psychogenic (eg, factitious, malingering).&nbsp; Evidence of secondary gain (eg, litigation/compensation) and an unusual pattern of signs and symptoms can help identify these patients.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Clinical features that suggest a malignant cause of back pain include persistent pain at night, no relief with rest, onset at age &gt;50, and systemic symptoms.&nbsp; Common malignancies with a propensity for bony metastasis include prostate, breast, kidney, thyroid, and lung.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pathology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Rheumatology/Orthopedics &amp; Sports</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Prostate cancer</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
