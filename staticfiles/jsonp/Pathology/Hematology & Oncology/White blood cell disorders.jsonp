utor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p>In multiple myeloma, neoplastic B-lymphocytes mature into plasma cells that synthesize abnormal (typically large) amounts of monoclonal immunoglobulin or immunoglobulin fragments (e.g. light chains).&amp;nbsp; Clinical manifestations of multiple myeloma include impaired hematopoiesis leading to a normochromic, normocytic anemia and weakness; lytic bone lesions classically affecting the vertebral column and causing back pain and pathologic fractures; and hypercalcemia and AL amyloidosis, which contribute to renal dysfunction.&amp;nbsp; Severe amyloidosis can also cause cardiac and cutaneous findings.</p><p>The classic laboratory abnormalities in multiple myeloma include erythrocyte rouleaux formation on peripheral blood smear and Bence-Jones proteins in the urine.&amp;nbsp; Serum protein electrophoresis (SPEP) is a more specific laboratory test used to determine if excessive monoclonal immunoglobulins are present in the serum.&amp;nbsp; An &amp;#x22;M peak&amp;#x22; on SPEP indicates the presence of such an immunoglobulin.&amp;nbsp; (An M peak may also be found in Waldenstrom macroglobulinemia and some lymphomas.)</p><p><b>(Choices A and B)</b> Iron deficiency can cause weakness due to a hypochromic, microcytic anemia and vitamin B12 (cobalamin) deficiency can cause weakness due to a macrocytic anemia.&amp;nbsp; Vitamin B12 deficiency can also cause neurologic signs.</p><p><b>(Choice C) </b>CLL is a lymphocytic malignancy that may cause a normochromic, normocytic anemia, but immunoglobulin production is classically depressed, not increased, in CLL.</p><p><b>(Choice D) </b>Aplastic anemia would cause a uniform depression of all hematologic cell lines, including leukocytes and platelets.</p><p><b>(Choice F) </b>Hodgkin lymphoma is a B-cell malignancy characterized by the presence of Reed-Sternberg cells on lymph node histology.&amp;nbsp; There may be anemia of chronic disease in patients with this condition, but a monoclonal gammopathy would not be expected.</p><p><b>(Choice G) </b>Patients with hypothyroidism may be anemic due to associated iron deficiency or pernicious anemia.&amp;nbsp; However, monoclonal gammopathy would not be expected.</p><p><b>Educational Objective:</b><br>The finding of a high peak in the gamma-globulin region on serum protein electrophoresis (SPEP) usually represents an M protein consisting of an overproduced monoclonal immunoglobulin.&amp;nbsp; Multiple myeloma causes an M protein peak on SPEP as well as anemia (weakness), lytic bone lesions (back pain, pathologic fractures), and renal insufficiency (related to amyloid deposition and hypercalcemia).</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\">In multiple myeloma, neoplastic B-lymphocytes mature into plasma cells that synthesize abnormal (typically large) amounts of monoclonal immunoglobulin or immunoglobulin fragments (e.g. light chains).&nbsp; Clinical manifestations of multiple myeloma include impaired hematopoiesis leading to a normochromic, normocytic anemia and weakness; lytic bone lesions classically affecting the vertebral column and causing back pain and pathologic fractures; and hypercalcemia and AL amyloidosis, which contribute to renal dysfunction.&nbsp; Severe amyloidosis can also cause cardiac and cutaneous findings.</p><p class=\"ng-scope\">The classic laboratory abnormalities in multiple myeloma include erythrocyte rouleaux formation on peripheral blood smear and Bence-Jones proteins in the urine.&nbsp; Serum protein electrophoresis (SPEP) is a more specific laboratory test used to determine if excessive monoclonal immunoglobulins are present in the serum.&nbsp; An \"M peak\" on SPEP indicates the presence of such an immunoglobulin.&nbsp; (An M peak may also be found in Waldenstrom macroglobulinemia and some lymphomas.)</p><p class=\"ng-scope\"><b>(Choices A and B)</b> Iron deficiency can cause weakness due to a hypochromic, microcytic anemia and vitamin B12 (cobalamin) deficiency can cause weakness due to a macrocytic anemia.&nbsp; Vitamin B12 deficiency can also cause neurologic signs.</p><p class=\"ng-scope\"><b>(Choice C) </b>CLL is a lymphocytic malignancy that may cause a normochromic, normocytic anemia, but immunoglobulin production is classically depressed, not increased, in CLL.</p><p class=\"ng-scope\"><b>(Choice D) </b>Aplastic anemia would cause a uniform depression of all hematologic cell lines, including leukocytes and platelets.</p><p class=\"ng-scope\"><b>(Choice F) </b>Hodgkin lymphoma is a B-cell malignancy characterized by the presence of Reed-Sternberg cells on lymph node histology.&nbsp; There may be anemia of chronic disease in patients with this condition, but a monoclonal gammopathy would not be expected.</p><p class=\"ng-scope\"><b>(Choice G) </b>Patients with hypothyroidism may be anemic due to associated iron deficiency or pernicious anemia.&nbsp; However, monoclonal gammopathy would not be expected.</p><p class=\"ng-scope\"><b>Educational Objective:</b><br>The finding of a high peak in the gamma-globulin region on serum protein electrophoresis (SPEP) usually represents an M protein consisting of an overproduced monoclonal immunoglobulin.&nbsp; Multiple myeloma causes an M protein peak on SPEP as well as anemia (weakness), lytic bone lesions (back pain, pathologic fractures), and renal insufficiency (related to amyloid deposition and hypercalcemia).</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pathology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Hematology &amp; Oncology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Multiple myeloma</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
