          <input class=\"answer-radio\" type=\"radio\">\n                            <strong>G.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight7\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Nasopharyngeal carcinoma</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; id=&quot;3146&quot;  src=&quot;https://www.uworld.com/media/L19419.jpg&quot; draggable=&quot;false&quot; ></p><p>Sore throat, moderate to high fever, palatal petechiae, <strong>lymphadenopathy</strong> (commonly posterior cervical or auricular), splenomegaly, and <strong>atypical</strong> (reactive) <strong>lymphocytosis</strong> are classic signs and symptoms of <strong>infectious mononucleosis</strong>, a disease common among teenagers and young adults.&amp;nbsp; Ninety percent of cases of infectious mononucleosis are caused by the <strong>Epstein-Barr virus</strong> (EBV); however, cytomegalovirus, HIV, and <em>Toxoplasma</em> are some of the other agents known to cause non-EBV infectious mononucleosis.&amp;nbsp; EBV infection induces <strong>heterophile antibodies</strong>, which react to antigens from animal erythrocytes (sheep in the Paul-Bunnell test and horse in the Monospot test).&amp;nbsp; The heterophile antibody tests are sensitive and specific for EBV-associated infectious mononucleosis.</p><p>EBV replicates primarily in B lymphocytes and is also associated with <strong>malignancies</strong>, including <strong>Burkitt lymphoma</strong> (especially the endemic African type) and <strong>nasopharyngeal carcinoma</strong>.&amp;nbsp; Many disorders in immunocompromised patients are caused by EBV, including central nervous system lymphoma in HIV-positive patients and post-transplant lymphoproliferative disorder (PTLD) in allograft recipients.</p><p><strong>(Choices A and F)</strong>&amp;nbsp; There is no classic association between acute myeloid leukemia or multiple myeloma and viral infection.</p><p><strong>(Choice B)</strong>&amp;nbsp; Cervical carcinoma is strongly associated with human papillomavirus (types 16, 18, 31, and 33) infection.</p><p><strong>(Choice C)</strong>&amp;nbsp; Chronic infection with either hepatitis B or C virus significantly increases the risk of hepatocellular carcinoma.</p><p><strong>(Choice D)</strong>&amp;nbsp; Kaposi sarcoma is strongly associated with HIV and human herpes virus type 8 infections.</p><p><strong>(Choice E)</strong>&amp;nbsp; Gastric adenocarcinoma and mucosa-associated lymphoid tissue tumor (MALToma) are associated with <em>Helicobacter pylori</em> infection.</p><p><strong>Educational objective:</strong><br>Epstein-Barr virus causes infectious mononucleosis in teenagers and young adults.&amp;nbsp; It is also associated with a number of malignant conditions, including Burkitt lymphoma and nasopharyngeal carcinoma.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" id=\"3146\" src=\"image/L19419.jpg\" draggable=\"false\"></p><p class=\"ng-scope\">Sore throat, moderate to high fever, palatal petechiae, <strong>lymphadenopathy</strong> (commonly posterior cervical or auricular), splenomegaly, and <strong>atypical</strong> (reactive) <strong>lymphocytosis</strong> are classic signs and symptoms of <strong>infectious mononucleosis</strong>, a disease common among teenagers and young adults.&nbsp; Ninety percent of cases of infectious mononucleosis are caused by the <strong>Epstein-Barr virus</strong> (EBV); however, cytomegalovirus, HIV, and <em>Toxoplasma</em> are some of the other agents known to cause non-EBV infectious mononucleosis.&nbsp; EBV infection induces <strong>heterophile antibodies</strong>, which react to antigens from animal erythrocytes (sheep in the Paul-Bunnell test and horse in the Monospot test).&nbsp; The heterophile antibody tests are sensitive and specific for EBV-associated infectious mononucleosis.</p><p class=\"ng-scope\">EBV replicates primarily in B lymphocytes and is also associated with <strong>malignancies</strong>, including <strong>Burkitt lymphoma</strong> (especially the endemic African type) and <strong>nasopharyngeal carcinoma</strong>.&nbsp; Many disorders in immunocompromised patients are caused by EBV, including central nervous system lymphoma in HIV-positive patients and post-transplant lymphoproliferative disorder (PTLD) in allograft recipients.</p><p class=\"ng-scope\"><strong>(Choices A and F)</strong>&nbsp; There is no classic association between acute myeloid leukemia or multiple myeloma and viral infection.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Cervical carcinoma is strongly associated with human papillomavirus (types 16, 18, 31, and 33) infection.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Chronic infection with either hepatitis B or C virus significantly increases the risk of hepatocellular carcinoma.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Kaposi sarcoma is strongly associated with HIV and human herpes virus type 8 infections.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Gastric adenocarcinoma and mucosa-associated lymphoid tissue tumor (MALToma) are associated with <em>Helicobacter pylori</em> infection.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Epstein-Barr virus causes infectious mononucleosis in teenagers and young adults.&nbsp; It is also associated with a number of malignant conditions, including Burkitt lymphoma and nasopharyngeal carcinoma.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pathology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Hematology &amp; Oncology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards