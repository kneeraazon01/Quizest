    {
        "question": "<div class=\"questioncontent ui-resizable widthWithoutAbstract\" id=\"questionContent\" ng-show=\"showQuestion\" ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat, 'suggested-highlights-visible' : userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; currentQuestion.frequentHighlights != null &amp;&amp; (suggestedHighlightsPreference == 1 || suggestedHighlightsPreference == 2), 'only-suggested-highlights-visible' : userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; suggestedHighlightsPreference == 1 &amp;&amp; currentQuestion.frequentHighlights != null}\">\n            <div style=\"display:none;\" class=\"suggested-highlights-switch-buttons ng-hide\" ng-show=\"userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; currentQuestion.frequentHighlights != null\">\n                <span class=\"fa-stack\" ng-click=\"showUserHighlights()\" title=\"My Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fas\" ng-class=\"{'fas' : suggestedHighlightsPreference == 0, 'fal' : suggestedHighlightsPreference != 0}\"></i>\n                    <i class=\"fal fa-highlighter fa-stack-1x fa-inverse\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 0}\"></i>\n                </span>\n                <span class=\"fa-stack ng-hide\" ng-hide=\"currentQuestion.frequentHighlights == null\" ng-click=\"showSuggestedHighlights()\" title=\"Suggested Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fal\" ng-class=\"{'fas' : suggestedHighlightsPreference == 1, 'fal' : suggestedHighlightsPreference != 1}\"></i>\n                    <i class=\"fas fa-highlighter fa-stack-1x\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 1}\"></i>\n                </span>\n                <span class=\"fa-stack highlight-preference-unavailable highlight-button-with-popover\" ng-show=\"currentQuestion.frequentHighlights == null\" title=\"\" data-toggle=\"popover\" data-placement=\"bottom\" data-content=\"Collecting Data\" data-trigger=\"hover\" data-original-title=\"Suggested Highlights\">\n                    <i class=\"fal fa-circle fa-stack-2x\"></i>\n                    <i class=\"fas fa-highlighter fa-stack-1x\"></i>\n                </span>\n                <span class=\"fa-stack ng-hide\" ng-hide=\"currentQuestion.frequentHighlights == null\" ng-click=\"showUserAndSuggestedHighlights()\" title=\"Duo Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fal\" ng-class=\"{'fas' : suggestedHighlightsPreference == 2, 'fal' : suggestedHighlightsPreference != 2}\"></i>\n                    <i class=\"fad fa-highlighter fa-stack-1x\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 2}\"></i>\n                </span>\n                <span class=\"fa-stack highlight-preference-unavailable highlight-button-with-popover\" ng-show=\"currentQuestion.frequentHighlights == null\" title=\"\" data-toggle=\"popover\" data-placement=\"bottom\" data-content=\"Collecting Data\" data-trigger=\"hover\" data-original-title=\"Duo Highlights\">\n                    <i class=\"fal fa-circle fa-stack-2x\"></i>\n                    <i class=\"fad fa-highlighter fa-stack-1x\"></i>\n                </span>\n            </div>\n            <!-- ngIf: launchTestOptions.launchCode == 'nbome' && userPreferences.testPreferences.theme != clientConstants.themes.nbome -->\n            <div id=\"question\">\n                <!-- ngIf: settings.hints && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && launchTestOptions.canShowHint -->\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && (currentQuestion.qbankId != clientConstants.qbankIds.stats_faculty || currentQuestion.qbankId != clientConstants.qbankIds.stats) -->\n                <!-- ngIf: (currentQuestion.mobileExcerpt != '' && currentQuestion.mobileExcerpt != null ) -->\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat -->\n                <!--Periodic table button for MCAT -->\n                <!--<div class=\"text-center periodicTableDiv\" ng-show=\"(launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.subjectId != constants.noPeriodicTable.cars) && (currentQuestion.subjectId != constants.noPeriodicTable.pyschology) && (currentQuestion.subjectId != constants.noPeriodicTable.sociology)\">\n                    <button class=\"btn btn-default btn--custom\" style=\"color: black; padding: 4px 3px;\" ng-click=\"showresources()\">Periodic table&nbsp;<img src=\"img/mcat/Periodic_icon.png\" height=\"12\" style=\"margin-top: -2px; cursor: pointer\" title=\"Periodic Table\" /></button>\n                </div>-->\n                \n                <!--Inactive message for questions in usmle, boards and nclex and do not show for EXPERIMENTAL_INACTIVE hence the check !=3 -->\n                <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.nclex) && !currentQuestion.isActive && currentQuestion.activeStatusId != 3 -->\n                <div id=\"questionText\" class=\"questionText ng-scope\" ng-mouseup=\"invokeHighlights('questionText',constants.flashCardType.selection,$event)\" compiledata=\"\" htmlcontent=\"<p>A 46-year-old man comes to the emergency department with chest pain that began 30 minutes ago.&amp;nbsp; The patient describes the pain as a tight, squeezing sensation that radiates to the left arm.&amp;nbsp; He also has increased sweating and nausea.&amp;nbsp; ECG shows multi-lead ST-segment elevation, and cardiac troponin levels are high.&amp;nbsp; The patient has a history of alcohol and cocaine abuse, but no history of cardiac disease.&amp;nbsp; He undergoes emergency cardiac catheterization and is recovering appropriately in the postanesthetic care unit.&amp;nbsp; The physician is approached by a distraught woman who says she is the patient&amp;#39;s wife and asks about his condition.&amp;nbsp; She says, &amp;#x22;I am so worried; please tell me if my husband is okay.&amp;#x22;&amp;nbsp; Which of the following is the most appropriate course of action?</p>\"><p class=\"ng-scope\">A 46-year-old man comes to the emergency department with chest pain that began 30 minutes ago.&nbsp; The patient describes the pain as a tight, squeezing sensation that radiates to the left arm.&nbsp; He also has increased sweating and nausea.&nbsp; ECG shows multi-lead ST-segment elevation, and cardiac troponin levels are high.&nbsp; The patient has a history of alcohol and cocaine abuse, but no history of cardiac disease.&nbsp; He undergoes emergency cardiac catheterization and is recovering appropriately in the postanesthetic care unit.&nbsp; The physician is approached by a distraught woman who says she is the patient's wife and asks about his condition.&nbsp; She says, \"I am so worried; please tell me if my husband is okay.\"&nbsp; Which of the following is the most appropriate course of action?</p></div>\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && (currentQuestion.formatTypeId == constants.questionFormatType.fillInTheBlank ||currentQuestion.formatTypeId == constants.questionFormatType.fillInTheBlankRange )&& currentQuestion.sectionId == constants.collegeprepSectionIds.math -->\n                <!-- change to mediatype object -->\n                <!-- ngIf: currentQuestion.questionMedia.typeId == constants.questionMediaType.drugad || currentQuestion.questionTypeId == constants.questionType.drugad -->\n                <!-- ngIf: currentQuestion.questionMedia.typeId == constants.questionMediaType.audio || currentQuestion.questionMedia.typeId == constants.questionMediaType.video || currentQuestion.questionTypeId == constants.questionType.media -->\n\n                <div id=\"answerContainer\"><div class=\"options\">\n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>A.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight1\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Discuss the patient's medical information with the woman due to the seriousness of his condition</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>B.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight2\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Discuss the patient's status with the woman only if she has appropriate identification</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>C.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight3\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Explain to the woman that no information can be disclosed without the patient's permission</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>D.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight4\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Explain to the woman that the patient's status cannot be disclosed as there is no way to be sure she is his spouse</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>E.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight5\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Tell the woman that the patient is stable, but that further details will have to wait until the patient can give permission</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p>While this patient is undergoing emergency treatment for acute myocardial infarction, a woman who claims to be his wife arrives at the hospital and asks about his medical condition.&amp;nbsp; The Health Insurance Portability and Accountability Act <strong>(HIPAA)</strong> is designed to <strong>protect patient privacy</strong>.&amp;nbsp; This <strong>requires</strong> the patient&amp;#39;s explicit <strong>consent</strong> (or lack of objection when given the opportunity) <strong>to share medical information</strong> with others, including family and friends.</p><p>When the patient is <strong>incapacitated</strong> or is not present, <strong>basic information can be shared</strong> if, in the physician&amp;#39;s professional judgment, doing so is in the patient&amp;#39;s <strong>best interest</strong>.&amp;nbsp; In this case, there is no way of knowing for sure whether informing this woman (whether she is his wife or not) is in the patient&amp;#39;s best interest.&amp;nbsp; However, the woman is distressed, and leaving her to worry for an extended period while the patient is recovering could cause the patient emotional harm if she is indeed family.&amp;nbsp; The best approach to protect the patient&amp;#39;s privacy is to explain that he is stable, but that further information (including general diagnostic and treatment information) cannot be shared until he is asked for and provides consent.</p><p><strong>(Choice A)</strong>&amp;nbsp; This discussion would violate the patient&amp;#39;s privacy.</p><p><strong>(Choices B and D)</strong>&amp;nbsp; This patient has not given permission to release information to the presumed wife, even if she provides identification or marital status is confirmed.&amp;nbsp; For instance, the patient could be undergoing a contentious divorce and not want any information released to his wife.&amp;nbsp; HIPAA does not require proof of identity if the patient has given permission to share information.</p><p><strong>(Choice C)</strong>&amp;nbsp; Providing minimal information that the patient is &amp;#x22;stable&amp;#x22; would be appropriate.&amp;nbsp; This response protects the patient&amp;#39;s privacy by not revealing the diagnosis or other medical details, but also limits emotional harm to the patient by informing those who are likely to be family about the patient&amp;#39;s condition.&amp;nbsp; If permission to share information is obtained later, the physician should ask the patient whether he prefers that potentially sensitive details be omitted (eg, if the myocardial infarction was due to cocaine use and the patient did not want this revealed).</p><p><strong>Educational objective:</strong><br>Under the Health Insurance Portability and Accountability Act, physicians may disclose patient information to friends and family members in emergency situations or when the patient is otherwise incapacitated, depending on what is in the best interest of the patient.&amp;nbsp; Otherwise, patient information may be disclosed only when the patient gives explicit permission or does not object when given a reasonable opportunity.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\">While this patient is undergoing emergency treatment for acute myocardial infarction, a woman who claims to be his wife arrives at the hospital and asks about his medical condition.&nbsp; The Health Insurance Portability and Accountability Act <strong>(HIPAA)</strong> is designed to <strong>protect patient privacy</strong>.&nbsp; This <strong>requires</strong> the patient's explicit <strong>consent</strong> (or lack of objection when given the opportunity) <strong>to share medical information</strong> with others, including family and friends.</p><p class=\"ng-scope\">When the patient is <strong>incapacitated</strong> or is not present, <strong>basic information can be shared</strong> if, in the physician's professional judgment, doing so is in the patient's <strong>best interest</strong>.&nbsp; In this case, there is no way of knowing for sure whether informing this woman (whether she is his wife or not) is in the patient's best interest.&nbsp; However, the woman is distressed, and leaving her to worry for an extended period while the patient is recovering could cause the patient emotional harm if she is indeed family.&nbsp; The best approach to protect the patient's privacy is to explain that he is stable, but that further information (including general diagnostic and treatment information) cannot be shared until he is asked for and provides consent.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; This discussion would violate the patient's privacy.</p><p class=\"ng-scope\"><strong>(Choices B and D)</strong>&nbsp; This patient has not given permission to release information to the presumed wife, even if she provides identification or marital status is confirmed.&nbsp; For instance, the patient could be undergoing a contentious divorce and not want any information released to his wife.&nbsp; HIPAA does not require proof of identity if the patient has given permission to share information.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Providing minimal information that the patient is \"stable\" would be appropriate.&nbsp; This response protects the patient's privacy by not revealing the diagnosis or other medical details, but also limits emotional harm to the patient by informing those who are likely to be family about the patient's condition.&nbsp; If permission to share information is obtained later, the physician should ask the patient whether he prefers that potentially sensitive details be omitted (eg, if the myocardial infarction was due to cocaine use and the patient did not want this revealed).</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Under the Health Insurance Portability and Accountability Act, physicians may disclose patient information to friends and family members in emergency situations or when the patient is otherwise incapacitated, depending on what is in the best interest of the patient.&nbsp; Otherwise, patient information may be disclosed only when the patient gives explicit permission or does not object when given a reasonable opportunity.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionR