onstants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>F.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight6\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Zileuton</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p>The figure on the left shows a decrease in bronchial diameter of normal lungs following administration of an irritant but minimal change in bronchial diameter of lungs without an intact vagus nerve.&amp;nbsp; This implies that the bronchoconstriction is mediated by the <strong>parasympathetic</strong> nervous system.&amp;nbsp; When the vagus nerve is stimulated (eg, via an inhaled irritant), <strong>acetylcholine</strong> is released and produces <strong>bronchoconstriction</strong> by acting on muscarinic receptors.&amp;nbsp; The figure on the right demonstrates minimal decrease in bronchial diameter in normal lungs following administration of the irritant, suggesting that the study medication blocks the action of acetylcholine.&amp;nbsp; <strong>Ipratropium</strong>, an anticholinergic and derivative of atropine, has this effect.</p><p>Ipratropium and other anticholinergic drugs used for <strong>obstructive lung disease</strong> (eg, tiotropium) are less potent bronchodilators than beta-2 adrenergic agonists but can enhance the bronchodilatory effect of these agents.&amp;nbsp; In addition, ipratropium and similar drugs block the parasympathetic stimulation of tracheobronchial submucosal glands, decreasing mucus secretion and providing further benefit in treating asthma and chronic obstructive pulmonary disease (COPD).</p><p><strong>(Choice A)</strong>&amp;nbsp; Albuterol is a short-acting selective beta-2 adrenergic agonist that is the first-line treatment for asthma and COPD.&amp;nbsp; The bronchodilatory effects of albuterol are independent of parasympathetic innervation.</p><p><strong>(Choice B)</strong>&amp;nbsp; Inhaled glucocorticoids (eg, flunisolide) have potent local anti-inflammatory effects in the airway and are used in the treatment of persistent asthma and COPD with frequent exacerbations.</p><p><strong>(Choice D)</strong>&amp;nbsp; Nifedipine causes bronchodilation by blocking calcium influx into bronchial smooth muscle cells; however, the drug is generally not used in treating obstructive lung disease.</p><p><strong>(Choice E)</strong>&amp;nbsp; Methylxanthines, such as theophylline and aminophylline, cause bronchodilation by blocking phosphodiesterase activity, thereby increasing the intracellular concentration of cyclic AMP.</p><p><strong>(Choice F)</strong>&amp;nbsp; Zileuton inhibits 5-lipoxygenase and thereby decreases the production of leukotrienes to reduce bronchoconstriction and improve asthma symptoms.</p><p><strong>Educational objective:</strong><br>Ipratropium, an anticholinergic agent and derivative of atropine, treats obstructive lung disease by blocking acetylcholine at muscarinic receptors, which prevents bronchoconstriction and reduces mucus secretion from tracheobronchial submucosal glands.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\">The figure on the left shows a decrease in bronchial diameter of normal lungs following administration of an irritant but minimal change in bronchial diameter of lungs without an intact vagus nerve.&nbsp; This implies that the bronchoconstriction is mediated by the <strong>parasympathetic</strong> nervous system.&nbsp; When the vagus nerve is stimulated (eg, via an inhaled irritant), <strong>acetylcholine</strong> is released and produces <strong>bronchoconstriction</strong> by acting on muscarinic receptors.&nbsp; The figure on the right demonstrates minimal decrease in bronchial diameter in normal lungs following administration of the irritant, suggesting that the study medication blocks the action of acetylcholine.&nbsp; <strong>Ipratropium</strong>, an anticholinergic and derivative of atropine, has this effect.</p><p class=\"ng-scope\">Ipratropium and other anticholinergic drugs used for <strong>obstructive lung disease</strong> (eg, tiotropium) are less potent bronchodilators than beta-2 adrenergic agonists but can enhance the bronchodilatory effect of these agents.&nbsp; In addition, ipratropium and similar drugs block the parasympathetic stimulation of tracheobronchial submucosal glands, decreasing mucus secretion and providing further benefit in treating asthma and chronic obstructive pulmonary disease (COPD).</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Albuterol is a short-acting selective beta-2 adrenergic agonist that is the first-line treatment for asthma and COPD.&nbsp; The bronchodilatory effects of albuterol are independent of parasympathetic innervation.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Inhaled glucocorticoids (eg, flunisolide) have potent local anti-inflammatory effects in the airway and are used in the treatment of persistent asthma and COPD with frequent exacerbations.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Nifedipine causes bronchodilation by blocking calcium influx into bronchial smooth muscle cells; however, the drug is generally not used in treating obstructive lung disease.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Methylxanthines, such as theophylline and aminophylline, cause bronchodilation by blocking phosphodiesterase activity, thereby increasing the intracellular concentration of cyclic AMP.</p><p class=\"ng-scope\"><strong>(Choice F)</strong>&nbsp; Zileuton inhibits 5-lipoxygenase and thereby decreases the production of leukotrienes to reduce bronchoconstriction and improve asthma symptoms.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Ipratropium, an anticholinergic agent and derivative of atropine, treats obstructive lung disease by blocking acetylcholine at muscarinic receptors, which prevents bronchoconstriction and reduces mucus secretion from tracheobronchial submucosal glands.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/22222705\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/22222705\"><span compiledata=\"\" htmlcontent=\"Muscarinic receptor antagonists:  effects on pulmonary func