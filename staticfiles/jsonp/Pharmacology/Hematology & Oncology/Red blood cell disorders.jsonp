                   </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><div id=&quot;div_t60021&quot;><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; class=&quot;table-default-style&quot; style=&quot;width:464px&quot;><tbody><tr><td colspan=&quot;2&quot;><p align=&quot;center&quot; style=&quot;margin-left:.25in;&quot;><strong>Drug-induced immune-mediated hemolytic anemia</strong></p></td></tr><tr><td><p align=&quot;center&quot;><strong>Pathogenesis</strong></p></td><td><ul><li>Drug coats erythrocytes &amp;rarr; IgG binding &amp;rarr; splenic destruction of RBCs (extravascular hemolysis)</li><li>Drug triggers immune complexes &amp;rarr; complement-mediated destruction of RBCs (intravascular hemolysis)</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Manifestations</strong></p></td><td><ul><li>Sudden onset (within hours of exposure)</li><li>Anemia: fatigue, pallor, dyspnea</li><li>Hemolysis: jaundice, dark urine, abdomen/back pain<ul style=&quot;list-style-type:circle;&quot;><li>&amp;uarr; Reticulocytes, indirect bilirubin &amp;amp; LDH</li><li>&amp;darr; Haptoglobin</li><li>Spherocytes on peripheral blood smear</li><li>+ Direct Coombs test (anti-IgG, anti-C3)</li></ul></li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Treatment</strong></p></td><td><ul><li>Discontinue offending drug</li></ul></td></tr><tr><td colspan=&quot;2&quot;><p><strong>LDH</strong> = lactate dehydrogenase; <strong>RBC</strong> = red blood cell.</p></td></tr></tbody></table></div><p>This patient developed anemia and hyperbilirubinemia during treatment with ceftriaxone, raising strong suspicion for <strong>drug-induced hemolytic anemia</strong>.&amp;nbsp; The most common triggers of drug-induced hemolysis are anti-inflammatory medications, penicillins, and cephalosporins (eg, <strong>ceftriaxone</strong>).&amp;nbsp; Although the mechanism of erythrocyte injury (eg, oxidative, complement mediated) varies depending on the medication, penicillins and cephalosporins typically cause hemolysis by binding to the erythrocyte surface, which creates a <strong>hapten for IgG attachment</strong>.&amp;nbsp; Antibody-coated erythrocytes are then partially or wholly phagocytized by splenic macrophages, leading to <strong>extravascular hemolysis</strong>.</p><p>Hemolysis is often associated with symptomatic anemia (eg, fatigue, pallor) and signs of hyperbilirubinemia (eg, jaundice, dark urine).&amp;nbsp; Laboratory evaluation will typically reveal <strong>elevated indirect bilirubin</strong> (heme&amp;nbsp;breakdown product), elevated lactate dehydrogenase (intracellular enzyme), reticulocytosis (increased immature RBCs), and lowered haptoglobin (binds free hemoglobin).&amp;nbsp; The diagnosis is confirmed with the <strong>direct antiglobulin (Coombs) test</strong>, which detects IgG or C3 (a complement fragment) on the erythrocyte surface.&amp;nbsp; Because hemolysis is triggered by the direct binding of the penicillin or cephalosporin to the erythrocyte membrane, <strong>cessation of the offending drug</strong> eliminates the antibody target and generally leads to complete resolution of symptoms within days.</p><p><strong>(Choice B)</strong>&amp;nbsp; Anemia of chronic disease is caused by cytokine-mediated iron dysregulation.&amp;nbsp; Although chronic infection (eg, viral hepatitis, tuberculosis) is a common trigger, anemia of chronic disease is gradual in onset and is not associated with hyperbilirubinemia (no RBC lysis).</p><p><strong>(Choice C)</strong>&amp;nbsp; Certain antibiotics (eg, sulfa medications, chloramphenicol) are associated with myelosuppression.&amp;nbsp; However, these medications generally cause pancytopenia, not isolated anemia; in addition, hyperbilirubinemia is not seen.</p><p><strong>(Choice D)</strong>&amp;nbsp; Glucose-6-phosphate dehydrogenase (G6PD) deficiency is marked by hemolysis in the setting of oxidative stress due to infection, oxidizing medication, or oxidative foods.&amp;nbsp; Although this patient has an infection, ceftriaxone is not an oxidizing medication.&amp;nbsp; In addition, G6PD deficiency is X-linked; therefore, symptoms are less common in women.</p><p><strong>(Choice E)</strong>&amp;nbsp; Severe infections can trigger disseminated intravascular coagulation, a consumptive coagulopathy associated with anemia due to erythrocyte injury from microvascular thrombi.&amp;nbsp; However, these patients have prolonged coagulation times and thrombocytopenia.</p><p><strong>Educational objective:</strong><br>Penicillins and cephalosporins are common triggers for drug-induced hemolytic anemia because they can bind to the erythrocyte surface and act as haptens for IgG attachment.&amp;nbsp; Subsequent clearance of IgG-coated erythrocytes by splenic macrophages leads to hemolytic anemia.&amp;nbsp; Withdrawal of the offending drug usually leads to complete resolution of symptoms.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><div id=\"div_t60021\" class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" class=\"table-default-style\" style=\"width:464px\"><tbody><tr><td colspan=\"2\"><p align=\"center\" style=\"margin-left:.25in;\"><strong>Drug-induced immune-mediated hemolytic anemia</strong></p></td></tr><tr><td><p align=\"center\"><strong>Pathogenesis</strong></p></td><td><ul><li>Drug coats erythrocytes \u2192 IgG binding \u2192 splenic destruction of RBCs (extravascular hemolysis)</li><li>Drug triggers immune complexes \u2192 complement-mediated destruction of RBCs (intravascular hemolysis)</li></ul></td></tr><tr><td><p align=\"center\"><strong>Manifestations</strong></p></td><td><ul><li>Sudden onset (within hours of exposure)</li><li>Anemia: fatigue, pallor, dyspnea</li><li>Hemolysis: jaundice, dark urine, abdomen/back pain<ul style=\"list-style-type:circle;\"><li>\u2191 Reticulocytes, indirect bilirubin &amp; LDH</li><li>\u2193 Haptoglobin</li><li>Spherocytes on peripheral blood smear</li><li>+ Direct Coombs test (anti-IgG, anti-C3)</li></ul></li></ul></td></tr><tr><td><p align=\"center\"><strong>Treatment</strong></p></td><td><ul><li>Discontinue offending drug</li></ul></td></tr><tr><td colspan=\"2\"><p><strong>LDH</strong> = lactate dehydrogenase; <strong>RBC</strong> = red blood cell.</p></td></tr></tbody></table></div><p class=\"ng-scope\">This patient developed anemia and hyperbilirubinemia during treatment with ceftriaxone, raising strong suspicion for <strong>drug-induced hemolytic anemia</strong>.&nbsp; The most common triggers of drug-induced hemolysis are anti-inflammatory medications, penicillins, and cephalosporins (eg, <strong>ceftriaxone</strong>).&nbsp; Although the mechanism of erythrocyte injury (eg, oxidative, complement mediated) varies depending on the medication, penicillins and cephalosporins typically cause hemolysis by binding to the erythrocyte surface, which creates a <strong>hapten for IgG attachment</strong>.&nbsp; Antibody-coated erythrocytes are then partially or wholly phagocytized by splenic macrophages, leading to <strong>extravascular hemolysis</strong>.</p><p class=\"ng-scope\">Hemolysis is often associated with symptomatic anemia (eg, fatigue, pallor) and signs of hyperbilirubinemia (eg, jaundice, dark urine).&nbsp; Laboratory evaluation will typically reveal <strong>elevated indirect bilirubin</strong> (heme&nbsp;breakdown product), elevated lactate dehydrogenase (intracellular enzyme), reticulocytosis (increased immature RBCs), and lowered haptoglobin (binds free hemoglobin).&nbsp; The diagnosis is confirmed with the <strong>direct antiglobulin (Coombs) test</strong>, which detects IgG or C3 (a complement fragment) on the erythrocyte surface.&nbsp; Because hemolysis is triggered by the direct binding of the penicillin or cephalosporin to the erythrocyte membrane, <strong>cessation of the offending drug</strong> eliminates the antibody target and generally leads to complete resolution of symptoms within days.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Anemia of chronic disease is caused by cytokine-mediated iron dysregulation.&nbsp; Although chronic infection (eg, viral hepatitis, tuberculosis) is a common trigger, anemia of chronic disease is gradual in onset and is not associated with hyperbilirubinemia (no RBC lysis).</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Certain antibiotics (eg, sulfa medications, chloramphenicol) are associated with myelosuppression.&nbsp; However, these medications generally cause pancytopenia, not isolated anemia; in addition, hyperbilirubinemia is not seen.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Glucose-6-phosphate dehydrogenase (G6PD) deficiency is marked by hemolysis in the setting of oxidative stress due to infection, oxidizing medication, or oxidative foods.&nbsp; Although this patient has an infection, ceftriaxone is not an oxidizing medication.&nbsp; In addition, G6PD deficiency is X-linked; therefore, symptoms are less common in women.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Severe infections can trigger disseminated intravascular coagulation, a consumptive coagulopathy associated with anemia due to erythrocyte injury from microvascular thrombi.&nbsp; However, these patients have prolonged coagulation times and thrombocytopenia.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Penicillins and cephalosporins are common triggers for drug-induced hemolytic anemia because they can bind to the erythrocyte surface and act as haptens for IgG attachment.&nbsp; Subsequent clearance of IgG-coated erythrocytes by splenic macrophages leads to hemolytic anemia.&nbsp; Withdrawal of the offending drug usually leads to complete resolution of symptoms.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pharmacology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                   