!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;3270&quot; src=&quot;https://www.uworld.com/media/L24827.jpg&quot; ></p><p><strong>Warfarin</strong> is a vitamin K antagonist that blocks <strong>epoxide reductase</strong> in the liver, leading to impaired vitamin K recycling.&amp;nbsp; This depletes the reduced form of vitamin K, which is required for gamma carboxylation of <strong>vitamin K&amp;ndash;dependent clotting factors</strong> (II, VII, IX, and X).&amp;nbsp; In the absence of gamma carboxylation, vitamin K&amp;ndash;dependent clotting factors cannot bind calcium or phospholipid membranes to induce coagulation.</p><p>Because warfarin inhibits gamma carboxylation of <strong>new</strong> vitamin K&amp;ndash;dependent clotting factors, therapeutic efficacy is delayed until <strong>preexisting clotting factors</strong> in the plasma are consumed.&amp;nbsp; Although INR tends to slowly increase in the first few days of administration due to the short half-life of factor VII (4-6 hours), full <strong>therapeutic effect</strong> does not typically occur for <strong>3 days</strong> due to the long half-life of <strong>factor II</strong>.</p><p>Warfarin also inhibits gamma carboxylation of the proteins C and S.&amp;nbsp; Because these proteins exert an anticoagulant effect, the initiation of warfarin can be associated with an initial procoagulant state.&amp;nbsp; However, once preexisting vitamin K&amp;ndash;dependent clotting factors are consumed, an anticoagulant effect is established.</p><p><strong>(Choice B)</strong>&amp;nbsp; Anti-platelet factor 4/heparin antibodies cause heparin-induced thrombocytopenia, which typically arise 5-10 days after heparin initiation.&amp;nbsp; This condition is marked by thrombocytopenia and a high risk of arterial and venous thrombosis,&amp;nbsp;but would&amp;nbsp;not result in a delay in therapeutic INR following warfarin administration.</p><p><strong>(Choice C)</strong>&amp;nbsp; Total body vitamin K is sufficient for several weeks; it does not contribute to a delay in warfarin efficacy.&amp;nbsp; However, increased intake of foods rich in vitamin K (eg, green leafy vegetables) can lead to subtherapeutic INR due to increased vitamin K availability in the liver.</p><p><strong>(Choice D)</strong>&amp;nbsp; Warfarin is metabolized by the hepatic cytochrome P450 2C9 enzyme.&amp;nbsp; Because many other medications induce or inhibit this enzyme, warfarin is susceptible to a wide range of drug interactions.&amp;nbsp; However, the delay in achieving therapeutic INR is primarily due to the presence of preexisting clotting factors.</p><p><strong>(Choice E)</strong>&amp;nbsp; Warfarin reaches maximal plasma concentration approximately 90 minutes after administration.&amp;nbsp; The delay in efficacy is due to the presence of preexisting clotting factors.</p><p><strong>Educational objective:</strong><br>Warfarin blocks epoxide reductase, which lowers the reduced form of vitamin K in the liver; this prevents gamma carboxylation of vitamin K&amp;ndash;dependent clotting factors (II, VII, IX, and X).&amp;nbsp; Because warfarin only blocks the generation of new clotting factors, therapeutic effect is delayed 3-5 days until preexisting clotting factors are consumed.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"3270\" src=\"image/L24827.jpg\"></p><p class=\"ng-scope\"><strong>Warfarin</strong> is a vitamin K antagonist that blocks <strong>epoxide reductase</strong> in the liver, leading to impaired vitamin K recycling.&nbsp; This depletes the reduced form of vitamin K, which is required for gamma carboxylation of <strong>vitamin K\u2013dependent clotting factors</strong> (II, VII, IX, and X).&nbsp; In the absence of gamma carboxylation, vitamin K\u2013dependent clotting factors cannot bind calcium or phospholipid membranes to induce coagulation.</p><p class=\"ng-scope\">Because warfarin inhibits gamma carboxylation of <strong>new</strong> vitamin K\u2013dependent clotting factors, therapeutic efficacy is delayed until <strong>preexisting clotting factors</strong> in the plasma are consumed.&nbsp; Although INR tends to slowly increase in the first few days of administration due to the short half-life of factor VII (4-6 hours), full <strong>therapeutic effect</strong> does not typically occur for <strong>3 days</strong> due to the long half-life of <strong>factor II</strong>.</p><p class=\"ng-scope\">Warfarin also inhibits gamma carboxylation of the proteins C and S.&nbsp; Because these proteins exert an anticoagulant effect, the initiation of warfarin can be associated with an initial procoagulant state.&nbsp; However, once preexisting vitamin K\u2013dependent clotting factors are consumed, an anticoagulant effect is established.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Anti-platelet factor 4/heparin antibodies cause heparin-induced thrombocytopenia, which typically arise 5-10 days after heparin initiation.&nbsp; This condition is marked by thrombocytopenia and a high risk of arterial and venous thrombosis,&nbsp;but would&nbsp;not result in a delay in therapeutic INR following warfarin administration.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Total body vitamin K is sufficient for several weeks; it does not contribute to a delay in warfarin efficacy.&nbsp; However, increased intake of foods rich in vitamin K (eg, green leafy vegetables) can lead to subtherapeutic INR due to increased vitamin K availability in the liver.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Warfarin is metabolized by the hepatic cytochrome P450 2C9 enzyme.&nbsp; Because many other medications induce or inhibit this enzyme, warfarin is susceptible to a wide range of drug interactions.&nbsp; However, the delay in achieving therapeutic INR is primarily due to the presence of preexisting clotting factors.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Warfarin reaches maximal plasma concentration approximately 90 minutes after administration.&nbsp; The delay in efficacy is due to the presence of preexisting clotting factors.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Warfarin blocks epoxide reductase, which lowers the reduced form of vitamin K in the liver; this prevents gamma carboxylation of vitamin K\u2013dependent clotting factors (II, VII, IX, and X).&nbsp; Because warfarin only blocks the generation of new clotting factors, therapeutic effect is delayed 3-5 days until preexisting clotting factors are consumed.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstan