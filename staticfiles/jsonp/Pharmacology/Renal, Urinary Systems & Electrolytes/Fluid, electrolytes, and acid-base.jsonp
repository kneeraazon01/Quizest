eTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot;  draggable=&quot;false&quot; id=&quot;2884&quot; src=&quot;https://www.uworld.com/media/L12298.jpg&quot; ></p><p><strong>Aldosterone</strong> is a mineralocorticoid produced by the adrenal <a ng-click=&quot;showexhibit('27888',true);&quot;>zona glomerulosa</a> in response to hypotension and reduced renal blood flow (<a ng-click=&quot;showexhibit('18382',true);&quot;>renin-angiotensin-aldosterone system</a>).&amp;nbsp; Hyperkalemia can also stimulate aldosterone secretion.</p><p>Aldosterone acts primarily in the collecting duct, where it increases the number of <a ng-click=&quot;showexhibit('2802',true);&quot;>basolateral Na<font size=&quot;2&quot;><sup>+</sup></font>/K<font size=&quot;2&quot;><sup>+</sup></font>-ATPase pumps</a> and apical sodium channels on principal cells, increasing sodium and water reabsorption.&amp;nbsp; It also promotes <strong>potassium and hydrogen ion secretion</strong> from the principal and intercalated cells, respectively.</p><p><strong>Aldosterone receptor antagonists</strong> (eg, spironolactone, eplerenone) inhibit the effects of aldosterone and reduce sodium reabsorption and potassium and hydrogen ion secretion in the collecting duct.&amp;nbsp; Benefits include enhanced diuresis and lower blood pressure.&amp;nbsp; Adverse effects of these medications include hyperkalemia, hyponatremia, metabolic acidosis, and gynecomastia (with spironolactone, which also inhibits the testosterone receptor).</p><p><strong>(Choice A)</strong>&amp;nbsp; Intracellular <a ng-click=&quot;showexhibit('33966',true);&quot;>carbonic anhydrase</a> in the proximal tubule generates H<font size=&quot;2&quot;><sup>+</sup></font>, which is secreted into tubular fluid and used by brush border carbonic anhydrase to help resorb filtered HCO<font size=&quot;2&quot;><sub>3</sub></font><font size=&quot;2&quot;><sup>&amp;#8722;</sup></font>.&amp;nbsp; Carbonic anhydrase inhibitors (eg, acetazolamide) suppress both membrane-bound and cytoplasmic forms.</p><p><strong>(Choice C)</strong>&amp;nbsp; The Na<font size=&quot;2&quot;><sup>+</sup></font>/K<font size=&quot;2&quot;><sup>+</sup></font>/2Cl<font size=&quot;2&quot;><sup>&amp;#8722;</sup></font> cotransporter in the thick ascending limb is the target of loop diuretics (eg, furosemide), which cause brisk diuresis by inhibiting solute reabsorption.</p><p><strong>(Choice D)</strong>&amp;nbsp; The microvilli-covered epithelial cells (brush border) of the proximal convoluted tubule have a high capacity for reabsorption of electrolytes, glucose, and amino acids.&amp;nbsp; Isosmotic water reabsorption follows passively.&amp;nbsp; Reabsorption in this segment of the nephron is not affected by aldosterone, but is enhanced by angiotensin II.</p><p><strong>(Choice E)</strong>&amp;nbsp; In addition to increasing the water permeability of the collecting duct, vasopressin (antidiuretic hormone) raises urea reabsorption by increasing the number of cell surface urea transporters.&amp;nbsp; This strengthens the corticomedullary interstitial osmotic gradient, facilitating water reabsorption and increasing urine concentration.</p><p><strong>Educational objective:</strong><br>Aldosterone acts on the principal and intercalated cells of the renal collecting tubules to increase resorption of sodium and water and increase excretion of potassium and hydrogen ions.&amp;nbsp; Aldosterone receptor antagonists (eg, spironolactone, eplerenone) inhibit these effects.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"2884\" src=\"image/L12298.jpg\"></p><p class=\"ng-scope\"><strong>Aldosterone</strong> is a mineralocorticoid produced by the adrenal <a ng-click=\"showexhibit('27888',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L53840.jpg&quot;,&quot;title&quot;:&quot;L53840&quot;}]\">zona glomerulosa</a> in response to hypotension and reduced renal blood flow (<a ng-click=\"showexhibit('18382',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L42607.jpg&quot;,&quot;title&quot;:&quot;L42607&quot;}]\">renin-angiotensin-aldosterone system</a>).&nbsp; Hyperkalemia can also stimulate aldosterone secretion.</p><p class=\"ng-scope\">Aldosterone acts primarily in the collecting duct, where it increases the number of <a ng-click=\"showexhibit('2802',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L16602.jpg&quot;,&quot;title&quot;:&quot;L16602&quot;}]\">basolateral Na<font size=\"2\"><sup>+</sup></font>/K<font size=\"2\"><sup>+</sup></font>-ATPase pumps</a> and apical sodium channels on principal cells, increasing sodium and water reabsorption.&nbsp; It also promotes <strong>potassium and hydrogen ion secretion</strong> from the principal and intercalated cells, respectively.</p><p class=\"ng-scope\"><strong>Aldosterone receptor antagonists</strong> (eg, spironolactone, eplerenone) inhibit the effects of aldosterone and reduce sodium reabsorption and potassium and hydrogen ion secretion in the collecting duct.&nbsp; Benefits include enhanced diuresis and lower blood pressure.&nbsp; Adverse effects of these medications include hyperkalemia, hyponatremia, metabolic acidosis, and gynecomastia (with spironolactone, which also inhibits the testosterone receptor).</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Intracellular <a ng-click=\"showexhibit('33966',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L59841.jpg&quot;,&quot;title&quot;:&quot;L59841&quot;}]\">carbonic anhydrase</a> in the proximal tubule generates H<font size=\"2\"><sup>+</sup></font>, which is secreted into tubular fluid and used by brush border carbonic anhydrase to help resorb filtered HCO<font size=\"2\"><sub>3</sub></font><font size=\"2\"><sup>\u2212</sup></font>.&nbsp; Carbonic anhydrase inhibitors (eg, acetazolamide) suppress both membrane-bound and cytoplasmic forms.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; The Na<font size=\"2\"><sup>+</sup></font>/K<font size=\"2\"><sup>+</sup></font>/2Cl<font size=\"2\"><sup>\u2212</sup></font> cotransporter in the thick ascending limb is the target of loop diuretics (eg, furosemide), which cause brisk diuresis by inhibiting solute reabsorption.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; The microvilli-covered epithelial cells (brush border) of the proximal convoluted tubule have a high capacity for reabsorption of electrolytes, glucose, and amino acids.&nbsp; Isosmotic water reabsorption follows passively.&nbsp; Reabsorption in this segment of the nephron is not affected by aldosterone, but is enhanced by angiotensin II.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; In addition to increasing the water permeability of the collecting duct, vasopressin (antidiuretic hormone) raises urea reabsorption by increasing the number of cell surface urea transporters.&nbsp; This strengthens the corticomedullary interstitial osmotic gradient, facilitating water reabsorption and increasing urine concentration.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Aldosterone acts on the principal and intercalated cells of the renal collecting tubules to increase resorption of sodium and water and increase excretion of potassium and hydrogen ions.&nbsp; Aldosterone receptor antagonists (eg, spironolactone, eplerenone) inhibit these effects.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pharmacology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Renal, Urinary Systems &amp; Electrolytes</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Aldosterone antagonists</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
