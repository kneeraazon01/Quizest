    {
        "question": "<div class=\"questioncontent ui-resizable widthWithoutAbstract\" id=\"questionContent\" ng-show=\"showQuestion\" ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat, 'suggested-highlights-visible' : userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; currentQuestion.frequentHighlights != null &amp;&amp; (suggestedHighlightsPreference == 1 || suggestedHighlightsPreference == 2), 'only-suggested-highlights-visible' : userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; suggestedHighlightsPreference == 1 &amp;&amp; currentQuestion.frequentHighlights != null}\">\n            <div style=\"display:none;\" class=\"suggested-highlights-switch-buttons ng-hide\" ng-show=\"userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; currentQuestion.frequentHighlights != null\">\n                <span class=\"fa-stack\" ng-click=\"showUserHighlights()\" title=\"My Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fas\" ng-class=\"{'fas' : suggestedHighlightsPreference == 0, 'fal' : suggestedHighlightsPreference != 0}\"></i>\n                    <i class=\"fal fa-highlighter fa-stack-1x fa-inverse\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 0}\"></i>\n                </span>\n                <span class=\"fa-stack ng-hide\" ng-hide=\"currentQuestion.frequentHighlights == null\" ng-click=\"showSuggestedHighlights()\" title=\"Suggested Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fal\" ng-class=\"{'fas' : suggestedHighlightsPreference == 1, 'fal' : suggestedHighlightsPreference != 1}\"></i>\n                    <i class=\"fas fa-highlighter fa-stack-1x\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 1}\"></i>\n                </span>\n                <span class=\"fa-stack highlight-preference-unavailable highlight-button-with-popover\" ng-show=\"currentQuestion.frequentHighlights == null\" title=\"\" data-toggle=\"popover\" data-placement=\"bottom\" data-content=\"Collecting Data\" data-trigger=\"hover\" data-original-title=\"Suggested Highlights\">\n                    <i class=\"fal fa-circle fa-stack-2x\"></i>\n                    <i class=\"fas fa-highlighter fa-stack-1x\"></i>\n                </span>\n                <span class=\"fa-stack ng-hide\" ng-hide=\"currentQuestion.frequentHighlights == null\" ng-click=\"showUserAndSuggestedHighlights()\" title=\"Duo Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fal\" ng-class=\"{'fas' : suggestedHighlightsPreference == 2, 'fal' : suggestedHighlightsPreference != 2}\"></i>\n                    <i class=\"fad fa-highlighter fa-stack-1x\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 2}\"></i>\n                </span>\n                <span class=\"fa-stack highlight-preference-unavailable highlight-button-with-popover\" ng-show=\"currentQuestion.frequentHighlights == null\" title=\"\" data-toggle=\"popover\" data-placement=\"bottom\" data-content=\"Collecting Data\" data-trigger=\"hover\" data-original-title=\"Duo Highlights\">\n                    <i class=\"fal fa-circle fa-stack-2x\"></i>\n                    <i class=\"fad fa-highlighter fa-stack-1x\"></i>\n                </span>\n            </div>\n            <!-- ngIf: launchTestOptions.launchCode == 'nbome' && userPreferences.testPreferences.theme != clientConstants.themes.nbome -->\n            <div id=\"question\">\n                <!-- ngIf: settings.hints && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && launchTestOptions.canShowHint -->\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && (currentQuestion.qbankId != clientConstants.qbankIds.stats_faculty || currentQuestion.qbankId != clientConstants.qbankIds.stats) -->\n                <!-- ngIf: (currentQuestion.mobileExcerpt != '' && currentQuestion.mobileExcerpt != null ) -->\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat -->\n                <!--Periodic table button for MCAT -->\n                <!--<div class=\"text-center periodicTableDiv\" ng-show=\"(launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.subjectId != constants.noPeriodicTable.cars) && (currentQuestion.subjectId != constants.noPeriodicTable.pyschology) && (currentQuestion.subjectId != constants.noPeriodicTable.sociology)\">\n                    <button class=\"btn btn-default btn--custom\" style=\"color: black; padding: 4px 3px;\" ng-click=\"showresources()\">Periodic table&nbsp;<img src=\"img/mcat/Periodic_icon.png\" height=\"12\" style=\"margin-top: -2px; cursor: pointer\" title=\"Periodic Table\" /></button>\n                </div>-->\n                \n                <!--Inactive message for questions in usmle, boards and nclex and do not show for EXPERIMENTAL_INACTIVE hence the check !=3 -->\n                <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.nclex) && !currentQuestion.isActive && currentQuestion.activeStatusId != 3 -->\n                <div id=\"questionText\" class=\"questionText ng-scope\" ng-mouseup=\"invokeHighlights('questionText',constants.flashCardType.selection,$event)\" compiledata=\"\" htmlcontent=\"<p>A 22-year-old woman comes to the emergency department after development of a diffuse pruritic rash following a meal at a buffet restaurant an hour ago.&amp;nbsp; She also felt light-headed and nearly fainted.&amp;nbsp; The patient has no prior medical conditions and takes no medications.&amp;nbsp; Temperature is 37 C (98.6 F), blood pressure is 96/52 mm Hg, pulse is 126/min, and respirations are 16/min.&amp;nbsp; Physical examination shows mild lip swelling; the tongue appears normal.&amp;nbsp; Heart auscultation demonstrates regular tachycardia without extraneous sounds.&amp;nbsp; Lung sounds are clear with normal work of breathing.&amp;nbsp; Skin examination shows confluent urticaria on the neck, shoulders, and abdomen.&amp;nbsp; Intravenous fluids are begun, and intramuscular epinephrine is administered immediately.&amp;nbsp; In addition to its effect on airway and circulation, this medication is most likely to improve this patient&amp;#39;s condition through which of the following mechanisms?</p>\"><p class=\"ng-scope\">A 22-year-old woman comes to the emergency department after development of a diffuse pruritic rash following a meal at a buffet restaurant an hour ago.&nbsp; She also felt light-headed and nearly fainted.&nbsp; The patient has no prior medical conditions and takes no medications.&nbsp; Temperature is 37 C (98.6 F), blood pressure is 96/52 mm Hg, pulse is 126/min, and respirations are 16/min.&nbsp; Physical examination shows mild lip swelling; the tongue appears normal.&nbsp; Heart auscultation demonstrates regular tachycardia without extraneous sounds.&nbsp; Lung sounds are clear with normal work of breathing.&nbsp; Skin examination shows confluent urticaria on the neck, shoulders, and abdomen.&nbsp; Intravenous fluids are begun, and intramuscular epinephrine is administered immediately.&nbsp; In addition to its effect on airway and circulation, this medication is most likely to improve this patient's condition through which of the following mechanisms?</p></div>\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && (currentQuestion.formatTypeId == constants.questionFormatType.fillInTheBlank ||currentQuestion.formatTypeId == constants.questionFormatType.fillInTheBlankRange )&& currentQuestion.sectionId == constants.collegeprepSectionIds.math -->\n                <!-- change to mediatype object -->\n                <!-- ngIf: currentQuestion.questionMedia.typeId == constants.questionMediaType.drugad || currentQuestion.questionTypeId == constants.questionType.drugad -->\n                <!-- ngIf: currentQuestion.questionMedia.typeId == constants.questionMediaType.audio || currentQuestion.questionMedia.typeId == constants.questionMediaType.video || currentQuestion.questionTypeId == constants.questionType.media -->\n\n                <div id=\"answerContainer\"><div class=\"options\">\n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>A.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight1\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Blockade of tissue histamine receptors</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>B.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight2\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Decreased IgE Fc receptors on mast cells</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>C.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight3\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Decreased leukotriene synthesis in leukocytes</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>D.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight4\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Decreased mediator release from mast cells</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>E.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight5\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Decreased number of circulating eosinophils</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><div id=&quot;div_t72455&quot;><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; class=&quot;table-default-style&quot;><tbody><tr><td colspan=&quot;2&quot;><p align=&quot;center&quot;><strong>Anaphylaxis</strong></p></td></tr><tr><td><p align=&quot;center&quot;><strong>Triggers</strong></p></td><td><ul><li>Immune mediated (eg,&amp;nbsp;medications, food, insect stings)</li><li>Nonimmune mediated (eg,&amp;nbsp;intravenous contrast, mast cell activation syndrome)</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Clinical<br />manifestations</strong></p></td><td><ul><li>Cardiovascular<ul style=&quot;list-style-type:circle;&quot;><li>Vasodilation &amp;rarr; hypotension &amp;amp; tissue edema</li><li>Tachycardia</li></ul></li><li>Respiratory<ul style=&quot;list-style-type:circle;&quot;><li>Upper airway edema &amp;rarr; stridor &amp;amp; hoarseness</li><li>Bronchospasm &amp;rarr; wheezing</li></ul></li><li>Cutaneous<ul style=&quot;list-style-type:circle;&quot;><li>Urticarial rash, pruritus, flushing</li></ul></li><li>Gastrointestinal<ul style=&quot;list-style-type:circle;&quot;><li>Nausea, vomiting, abdominal pain</li></ul></li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Treatment</strong></p></td><td><ul><li>Intramuscular epinephrine</li></ul></td></tr></tbody></table></div><p>This patient with sudden-onset rash and hypotension after ingesting a likely allergen has <strong>anaphylaxis</strong>.&amp;nbsp; Anaphylaxis classically involves a <a ng-click=&quot;showexhibit('34682',true);&quot;>type 1 (immediate) hypersensitivity</a> reaction to an antigen, resulting in IgE-mediated mast cell and basophil degranulation.&amp;nbsp; Sudden and <strong>massive release of inflammatory mediators</strong> (eg, histamine, leukotrienes) into the circulation can quickly lead to urticaria, oropharyngeal edema (eg, lip swelling), and shock (eg, hypotension, tachycardia).</p><p><strong>Epinephrine</strong> is the only pharmacologic therapy that both decreases the release of inflammatory mediators from mast cells and addresses all other manifestations of anaphylaxis.&amp;nbsp; It binds to adrenergic receptors (eg, beta-2 receptors) on the surface of mast cells, <strong>inhibiting mast cell degranulation</strong>.&amp;nbsp; In addition, it <strong>counteracts existing inflammatory effects</strong> via widespread stimulation of alpha- and beta-adrenergic receptors:</p><ul><li><p>Alpha-1: causes vasoconstriction and decreases vascular permeability, raising blood pressure and decreasing upper airway edema</p></li><li><p>Beta-1: increases cardiac contractility and cardiac output, improving blood pressure and peripheral perfusion</p></li><li><p>Beta-2: causes bronchodilation, relieving airflow obstruction</p></li></ul><p>Because of its ability to both treat and prevent further inflammatory mediator effects, epinephrine is the first-line treatment for anaphylaxis.</p><p><strong>(Choice A)</strong>&amp;nbsp; <a ng-click=&quot;showexhibit('3763',true);&quot;>H1 antagonists</a> (eg, diphenhydramine) block tissue histamine receptors, preventing the effects of circulating histamine on capillaries (eg, vasodilation, increased vascular permeability).&amp;nbsp; However, they do not decrease the mast cell release of histamine or other inflammatory mediators.</p><p><strong>(Choice B)</strong>&amp;nbsp; Omalizumab is a monoclonal anti-IgE antibody that binds to free IgE at the same site where IgE normally binds to mast cell IgE Fc receptors.&amp;nbsp; It prevents receptor binding and decreases circulating IgE levels, which eventually induces IgE Fc receptor down-regulation to help stabilize mast cells.</p><p><strong>(Choice C)</strong>&amp;nbsp; Zileuton, a medication commonly used in the management of asthma, decreases the leukocyte synthesis of leukotrienes by <a ng-click=&quot;showexhibit('16977',true);&quot;>inhibiting 5-lipoxygenase</a>.&amp;nbsp; This results in decreased leukotriene activity, reducing inflammation and bronchospasm.</p><p><strong>(Choice E)</strong>&amp;nbsp; Glucocorticoids reduce inflammation by several mechanisms.&amp;nbsp; One such mechanism is by promoting eosinophil apoptosis, either directly or by decreasing production of IL-5, which usually promotes eosinophil survival.&amp;nbsp; This decreases the number of circulating eosinophils.</p><p><strong>Educational objective:</strong><br>Anaphylaxis is a type 1 (immediate) hypersensitivity reaction involving IgE-mediated mast cell and basophil degranulation.&amp;nbsp; Epinephrine is the primary treatment for anaphylaxis because it decreases further mast cell release of inflammatory mediators and counteracts existing systemic inflammatory effects (eg, shock, bronchoconstriction).</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><div id=\"div_t72455\" class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" class=\"table-default-style\"><tbody><tr><td colspan=\"2\"><p align=\"center\"><strong>Anaphylaxis</strong></p></td></tr><tr><td><p align=\"center\"><strong>Triggers</strong></p></td><td><ul><li>Immune mediated (eg,&nbsp;medications, food, insect stings)</li><li>Nonimmune mediated (eg,&nbsp;intravenous contrast, mast cell activation syndrome)</li></ul></td></tr><tr><td><p align=\"center\"><strong>Clinical<br>manifestations</strong></p></td><td><ul><li>Cardiovascular<ul style=\"list-style-type:circle;\"><li>Vasodilation \u2192 hypotension &amp; tissue edema</li><li>Tachycardia</li></ul></li><li>Respiratory<ul style=\"list-style-type:circle;\"><li>Upper airway edema \u2192 stridor &amp; hoarseness</li><li>Bronchospasm \u2192 wheezing</li></ul></li><li>Cutaneous<ul style=\"list-style-type:circle;\"><li>Urticarial rash, pruritus, flushing</li></ul></li><li>Gastrointestinal<ul style=\"list-style-type:circle;\"><li>Nausea, vomiting, abdominal pain</li></ul></li></ul></td></tr><tr><td><p align=\"center\"><strong>Treatment</strong></p></td><td><ul><li>Intramuscular epinephrine</li></ul></td></tr></tbody></table></div><p class=\"ng-scope\">This patient with sudden-onset rash and hypotension after ingesting a likely allergen has <strong>anaphylaxis</strong>.&nbsp; Anaphylaxis classically involves a <a ng-click=\"showexhibit('34682',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L60230.jpg&quot;,&quot;title&quot;:&quot;L60230&quot;}]\">type 1 (immediate) hypersensitivity</a> reaction to an antigen, resulting in IgE-mediated mast cell and basophil degranulation.&nbsp; Sudden and <strong>massive release of inflammatory mediators</strong> (eg, histamine, leukotrienes) into the circulation can quickly lead to urticaria, oropharyngeal edema (eg, lip swelling), and shock (eg, hypotension, tachycardia).</p><p class=\"ng-scope\"><strong>Epinephrine</strong> is the only pharmacologic therapy that both decreases the release of inflammatory mediators from mast cells and addresses all other manifestations of anaphylaxis.&nbsp; It binds to adrenergic receptors (eg, beta-2 receptors) on the surface of mast cells, <strong>inhibiting mast cell degranulation</strong>.&nbsp; In addition, it <strong>counteracts existing inflammatory effects</strong> via widespread stimulation of alpha- and beta-adrenergic receptors:</p><ul class=\"ng-scope\"><li><p>Alpha-1: causes vasoconstriction and decreases vascular permeability, raising blood pressure and decreasing upper airway edema</p></li><li><p>Beta-1: increases cardiac contractility and cardiac output, improving blood pressure and peripheral perfusion</p></li><li><p>Beta-2: causes bronchodilation, relieving airflow obstruction</p></li></ul><p class=\"ng-scope\">Because of its ability to both treat and prevent further inflammatory mediator effects, epinephrine is the first-line treatment for anaphylaxis.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; <a ng-click=\"showexhibit('3763',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L26150.jpg&quot;,&quot;title&quot;:&quot;L26150&quot;}]\">H1 antagonists</a> (eg, diphenhydramine) block tissue histamine receptors, preventing the effects of circulating histamine on capillaries (eg, vasodilation, increased vascular permeability).&nbsp; However, they do not decrease the mast cell release of histamine or other inflammatory mediators.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Omalizumab is a monoclonal anti-IgE antibody that binds to free IgE at the same site where IgE normally binds to mast cell IgE Fc receptors.&nbsp; It prevents receptor binding and decreases circulating IgE levels, which eventually induces IgE Fc receptor down-regulation to help stabilize mast cells.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Zileuton, a medication commonly used in the management of asthma, decreases the leukocyte synthesis of leukotrienes by <a ng-click=\"showexhibit('16977',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L41760.jpg&quot;,&quot;title&quot;:&quot;L41760&quot;}]\">inhibiting 5-lipoxygenase</a>.&nbsp; This results in decreased leukotriene activity, reducing inflammation and bronchospasm.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Glucocorticoids reduce inflammation by several mechanisms.&nbsp; One such mechanism is by promoting eosinophil apoptosis, either directly or by decreasing production of IL-5, which usually promotes eosinophil survival.&nbsp; This decreases the number of circulating eosinophils.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Anaphylaxis is a type 1 (immediate) hypersensitivity reaction involving IgE-mediated mast cell and basophil degranulation.&nbsp; Epinephrine is the primary treatment for anaphylaxis because it decreases further mast cell release of inflammatory mediators and counteracts existing systemic inflammatory effects (eg, shock, bronchoconstriction).</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/23282530\" target=\"_blank\" class=\"ng-sco