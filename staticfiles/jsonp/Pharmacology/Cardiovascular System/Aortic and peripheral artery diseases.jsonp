med || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p>Several molecules, including thrombin, adenosine diphosphate, and thromboxane A2, activate platelets by acting on cell surface receptors.&amp;nbsp; However, interference with post-receptor signaling can alter platelet function.&amp;nbsp; In particular, cyclic adenosine monophosphate (cAMP) activates a family of enzymes known as the cAMP-dependent protein kinases, or protein kinase A, and leads to inhibition of platelet aggregation.&amp;nbsp; Agents that increase intra-platelet cAMP levels decrease platelet aggregation by preventing platelet shape change and granule release.</p><p><strong>Cilostazol</strong> reduces <strong>platelet activation</strong> by inhibiting platelet phosphodiesterase, the enzyme responsible for the breakdown of cAMP.&amp;nbsp; It is also a direct arterial <strong>vasodilator</strong>.&amp;nbsp; The net effect is a decrease in claudication <strong>symptoms</strong> and an increase in pain-free walking distances in patients with peripheral arterial disease (PAD).&amp;nbsp; Patients with PAD should be initiated on a graded exercise program, which has also been shown to improve symptoms.</p><p><strong>(Choices A, E, and F)</strong>&amp;nbsp; Abciximab is a monoclonal antibody that inhibits platelet aggregation by targeting the platelet IIb/IIIa receptor.&amp;nbsp; Heparin is an anticoagulant that potentiates antithrombin III, resulting in the inactivation of thrombin and other factors.&amp;nbsp; Tissue plasminogen activator (alteplase) converts plasminogen to plasmin and causes fibrinolysis.&amp;nbsp; These agents are used in the acute management of cardiovascular events but are not useful for long-term management.</p><p><strong>(Choice B)</strong>&amp;nbsp; Argatroban is a direct thrombin inhibitor primarily used in the treatment of heparin-induced thrombocytopenia.</p><p><strong>(Choice C)</strong>&amp;nbsp; Aspirin is an irreversible cyclooxygenase 1 &amp;amp; 2 inhibitor that has been shown to reduce the risk of cardiovascular events in high-risk patients.&amp;nbsp; Patients with clinically significant PAD should take antiplatelet agents (aspirin or clopidogrel) to reduce the risk of cardiovascular events (secondary prevention), but antiplatelet agents do not improve PAD symptoms.</p><p><strong>(Choice G)</strong>&amp;nbsp; Warfarin is an oral anticoagulant that is a competitive inhibitor of vitamin K epoxide reductase and reduces production of vitamin K-dependent coagulation factors (II, VII, IX, X, protein C, protein S).&amp;nbsp; It is used in the long-term treatment and prevention of thromboembolic events but does not reduce claudication in symptomatic PAD.</p><p><strong>Educational objective:</strong><br>Symptomatic management of peripheral artery disease (PAD) includes a graded exercise program and cilostazol.&amp;nbsp; Cilostazol is a phosphodiesterase inhibitor that inhibits platelet aggregation and acts as a direct arterial vasodilator.&amp;nbsp; Patients with PAD should also receive an antiplatelet agent (aspirin or clopidogrel) for secondary prevention of coronary heart disease and stroke.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\">Several molecules, including thrombin, adenosine diphosphate, and thromboxane A2, activate platelets by acting on cell surface receptors.&nbsp; However, interference with post-receptor signaling can alter platelet function.&nbsp; In particular, cyclic adenosine monophosphate (cAMP) activates a family of enzymes known as the cAMP-dependent protein kinases, or protein kinase A, and leads to inhibition of platelet aggregation.&nbsp; Agents that increase intra-platelet cAMP levels decrease platelet aggregation by preventing platelet shape change and granule release.</p><p class=\"ng-scope\"><strong>Cilostazol</strong> reduces <strong>platelet activation</strong> by inhibiting platelet phosphodiesterase, the enzyme responsible for the breakdown of cAMP.&nbsp; It is also a direct arterial <strong>vasodilator</strong>.&nbsp; The net effect is a decrease in claudication <strong>symptoms</strong> and an increase in pain-free walking distances in patients with peripheral arterial disease (PAD).&nbsp; Patients with PAD should be initiated on a graded exercise program, which has also been shown to improve symptoms.</p><p class=\"ng-scope\"><strong>(Choices A, E, and F)</strong>&nbsp; Abciximab is a monoclonal antibody that inhibits platelet aggregation by targeting the platelet IIb/IIIa receptor.&nbsp; Heparin is an anticoagulant that potentiates antithrombin III, resulting in the inactivation of thrombin and other factors.&nbsp; Tissue plasminogen activator (alteplase) converts plasminogen to plasmin and causes fibrinolysis.&nbsp; These agents are used in the acute management of cardiovascular events but are not useful for long-term management.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Argatroban is a direct thrombin inhibitor primarily used in the treatment of heparin-induced thrombocytopenia.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Aspirin is an irreversible cyclooxygenase 1 &amp; 2 inhibitor that has been shown to reduce the risk of cardiovascular events in high-risk patients.&nbsp; Patients with clinically significant PAD should take antiplatelet agents (aspirin or clopidogrel) to reduce the risk of cardiovascular events (secondary prevention), but antiplatelet agents do not improve PAD symptoms.</p><p class=\"ng-scope\"><strong>(Choice G)</strong>&nbsp; Warfarin is an oral anticoagulant that is a competitive inhibitor of vitamin K epoxide reductase and reduces production of vitamin K-dependent coagulation factors (II, VII, IX, X, protein C, protein S).&nbsp; It is used in the long-term treatment and prevention of thromboembolic events but does not reduce claudication in symptomatic PAD.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Symptomatic management of peripheral artery disease (PAD) includes a graded exercise program and cilostazol.&nbsp; Cilostazol is a phosphodiesterase inhibitor that inhibits platelet aggregation and acts as a direct arterial vasodilator.&nbsp; Patients with PAD should also receive an antiplatelet agent (aspirin or clopidogrel) for secondary prevention of coronary heart disease and stroke.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingRefere