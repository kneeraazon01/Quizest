f: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><div id=&quot;div_T60397&quot;><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; class=&quot;table-default-style table-header-footer-style&quot;><tbody><tr><td colspan=&quot;5&quot;><p align=&quot;center&quot;><strong>Hemodynamic effects of adrenergic agonists</strong></p></td></tr><tr><td colspan=&quot;5&quot;><ul><li>&amp;#945;<sub>1</sub>: peripheral vasoconstriction, &amp;#8593; SVR</li><li>&amp;#946;<sub>1</sub>: &amp;#8593; heart rate &amp;amp; contractility, &amp;#8593; cardiac output</li><li>&amp;#946;<sub>2</sub>: peripheral vasodilation, &amp;#8595; SVR</li><li>D<sub>1</sub>: renal arteriolar vasodilation</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Agonist</strong></p></td><td><p align=&quot;center&quot;><strong>Receptor<br>binding</strong></p></td><td><p align=&quot;center&quot;><strong>SVR</strong></p></td><td><p align=&quot;center&quot;><strong>Cardiac<br>output</strong></p></td><td><p align=&quot;center&quot;><strong>Renal<br>blood flow</strong></p></td></tr><tr><td><p align=&quot;center&quot;><strong>Dobutamine</strong></p></td><td><p align=&quot;center&quot;>&amp;#946;<sub>1</sub> &amp;gt; &amp;#946;<sub>2</sub></p></td><td><p align=&quot;center&quot;>&amp;#8595;</p></td><td><p align=&quot;center&quot;>&amp;#8593;</p></td><td><p align=&quot;center&quot;>&amp;ndash;</p></td></tr><tr><td rowspan=&quot;2&quot;><p align=&quot;center&quot;><strong>Dopamine</strong></p></td><td><p align=&quot;center&quot;>D<sub>1</sub> &amp;gt; &amp;#946;<sub>1</sub> &amp;gt; &amp;#945;<sub>1</sub><br>(low-dose)</p></td><td><p align=&quot;center&quot;>&amp;ndash;/&amp;#8595;</p></td><td><p align=&quot;center&quot;>&amp;#8593;</p></td><td><p align=&quot;center&quot;>&amp;#8593;</p></td></tr><tr><td><p align=&quot;center&quot;>&amp;#945;<sub>1 </sub>&amp;gt; &amp;#946;<sub>1 </sub>&amp;gt; D<sub>1</sub><br>(high-dose)</p></td><td><p align=&quot;center&quot;>&amp;#8593;</p></td><td><p align=&quot;center&quot;>&amp;ndash;/&amp;#8595;</p></td><td><p align=&quot;center&quot;>&amp;#8595;</p></td></tr><tr><td rowspan=&quot;2&quot;><p align=&quot;center&quot;><strong>Epinephrine</strong></p></td><td><p align=&quot;center&quot;>&amp;#946;<sub>1 </sub>&amp;gt; &amp;#946;<sub>2</sub> &amp;gt; &amp;#945;<sub>1</sub><br>(low-dose)</p></td><td><p align=&quot;center&quot;>-/&amp;#8595;</p></td><td><p align=&quot;center&quot;>&amp;#8593;</p></td><td><p align=&quot;center&quot;>&amp;ndash;</p></td></tr><tr><td><p align=&quot;center&quot;>&amp;#945;<sub>1 </sub>&amp;gt; &amp;#946;<sub>1</sub> &amp;gt; &amp;#946;<sub>2</sub><br>(high-dose)</p></td><td><p align=&quot;center&quot;>&amp;#8593;</p></td><td><p align=&quot;center&quot;>&amp;ndash;/&amp;#8595;</p></td><td><p align=&quot;center&quot;>&amp;#8595;</p></td></tr><tr><td><p align=&quot;center&quot;><strong>Isoproterenol</strong></p></td><td><p align=&quot;center&quot;>&amp;#946;<sub>1</sub> = &amp;#946;<sub>2</sub></p></td><td><p align=&quot;center&quot;>&amp;#8595;</p></td><td><p align=&quot;center&quot;>&amp;#8593;</p></td><td><p align=&quot;center&quot;>&amp;ndash;</p></td></tr><tr><td><p align=&quot;center&quot;><strong>Norepinephrine</strong></p></td><td><p align=&quot;center&quot;>&amp;#945;<sub>1</sub> = &amp;#946;<sub>1</sub> &amp;gt; &amp;#946;<sub>2</sub></p></td><td><p align=&quot;center&quot;>&amp;#8593;</p></td><td><p align=&quot;center&quot;>&amp;ndash;</p></td><td><p align=&quot;center&quot;>&amp;#8595;</p></td></tr><tr><td><p align=&quot;center&quot;><strong>Phenylephrine</strong></p></td><td><p align=&quot;center&quot;>&amp;#945;<sub>1</sub></p></td><td><p align=&quot;center&quot;>&amp;#8593;</p></td><td><p align=&quot;center&quot;>&amp;#8595;</p></td><td><p align=&quot;center&quot;>&amp;#8595;</p></td></tr><tr><td colspan=&quot;5&quot;><p><strong>SVR</strong> = systemic vascular resistance.</p></td></tr></tbody></table></div><p><strong>Dopamine</strong> is an adrenergic agonist that has varying effects based on the dose.&amp;nbsp; At <strong>low doses</strong>, dopamine stimulates <strong>D1 receptors</strong> in the renal vasculature and tubules, inducing an <strong>increase in renal blood flow</strong>, glomerular filtration rate, and sodium excretion.&amp;nbsp; Vasodilation of the mesenteric, cerebral, and coronary vascular beds is also prominent at low doses.</p><p>As the dosage increases, dopamine begins to stimulate <strong>&amp;#946;</strong><font size=&quot;2&quot;><sub><strong>1</strong></sub></font><strong>adrenergic receptors</strong> in the heart, increasing cardiac contractility, heart rate, and systolic blood pressure.&amp;nbsp; At the high end of the dose range, stimulation of <strong>&amp;#945;</strong><font size=&quot;2&quot;><sub><strong>1</strong></sub></font><strong>receptors</strong> in the systemic vasculature leads to generalized vasoconstriction, which can result in decreased cardiac output due to the increased afterload.</p><p><strong>(Choice B)</strong>&amp;nbsp; Edrophonium is a short-acting acetylcholinesterase inhibitor used in the diagnosis of myasthenia gravis (ie, Tensilon test).&amp;nbsp; Through its cholinergic effects, edrophonium reduces heart rate, cardiac conduction, and cardiac contractility.</p><p><strong>(Choice C)</strong>&amp;nbsp; Epinephrine increases heart rate, myocardial contractility, and cardiac output by stimulating &amp;#946;<font size=&quot;2&quot;><sub>1</sub></font> receptors.&amp;nbsp; At low doses, stimulation of &amp;#946;<font size=&quot;2&quot;><sub>2</sub></font> receptors in the skeletal muscle vasculature produces vasodilation that can cause decreased blood pressure.&amp;nbsp; At high doses, &amp;#945;<font size=&quot;2&quot;><sub>1</sub></font>-induced vasoconstriction in the skin and viscera becomes more pronounced.&amp;nbsp; Epinephrine does not significantly increase renal blood flow (even at lower doses) due to the lack of D<font size=&quot;2&quot;><sub>1</sub></font> receptor stimulation.</p><p><strong>(Choice D)</strong>&amp;nbsp; Esmolol is a cardioselective &amp;#946;<font size=&quot;2&quot;><sub>1</sub></font> blocker with a short duration of action.&amp;nbsp; Esmolol decreases heart rate, myocardial contractility, and cardiac conduction without affecting renal blood flow.</p><p><strong>(Choice E)</strong>&amp;nbsp; Phenylephrine is a selective &amp;#945;<font size=&quot;2&quot;><sub>1</sub></font>-adrenergic receptor agonist that causes a generalized increase in peripheral vascular resistance and blood pressure.&amp;nbsp; Its use results in significant renal, splanchnic, and mesenteric vasoconstriction.</p><p><strong>Educational objective:</strong><br>Low-dose dopamine infusion stimulates D<font size=&quot;2&quot;><sub>1</sub></font> receptors in the renal and mesenteric vasculature, resulting in vasodilation and increased blood flow to these sites.&amp;nbsp; Increasing doses of dopamine stimulate &amp;#946;<font size=&quot;2&quot;><sub>1</sub></font> and &amp;#945;<font size=&quot;2&quot;><sub>1</sub></font> receptors, resulting in increased cardiac output and elevated systemic vascular resistance.&amp;nbsp; At the high end of the dose range, the increase in afterload can result in decreased cardiac output.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><div id=\"div_T60397\" class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" class=\"table-default-style table-header-footer-style\"><tbody><tr><td colspan=\"5\"><p align=\"center\"><strong>Hemodynamic effects of adrenergic agonists</strong></p></td></tr><tr><td colspan=\"5\"><ul><li>\u03b1<sub>1</sub>: peripheral vasoconstriction, \u2191 SVR</li><li>\u03b2<sub>1</sub>: \u2191 heart rate &amp; contractility, \u2191 cardiac output</li><li>\u03b2<sub>2</sub>: peripheral vasodilation, \u2193 SVR</li><li>D<sub>1</sub>: renal arteriolar vasodilation</li></ul></td></tr><tr><td><p align=\"center\"><strong>Agonist</strong></p></td><td><p align=\"center\"><strong>Receptor<br>binding</strong></p></td><td><p align=\"center\"><strong>SVR</strong></p></td><td><p align=\"center\"><strong>Cardiac<br>output</strong></p></td><td><p align=\"center\"><strong>Renal<br>blood flow</strong></p></td></tr><tr><td><p align=\"center\"><strong>Dobutamine</strong></p></td><td><p align=\"center\">\u03b2<sub>1</sub> &gt; \u03b2<sub>2</sub></p></td><td><p align=\"center\">\u2193</p></td><td><p align=\"center\">\u2191</p></td><td><p align=\"center\">\u2013</p></td></tr><tr><td rowspan=\"2\"><p align=\"center\"><strong>Dopamine</strong></p></td><td><p align=\"center\">D<sub>1</sub> &gt; \u03b2<sub>1</sub> &gt; \u03b1<sub>1</sub><br>(low-dose)</p></td><td><p align=\"center\">\u2013/\u2193</p></td><td><p align=\"center\">\u2191</p></td><td><p align=\"center\">\u2191</p></td></tr><tr><td><p align=\"center\">\u03b1<sub>1 </sub>&gt; \u03b2<sub>1 </sub>&gt; D<sub>1</sub><br>(high-dose)</p></td><td><p align=\"center\">\u2191</p></td><td><p align=\"center\">\u2013/\u2193</p></td><td><p align=\"center\">\u2193</p></td></tr><tr><td rowspan=\"2\"><p align=\"center\"><strong>Epinephrine</strong></p></td><td><p align=\"center\">\u03b2<sub>1 </sub>&gt; \u03b2<sub>2</sub> &gt; \u03b1<sub>1</sub><br>(low-dose)</p></td><td><p align=\"center\">-/\u2193</p></td><td><p align=\"center\">\u2191</p></td><td><p align=\"center\">\u2013</p></td></tr><tr><td><p align=\"center\">\u03b1<sub>1 </sub>&gt; \u03b2<sub>1</sub> &gt; \u03b2<sub>2</sub><br>(high-dose)</p></td><td><p align=\"center\">\u2191</p></td><td><p align=\"center\">\u2013/\u2193</p></td><td><p align=\"center\">\u2193</p></td></tr><tr><td><p align=\"center\"><strong>Isoproterenol</strong></p></td><td><p align=\"center\">\u03b2<sub>1</sub> = \u03b2<sub>2</sub></p></td><td><p align=\"center\">\u2193</p></td><td><p align=\"center\">\u2191</p></td><td><p align=\"center\">\u2013</p></td></tr><tr><td><p align=\"center\"><strong>Norepinephrine</strong></p></td><td><p align=\"center\">\u03b1<sub>1</sub> = \u03b2<sub>1</sub> &gt; \u03b2<sub>2</sub></p></td><td><p align=\"center\">\u2191</p></td><td><p align=\"center\">\u2013</p></td><td><p align=\"center\">\u2193</p></td></tr><tr><td><p align=\"center\"><strong>Phenylephrine</strong></p></td><td><p align=\"center\">\u03b1<sub>1</sub></p></td><td><p align=\"center\">\u2191</p></td><td><p align=\"center\">\u2193</p></td><td><p align=\"center\">\u2193</p></td></tr><tr><td colspan=\"5\"><p><strong>SVR</strong> = systemic vascular resistance.</p></td></tr></tbody></table></div><p class=\"ng-scope\"><strong>Dopamine</strong> is an adrenergic agonist that has varying effects based on the dose.&nbsp; At <strong>low doses</strong>, dopamine stimulates <strong>D1 receptors</strong> in the renal vasculature and tubules, inducing an <strong>increase in renal blood flow</strong>, glomerular filtration rate, and sodium excretion.&nbsp; Vasodilation of the mesenteric, cerebral, and coronary vascular beds is also prominent at low doses.</p><p class=\"ng-scope\">As the dosage increases, dopamine begins to stimulate <strong>\u03b2</strong><font size=\"2\"><sub><strong>1</strong></sub></font><strong>adrenergic receptors</strong> in the heart, increasing cardiac contractility, heart rate, and systolic blood pressure.&nbsp; At the high end of the dose range, stimulation of <strong>\u03b1</strong><font size=\"2\"><sub><strong>1</strong></sub></font><strong>receptors</strong> in the systemic vasculature leads to generalized vasoconstriction, which can result in decreased cardiac output due to the increased afterload.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Edrophonium is a short-acting acetylcholinesterase inhibitor used in the diagnosis of myasthenia gravis (ie, Tensilon test).&nbsp; Through its cholinergic effects, edrophonium reduces heart rate, cardiac conduction, and cardiac contractility.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Epinephrine increases heart rate, myocardial contractility, and cardiac output by stimulating \u03b2<font size=\"2\"><sub>1</sub></font> receptors.&nbsp; At low doses, stimulation of \u03b2<font size=\"2\"><sub>2</sub></font> receptors in the skeletal muscle vasculature produces vasodilation that can cause decreased blood pressure.&nbsp; At high doses, \u03b1<font size=\"2\"><sub>1</sub></font>-induced vasoconstriction in the skin and viscera becomes more pronounced.&nbsp; Epinephrine does not significantly increase renal blood flow (even at lower doses) due to the lack of D<font size=\"2\"><sub>1</sub></font> receptor stimulation.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Esmolol is a cardioselective \u03b2<font size=\"2\"><sub>1</sub></font> blocker with a short duration of action.&nbsp; Esmolol decreases heart rate, myocardial contractility, and cardiac conduction without affecting renal blood flow.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Phenylephrine is a selective \u03b1<font size=\"2\"><sub>1</sub></font>-adrenergic receptor agonist that causes a generalized increase in peripheral vascular resistance and blood pressure.&nbsp; Its use results in significant renal, splanchnic, and mesenteric vasoconstriction.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Low-dose dopamine infusion stimulates D<font size=\"2\"><sub>1</sub></font> receptors in the renal and mesenteric vasculature, resulting in vasodilation and increased blood flow to these sites.&nbsp; Increasing doses of dopamine stimulate \u03b2<font size=\"2\"><sub>1</sub></font> and \u03b1<font size=\"2\"><sub>1</sub></font> receptors, resulting in increased cardiac output and elevated systemic vascular resistance.&nbsp; At the high end of the dose range, the increase in afterload can result in decreased cardiac output.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pharmacology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Cardiovascular System</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Sympathomimetic agents</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
