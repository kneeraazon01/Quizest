Info.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot;  draggable=&quot;false&quot; id=&quot;40495&quot; src=&quot;https://www.uworld.com/media/L66514.jpg&quot;  ></p><p>This patient has a <strong>reduced left ventricular (LV) ejection fraction</strong> (&amp;lt;45%) due to acute <strong>myocardial infarction</strong>; echocardiography shows akinesia of the anterior and apical segments, findings consistent with infarction in those locations.&amp;nbsp; In the months following infarction, the affected myocardium undergoes fibroblastic proliferation and fibrosis to repair the necrotic region (ie, initial <strong>remodeling</strong> phase); disproportionate thinning of the infarcted area can lead to ballooning of affected segments and marked <strong>LV cavity enlargement</strong>.&amp;nbsp; Although LV cavity enlargement is initially beneficial as it acts as a compensatory mechanism for LV volume overload, progressive cavity enlargement (eg, eccentric hypertrophy) leads to <a ng-click=&quot;showexhibit('16018',true);&quot;>overwhelming wall stress</a> and further worsening of LV contractile dysfunction.&amp;nbsp; With progressive dilation, the LV begins to assume a spherical, rather than its normal elliptical, shape.</p><p>Much of the deleterious remodeling that occurs following myocardial infarction is likely driven by neurohormonal signaling via <strong>angiotensin II</strong>.&amp;nbsp; Accordingly, <strong>ACE inhibitors</strong> (eg, lisinopril) <strong>reduce the deleterious remodeling</strong> that takes place following myocardial infarction, <strong>minimizing LV dilation</strong> and helping preserve LV function.</p><p><strong>(Choices B and C)</strong>&amp;nbsp; Angiotensin II enhances post&amp;ndash;myocardial infarction collagen deposition to contribute to deleterious cardiac remodeling.&amp;nbsp; This effect is especially prominent in peri-infarct cardiomyocytes (cardiomyocytes at the edge of an infarction), which have high angiotensin II levels.&amp;nbsp; ACE inhibitors reduce deleterious cardiac remodeling at least in part via inhibition of myocardial collagen deposition and fibrosis.</p><p><strong>(Choice D)</strong>&amp;nbsp; Noninfarcted cardiac segments (eg, the basal segments in this patient) are subject to postinfarction remodeling caused by LV hemodynamic influences (eg, pressure and volume changes).&amp;nbsp; ACE inhibitors also reduce remodeling of these segments, further contributing to maintenance of the normal LV elliptical shape and preservation of LV contractile function.</p><p><strong>(Choice E)</strong>&amp;nbsp; Stroke work is the work done by the left ventricle with each contraction and is estimated by the product of stroke volume and LV systolic pressure (ie, stroke volume &amp;times; LV systolic pressure).&amp;nbsp; ACE inhibitors reduce stroke work by causing peripheral vasodilation and reducing systemic blood pressure (ie, reducing the pressure the left ventricle must overcome during each contraction).</p><p><strong>Educational objective:</strong><br>Following myocardial infarction, collagen deposition and fibrosis within the infarct region and&amp;nbsp;eccentric hypertrophy of surviving myocytes can lead to progressive left ventricular (LV) dilation and worsening contractile dysfunction.&amp;nbsp; Because much of this remodeling is driven by neurohormonal signaling via angiotensin II, ACE inhibitors (eg, lisinopril) reduce deleterious cardiac remodeling following myocardial infarction, minimizing LV dilation and helping preserve contractile function.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"40495\" src=\"image/L66514.jpg\"></p><p class=\"ng-scope\">This patient has a <strong>reduced left ventricular (LV) ejection fraction</strong> (&lt;45%) due to acute <strong>myocardial infarction</strong>; echocardiography shows akinesia of the anterior and apical segments, findings consistent with infarction in those locations.&nbsp; In the months following infarction, the affected myocardium undergoes fibroblastic proliferation and fibrosis to repair the necrotic region (ie, initial <strong>remodeling</strong> phase); disproportionate thinning of the infarcted area can lead to ballooning of affected segments and marked <strong>LV cavity enlargement</strong>.&nbsp; Although LV cavity enlargement is initially beneficial as it acts as a compensatory mechanism for LV volume overload, progressive cavity enlargement (eg, eccentric hypertrophy) leads to <a ng-click=\"showexhibit('16018',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L40799.jpg&quot;,&quot;title&quot;:&quot;L40799&quot;}]\">overwhelming wall stress</a> and further worsening of LV contractile dysfunction.&nbsp; With progressive dilation, the LV begins to assume a spherical, rather than its normal elliptical, shape.</p><p class=\"ng-scope\">Much of the deleterious remodeling that occurs following myocardial infarction is likely driven by neurohormonal signaling via <strong>angiotensin II</strong>.&nbsp; Accordingly, <strong>ACE inhibitors</strong> (eg, lisinopril) <strong>reduce the deleterious remodeling</strong> that takes place following myocardial infarction, <strong>minimizing LV dilation</strong> and helping preserve LV function.</p><p class=\"ng-scope\"><strong>(Choices B and C)</strong>&nbsp; Angiotensin II enhances post\u2013myocardial infarction collagen deposition to contribute to deleterious cardiac remodeling.&nbsp; This effect is especially prominent in peri-infarct cardiomyocytes (cardiomyocytes at the edge of an infarction), which have high angiotensin II levels.&nbsp; ACE inhibitors reduce deleterious cardiac remodeling at least in part via inhibition of myocardial collagen deposition and fibrosis.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Noninfarcted cardiac segments (eg, the basal segments in this patient) are subject to postinfarction remodeling caused by LV hemodynamic influences (eg, pressure and volume changes).&nbsp; ACE inhibitors also reduce remodeling of these segments, further contributing to maintenance of the normal LV elliptical shape and preservation of LV contractile function.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Stroke work is the work done by the left ventricle with each contraction and is estimated by the product of stroke volume and LV systolic pressure (ie, stroke volume \u00d7 LV systolic pressure).&nbsp; ACE inhibitors reduce stroke work by causing peripheral vasodilation and reducing systemic blood pressure (ie, reducing the pressure the left ventricle must overcome during each contraction).</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Following myocardial infarction, collagen deposition and fibrosis within the infarct region and&nbsp;eccentric hypertrophy of surviving myocytes can lead to progressive left ventricular (LV) dilation and worsening contractile dysfunction.&nbsp; Because much of this remodeling is driven by neurohormonal signaling via angiotensin II, ACE inhibitors (eg, lisinopril) reduce deleterious cardiac remodeling following myocardial infarction, minimizing LV dilation and helping preserve contractile function.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pharmacology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Cardiovascular System</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">ACE inhibitors</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
