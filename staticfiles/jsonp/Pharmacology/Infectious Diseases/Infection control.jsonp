            </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><div id=&quot;div_t79874&quot;><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; class=&quot;table-default-style&quot;><tbody><tr><td colspan=&quot;3&quot;><p align=&quot;center&quot;><strong>Commonly used clinical disinfectants</strong></p></td></tr><tr><td><p align=&quot;center&quot;><strong>Agent</strong></p></td><td><p align=&quot;center&quot;><strong>Mechanism</strong></p></td><td><p align=&quot;center&quot;><strong>Sporicidal</strong></p></td></tr><tr><td><p>Alcohols</p><p>(isopropanol, ethanol)</p></td><td><ul><li>Disruption of cell membranes</li><li>Denaturation of proteins</li></ul></td><td><p align=&quot;center&quot;>No</p></td></tr><tr><td><p>Chlorhexidine</p></td><td><ul><li>Disruption of cell membranes</li><li>Coagulation of cytoplasm</li></ul></td><td><p align=&quot;center&quot;>No</p></td></tr><tr><td><p>Hydrogen peroxide</p></td><td><ul><li>Produces destructive free radicals that oxidize cellular components</li></ul></td><td><p align=&quot;center&quot;>Yes</p></td></tr><tr><td><p>Iodine</p></td><td><ul><li>Halogenation of proteins &amp;amp; nucleic acids</li></ul></td><td><p align=&quot;center&quot;>Yes</p></td></tr></tbody></table></div></p><p>Alcohols, including ethanol and isopropanol, are widely used as disinfecting agents in the health care setting.&amp;nbsp; They are commonly used to clean the skin before immunization or venipuncture and to disinfect external surfaces of equipment.&amp;nbsp; They function by disorganizing the lipid structure in membranes, causing them to be leaky, and by denaturing cellular proteins.&amp;nbsp; Alcohols require the presence of water for maximal activity and are most effective at 60%-90% concentration.&amp;nbsp; They are rapidly bactericidal and also tuberculocidal, fungicidal, and virucidal, but do not destroy bacterial spores.</p><p><strong>(Choice A)</strong>&amp;nbsp; Chlorhexidine works by destabilizing cell membranes and coagulating intracellular constituents.&amp;nbsp; In combination with 70% alcohol, it is the antiseptic of choice for many surgical and percutaneous procedures.&amp;nbsp; Its use is contraindicated in neurologic, otologic, and ophthalmologic procedures due to neurotoxicity.</p><p><strong>(Choice B)</strong>&amp;nbsp; Formaldehyde and glutaraldehyde function by alkylating and cross-linking DNA and proteins.&amp;nbsp; They are most commonly used for sterilization of hospital instruments that cannot withstand autoclave temperatures.</p><p><strong>(Choice D)</strong>&amp;nbsp; Iodine causes halogenation of proteins and DNA.&amp;nbsp; Solutions containing iodine are frequently used for antisepsis in surgical and percutaneous procedures.&amp;nbsp; However, they are less effective than chlorhexidine-alcohol and cause more skin irritation and toxicity.</p><p><strong>(Choice E)</strong>&amp;nbsp; Hydrogen peroxide functions as an oxidant by producing hydroxyl free radicals that attack cellular components.&amp;nbsp; It is excellent at disinfecting inanimate objects but is less effective when used on organic materials.&amp;nbsp; Hydrogen peroxide is often used for skin cleansing and wound debridement due to its effervescent quality.</p><p><strong>Educational objective:</strong><br>Alcohols function by disorganizing the lipid structure in membranes, causing them to be leaky, and by denaturing cellular proteins.&amp;nbsp; They are bactericidal, tuberculocidal, fungicidal, and virucidal, but do not destroy bacterial spores.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"></p><div id=\"div_t79874\" class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" class=\"table-default-style\"><tbody><tr><td colspan=\"3\"><p align=\"center\"><strong>Commonly used clinical disinfectants</strong></p></td></tr><tr><td><p align=\"center\"><strong>Agent</strong></p></td><td><p align=\"center\"><strong>Mechanism</strong></p></td><td><p align=\"center\"><strong>Sporicidal</strong></p></td></tr><tr><td><p>Alcohols</p><p>(isopropanol, ethanol)</p></td><td><ul><li>Disruption of cell membranes</li><li>Denaturation of proteins</li></ul></td><td><p align=\"center\">No</p></td></tr><tr><td><p>Chlorhexidine</p></td><td><ul><li>Disruption of cell membranes</li><li>Coagulation of cytoplasm</li></ul></td><td><p align=\"center\">No</p></td></tr><tr><td><p>Hydrogen peroxide</p></td><td><ul><li>Produces destructive free radicals that oxidize cellular components</li></ul></td><td><p align=\"center\">Yes</p></td></tr><tr><td><p>Iodine</p></td><td><ul><li>Halogenation of proteins &amp; nucleic acids</li></ul></td><td><p align=\"center\">Yes</p></td></tr></tbody></table></div><p class=\"ng-scope\"></p><p class=\"ng-scope\">Alcohols, including ethanol and isopropanol, are widely used as disinfecting agents in the health care setting.&nbsp; They are commonly used to clean the skin before immunization or venipuncture and to disinfect external surfaces of equipment.&nbsp; They function by disorganizing the lipid structure in membranes, causing them to be leaky, and by denaturing cellular proteins.&nbsp; Alcohols require the presence of water for maximal activity and are most effective at 60%-90% concentration.&nbsp; They are rapidly bactericidal and also tuberculocidal, fungicidal, and virucidal, but do not destroy bacterial spores.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Chlorhexidine works by destabilizing cell membranes and coagulating intracellular constituents.&nbsp; In combination with 70% alcohol, it is the antiseptic of choice for many surgical and percutaneous procedures.&nbsp; Its use is contraindicated in neurologic, otologic, and ophthalmologic procedures due to neurotoxicity.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Formaldehyde and glutaraldehyde function by alkylating and cross-linking DNA and proteins.&nbsp; They are most commonly used for sterilization of hospital instruments that cannot withstand autoclave temperatures.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Iodine causes halogenation of proteins and DNA.&nbsp; Solutions containing iodine are frequently used for antisepsis in surgical and percutaneous procedures.&nbsp; However, they are less effective than chlorhexidine-alcohol and cause more skin irritation and toxicity.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Hydrogen peroxide functions as an oxidant by producing hydroxyl free radicals that attack cellular components.&nbsp; It is excellent at disinfecting inanimate objects but is less effective when used on organic materials.&nbsp; Hydrogen peroxide is often used for skin cleansing and wound debridement due to its effervescent quality.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Alcohols function by disorganizing the lipid structure in membranes, causing them to be leaky, and by denaturing cellular proteins.&nbsp; They are bactericidal, tuberculocidal, fungicidal, and virucidal, but do not destroy bacterial spores.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/9880479\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/9880479\"><span compiledata=\"\" htmlcontent=\"Antiseptics and disinfectants:  activity, action, and resistance.\" class=\"ng-scope\"><p class=\"ng-scope\">Antiseptics and disinfectants:  activity, action, and resistance.</p></span></a><!-- end ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal -->\n                                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal -->\n                                </li><!-- end ngRepeat: questionReference in currentQuestion.questionMappingReferencesList -->\n                            </ul>\n                        </div>\n                    </div><!-- end ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQ