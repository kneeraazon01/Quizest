!currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;1690&quot; src=&quot;https://www.uworld.com/media/L10969.jpg&quot; ></p><p><strong>Enterobiasis</strong>, caused by the <strong>pinworm</strong> (<em>Enterobius vermicularis</em>), is the most prevalent helminthic infection in the United States and is frequently seen in <strong>children</strong>.&amp;nbsp; The life cycle occurs in the gastrointestinal tract, and the adult worms typically live in the cecum and appendix.&amp;nbsp; In contrast to other worms that release eggs into the intestine, the female <strong>worm migrates to the perianal area at night</strong> to deposit eggs.&amp;nbsp; Larvae mature in the eggs within 6 hours, and the infective eggs can either be ingested by the same individual or spread to other humans.&amp;nbsp; Eggs and worms in the perianal area cause an inflammatory reaction, resulting in <strong>perianal itching</strong> (pruritus ani).&amp;nbsp; Scratching contaminates the hands and furthers transmission.</p><p>Diagnosis is made by the <strong>cellulose tape test</strong> (ie, microscopy of adhesive tape applied to the perianal region), which reveals oval <a ng-click=&quot;showexhibit('1736',true);&quot;>eggs</a> flattened on one side.&amp;nbsp; Treatment options include <strong>albendazole</strong>, mebendazole, and pyrantel pamoate.</p><p><strong>(Choice B)</strong>&amp;nbsp; Diethylcarbamazine is indicated for the treatment of <em>Loa loa</em> (loiasis) and <em>Wuchereria bancrofti</em> (lymphatic filariasis) infections.</p><p><strong>(Choice C)</strong>&amp;nbsp; Ivermectin is used to treat infections caused by <em>Strongyloides stercoralis</em> (strongyloidiasis) and <em>Onchocerca volvulus</em> (onchocerciasis).</p><p><strong>(Choice D)</strong>&amp;nbsp; Nifurtimox is used to treat diseases caused by trypanosomes, including <em>Trypanosoma cruzi</em> (Chagas disease).</p><p><strong>(Choice E)</strong>&amp;nbsp; Infections caused by <em>Schistosoma</em> (schistosomiasis), <em>Clonorchis sinensis</em> (clonorchiasis), and <em>Paragonimus westermani</em> (paragonimiasis) are treated with praziquantel.</p><p><strong>Educational objective:</strong><br><em>Enterobius vermicularis</em> infection (enterobiasis) occurs most frequently in children and presents with perianal pruritus.&amp;nbsp; Diagnosis is made by identifying eggs in the perianal region (cellulose tape test).&amp;nbsp; Treatment options include albendazole, mebendazole, and pyrantel pamoate.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"1690\" src=\"image/L10969.jpg\"></p><p class=\"ng-scope\"><strong>Enterobiasis</strong>, caused by the <strong>pinworm</strong> (<em>Enterobius vermicularis</em>), is the most prevalent helminthic infection in the United States and is frequently seen in <strong>children</strong>.&nbsp; The life cycle occurs in the gastrointestinal tract, and the adult worms typically live in the cecum and appendix.&nbsp; In contrast to other worms that release eggs into the intestine, the female <strong>worm migrates to the perianal area at night</strong> to deposit eggs.&nbsp; Larvae mature in the eggs within 6 hours, and the infective eggs can either be ingested by the same individual or spread to other humans.&nbsp; Eggs and worms in the perianal area cause an inflammatory reaction, resulting in <strong>perianal itching</strong> (pruritus ani).&nbsp; Scratching contaminates the hands and furthers transmission.</p><p class=\"ng-scope\">Diagnosis is made by the <strong>cellulose tape test</strong> (ie, microscopy of adhesive tape applied to the perianal region), which reveals oval <a ng-click=\"showexhibit('1736',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L13506.jpg&quot;,&quot;title&quot;:&quot;L13506&quot;}]\">eggs</a> flattened on one side.&nbsp; Treatment options include <strong>albendazole</strong>, mebendazole, and pyrantel pamoate.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Diethylcarbamazine is indicated for the treatment of <em>Loa loa</em> (loiasis) and <em>Wuchereria bancrofti</em> (lymphatic filariasis) infections.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Ivermectin is used to treat infections caused by <em>Strongyloides stercoralis</em> (strongyloidiasis) and <em>Onchocerca volvulus</em> (onchocerciasis).</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Nifurtimox is used to treat diseases caused by trypanosomes, including <em>Trypanosoma cruzi</em> (Chagas disease).</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Infections caused by <em>Schistosoma</em> (schistosomiasis), <em>Clonorchis sinensis</em> (clonorchiasis), and <em>Paragonimus westermani</em> (paragonimiasis) are treated with praziquantel.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br><em>Enterobius vermicularis</em> infection (enterobiasis) occurs most frequently in children and presents with perianal pruritus.&nbsp; Diagnosis is made by identifying eggs in the perianal region (cellulose tape test).&nbsp; Treatment options include albendazole, mebendazole, and pyrantel pamoate.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pharmacology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Infectious Diseases</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Intestinal helminth parasites</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
