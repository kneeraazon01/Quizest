\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;34028&quot; src=&quot;https://www.uworld.com/media/L17816.jpg&quot; ></p><p>This patient with <strong>symptomatic anemia</strong> (eg, fatigue, exercise intolerance) has peripheral blood smear findings that indicate <strong>hemolytic anemia</strong>, including the following:</p><ul><li><p><strong>Increased reticulocytes:</strong>&amp;nbsp; Reticulocytes are larger than mature red blood cells (RBCs), lack central pallor, and have a bluish coloration (<strong>polychromatic RBCs</strong>) due to the presence of ribosomes, which are not found in mature RBCs.&amp;nbsp; Increased reticulocytes in the peripheral blood is a nonspecific finding that indicates the bone marrow is responding appropriately to anemia by increasing production of new erythrocytes.</p></li><li><p><strong>Spherocytes:</strong>&amp;nbsp; Spherocytes are erythrocytes with reduced cell membrane, which makes the cell spherical rather than biconcave.&amp;nbsp; In otherwise healthy individuals, spherocytes generally arise in the setting of <strong>immune-mediated</strong> hemolytic anemia; in this condition, IgG-coated erythrocytes are partially phagocytized by splenic macrophages, which reduces the surface area of the erythrocyte cell membrane, thereby causing the erythrocyte to assume a spherical shape.</p></li><li><p><strong>Nucleated erythrocytes:</strong>&amp;nbsp; Nucleated erythrocytes are very immature RBCs.&amp;nbsp; The presence of nucleated RBCs in the peripheral blood indicates a strong bone marrow response to compensate for the anemia.</p></li></ul><p><strong>(Choice A)</strong>&amp;nbsp; Anemia due to erythropoietin deficiency is often seen in patients with end-stage renal disease because erythropoietin production by the kidneys is impaired.&amp;nbsp; Individuals with erythropoietin deficiency are not able to sufficiently increase erythrocytosis in response to anemia; therefore, peripheral blood smear does not typically show increased reticulocytes or nucleated RBCs.</p><p><strong>(Choice C)</strong>&amp;nbsp; <a ng-click=&quot;showexhibit('34029',true);&quot;>Iron deficiency anemia</a> can be seen in young women with heavy menstruation or gastrointestinal bleeding.&amp;nbsp; However, peripheral blood smear typically shows hypochromatic RBCs and microcytosis.</p><p><strong>(Choice D)</strong>&amp;nbsp; <a ng-click=&quot;showexhibit('1016',true);&quot;>Thrombotic microangiopathy</a> is associated with microvascular clot formation, which causes anemia via RBC shearing.&amp;nbsp; In this condition, peripheral blood smear typically shows schistocytes, not spherocytes.</p><p><strong>(Choice E)</strong>&amp;nbsp; Vitamin B<font size=&quot;2&quot;><sub>12</sub></font> deficiency is common in patients with atrophic gastritis and alcohol use disorder.&amp;nbsp; It commonly causes <a ng-click=&quot;showexhibit('23850',true);&quot;>macrocytic anemia</a> with hypersegmented neutrophils on peripheral blood smear.</p><p><strong>Educational objective:</strong><br>In patients with hemolytic anemia, peripheral blood smear usually reveals evidence of reticulocytosis, spherocytes, and nucleated red blood cells.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"34028\" src=\"image/L17816.jpg\"></p><p class=\"ng-scope\">This patient with <strong>symptomatic anemia</strong> (eg, fatigue, exercise intolerance) has peripheral blood smear findings that indicate <strong>hemolytic anemia</strong>, including the following:</p><ul class=\"ng-scope\"><li><p><strong>Increased reticulocytes:</strong>&nbsp; Reticulocytes are larger than mature red blood cells (RBCs), lack central pallor, and have a bluish coloration (<strong>polychromatic RBCs</strong>) due to the presence of ribosomes, which are not found in mature RBCs.&nbsp; Increased reticulocytes in the peripheral blood is a nonspecific finding that indicates the bone marrow is responding appropriately to anemia by increasing production of new erythrocytes.</p></li><li><p><strong>Spherocytes:</strong>&nbsp; Spherocytes are erythrocytes with reduced cell membrane, which makes the cell spherical rather than biconcave.&nbsp; In otherwise healthy individuals, spherocytes generally arise in the setting of <strong>immune-mediated</strong> hemolytic anemia; in this condition, IgG-coated erythrocytes are partially phagocytized by splenic macrophages, which reduces the surface area of the erythrocyte cell membrane, thereby causing the erythrocyte to assume a spherical shape.</p></li><li><p><strong>Nucleated erythrocytes:</strong>&nbsp; Nucleated erythrocytes are very immature RBCs.&nbsp; The presence of nucleated RBCs in the peripheral blood indicates a strong bone marrow response to compensate for the anemia.</p></li></ul><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Anemia due to erythropoietin deficiency is often seen in patients with end-stage renal disease because erythropoietin production by the kidneys is impaired.&nbsp; Individuals with erythropoietin deficiency are not able to sufficiently increase erythrocytosis in response to anemia; therefore, peripheral blood smear does not typically show increased reticulocytes or nucleated RBCs.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; <a ng-click=\"showexhibit('34029',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L23445.jpg&quot;,&quot;title&quot;:&quot;L23445&quot;}]\">Iron deficiency anemia</a> can be seen in young women with heavy menstruation or gastrointestinal bleeding.&nbsp; However, peripheral blood smear typically shows hypochromatic RBCs and microcytosis.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; <a ng-click=\"showexhibit('1016',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L6777.jpg&quot;,&quot;title&quot;:&quot;L6777&quot;}]\">Thrombotic microangiopathy</a> is associated with microvascular clot formation, which causes anemia via RBC shearing.&nbsp; In this condition, peripheral blood smear typically shows schistocytes, not spherocytes.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Vitamin B<font size=\"2\"><sub>12</sub></font> deficiency is common in patients with atrophic gastritis and alcohol use disorder.&nbsp; It commonly causes <a ng-click=\"showexhibit('23850',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L25598.png&quot;,&quot;title&quot;:&quot;L25598&quot;}]\">macrocytic anemia</a> with hypersegmented neutrophils on peripheral blood smear.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>In patients with hemolytic anemia, peripheral blood smear usually reveals evidence of reticulocytosis, spherocytes, and nucleated red blood cells.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.