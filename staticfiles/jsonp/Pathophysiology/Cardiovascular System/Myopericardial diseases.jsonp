n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p>This patient&amp;#39;s echocardiogram findings are consistent with <strong>athlete&amp;#39;s heart</strong> (ie, the physiologic cardiac adaptations that occur in highly trained athletes).&amp;nbsp; <strong>Endurance exercise</strong> (eg, long-distance running) mostly increases the volume load on the heart (increased cardiac output) with a small component of increased pressure load (increased afterload).&amp;nbsp; As a result, the left ventricle predominantly undergoes eccentric hypertrophy (<strong>cavity enlargement</strong> improves diastolic function) with a small amount of concentric <a ng-click=&quot;showexhibit('2795',true);&quot;>hypertrophy</a> (wall thickening increases systolic function).&amp;nbsp; The right ventricular cavity also slightly enlarges to support an increase in cardiac output, and there is <strong>enhanced coronary capillary development</strong> and a <strong>decreased resting heart rate</strong> due to improved efficiency of cardiac pumping.</p><p>Because these <strong>physiologic changes improve cardiac function</strong>, athlete&amp;#39;s heart is not associated with an increased risk of cardiovascular morbidity or mortality.&amp;nbsp; However, it is imperative that the changes of athlete&amp;#39;s heart are differentiated from similar cardiac anomalies that may reflect serious underlying pathology that could increase the risk of sudden cardiac death (eg, hypertrophic cardiomyopathy).&amp;nbsp; Findings that favor the presence of hypertrophic cardiomyopathy <strong>(Choice C)</strong> include left ventricular (LV) hypertrophy predominantly affecting the septum, LV hypertrophy with reduced LV cavity size, impaired diastolic function, and a poorly developed coronary capillary network.</p><p><strong>(Choice A)</strong>&amp;nbsp; Anabolic steroid abuse can cause pathologic concentric LV hypertrophy, characterized by reduced diastolic function and poor coronary vascularization.</p><p><strong>(Choice D)</strong>&amp;nbsp; Severe coronary artery disease can lead to ischemic cardiomyopathy, which typically manifests as dilated cardiomyopathy with an enlarged LV cavity but thin LV walls with impaired systolic function.</p><p><strong>(Choice E)</strong>&amp;nbsp; Viral myocarditis may sometimes lead to dilated cardiomyopathy with eccentric hypertrophy and impaired LV systolic function.&amp;nbsp; Subclinical disease is typically quite mild and resolves without sequelae.</p><p><strong>Educational objective:</strong><br>Athlete&amp;#39;s heart refers to physiologic cardiac adaptations that improve cardiac function in response to high-level endurance training.&amp;nbsp; There is predominant left ventricular (LV) eccentric hypertrophy with a small component of concentric hypertrophy, leading to an overall increase in LV mass, enlarged LV cavity size, slightly increased LV wall thickness, and decreased resting heart rate.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\">This patient's echocardiogram findings are consistent with <strong>athlete's heart</strong> (ie, the physiologic cardiac adaptations that occur in highly trained athletes).&nbsp; <strong>Endurance exercise</strong> (eg, long-distance running) mostly increases the volume load on the heart (increased cardiac output) with a small component of increased pressure load (increased afterload).&nbsp; As a result, the left ventricle predominantly undergoes eccentric hypertrophy (<strong>cavity enlargement</strong> improves diastolic function) with a small amount of concentric <a ng-click=\"showexhibit('2795',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L13550.jpg&quot;,&quot;title&quot;:&quot;L13550&quot;}]\">hypertrophy</a> (wall thickening increases systolic function).&nbsp; The right ventricular cavity also slightly enlarges to support an increase in cardiac output, and there is <strong>enhanced coronary capillary development</strong> and a <strong>decreased resting heart rate</strong> due to improved efficiency of cardiac pumping.</p><p class=\"ng-scope\">Because these <strong>physiologic changes improve cardiac function</strong>, athlete's heart is not associated with an increased risk of cardiovascular morbidity or mortality.&nbsp; However, it is imperative that the changes of athlete's heart are differentiated from similar cardiac anomalies that may reflect serious underlying pathology that could increase the risk of sudden cardiac death (eg, hypertrophic cardiomyopathy).&nbsp; Findings that favor the presence of hypertrophic cardiomyopathy <strong>(Choice C)</strong> include left ventricular (LV) hypertrophy predominantly affecting the septum, LV hypertrophy with reduced LV cavity size, impaired diastolic function, and a poorly developed coronary capillary network.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Anabolic steroid abuse can cause pathologic concentric LV hypertrophy, characterized by reduced diastolic function and poor coronary vascularization.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Severe coronary artery disease can lead to ischemic cardiomyopathy, which typically manifests as dilated cardiomyopathy with an enlarged LV cavity but thin LV walls with impaired systolic function.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Viral myocarditis may sometimes lead to dilated cardiomyopathy with eccentric hypertrophy and impaired LV systolic function.&nbsp; Subclinical disease is typically quite mild and resolves without sequelae.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Athlete's heart refers to physiologic cardiac adaptations that improve cardiac function in response to high-level endurance training.&nbsp; There is predominant left ventricular (LV) eccentric hypertrophy with a small component of concentric hypertrophy, leading to an overall increase in LV mass, enlarged LV cavity size, slightly increased LV wall thickness, and decreased resting heart rate.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.lega