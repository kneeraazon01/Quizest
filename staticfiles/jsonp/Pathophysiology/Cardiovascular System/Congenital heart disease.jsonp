stInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;37810&quot; src=&quot;https://www.uworld.com/media/L63361.jpg&quot; ></p><p>This infant has a harsh <strong>systolic ejection murmur</strong> that disappears during an episode of <strong>cyanosis</strong>, an expected finding with <strong>tetralogy of Fallot</strong> (ToF).&amp;nbsp; The major <a ng-click=&quot;showexhibit('752',true);&quot;>defects</a> in ToF are right ventricular outflow tract (RVOT) obstruction and a ventricular septal defect (VSD); the murmur is created by blood flow through the narrowed RVOT, not the VSD.</p><p>The degree of RVOT obstruction is the major driver of symptoms in ToF because it determines the direction and magnitude of shunting through the VSD.&amp;nbsp; A certain degree of RVOT obstruction is fixed, but some degree is dynamic and can change based on activity (eg, feeding, squatting) that alters the underlying hemodynamics.&amp;nbsp; Many patients have left-to-right or neutral shunting at rest (and are acyanotic), but they develop cyanosis <strong>(&amp;#x22;tet&amp;#x22; episodes)</strong> when a <strong>dynamic increase in RVOT obstruction</strong> (eg, precipitated by crying or feeding) leads to <strong>increased right-to-left shunting</strong> through the VSD.</p><p>During such an episode, decreased blood flow through the RVOT causes the systolic murmur to disappear.&amp;nbsp; In addition, <strong>oxygen saturation (SaO<font size=&quot;2&quot;><sub>2</sub></font>) in the left ventricle is reduced</strong> (eg, &amp;lt;90%) due to mixing with deoxygenated blood from the RV.&amp;nbsp; The SaO<font size=&quot;2&quot;><sub>2</sub></font> in the left atrium (LA) remains normal as the LA continues to receive oxygenated blood from the pulmonary veins.</p><p><strong>(Choice B)</strong>&amp;nbsp; These values show a step-down in SaO<font size=&quot;2&quot;><sub>2</sub></font> at the LA (pulmonary vein SaO<font size=&quot;2&quot;><sub>2</sub></font> &amp;gt; LA SaO<font size=&quot;2&quot;><sub>2</sub></font>), consistent with a right-to-left shunt through an atrial septal defect (ASD).&amp;nbsp; Right-to-left shunting through an ASD (ie, Eisenmenger syndrome) takes years to develop and is not an expected cause of episodic cyanosis in infants.</p><p><strong>(Choice C)</strong>&amp;nbsp; These values show a step-up in SaO<font size=&quot;2&quot;><sub>2</sub></font> at the RV (RV SaO<font size=&quot;2&quot;><sub>2</sub></font> &amp;gt; right atrium SaO<font size=&quot;2&quot;><sub>2</sub></font>), consistent with left-to-right shunting through a VSD, as occurs during acyanotic periods in patients with ToF.</p><p><strong>(Choice D)</strong>&amp;nbsp; These values reflect the SaO<font size=&quot;2&quot;><sub>2</sub></font> distribution in a normal heart.</p><p><strong>(Choice E)</strong>&amp;nbsp; These values show a step-up in SaO<font size=&quot;2&quot;><sub>2</sub></font> at the pulmonary artery (pulmonary artery SaO<font size=&quot;2&quot;><sub>2</sub></font> &amp;gt; RV SaO<font size=&quot;2&quot;><sub>2</sub></font>), consistent with left-to-right shunting through a <a ng-click=&quot;showexhibit('8402',true);&quot;>patent ductus arteriosus</a>.&amp;nbsp; Left-to-right shunts do not cause cyanosis.</p><p><strong>Educational objective:</strong><br>Tetralogy of Fallot involves right ventricular outflow tract (RVOT) obstruction and a ventricular septal defect (VSD).&amp;nbsp; Cyanotic (&amp;#x22;tet&amp;#x22;) episodes are commonly precipitated by a dynamic increase in RVOT obstruction that increases right-to-left shunting through the VSD.&amp;nbsp; These episodes involve decreased blood flow through the RVOT, causing disappearance of the associated systolic murmur, and reduced oxygen saturation of the left ventricular blood being pumped into the systemic circulation.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"37810\" src=\"image/L63361.jpg\"></p><p class=\"ng-scope\">This infant has a harsh <strong>systolic ejection murmur</strong> that disappears during an episode of <strong>cyanosis</strong>, an expected finding with <strong>tetralogy of Fallot</strong> (ToF).&nbsp; The major <a ng-click=\"showexhibit('752',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L1458.jpg&quot;,&quot;title&quot;:&quot;L1458&quot;}]\">defects</a> in ToF are right ventricular outflow tract (RVOT) obstruction and a ventricular septal defect (VSD); the murmur is created by blood flow through the narrowed RVOT, not the VSD.</p><p class=\"ng-scope\">The degree of RVOT obstruction is the major driver of symptoms in ToF because it determines the direction and magnitude of shunting through the VSD.&nbsp; A certain degree of RVOT obstruction is fixed, but some degree is dynamic and can change based on activity (eg, feeding, squatting) that alters the underlying hemodynamics.&nbsp; Many patients have left-to-right or neutral shunting at rest (and are acyanotic), but they develop cyanosis <strong>(\"tet\" episodes)</strong> when a <strong>dynamic increase in RVOT obstruction</strong> (eg, precipitated by crying or feeding) leads to <strong>increased right-to-left shunting</strong> through the VSD.</p><p class=\"ng-scope\">During such an episode, decreased blood flow through the RVOT causes the systolic murmur to disappear.&nbsp; In addition, <strong>oxygen saturation (SaO<font size=\"2\"><sub>2</sub></font>) in the left ventricle is reduced</strong> (eg, &lt;90%) due to mixing with deoxygenated blood from the RV.&nbsp; The SaO<font size=\"2\"><sub>2</sub></font> in the left atrium (LA) remains normal as the LA continues to receive oxygenated blood from the pulmonary veins.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; These values show a step-down in SaO<font size=\"2\"><sub>2</sub></font> at the LA (pulmonary vein SaO<font size=\"2\"><sub>2</sub></font> &gt; LA SaO<font size=\"2\"><sub>2</sub></font>), consistent with a right-to-left shunt through an atrial septal defect (ASD).&nbsp; Right-to-left shunting through an ASD (ie, Eisenmenger syndrome) takes years to develop and is not an expected cause of episodic cyanosis in infants.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; These values show a step-up in SaO<font size=\"2\"><sub>2</sub></font> at the RV (RV SaO<font size=\"2\"><sub>2</sub></font> &gt; right atrium SaO<font size=\"2\"><sub>2</sub></font>), consistent with left-to-right shunting through a VSD, as occurs during acyanotic periods in patients with ToF.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; These values reflect the SaO<font size=\"2\"><sub>2</sub></font> distribution in a normal heart.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; These values show a step-up in SaO<font size=\"2\"><sub>2</sub></font> at the pulmonary artery (pulmonary artery SaO<font size=\"2\"><sub>2</sub></font> &gt; RV SaO<font size=\"2\"><sub>2</sub></font>), consistent with left-to-right shunting through a <a ng-click=\"showexhibit('8402',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L31501.jpg&quot;,&quot;title&quot;:&quot;L31501&quot;}]\">patent ductus arteriosus</a>.&nbsp; Left-to-right shunts do not cause cyanosis.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Tetralogy of Fallot involves right ventricular outflow tract (RVOT) obstruction and a ventricular septal defect (VSD).&nbsp; Cyanotic (\"tet\") episodes are commonly precipitated by a dynamic increase in RVOT obstruction that increases right-to-left shunting through the VSD.&nbsp; These episodes involve decreased blood flow through the RVOT, causing disappearance of the associated systolic murmur, and reduced oxygen saturation of the left ventricular blood being pumped into the systemic circulation.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi