p) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p>This patient has manifestations characteristic of <strong>severe combined immunodeficiency</strong> (SCID).&amp;nbsp; <strong>Adenosine deaminase (ADA) deficiency</strong> is the second most common cause of SCID, after X-linked SCID (eg, IL-2 receptor mutation).&amp;nbsp; ADA is present throughout the human body and functions to eliminate excess adenosine from cells by deaminating adenosine to inosine.&amp;nbsp; Adenosine accumulation is highly toxic to mitotically active cells such as developing lymphocytes; ADA insufficiency leads to widespread death of both T- and B-lymphocytes with resultant combined <strong>cellular and humoral immunodeficiency</strong>.&amp;nbsp; Because both humoral and cell-mediated immunity are deficient in these patients, they are vulnerable to recurrent, severe infections by bacteria, viruses, and fungi.</p><p>Treatment is with hematopoietic (bone marrow) transplantation, but <strong>retroviral gene therapy</strong> is a promising treatment for patients without an HLA match.&amp;nbsp; Retroviral vectors are used to infect patient hematopoietic stem cells with the genetic code for ADA, resulting in production of ADA by all daughter cells of that stem cell.</p><p><strong>(Choice B)</strong>&amp;nbsp; Myeloperoxidase is a neutrophil enzyme that helps kill phagocytosed organisms by catalyzing the production of hypochlorite (bleach) from hydrogen peroxide and chloride.&amp;nbsp; A deficiency of this enzyme results in immunodeficiency characterized by recurrent <em>Candida</em> infections.</p><p><strong>(Choice C)</strong>&amp;nbsp; NADPH oxidase is the enzyme that is deficient in chronic granulomatous disease (CGD).&amp;nbsp; Without NADPH oxidase, neutrophils are unable to form reactive oxygen species such as superoxide.&amp;nbsp; CGD manifests clinically with recurrent infections by catalase-producing organisms (eg, staphylococci).&amp;nbsp; Dihydrorhodamine or nitroblue tetrazolium testing can be used to diagnose CGD.</p><p><strong>(Choice D)</strong>&amp;nbsp; Reverse transcriptase is a retroviral enzyme that converts viral genomic RNA (in this case, ADA) to DNA.&amp;nbsp; Although it plays a role in the effectiveness of the retrovirus as a vector for the treatment of SCID, reverse transcriptase is not deficient in patients with this condition.</p><p><strong>(Choice E)</strong>&amp;nbsp; Xanthine oxidase is the enzyme responsible for the formation of uric acid from hypoxanthine and xanthine and is essential as the final step of purine degradation.&amp;nbsp; Xanthine oxidase deficiency leads to abnormally low levels of uric acid in the serum; it can also lead to renal calculi due to the relative insolubility of xanthine at urine&amp;#39;s pH level.</p><p><strong>Educational objective:</strong><br>The second most common cause of severe combined immunodeficiency is autosomal recessive deficiency of adenosine deaminase, an enzyme necessary for the elimination of excess adenosine within cells.&amp;nbsp; Toxic levels of adenosine accumulate within lymphocytes in this condition, leading to lymphocyte cell death and resultant cellular and humoral immunodeficiency.&amp;nbsp; Patients with this condition can be treated with hematopoietic cell transplantation or gene therapy.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\">This patient has manifestations characteristic of <strong>severe combined immunodeficiency</strong> (SCID).&nbsp; <strong>Adenosine deaminase (ADA) deficiency</strong> is the second most common cause of SCID, after X-linked SCID (eg, IL-2 receptor mutation).&nbsp; ADA is present throughout the human body and functions to eliminate excess adenosine from cells by deaminating adenosine to inosine.&nbsp; Adenosine accumulation is highly toxic to mitotically active cells such as developing lymphocytes; ADA insufficiency leads to widespread death of both T- and B-lymphocytes with resultant combined <strong>cellular and humoral immunodeficiency</strong>.&nbsp; Because both humoral and cell-mediated immunity are deficient in these patients, they are vulnerable to recurrent, severe infections by bacteria, viruses, and fungi.</p><p class=\"ng-scope\">Treatment is with hematopoietic (bone marrow) transplantation, but <strong>retroviral gene therapy</strong> is a promising treatment for patients without an HLA match.&nbsp; Retroviral vectors are used to infect patient hematopoietic stem cells with the genetic code for ADA, resulting in production of ADA by all daughter cells of that stem cell.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Myeloperoxidase is a neutrophil enzyme that helps kill phagocytosed organisms by catalyzing the production of hypochlorite (bleach) from hydrogen peroxide and chloride.&nbsp; A deficiency of this enzyme results in immunodeficiency characterized by recurrent <em>Candida</em> infections.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; NADPH oxidase is the enzyme that is deficient in chronic granulomatous disease (CGD).&nbsp; Without NADPH oxidase, neutrophils are unable to form reactive oxygen species such as superoxide.&nbsp; CGD manifests clinically with recurrent infections by catalase-producing organisms (eg, staphylococci).&nbsp; Dihydrorhodamine or nitroblue tetrazolium testing can be used to diagnose CGD.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Reverse transcriptase is a retroviral enzyme that converts viral genomic RNA (in this case, ADA) to DNA.&nbsp; Although it plays a role in the effectiveness of the retrovirus as a vector for the treatment of SCID, reverse transcriptase is not deficient in patients with this condition.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Xanthine oxidase is the enzyme responsible for the formation of uric acid from hypoxanthine and xanthine and is essential as the final step of purine degradation.&nbsp; Xanthine oxidase deficiency leads to abnormally low levels of uric acid in the serum; it can also lead to renal calculi due to the relative insolubility of xanthine at urine's pH level.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>The second most common cause of severe combined immunodeficiency is autosomal recessive deficiency of adenosine deaminase, an enzyme necessary for the elimination of excess adenosine within cells.&nbsp; Toxic levels of adenosine accumulate within lymphocytes in this condition, leading to lymphocyte cell death and resultant cellular and humoral immunodeficiency.&nbsp; Patients with this condition can be treated with hematopoietic cell transplantation or gene therapy.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pathophysiology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Allergy &amp; Immunology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Severe combined immunodeficiency disease</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        