ice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><div id=&quot;div_t39754&quot;><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; class=&quot;table-default-style&quot;><tbody><tr><td colspan=&quot;3&quot;><p align=&quot;center&quot;><strong>Common causes of myopathy </strong></p></td></tr><tr><td><p align=&quot;center&quot;><strong>Disorder</strong></p></td><td><p align=&quot;center&quot;><strong>Clinical features</strong></p></td><td><p align=&quot;center&quot;><strong>Creatine<br>kinase</strong></p></td></tr><tr><td><p align=&quot;center&quot;><strong>Glucocorticoid-<br>induced<br>myopathy</strong></p></td><td><ul><li>Progressive proximal muscle weakness<br>&amp;amp; atrophy <strong>without pain</strong></li><li>Lower extremity muscles more involved</li></ul></td><td><p align=&quot;center&quot;>Normal</p></td></tr><tr><td><p align=&quot;center&quot;><strong>Polymyalgia<br>rheumatica</strong></p></td><td><ul><li>Muscle <strong>pain &amp;amp; stiffness</strong> in shoulder, neck<br>&amp;amp; pelvic girdle</li><li>Worse in morning &amp;amp; with activity</li></ul></td><td><p align=&quot;center&quot;>Normal</p></td></tr><tr><td><p align=&quot;center&quot;><strong>Inflammatory<br>myopathies </strong><br>(eg, polymyositis,<br>dermatomyositis)</p></td><td style=&quot;vertical-align: top;&quot;><ul><li>Proximal muscle weakness</li><li><strong>Skin rash</strong> &amp;amp; <strong>inflammatory arthritis</strong><br>possibly present</li></ul></td><td><p align=&quot;center&quot;>&amp;#8593;</p></td></tr><tr><td><p align=&quot;center&quot;><strong>Statin-induced<br>myopathy</strong></p></td><td><ul><li>Proximal muscle pain &amp;amp; weakness usually<br>within weeks to months<strong> after starting<br>statin</strong></li></ul></td><td><p align=&quot;center&quot;>&amp;#8593;</p></td></tr><tr><td><p align=&quot;center&quot;><strong>Hypothyroid<br>myopathy</strong></p></td><td><ul><li>Muscle pain, cramps &amp;amp; weakness involving<br>the proximal muscles</li><li><strong>Delayed tendon reflexes</strong> &amp;amp; myoedema</li><li>Features of<strong> </strong>hypothyroidism</li></ul></td><td><p align=&quot;center&quot;>&amp;#8593;</p></td></tr></tbody></table></div><p>An <strong>elevated creatine kinase</strong> (CK) level suggests a myopathic process with <strong>myocyte damage</strong> and release of muscle enzymes into the circulation.&amp;nbsp; Common causes of myopathy with elevated CK include hypothyroidism, muscular dystrophies, inflammatory muscle diseases, and medications such as HMG-CoA reductase inhibitors.</p><p>This patient&amp;#39;s fatigue, <strong>weight gain</strong>, <strong>bradycardia</strong>, and <strong>brittle nails</strong> are highly suggestive of <strong>hypothyroid myopathy</strong>.&amp;nbsp; Decreased thyroid hormone levels impair muscle glycogen and triglyceride use and increase oxidative stress, resulting in myocyte injury.&amp;nbsp; Patients typically presents with myalgias, proximal muscle weakness, decreased exercise tolerance, and cramping.&amp;nbsp; <strong>Myoedema </strong>(focal muscle contraction at the site of percussion) and <strong>delayed relaxation</strong> of deep tendon reflexes are characteristic findings that are caused by impaired reuptake of calcium by the sarcoplasmic reticulum.</p><p>Hypothyroid myopathy and elevations in CK can be the first signs of hypothyroidism and can precede other manifestations by months to years.&amp;nbsp; The diagnosis can be confirmed with an <strong>elevated TSH level</strong>.</p><p><strong>(Choice A)</strong>&amp;nbsp; A 24-hour urine cortisol test is used as an initial test for Cushing syndrome.&amp;nbsp; This disorder can present with proximal muscle weakness and atrophy due to the direct catabolic effects of cortisol on skeletal muscle.&amp;nbsp; However, there is no associated muscle pain or rise in CK, reflexes are normal, and other cushingoid features (eg, stria, dorsocervical fatty hypertrophy) are usually present.</p><p><strong>(Choices B and C)</strong>&amp;nbsp; Myasthenia gravis is caused by autoantibodies against nicotinic acetylcholine receptors on the postsynaptic membrane of the neuromuscular junction.&amp;nbsp; It presents with fluctuating weakness that worsens through the day.&amp;nbsp; Lambert-Eaton syndrome is a paraneoplastic disorder associated with small cell carcinoma of the lung (which can be identified on CT scan).&amp;nbsp; It is caused by autoantibodies against voltage-gated calcium channels on the presynaptic membrane, which cause decreased release of acetylcholine.&amp;nbsp; These disorders affect the neuromuscular junction rather than the muscle itself, and CK is normal.</p><p><strong>(Choice D)</strong>&amp;nbsp; Muscle biopsy is often needed for diagnosis of inflammatory myositis (eg, polymyositis, dermatomyositis).&amp;nbsp; However, this patient has symptoms suggestive of hypothyroidism (eg, weight gain, bradycardia, brittle nails), and histologic findings in hypothyroid myopathy are often nonspecific.</p><p><strong>Educational objective:</strong><br>Hypothyroid myopathy presents with myalgias, proximal muscle weakness, elevated creatine kinase levels, and delayed relaxation of deep tendon reflexes.&amp;nbsp; The diagnosis can be confirmed with an elevated TSH level.&amp;nbsp; Other common causes of myopathy with elevated creatine kinase levels include inflammatory myopathies, muscular dystrophies, and HMG-CoA reductase inhibitors.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><div id=\"div_t39754\" class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" class=\"table-default-style\"><tbody><tr><td colspan=\"3\"><p align=\"center\"><strong>Common causes of myopathy </strong></p></td></tr><tr><td><p align=\"center\"><strong>Disorder</strong></p></td><td><p align=\"center\"><strong>Clinical features</strong></p></td><td><p align=\"center\"><strong>Creatine<br>kinase</strong></p></td></tr><tr><td><p align=\"center\"><strong>Glucocorticoid-<br>induced<br>myopathy</strong></p></td><td><ul><li>Progressive proximal muscle weakness<br>&amp; atrophy <strong>without pain</strong></li><li>Lower extremity muscles more involved</li></ul></td><td><p align=\"center\">Normal</p></td></tr><tr><td><p align=\"center\"><strong>Polymyalgia<br>rheumatica</strong></p></td><td><ul><li>Muscle <strong>pain &amp; stiffness</strong> in shoulder, neck<br>&amp; pelvic girdle</li><li>Worse in morning &amp; with activity</li></ul></td><td><p align=\"center\">Normal</p></td></tr><tr><td><p align=\"center\"><strong>Inflammatory<br>myopathies </strong><br>(eg, polymyositis,<br>dermatomyositis)</p></td><td style=\"vertical-align: top;\"><ul><li>Proximal muscle weakness</li><li><strong>Skin rash</strong> &amp; <strong>inflammatory arthritis</strong><br>possibly present</li></ul></td><td><p align=\"center\">\u2191</p></td></tr><tr><td><p align=\"center\"><strong>Statin-induced<br>myopathy</strong></p></td><td><ul><li>Proximal muscle pain &amp; weakness usually<br>within weeks to months<strong> after starting<br>statin</strong></li></ul></td><td><p align=\"center\">\u2191</p></td></tr><tr><td><p align=\"center\"><strong>Hypothyroid<br>myopathy</strong></p></td><td><ul><li>Muscle pain, cramps &amp; weakness involving<br>the proximal muscles</li><li><strong>Delayed tendon reflexes</strong> &amp; myoedema</li><li>Features of<strong> </strong>hypothyroidism</li></ul></td><td><p align=\"center\">\u2191</p></td></tr></tbody></table></div><p class=\"ng-scope\">An <strong>elevated creatine kinase</strong> (CK) level suggests a myopathic process with <strong>myocyte damage</strong> and release of muscle enzymes into the circulation.&nbsp; Common causes of myopathy with elevated CK include hypothyroidism, muscular dystrophies, inflammatory muscle diseases, and medications such as HMG-CoA reductase inhibitors.</p><p class=\"ng-scope\">This patient's fatigue, <strong>weight gain</strong>, <strong>bradycardia</strong>, and <strong>brittle nails</strong> are highly suggestive of <strong>hypothyroid myopathy</strong>.&nbsp; Decreased thyroid hormone levels impair muscle glycogen and triglyceride use and increase oxidative stress, resulting in myocyte injury.&nbsp; Patients typically presents with myalgias, proximal muscle weakness, decreased exercise tolerance, and cramping.&nbsp; <strong>Myoedema </strong>(focal muscle contraction at the site of percussion) and <strong>delayed relaxation</strong> of deep tendon reflexes are characteristic findings that are caused by impaired reuptake of calcium by the sarcoplasmic reticulum.</p><p class=\"ng-scope\">Hypothyroid myopathy and elevations in CK can be the first signs of hypothyroidism and can precede other manifestations by months to years.&nbsp; The diagnosis can be confirmed with an <strong>elevated TSH level</strong>.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; A 24-hour urine cortisol test is used as an initial test for Cushing syndrome.&nbsp; This disorder can present with proximal muscle weakness and atrophy due to the direct catabolic effects of cortisol on skeletal muscle.&nbsp; However, there is no associated muscle pain or rise in CK, reflexes are normal, and other cushingoid features (eg, stria, dorsocervical fatty hypertrophy) are usually present.</p><p class=\"ng-scope\"><strong>(Choices B and C)</strong>&nbsp; Myasthenia gravis is caused by autoantibodies against nicotinic acetylcholine receptors on the postsynaptic membrane of the neuromuscular junction.&nbsp; It presents with fluctuating weakness that worsens through the day.&nbsp; Lambert-Eaton syndrome is a paraneoplastic disorder associated with small cell carcinoma of the lung (which can be identified on CT scan).&nbsp; It is caused by autoantibodies against voltage-gated calcium channels on the presynaptic membrane, which cause decreased release of acetylcholine.&nbsp; These disorders affect the neuromuscular junction rather than the muscle itself, and CK is normal.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Muscle biopsy is often needed for diagnosis of inflammatory myositis (eg, polymyositis, dermatomyositis).&nbsp; However, this patient has symptoms suggestive of hypothyroidism (eg, weight gain, bradycardia, brittle nails), and histologic findings in hypothyroid myopathy are often nonspecific.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Hypothyroid myopathy presents with myalgias, proximal muscle weakness, elevated creatine kinase levels, and delayed relaxation of deep tendon reflexes.&nbsp; The diagnosis can be confirmed with an elevated TSH level.&nbsp; Other common causes of myopathy with elevated creatine kinase levels include inflammatory myopathies, muscular dystrophies, and HMG-CoA reductase inhibitors.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pathophysiology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Endocrine, Diabetes &amp; Metabolism</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Hypothyroidism</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
