nts.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; id=&quot;1487&quot;  src=&quot;https://www.uworld.com/media/L16657.jpg&quot;  draggable=&quot;false&quot; ></p><p>This young patient has both clinical (eg, polyuria, polydipsia, volume depletion) and biochemical (eg, hyperglycemia, low bicarbonate, high anion gap, decreased serum sodium levels) signs of diabetic ketoacidosis (DKA).&amp;nbsp; DKA is often precipitated by infections, including urinary tract infections (likely in this patient).&amp;nbsp; Patients with DKA lose significant amounts of potassium in the urine, primarily via glycosuria-induced osmotic diuresis and hypovolemia-mediated increase in aldosterone secretion.&amp;nbsp; This results in a <strong>total body potassium deficit</strong> that is reflected by decreased intracellular potassium stores (since 98% of the body&amp;#39;s potassium is stored intracellularly).&amp;nbsp; Despite the low intracellular potassium levels, <strong>extracellular potassium concentrations are normal or increased</strong> due to the following mechanisms:</p><ol><li>Loss of intracellular free water caused by increased plasma osmolality leads to extracellular movement of potassium secondary to increasing intracellular potassium concentration</li><li>Lack of insulin also causes extracellular shifting of potassium as insulin normally promotes cellular uptake of potassium</li></ol><p>Clinicians must be careful when giving insulin and intravenous fluids during resuscitation of dehydrated, hyperglycemic patients.&amp;nbsp; These interventions force potassium back into the cells, causing a precipitous drop in serum potassium due to the total body deficit.&amp;nbsp; Despite normal or elevated serum potassium, timely administration of potassium supplementation is essential when treating a patient with DKA.</p><p><strong>(Choice A)</strong>&amp;nbsp; A decrease in both intra- and extracellular potassium levels is seen with mineralocorticoids excess, diuretic use, and chronic gastrointestinal losses.</p><p><strong>(Choice B)</strong>&amp;nbsp; Extracellular potassium concentration decreases following glucose and insulin administration as potassium moves into the intracellular compartment.&amp;nbsp; Stimulation of beta-adrenergic receptors also causes increased transfer of potassium to the intracellular compartment.</p><p><strong>(Choice D)</strong>&amp;nbsp; Renal excretion of potassium is impaired in most patients with chronic renal failure, resulting in increased total body potassium storage and hyperkalemia.</p><p><strong>(Choice E)</strong>&amp;nbsp; Normal extracellular potassium levels are common in patients with DKA as the extracellular potassium shift is often mitigated by increased urinary potassium losses.&amp;nbsp; However, intracellular potassium levels are always decreased in DKA.</p><p><strong>Educational objective:</strong><br>Most patients with diabetic ketoacidosis have normal to increased serum potassium levels despite a total body potassium deficit.&amp;nbsp; Replacement of potassium is a crucial step in the management of patients with diabetic ketoacidosis.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" id=\"1487\" src=\"image/L16657.jpg\" draggable=\"false\"></p><p class=\"ng-scope\">This young patient has both clinical (eg, polyuria, polydipsia, volume depletion) and biochemical (eg, hyperglycemia, low bicarbonate, high anion gap, decreased serum sodium levels) signs of diabetic ketoacidosis (DKA).&nbsp; DKA is often precipitated by infections, including urinary tract infections (likely in this patient).&nbsp; Patients with DKA lose significant amounts of potassium in the urine, primarily via glycosuria-induced osmotic diuresis and hypovolemia-mediated increase in aldosterone secretion.&nbsp; This results in a <strong>total body potassium deficit</strong> that is reflected by decreased intracellular potassium stores (since 98% of the body's potassium is stored intracellularly).&nbsp; Despite the low intracellular potassium levels, <strong>extracellular potassium concentrations are normal or increased</strong> due to the following mechanisms:</p><ol class=\"ng-scope\"><li>Loss of intracellular free water caused by increased plasma osmolality leads to extracellular movement of potassium secondary to increasing intracellular potassium concentration</li><li>Lack of insulin also causes extracellular shifting of potassium as insulin normally promotes cellular uptake of potassium</li></ol><p class=\"ng-scope\">Clinicians must be careful when giving insulin and intravenous fluids during resuscitation of dehydrated, hyperglycemic patients.&nbsp; These interventions force potassium back into the cells, causing a precipitous drop in serum potassium due to the total body deficit.&nbsp; Despite normal or elevated serum potassium, timely administration of potassium supplementation is essential when treating a patient with DKA.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; A decrease in both intra- and extracellular potassium levels is seen with mineralocorticoids excess, diuretic use, and chronic gastrointestinal losses.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Extracellular potassium concentration decreases following glucose and insulin administration as potassium moves into the intracellular compartment.&nbsp; Stimulation of beta-adrenergic receptors also causes increased transfer of potassium to the intracellular compartment.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Renal excretion of potassium is impaired in most patients with chronic renal failure, resulting in increased total body potassium storage and hyperkalemia.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Normal extracellular potassium levels are common in patients with DKA as the extracellular potassium shift is often mitigated by increased urinary potassium losses.&nbsp; However, intracellular potassium levels are always decreased in DKA.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Most patients with diabetic ketoacidosis have normal to increased serum potassium levels despite a total body potassium deficit.&nbsp; Replacement of potassium is a crucial step in the management of patients with diabetic ketoacidosis.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepea