tants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><div id=&quot;div_t80963&quot;><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; class=&quot;table-default-style&quot;><tbody><tr><td><p align=&quot;center&quot; style=&quot;margin-left:.1in;&quot;><strong>Symptoms/signs of ankylosing spondylitis</strong></p></td></tr><tr><td><ul><li>Low back pain (onset age &amp;lt;40, insidious onset, improves with exercise but not with rest, pain at night)</li><li>Hip &amp;amp; buttock pain</li><li>Limited chest expansion &amp;amp; spinal mobility</li><li>Enthesitis (inflammation at the site of insertion of a tendon to the bone)</li><li>Systemic symptoms (eg, fever, chills, fatigue, weight loss)</li><li>Acute anterior uveitis (unilateral pain, photophobia, blurry vision)</li></ul></td></tr></tbody></table></div></p><p>The symptoms of low back pain and morning stiffness in a young man suggest <strong>ankylosing spondylitis</strong> (AS), a chronic inflammatory condition associated with the HLA-B27 serotype.&amp;nbsp; AS is characterized by stiffness and fusion (<strong>ankylosis</strong>) of the axial joints.&amp;nbsp; The <a ng-click=&quot;showexhibit('2579',true);&quot;>sacroiliac</a> and apophyseal joints of the spine are the most commonly affected, leading to restricted spinal mobility.&amp;nbsp; Many patients also develop peripheral arthritis and <strong>enthesitis</strong>, which is defined as pain, tenderness, and swelling at the sites of tendon insertion into bone (eg, Achilles tendon insertion).</p><p>In addition, AS can cause complications in extraskeletal systems:</p><ol><li><strong>Respiratory</strong>: Involvement of the thoracic spine and enthesopathies of the costovertebral and costosternal junctions can limit chest wall expansion, leading to hypoventilation.&amp;nbsp; Chest expansion should be monitored regularly in patients with AS.</li><li><strong>Cardiovascular</strong>: The most common cardiovascular complication of AS is ascending aortitis, which can lead to dilation of the aortic ring and aortic insufficiency.</li><li><strong>Eye</strong>: <a ng-click=&quot;showexhibit('748',true);&quot;>Anterior uveitis</a> develops in some patients with AS and presents with pain, blurred vision, photophobia, and conjunctival erythema.</li></ol><p><strong>(Choice B)</strong>&amp;nbsp; Hand joint mobility testing is part of the assessment of rheumatoid arthritis severity.&amp;nbsp; However, the small joints of the hands are not prominently affected by AS.</p><p><strong>(Choice C)</strong>&amp;nbsp; Peak expiratory flow is determined by airway resistance (obstruction) and chest and abdominal wall muscular function.&amp;nbsp; These variables are not affected by chest wall restriction and are normal in AS.&amp;nbsp; Regular measurement of expiratory flow rates is important in patients with asthma.</p><p><strong>(Choice D)</strong>&amp;nbsp; Rheumatoid factor can be found in a variety of rheumatic (eg, rheumatoid arthritis, Sj&amp;ouml;gren&amp;#39;s syndrome, systemic lupus erythematosus [SLE]) and nonrheumatic disorders (eg, hepatitis C) and in up to 4% of young and healthy individuals.&amp;nbsp; However, it is not associated with AS.</p><p><strong>(Choice E)</strong>&amp;nbsp; Urine protein excretion should be monitored in SLE and diabetes mellitus.&amp;nbsp; Renal involvement is rare in AS.</p><p><strong>Educational objective:</strong><br>Ankylosing spondylitis is characterized by stiffness and fusion of axial joints (ankylosis) and inflammation at the site of insertion of tendons into bone (enthesitis).&amp;nbsp; Involvement of the thoracic spine and costovertebral and costosternal junctions can limit chest wall expansion, leading to hypoventilation.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"></p><div id=\"div_t80963\" class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" class=\"table-default-style\"><tbody><tr><td><p align=\"center\" style=\"margin-left:.1in;\"><strong>Symptoms/signs of ankylosing spondylitis</strong></p></td></tr><tr><td><ul><li>Low back pain (onset age &lt;40, insidious onset, improves with exercise but not with rest, pain at night)</li><li>Hip &amp; buttock pain</li><li>Limited chest expansion &amp; spinal mobility</li><li>Enthesitis (inflammation at the site of insertion of a tendon to the bone)</li><li>Systemic symptoms (eg, fever, chills, fatigue, weight loss)</li><li>Acute anterior uveitis (unilateral pain, photophobia, blurry vision)</li></ul></td></tr></tbody></table></div><p class=\"ng-scope\"></p><p class=\"ng-scope\">The symptoms of low back pain and morning stiffness in a young man suggest <strong>ankylosing spondylitis</strong> (AS), a chronic inflammatory condition associated with the HLA-B27 serotype.&nbsp; AS is characterized by stiffness and fusion (<strong>ankylosis</strong>) of the axial joints.&nbsp; The <a ng-click=\"showexhibit('2579',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L504.png&quot;,&quot;title&quot;:&quot;L504&quot;}]\">sacroiliac</a> and apophyseal joints of the spine are the most commonly affected, leading to restricted spinal mobility.&nbsp; Many patients also develop peripheral arthritis and <strong>enthesitis</strong>, which is defined as pain, tenderness, and swelling at the sites of tendon insertion into bone (eg, Achilles tendon insertion).</p><p class=\"ng-scope\">In addition, AS can cause complications in extraskeletal systems:</p><ol class=\"ng-scope\"><li><strong>Respiratory</strong>: Involvement of the thoracic spine and enthesopathies of the costovertebral and costosternal junctions can limit chest wall expansion, leading to hypoventilation.&nbsp; Chest expansion should be monitored regularly in patients with AS.</li><li><strong>Cardiovascular</strong>: The most common cardiovascular complication of AS is ascending aortitis, which can lead to dilation of the aortic ring and aortic insufficiency.</li><li><strong>Eye</strong>: <a ng-click=\"showexhibit('748',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/U11899.png&quot;,&quot;title&quot;:&quot;U11899&quot;}]\">Anterior uveitis</a> develops in some patients with AS and presents with pain, blurred vision, photophobia, and conjunctival erythema.</li></ol><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Hand joint mobility testing is part of the assessment of rheumatoid arthritis severity.&nbsp; However, the small joints of the hands are not prominently affected by AS.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Peak expiratory flow is determined by airway resistance (obstruction) and chest and abdominal wall muscular function.&nbsp; These variables are not affected by chest wall restriction and are normal in AS.&nbsp; Regular measurement of expiratory flow rates is important in patients with asthma.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Rheumatoid factor can be found in a variety of rheumatic (eg, rheumatoid arthritis, Sj\u00f6gren's syndrome, systemic lupus erythematosus [SLE]) and nonrheumatic disorders (eg, hepatitis C) and in up to 4% of young and healthy individuals.&nbsp; However, it is not associated with AS.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Urine protein excretion should be monitored in SLE and diabetes mellitus.&nbsp; Renal involvement is rare in AS.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Ankylosing spondylitis is characterized by stiffness and fusion of axial joints (ankylosis) and inflammation at the site of insertion of tendons into bone (enthesitis).&nbsp; Involvement of the thoracic spine and costovertebral and costosternal junctions can limit chest wall expansion, leading to hypoventilation.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/22277377\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/22277377\"><span compiledata=\"\" htmlcontent=\"Restrictive pulmonary function is more preval