Tutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><div id=&quot;div_T78825&quot;><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; class=&quot;table-default-style&quot;><tbody><tr><td colspan=&quot;2&quot;><p align=&quot;center&quot;><strong>Small intestinal bacterial overgrowth</strong></p></td></tr><tr><td><p align=&quot;center&quot;><strong>Risk factors</strong></p></td><td><ul><li>Anatomic abnormalities (eg, strictures, surgery)</li><li>Motility disorders (eg, diabetes mellitus, scleroderma, opioid use)</li><li>Immunodeficiency (IgA deficiency)</li><li>Gastric hypochlorhydria (proton pump inhibitor use)</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Pathogenesis</strong></p></td><td><ul><li>Proliferation of colonic bacteria in the small intestine</li><li>Maldigestion and possible malabsorption of nutrients</li><li>Fermentation of carbohydrates</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Clinical<br>manifestations</strong></p></td><td><ul><li>Bloating, flatulence</li><li>Chronic watery diarrhea</li><li>Severe: steatorrhea, vitamin deficiencies</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Treatment</strong></p></td><td><ul><li>Dietary changes (eg, high fat, low carbohydrate)</li><li>Oral antibiotics (eg, rifaximin, ciprofloxacin)</li></ul></td></tr></tbody></table></div><p>This patient has <strong>small intestinal bacterial overgrowth</strong> (SIBO) presenting with bloating and diarrhea.&amp;nbsp; SIBO is caused by bacterial proliferation (eg, <em>Escherichia coli</em>, <em>Bacteroides</em>, <em>Aeromonas</em>) in the small bowel.&amp;nbsp; It is a common complication of <strong>scleroderma</strong>, which can alter intestinal motility due to smooth muscle atrophy and fibrosis.&amp;nbsp; As in this patient, symptoms often improve with <strong>antibiotics</strong>.</p><p>SIBO is characterized by mucosal enterocyte injury due to bacterial adherence, invasion, and toxin injury.&amp;nbsp; The activity of brush border enzymes is often impaired.&amp;nbsp; These changes lead to <strong>maldigestion</strong> and often malabsorption in more severe cases.&amp;nbsp; Specific macronutrient effects include the following:</p><ul><li><p>Carbohydrates:&amp;nbsp; decreased digestion and absorption of carbohydrates facilitate <strong>bacterial fermentation</strong>, producing carbon dioxide, hydrogen, and methane gases (eg, bloating).</p></li><li><p>Fatty acids:&amp;nbsp; conjugated bile acids normally emulsify dietary fats in the proximal small bowel and are reabsorbed in the terminal ileum.&amp;nbsp; However, SIBO causes deconjugation of bile acids, leading to early bile acid resorption in the jejunum, reduced lipid emulsification, and <strong>fat malabsorption</strong>.</p></li><li><p>Amino acids:&amp;nbsp; SIBO causes maldigestion of proteins and malabsorption of amino acids, occasionally leading to protein-losing enteropathy in severe cases.</p></li></ul><p><strong>(Choice A)</strong>&amp;nbsp; Defective apoprotein structure occurs in abetalipoproteinemia, an autosomal recessive disorder that presents in infancy with fat malabsorption and growth abnormalities; symptoms would not improve with antibiotics.</p><p><strong>(Choice B)</strong>&amp;nbsp; Impaired release of pancreatic lipase occurs with chronic pancreatitis.&amp;nbsp; Patients typically have severe postprandial abdominal pain and steatorrhea (bulky, fat-laden stools); therapy involves pancreatic enzyme replacement, not antibiotics.</p><p><strong>(Choice D)</strong>&amp;nbsp; Gastrin-secreting tumors (ie, Zollinger-Ellison syndrome) cause gastric acid hypersecretion, which lowers duodenal pH and inactivates pancreatic lipase.&amp;nbsp; Although it can cause diarrhea and fat malabsorption, hypersecretion typically presents with abdominal pain and multiple gastrointestinal ulcers; antibiotics are ineffective.</p><p><strong>Educational objective:</strong><br>Small intestinal bacterial overgrowth is characterized by bacterial proliferation in the small bowel, leading to mucosal inflammation with subsequent fermentation of carbohydrates&amp;nbsp;and fat malabsorption; it manifests with diarrhea, abdominal distension, and flatulence.&amp;nbsp; Patients with impaired intestinal peristalsis (eg, systemic sclerosis) are at increased risk.&amp;nbsp; Treatment includes oral antibiotics.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><div id=\"div_T78825\" class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" class=\"table-default-style\"><tbody><tr><td colspan=\"2\"><p align=\"center\"><strong>Small intestinal bacterial overgrowth</strong></p></td></tr><tr><td><p align=\"center\"><strong>Risk factors</strong></p></td><td><ul><li>Anatomic abnormalities (eg, strictures, surgery)</li><li>Motility disorders (eg, diabetes mellitus, scleroderma, opioid use)</li><li>Immunodeficiency (IgA deficiency)</li><li>Gastric hypochlorhydria (proton pump inhibitor use)</li></ul></td></tr><tr><td><p align=\"center\"><strong>Pathogenesis</strong></p></td><td><ul><li>Proliferation of colonic bacteria in the small intestine</li><li>Maldigestion and possible malabsorption of nutrients</li><li>Fermentation of carbohydrates</li></ul></td></tr><tr><td><p align=\"center\"><strong>Clinical<br>manifestations</strong></p></td><td><ul><li>Bloating, flatulence</li><li>Chronic watery diarrhea</li><li>Severe: steatorrhea, vitamin deficiencies</li></ul></td></tr><tr><td><p align=\"center\"><strong>Treatment</strong></p></td><td><ul><li>Dietary changes (eg, high fat, low carbohydrate)</li><li>Oral antibiotics (eg, rifaximin, ciprofloxacin)</li></ul></td></tr></tbody></table></div><p class=\"ng-scope\">This patient has <strong>small intestinal bacterial overgrowth</strong> (SIBO) presenting with bloating and diarrhea.&nbsp; SIBO is caused by bacterial proliferation (eg, <em>Escherichia coli</em>, <em>Bacteroides</em>, <em>Aeromonas</em>) in the small bowel.&nbsp; It is a common complication of <strong>scleroderma</strong>, which can alter intestinal motility due to smooth muscle atrophy and fibrosis.&nbsp; As in this patient, symptoms often improve with <strong>antibiotics</strong>.</p><p class=\"ng-scope\">SIBO is characterized by mucosal enterocyte injury due to bacterial adherence, invasion, and toxin injury.&nbsp; The activity of brush border enzymes is often impaired.&nbsp; These changes lead to <strong>maldigestion</strong> and often malabsorption in more severe cases.&nbsp; Specific macronutrient effects include the following:</p><ul class=\"ng-scope\"><li><p>Carbohydrates:&nbsp; decreased digestion and absorption of carbohydrates facilitate <strong>bacterial fermentation</strong>, producing carbon dioxide, hydrogen, and methane gases (eg, bloating).</p></li><li><p>Fatty acids:&nbsp; conjugated bile acids normally emulsify dietary fats in the proximal small bowel and are reabsorbed in the terminal ileum.&nbsp; However, SIBO causes deconjugation of bile acids, leading to early bile acid resorption in the jejunum, reduced lipid emulsification, and <strong>fat malabsorption</strong>.</p></li><li><p>Amino acids:&nbsp; SIBO causes maldigestion of proteins and malabsorption of amino acids, occasionally leading to protein-losing enteropathy in severe cases.</p></li></ul><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Defective apoprotein structure occurs in abetalipoproteinemia, an autosomal recessive disorder that presents in infancy with fat malabsorption and growth abnormalities; symptoms would not improve with antibiotics.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Impaired release of pancreatic lipase occurs with chronic pancreatitis.&nbsp; Patients typically have severe postprandial abdominal pain and steatorrhea (bulky, fat-laden stools); therapy involves pancreatic enzyme replacement, not antibiotics.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Gastrin-secreting tumors (ie, Zollinger-Ellison syndrome) cause gastric acid hypersecretion, which lowers duodenal pH and inactivates pancreatic lipase.&nbsp; Although it can cause diarrhea and fat malabsorption, hypersecretion typically presents with abdominal pain and multiple gastrointestinal ulcers; antibiotics are ineffective.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Small intestinal bacterial overgrowth is characterized by bacterial proliferation in the small bowel, leading to mucosal inflammation with subsequent fermentation of carbohydrates&nbsp;and fat malabsorption; it manifests with diarrhea, abdominal distension, and flatulence.&nbsp; Patients with impaired intestinal peristalsis (eg, systemic sclerosis) are at increased risk.&nbsp; Treatment includes oral antibiotics.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pathophysiology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Gastrointestinal &amp; Nutrition</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Small intestinal bacterial overgrowth</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- e