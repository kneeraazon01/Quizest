o\">\n                            <strong>F.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight6\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\"><table border=\"0\" cellpadding=\"1\" cellspacing=\"1\" style=\"width:350px;\">\n\t<tbody>\n\t\t<tr>\n\t\t\t<td width=\"50%\">\n\t\t\t\tProximal convoluted tubule\n\t\t\t</td>\n\t\t\t<td width=\"50%\">\n\t\t\t\tSecretion of hydrogen ions\n\t\t\t</td>\n\t\t</tr>\n\t</tbody>\n</table></span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot;  draggable=&quot;false&quot; id=&quot;54402&quot; src=&quot;https://www.uworld.com/media/highresdefault/L84320.jpg&quot;  ></p><p>This patient&amp;#39;s glycosuria with normal serum glucose in combination with normal anion gap metabolic acidosis, hypokalemia, and hypophosphatemia is highly suggestive of impaired proximal tubular reabsorption.&amp;nbsp; The <strong>proximal convoluted tubule</strong> is responsible for reabsorption of bicarbonate in addition to amino acids, phosphate, glucose, water, and potassium.&amp;nbsp; Impaired bicarbonate reabsorption can occur as an isolated defect (renal tubular acidosis [RTA] type 2) or as part of a more generalized proximal tubular dysfunction (<strong>Fanconi syndrome</strong>):</p><ul><li><p><strong>Loss of bicarbonate</strong> causes persistent acidosis.</p></li><li><p><strong>Hypophosphatemia</strong> leads to <a ng-click=&quot;showexhibit('22289',true);&quot;>rickets</a> (eg, frontal bossing, leg bowing).</p></li><li><p><strong>Glucosuria</strong> contributes to polyuria (impaired renal concentrating ability also contributes).&amp;nbsp; Serum glucose levels are normal, as seen here, because they are tightly regulated by multiple mechanisms (eg, insulin, glucagon) that are independent of renal reabsorption.</p></li><li><p><strong>Hypokalemia</strong> can lead to constipation and muscle weakness.</p></li><li><p>Aminoaciduria, contributing to growth failure.</p></li></ul><p><strong>(Choices A and D)</strong>&amp;nbsp; The proximal tubule is also responsible for the generation of ammonia (NH<font size=&quot;2&quot;><sub>3</sub></font>).&amp;nbsp; Hypoaldosteronism or aldosterone resistance (RTA type 4, most commonly seen in adults with diabetes) can disrupt this process by inhibiting production of NH<font size=&quot;2&quot;><sub>3</sub></font> in the proximal tubule, resulting in reduced ammonium (NH<font size=&quot;2&quot;><sub>4</sub></font><font size=&quot;2&quot;><sup>+</sup></font>) excretion and metabolic acidosis.&amp;nbsp; This patient&amp;#39;s hypokalemia and hypophosphatemia are inconsistent with this diagnosis.</p><p><strong>(Choices B, C, and F)</strong>&amp;nbsp; Hydrogen ions (H<font size=&quot;2&quot;><sup>+</sup></font>) are primarily secreted into the urine from the distal convoluted tubule.&amp;nbsp; Failure of this process (RTA type 1) leads to decreased distal acid secretion and metabolic acidosis, but phosphate and glucose would be normal.</p><p><strong>Educational objective:</strong><br>Defects in either the proximal or distal convoluted tubule (eg, bicarbonate loss, acid retention) can result in a normal anion gap metabolic acidosis.&amp;nbsp; However, the proximal tubule is responsible for reabsorption of glucose, amino acids, and phosphate in addition to bicarbonate; therefore, generalized proximal tubular dysfunction (Fanconi syndrome) can also cause failure to thrive, glucosuria, and rickets.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"54402\" src=\"image/L84320.jpg\"></p><p class=\"ng-scope\">This patient's glycosuria with normal serum glucose in combination with normal anion gap metabolic acidosis, hypokalemia, and hypophosphatemia is highly suggestive of impaired proximal tubular reabsorption.&nbsp; The <strong>proximal convoluted tubule</strong> is responsible for reabsorption of bicarbonate in addition to amino acids, phosphate, glucose, water, and potassium.&nbsp; Impaired bicarbonate reabsorption can occur as an isolated defect (renal tubular acidosis [RTA] type 2) or as part of a more generalized proximal tubular dysfunction (<strong>Fanconi syndrome</strong>):</p><ul class=\"ng-scope\"><li><p><strong>Loss of bicarbonate</strong> causes persistent acidosis.</p></li><li><p><strong>Hypophosphatemia</strong> leads to <a ng-click=\"showexhibit('22289',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L28217.jpg&quot;,&quot;title&quot;:&quot;L28217&quot;}]\">rickets</a> (eg, frontal bossing, leg bowing).</p></li><li><p><strong>Glucosuria</strong> contributes to polyuria (impaired renal concentrating ability also contributes).&nbsp; Serum glucose levels are normal, as seen here, because they are tightly regulated by multiple mechanisms (eg, insulin, glucagon) that are independent of renal reabsorption.</p></li><li><p><strong>Hypokalemia</strong> can lead to constipation and muscle weakness.</p></li><li><p>Aminoaciduria, contributing to growth failure.</p></li></ul><p class=\"ng-scope\"><strong>(Choices A and D)</strong>&nbsp; The proximal tubule is also responsible for the generation of ammonia (NH<font size=\"2\"><sub>3</sub></font>).&nbsp; Hypoaldosteronism or aldosterone resistance (RTA type 4, most commonly seen in adults with diabetes) can disrupt this process by inhibiting production of NH<font size=\"2\"><sub>3</sub></font> in the proximal tubule, resulting in reduced ammonium (NH<font size=\"2\"><sub>4</sub></font><font size=\"2\"><sup>+</sup></font>) excretion and metabolic acidosis.&nbsp; This patient's hypokalemia and hypophosphatemia are inconsistent with this diagnosis.</p><p class=\"ng-scope\"><strong>(Choices B, C, and F)</strong>&nbsp; Hydrogen ions (H<font size=\"2\"><sup>+</sup></font>) are primarily secreted into the urine from the distal convoluted tubule.&nbsp; Failure of this process (RTA type 1) leads to decreased distal acid secretion and metabolic acidosis, but phosphate and glucose would be normal.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Defects in either the proximal or distal convoluted tubule (eg, bicarbonate loss, acid retention) can result in a normal anion gap metabolic acidosis.&nbsp; However, the proximal tubule is responsible for reabsorption of glucose, amino acids, and phosphate in addition to bicarbonate; therefore, generalized proximal tubular dysfunction (Fanconi syndrome) can also cause failure to thrive, glucosuria, and rickets.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pathophysiology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Renal, Urinary Systems &amp; Electrolytes</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Fanconis syndrome</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
