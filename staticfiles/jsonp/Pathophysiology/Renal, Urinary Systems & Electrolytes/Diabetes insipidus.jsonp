    {
        "question": "<div class=\"questioncontent ui-resizable widthWithoutAbstract\" id=\"questionContent\" ng-show=\"showQuestion\" ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat, 'suggested-highlights-visible' : userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; currentQuestion.frequentHighlights != null &amp;&amp; (suggestedHighlightsPreference == 1 || suggestedHighlightsPreference == 2), 'only-suggested-highlights-visible' : userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; suggestedHighlightsPreference == 1 &amp;&amp; currentQuestion.frequentHighlights != null}\">\n            <div style=\"display:none;\" class=\"suggested-highlights-switch-buttons ng-hide\" ng-show=\"userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; currentQuestion.frequentHighlights != null\">\n                <span class=\"fa-stack\" ng-click=\"showUserHighlights()\" title=\"My Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fas\" ng-class=\"{'fas' : suggestedHighlightsPreference == 0, 'fal' : suggestedHighlightsPreference != 0}\"></i>\n                    <i class=\"fal fa-highlighter fa-stack-1x fa-inverse\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 0}\"></i>\n                </span>\n                <span class=\"fa-stack ng-hide\" ng-hide=\"currentQuestion.frequentHighlights == null\" ng-click=\"showSuggestedHighlights()\" title=\"Suggested Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fal\" ng-class=\"{'fas' : suggestedHighlightsPreference == 1, 'fal' : suggestedHighlightsPreference != 1}\"></i>\n                    <i class=\"fas fa-highlighter fa-stack-1x\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 1}\"></i>\n                </span>\n                <span class=\"fa-stack highlight-preference-unavailable highlight-button-with-popover\" ng-show=\"currentQuestion.frequentHighlights == null\" title=\"\" data-toggle=\"popover\" data-placement=\"bottom\" data-content=\"Collecting Data\" data-trigger=\"hover\" data-original-title=\"Suggested Highlights\">\n                    <i class=\"fal fa-circle fa-stack-2x\"></i>\n                    <i class=\"fas fa-highlighter fa-stack-1x\"></i>\n                </span>\n                <span class=\"fa-stack ng-hide\" ng-hide=\"currentQuestion.frequentHighlights == null\" ng-click=\"showUserAndSuggestedHighlights()\" title=\"Duo Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fal\" ng-class=\"{'fas' : suggestedHighlightsPreference == 2, 'fal' : suggestedHighlightsPreference != 2}\"></i>\n                    <i class=\"fad fa-highlighter fa-stack-1x\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 2}\"></i>\n                </span>\n                <span class=\"fa-stack highlight-preference-unavailable highlight-button-with-popover\" ng-show=\"currentQuestion.frequentHighlights == null\" title=\"\" data-toggle=\"popover\" data-placement=\"bottom\" data-content=\"Collecting Data\" data-trigger=\"hover\" data-original-title=\"Duo Highlights\">\n                    <i class=\"fal fa-circle fa-stack-2x\"></i>\n                    <i class=\"fad fa-highlighter fa-stack-1x\"></i>\n                </span>\n            </div>\n            <!-- ngIf: launchTestOptions.launchCode == 'nbome' && userPreferences.testPreferences.theme != clientConstants.themes.nbome -->\n            <div id=\"question\">\n                <!-- ngIf: settings.hints && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && launchTestOptions.canShowHint -->\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && (currentQuestion.qbankId != clientConstants.qbankIds.stats_faculty || currentQuestion.qbankId != clientConstants.qbankIds.stats) -->\n                <!-- ngIf: (currentQuestion.mobileExcerpt != '' && currentQuestion.mobileExcerpt != null ) -->\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat -->\n                <!--Periodic table button for MCAT -->\n                <!--<div class=\"text-center periodicTableDiv\" ng-show=\"(launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.subjectId != constants.noPeriodicTable.cars) && (currentQuestion.subjectId != constants.noPeriodicTable.pyschology) && (currentQuestion.subjectId != constants.noPeriodicTable.sociology)\">\n                    <button class=\"btn btn-default btn--custom\" style=\"color: black; padding: 4px 3px;\" ng-click=\"showresources()\">Periodic table&nbsp;<img src=\"img/mcat/Periodic_icon.png\" height=\"12\" style=\"margin-top: -2px; cursor: pointer\" title=\"Periodic Table\" /></button>\n                </div>-->\n                \n                <!--Inactive message for questions in usmle, boards and nclex and do not show for EXPERIMENTAL_INACTIVE hence the check !=3 -->\n                <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.nclex) && !currentQuestion.isActive && currentQuestion.activeStatusId != 3 -->\n                <div id=\"questionText\" class=\"questionText ng-scope\" ng-mouseup=\"invokeHighlights('questionText',constants.flashCardType.selection,$event)\" compiledata=\"\" htmlcontent=\"<p>A 28-year-old woman comes to the clinic due to generalized weakness and frequent, high-volume urination.&amp;nbsp; She has had no dysuria, hematuria, or abdominal pain.&amp;nbsp; The patient&amp;#39;s symptoms have been ongoing for several months, but she cannot recall exactly when they began.&amp;nbsp; She has no other medical conditions.&amp;nbsp; The patient is a single mother of a 2-year-old child, has little social support, and occasionally uses alcohol and marijuana &amp;#x22;to cope with the stress.&amp;#x22;&amp;nbsp; Vital signs and physical examination are normal.&amp;nbsp; Blood glucose is 95 mg/dL and serum sodium is 132 mEq/L.&amp;nbsp; Urinalysis shows no white or red blood cells.&amp;nbsp; During further evaluation, urine osmolality is serially measured while fluid intake is restricted; vasopressin is subsequently administered 7 hours into the test.&amp;nbsp; The results are shown below.</p><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;24007&quot; src=&quot;https://www.uworld.com/media/L49514.jpg&quot; ></p><p>Which of the following is the most appropriate long-term treatment for this patient?</p>\"><p class=\"ng-scope\">A 28-year-old woman comes to the clinic due to generalized weakness and frequent, high-volume urination.&nbsp; She has had no dysuria, hematuria, or abdominal pain.&nbsp; The patient's symptoms have been ongoing for several months, but she cannot recall exactly when they began.&nbsp; She has no other medical conditions.&nbsp; The patient is a single mother of a 2-year-old child, has little social support, and occasionally uses alcohol and marijuana \"to cope with the stress.\"&nbsp; Vital signs and physical examination are normal.&nbsp; Blood glucose is 95 mg/dL and serum sodium is 132 mEq/L.&nbsp; Urinalysis shows no white or red blood cells.&nbsp; During further evaluation, urine osmolality is serially measured while fluid intake is restricted; vasopressin is subsequently administered 7 hours into the test.&nbsp; The results are shown below.</p><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"24007\" src=\"image/L49514.jpg\"></p><p class=\"ng-scope\">Which of the following is the most appropriate long-term treatment for this patient?</p></div>\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && (currentQuestion.formatTypeId == constants.questionFormatType.fillInTheBlank ||currentQuestion.formatTypeId == constants.questionFormatType.fillInTheBlankRange )&& currentQuestion.sectionId == constants.collegeprepSectionIds.math -->\n                <!-- change to mediatype object -->\n                <!-- ngIf: currentQuestion.questionMedia.typeId == constants.questionMediaType.drugad || currentQuestion.questionTypeId == constants.questionType.drugad -->\n                <!-- ngIf: currentQuestion.questionMedia.typeId == constants.questionMediaType.audio || currentQuestion.questionMedia.typeId == constants.questionMediaType.video || currentQuestion.questionTypeId == constants.questionType.media -->\n\n                <div id=\"answerContainer\"><div class=\"options\">\n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>A.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight1\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Desmopressin</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>B.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight2\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Hydrochlorothiazide</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>C.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight3\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Indomethacin</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>D.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight4\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Insulin</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>E.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight5\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Water restriction</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><div id=&quot;div_t50342&quot;><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; class=&quot;table-default-style&quot;><tbody><tr><td colspan=&quot;4&quot;><p align=&quot;center&quot;><strong>Polyuria &amp;amp; polydipsia</strong></p></td></tr><tr><td></td><td></td><td colspan=&quot;2&quot;><p align=&quot;center&quot;><strong>Water deprivation test</strong></p></td></tr><tr><td></td><td><p align=&quot;center&quot;><strong>Serum sodium</strong></p></td><td><p align=&quot;center&quot;><strong>Urine osmolality after water deprivation</strong></p></td><td><p align=&quot;center&quot;><strong>Urine osmolality with vasopressin injection</strong></p></td></tr><tr><td><p align=&quot;center&quot;><strong>Normal</strong></p></td><td><p align=&quot;center&quot;>Normal</p></td><td><p align=&quot;center&quot;>Increased</p></td><td><p align=&quot;center&quot;>No additional increase</p></td></tr><tr><td><p align=&quot;center&quot;><strong>Central diabetes insipidus</strong></p></td><td><p align=&quot;center&quot;>High</p></td><td><p align=&quot;center&quot;>No change or mild increase</p></td><td><p align=&quot;center&quot;>Large increase</p></td></tr><tr><td><p align=&quot;center&quot;><strong>Nephrogenic diabetes insipidus</strong></p></td><td><p align=&quot;center&quot;>High</p></td><td><p align=&quot;center&quot;>No change or mild increase</p></td><td><p align=&quot;center&quot;>Mild increase</p></td></tr><tr><td><p align=&quot;center&quot;><strong>Primary polydipsia</strong></p></td><td><p align=&quot;center&quot;>Low</p></td><td><p align=&quot;center&quot;>Increased</p></td><td><p align=&quot;center&quot;>No additional increase</p></td></tr></tbody></table></div><p>This patient has <strong>polyuria</strong> (urine output &amp;gt;3 L/day) with a <strong>low initial urine osmolality</strong>, which is indicative of <strong>water diuresis</strong> caused by one of the following conditions:</p><ul><li>Diabetes insipidus (DI), which occurs due to either deficient antidiuretic hormone (ADH) production (central DI) or lack of renal response to ADH (nephrogenic DI)</li><li>Primary polydipsia, which results from excessive water consumption (most common in patients with underlying psychiatric disease or emotional distress)</li></ul><p>In patients with <strong>primary polydipsia</strong>, excessive water intake often leads to mild <strong>hyponatremia</strong>.&amp;nbsp; In contrast, primary ADH deficiency in DI leads to free water loss often with ensuing hypernatremia.&amp;nbsp; In certain patients, water deprivation testing may be helpful for differentiating PP from DI.</p><p>During a <strong>water deprivation test</strong>, drinking water is withheld and urine osmolality is monitored until it reaches a steady-state plateau (representing the maximal concentrating ability of the kidneys).&amp;nbsp; Water deprivation in patients with PP stimulates ADH secretion and leads to a significant <strong>rise in urine concentration</strong>, whereas urine in patients with DI remains dilute.&amp;nbsp; Once a plateau is reached, <strong>vasopressin is administered</strong>.&amp;nbsp; In DI, exogenous vasopressin causes an increase in urine osmolality; in PP, urine <strong>concentration remains unchanged</strong> because at this point in the test, the endogenous ADH effect is already at maximum.</p><p>If PP is confirmed, long-term management includes <strong>restriction of free water </strong>intake.</p><p><strong>(Choices A, B, and C)</strong>&amp;nbsp; Central DI can be treated with desmopressin (synthetic ADH analog), while nephrogenic DI is typically treated with thiazide diuretics (induce mild hypovolemia that decreases polyuria) or NSAIDs (inhibit renal prostaglandins that act as ADH antagonists).&amp;nbsp; However, during water deprivation, patients with central or nephrogenic DI typically have no change or only a mild increase in urine osmolality due to lack of adequate ADH effect; subsequent vasopressin administration would also be expected to cause an increase in urine osmolality.</p><p><strong>(Choice D)</strong>&amp;nbsp; Severe hyperglycemia in diabetes mellitus can overwhelm the ability of the kidneys to recover the filtered glucose load, leading to osmotic diuresis and polyuria.&amp;nbsp; However, this typically occurs with blood glucose &amp;gt;180 mg/dL; this patient&amp;#39;s glucose is normal.</p><p><strong>Educational objective:</strong><br>Primary (psychogenic) polydipsia is characterized by excessive intake of free water, leading to hyponatremia and production of large volumes of dilute urine.&amp;nbsp; Water restriction normalizes serum sodium levels and increases urine osmolality.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><div id=\"div_t50342\" class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" class=\"table-default-style\"><tbody><tr><td colspan=\"4\"><p align=\"center\"><strong>Polyuria &amp; polydipsia</strong></p></td></tr><tr><td></td><td></td><td colspan=\"2\"><p align=\"center\"><strong>Water deprivation test</strong></p></td></tr><tr><td></td><td><p align=\"center\"><strong>Serum sodium</strong></p></td><td><p align=\"center\"><strong>Urine osmolality after water deprivation</strong></p></td><td><p align=\"center\"><strong>Urine osmolality with vasopressin injection</strong></p></td></tr><tr><td><p align=\"center\"><strong>Normal</strong></p></td><td><p align=\"center\">Normal</p></td><td><p align=\"center\">Increased</p></td><td><p align=\"center\">No additional increase</p></td></tr><tr><td><p align=\"center\"><strong>Central diabetes insipidus</strong></p></td><td><p align=\"center\">High</p></td><td><p align=\"center\">No change or mild increase</p></td><td><p align=\"center\">Large increase</p></td></tr><tr><td><p align=\"center\"><strong>Nephrogenic diabetes insipidus</strong></p></td><td><p align=\"center\">High</p></td><td><p align=\"center\">No change or mild increase</p></td><td><p align=\"center\">Mild increase</p></td></tr><tr><td><p align=\"center\"><strong>Primary polydipsia</strong></p></td><td><p align=\"center\">Low</p></td><td><p align=\"center\">Increased</p></td><td><p align=\"center\">No additional increase</p></td></tr></tbody></table></div><p class=\"ng-scope\">This patient has <strong>polyuria</strong> (urine output &gt;3 L/day) with a <strong>low initial urine osmolality</strong>, which is indicative of <strong>water diuresis</strong> caused by one of the following conditions:</p><ul class=\"ng-scope\"><li>Diabetes insipidus (DI), which occurs due to either deficient antidiuretic hormone (ADH) production (central DI) or lack of renal response to ADH (nephrogenic DI)</li><li>Primary polydipsia, which results from excessive water consumption (most common in patients with underlying psychiatric disease or emotional distress)</li></ul><p class=\"ng-scope\">In patients with <strong>primary polydipsia</strong>, excessive water intake often leads to mild <strong>hyponatremia</strong>.&nbsp; In contrast, primary ADH deficiency in DI leads to free water loss often with ensuing hypernatremia.&nbsp; In certain patients, water deprivation testing may be helpful for differentiating PP from DI.</p><p class=\"ng-scope\">During a <strong>water deprivation test</strong>, drinking water is withheld and urine osmolality is monitored until it reaches a steady-state plateau (representing the maximal concentrating ability of the kidneys).&nbsp; Water deprivation in patients with PP stimulates ADH secretion and leads to a significant <strong>rise in urine concentration</strong>, whereas urine in patients with DI remains dilute.&nbsp; Once a plateau is reached, <strong>vasopressin is administered</strong>.&nbsp; In DI, exogenous vasopressin causes an increase in urine osmolality; in PP, urine <strong>concentration remains unchanged</strong> because at this point in the test, the endogenous ADH effect is already at maximum.</p><p class=\"ng-scope\">If PP is confirmed, long-term management includes <strong>restriction of free water </strong>intake.</p><p class=\"ng-scope\"><strong>(Choices A, B, and C)</strong>&nbsp; Central DI can be treated with desmopressin (synthetic ADH analog), while nephrogenic DI is typically treated with thiazide diuretics (induce mild hypovolemia that decreases polyuria) or NSAIDs (inhibit renal prostaglandins that act as ADH antagonists).&nbsp; However, during water deprivation, patients with central or nephrogenic DI typically have no change or only a mild increase in urine osmolality due to lack of adequate ADH effect; subsequent vasopressin administration would also be expected to cause an increase in urine osmolality.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Severe hyperglycemia in diabetes mellitus can overwhelm the ability of the kidneys to recover the filtered glucose load, leading to osmotic diuresis and polyuria.&nbsp; However, this typically occurs with blood glucose &gt;180 mg/dL; this patient's glucose is normal.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Primary (psychogenic) polydipsia is characterized by excessive intake of free water, leading to hyponatremia and production of large volumes of dilute urine.&nbsp; Water restriction normalizes serum sodium levels and increases urine osmolality.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/25712898\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/25712898\"><span compiledata=\"\" htmlcontent=\"The water deprivation test and a potential role for the arginine vasopressin precursor copeptin to differentiate diabetes insipidus from primary polydipsia.\" class=\"ng-scope\"><p class=\"ng-scope\">The water deprivation test and a potential role for the arginine vasopressin precursor copeptin to differentiate diabetes insipidus from primary polydipsia.</p></span></a><!-- end ngIf: launchTestOptions.topLevelProductId != constants