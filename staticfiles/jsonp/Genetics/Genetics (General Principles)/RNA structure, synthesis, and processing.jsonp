 (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p>Amino acid activation and attachment to the 3&amp;#39; end of tRNA are catalyzed by <strong>aminoacyl-tRNA synthetases</strong> (AA-tRNA synthetases).&amp;nbsp; Each amino acid/tRNA pair has a specific AA-tRNA synthetase that links them together.&amp;nbsp; These enzymes are responsible for ensuring that each amino acid binds to the tRNA with the proper anticodon.&amp;nbsp; AA-tRNA synthetase activation and binding sites are <strong>highly specific</strong> for their correct amino acids and tRNA molecules.&amp;nbsp; In addition, some AA-tRNA synthetases can <strong>&amp;#x22;proofread&amp;#x22;</strong> their specific tRNA molecules and hydrolyze the amino acid bond when their tRNAs are incorrectly charged.&amp;nbsp; The error rate for AA-tRNA synthetases is therefore very low, with less than 1 error per 10<font size=&quot;2&quot;><sup>4</sup></font> charges.</p><p>During protein synthesis, tRNA acts as an adaptor molecule between the codons found on mRNA and the amino acids being incorporated into the polypeptide chain.&amp;nbsp; The amino acid sequence in a polypeptide chain is dictated by the binding of a tRNA anticodon to its complementary codon on the mRNA molecule being translated.&amp;nbsp; <strong>Erroneous amino acid/tRNA coupling</strong> by AA-tRNA synthetase causes the <strong>wrong amino acid</strong> to be <strong>incorporated</strong> into the growing polypeptide chain <strong>(Choice E)</strong>.</p><p>For example, under normal circumstances, when the ribosome encounters a cysteine codon (eg, UGU) on mRNA, the complementary tRNA anticodon (eg, ACA) binds.&amp;nbsp; If this tRNA is improperly charged with alanine, as described in the experiment above, alanine will be incorrectly incorporated into the growing polypeptide chain in place of cysteine <strong>(Choice A)</strong>.</p><p><strong>(Choice C)</strong>&amp;nbsp; During polypeptide chain elongation, ribosomes move from codon to codon on mRNA in the 5&amp;#39; to 3&amp;#39; direction, sequentially adding amino acids from aminoacyl-tRNA to the peptide chain.&amp;nbsp; This continues until the ribosome encounters a stop codon (ie, UAA, UAG, or UGA).&amp;nbsp; Releasing factors then assist in polypeptide chain termination.</p><p><strong>(Choice D)</strong>&amp;nbsp; DNA glycosylases are enzymes involved in DNA base excision repair.</p><p><strong>Educational objective:</strong><br>The sequence of amino acids in a growing polypeptide chain is dictated by the interaction of the mRNA codon with the tRNA anticodon.&amp;nbsp; tRNA that is mischarged with the incorrect amino acid (and not corrected by aminoacyl-tRNA synthetase proofreading) will incorporate the wrong amino acid into the growing polypeptide chain.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\">Amino acid activation and attachment to the 3' end of tRNA are catalyzed by <strong>aminoacyl-tRNA synthetases</strong> (AA-tRNA synthetases).&nbsp; Each amino acid/tRNA pair has a specific AA-tRNA synthetase that links them together.&nbsp; These enzymes are responsible for ensuring that each amino acid binds to the tRNA with the proper anticodon.&nbsp; AA-tRNA synthetase activation and binding sites are <strong>highly specific</strong> for their correct amino acids and tRNA molecules.&nbsp; In addition, some AA-tRNA synthetases can <strong>\"proofread\"</strong> their specific tRNA molecules and hydrolyze the amino acid bond when their tRNAs are incorrectly charged.&nbsp; The error rate for AA-tRNA synthetases is therefore very low, with less than 1 error per 10<font size=\"2\"><sup>4</sup></font> charges.</p><p class=\"ng-scope\">During protein synthesis, tRNA acts as an adaptor molecule between the codons found on mRNA and the amino acids being incorporated into the polypeptide chain.&nbsp; The amino acid sequence in a polypeptide chain is dictated by the binding of a tRNA anticodon to its complementary codon on the mRNA molecule being translated.&nbsp; <strong>Erroneous amino acid/tRNA coupling</strong> by AA-tRNA synthetase causes the <strong>wrong amino acid</strong> to be <strong>incorporated</strong> into the growing polypeptide chain <strong>(Choice E)</strong>.</p><p class=\"ng-scope\">For example, under normal circumstances, when the ribosome encounters a cysteine codon (eg, UGU) on mRNA, the complementary tRNA anticodon (eg, ACA) binds.&nbsp; If this tRNA is improperly charged with alanine, as described in the experiment above, alanine will be incorrectly incorporated into the growing polypeptide chain in place of cysteine <strong>(Choice A)</strong>.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; During polypeptide chain elongation, ribosomes move from codon to codon on mRNA in the 5' to 3' direction, sequentially adding amino acids from aminoacyl-tRNA to the peptide chain.&nbsp; This continues until the ribosome encounters a stop codon (ie, UAA, UAG, or UGA).&nbsp; Releasing factors then assist in polypeptide chain termination.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; DNA glycosylases are enzymes involved in DNA base excision repair.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>The sequence of amino acids in a growing polypeptide chain is dictated by the interaction of the mRNA codon with the tRNA anticodon.&nbsp; tRNA that is mischarged with the incorrect amino acid (and not corrected by aminoacyl-tRNA synthetase proofreading) will incorporate the wrong amino acid into the growing polypeptide chain.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/22243580\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/22243580\"><span compiledata=\"\" htmlcontent=\"Quality control in aminoacyl-tRNA synthesis its role in translational fidelity.\" class=\"ng-scope\"><p class=\"ng-scope\">Quality control in aminoacyl-tRNA synthesis its role in translational fidelity.</p></span></a><!-- end ngIf: launchTestOptions.topLevelProdu