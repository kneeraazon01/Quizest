\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; id=&quot;3587&quot;  src=&quot;https://www.uworld.com/media/L26650.jpg&quot; draggable=&quot;false&quot; ></p><p>The key steps of DNA replication are similar in eukaryotes and prokaryotes (eg, <em>Escherichia coli</em>) and are as follows:</p><ol><li>Unwinding of double-stranded DNA by helicase to produce single-stranded DNA</li><li>Formation of a replication fork</li><li>Synthesis of RNA primers by the enzyme primase</li><li>Synthesis and concurrent proofreading of daughter DNA strands by DNA polymerases</li><li>Removal and replacement of RNA primers with DNA</li><li>Ligation of Okazaki fragments on lagging strands by the enzyme ligase</li></ol><p>Despite the similarities of prokaryotic and eukaryotic DNA replication, there are important differences between these processes.&amp;nbsp; Prokaryotes possess 3 major <a ng-click=&quot;showexhibit('3588',true);&quot;>DNA polymerases</a> (I, II, and III), whereas eukaryotes have 5 major DNA polymerases (&amp;#945;, &amp;#946;, &amp;#947;, &amp;#948;, and &amp;#949;).&amp;nbsp; The <strong>eukaryotic genome</strong> is also much larger and more complex than the prokaryotic genome, which can be partly explained by the abundance of noncoding DNA regions (introns) located between coding regions (exons).&amp;nbsp; In addition, prokaryotes typically have circular DNA with a single origin of replication, whereas eukaryotes have linear DNA with <strong>multiple origins of replication</strong>.&amp;nbsp; This feature allows the eukaryotic genome to be copied in a quick and effective manner despite its large size.</p><p><strong>(Choice A)</strong>&amp;nbsp; Eukaryotic and prokaryotic DNA polymerases synthesize daughter strand DNA in the 5&amp;#39; to 3&amp;#39; direction.&amp;nbsp; The leading strand is formed continuously, whereas the lagging strand is formed discontinuously, creating Okazaki fragments.</p><p><strong>(Choice B)</strong>&amp;nbsp; DNA unwinding is an energy-dependent process performed by the enzyme helicase in both prokaryotes and eukaryotes.</p><p><strong>(Choice D)</strong>&amp;nbsp; Proofreading of daughter strands during DNA replication is necessary to preserve the genetic code and prevent potentially lethal mutations.&amp;nbsp; All 3 prokaryotic DNA polymerases and most eukaryotic DNA polymerases (eg, &amp;#947;, &amp;#948;, &amp;#949;) possess 3&amp;#39; to 5&amp;#39; exonuclease (&amp;#x22;proofreading&amp;#x22;) activity.</p><p><strong>(Choice E)</strong>&amp;nbsp; Prokaryotic and eukaryotic DNA polymerases require an RNA primer before they can initiate synthesis of complementary DNA on a single-stranded template.&amp;nbsp; Primase (prokaryotes) and DNA polymerase &amp;#945; (eukaryotes) are the enzymes responsible for synthesizing this primer.</p><p><strong>Educational objective:</strong><br>Multiple origins of replication make eukaryotic DNA replication quick and effective despite the large size and complexity of the genome compared to that of prokaryotic organisms.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" id=\"3587\" src=\"image/L26650.jpg\" draggable=\"false\"></p><p class=\"ng-scope\">The key steps of DNA replication are similar in eukaryotes and prokaryotes (eg, <em>Escherichia coli</em>) and are as follows:</p><ol class=\"ng-scope\"><li>Unwinding of double-stranded DNA by helicase to produce single-stranded DNA</li><li>Formation of a replication fork</li><li>Synthesis of RNA primers by the enzyme primase</li><li>Synthesis and concurrent proofreading of daughter DNA strands by DNA polymerases</li><li>Removal and replacement of RNA primers with DNA</li><li>Ligation of Okazaki fragments on lagging strands by the enzyme ligase</li></ol><p class=\"ng-scope\">Despite the similarities of prokaryotic and eukaryotic DNA replication, there are important differences between these processes.&nbsp; Prokaryotes possess 3 major <a ng-click=\"showexhibit('3588',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/81307.html&quot;,&quot;title&quot;:&quot;81307&quot;}]\">DNA polymerases</a> (I, II, and III), whereas eukaryotes have 5 major DNA polymerases (\u03b1, \u03b2, \u03b3, \u03b4, and \u03b5).&nbsp; The <strong>eukaryotic genome</strong> is also much larger and more complex than the prokaryotic genome, which can be partly explained by the abundance of noncoding DNA regions (introns) located between coding regions (exons).&nbsp; In addition, prokaryotes typically have circular DNA with a single origin of replication, whereas eukaryotes have linear DNA with <strong>multiple origins of replication</strong>.&nbsp; This feature allows the eukaryotic genome to be copied in a quick and effective manner despite its large size.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Eukaryotic and prokaryotic DNA polymerases synthesize daughter strand DNA in the 5' to 3' direction.&nbsp; The leading strand is formed continuously, whereas the lagging strand is formed discontinuously, creating Okazaki fragments.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; DNA unwinding is an energy-dependent process performed by the enzyme helicase in both prokaryotes and eukaryotes.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Proofreading of daughter strands during DNA replication is necessary to preserve the genetic code and prevent potentially lethal mutations.&nbsp; All 3 prokaryotic DNA polymerases and most eukaryotic DNA polymerases (eg, \u03b3, \u03b4, \u03b5) possess 3' to 5' exonuclease (\"proofreading\") activity.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Prokaryotic and eukaryotic DNA polymerases require an RNA primer before they can initiate synthesis of complementary DNA on a single-stranded template.&nbsp; Primase (prokaryotes) and DNA polymerase \u03b1 (eukaryotes) are the enzymes responsible for synthesizing this primer.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Multiple origins of replication make eukaryotic DNA replication quick and effective despite the large size and complexity of the genome compared to that of prokaryotic organisms.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/24699916\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/24699916\"><span compiledata=\"\" htmlcontent=\"Mechanism of chromosomal DNA replication initiation and replication fork stabilization in eukaryotes.\" class=\"ng-scope\"><p class=\"ng-scope\">Mechanism of chromosomal DNA replication initiation and replication fork stabilization in eukaryotes.</p></span></a><!-- end ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal -->\n                                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal -->\n                                </li><!-- end ngRepeat: questionReference in currentQuestion.questionMappingReferencesList -->\n                            </ul>\n                        </div>\n                    </div><!-- end ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankI