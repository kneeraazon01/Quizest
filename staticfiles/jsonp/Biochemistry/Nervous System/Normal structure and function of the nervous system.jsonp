topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><div id=&quot;div_t35643&quot;><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; class=&quot;table-default-style&quot;><tbody><tr><td colspan=&quot;2&quot;><p align=&quot;center&quot;><strong>&amp;nbsp;Functions of cellular organelles</strong></p></td></tr><tr><td><p align=&quot;center&quot;><strong>Golgi apparatus</strong></p></td><td><ul><li>Protein modification, sorting &amp;amp; transport</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Lysosomes</strong></p></td><td><ul><li>Digestion of cellular debris &amp;amp; pathogens</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Mitochondria</strong></p></td><td><ul><li>TCA cycle &amp;amp; fatty acid oxidation</li><li>Electron transport chain (ATP synthesis)</li><li>Apoptosis</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Peroxisomes</strong></p></td><td><ul><li>Oxidation of very long &amp;amp; branched-chain fatty acids</li><li>Hydrogen peroxide degradation</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Proteasomes</strong></p></td><td><ul><li>Degradation of ubiquitinated proteins</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Rough</strong>&amp;nbsp;<strong>endoplasmic reticulum</strong></p></td><td><ul><li>Synthesis of integral membrane &amp;amp; secretory proteins</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Smooth endoplasmic reticulum</strong></p></td><td><ul><li>Drug detoxification</li><li>Lipid, phospholipid &amp;amp; steroid synthesis</li></ul></td></tr></tbody></table></div><p>Peroxisomal disorders are rare inborn errors of metabolism in which <strong>peroxisomes</strong> are either absent or nonfunctional.&amp;nbsp; <strong>Very long chain fatty acids</strong> (VLCFAs) and certain <strong>branched-chain fatty acids</strong> (eg, phytanic acid) cannot undergo mitochondrial beta-oxidation; these fatty acids are metabolized by a special form of beta-oxidation (VLCFAs) or by alpha-oxidation (branched-chain fatty acids) within peroxisomes.&amp;nbsp; In peroxisomal disorders, these fatty acids build up in various tissues.&amp;nbsp; Accumulation in neuronal cell membranes leads to <strong>neurologic defects</strong> due to improper neuronal migration, myelination, and degeneration.</p><p>This infant most likely has <strong>Zellweger syndrome</strong>, which is caused by defective peroxisomal biogenesis.&amp;nbsp; It typically presents in early infancy with craniofacial abnormalities (eg, widened sutures, large anterior fontanelle), hepatomegaly, and profound neurologic defects (eg, hypotonia, seizures, developmental delay).&amp;nbsp; Death usually occurs within months of initial presentation.</p><p>Another common peroxisomal disease is <strong>X-linked adrenoleukodystrophy</strong>, which is characterized by defective transport of VLCFAs into peroxisomes.&amp;nbsp; Patients present in childhood or adulthood with neurologic deterioration and adrenal insufficiency as VLCFAs accumulate in the central nervous system and adrenal glands.</p><p><strong>(Choice A)</strong>&amp;nbsp; The Golgi apparatus is involved in posttranslational modification of proteins from the rough endoplasmic reticulum.&amp;nbsp; It also routes proteins to their ultimate location (eg, lysosomes, extracellular space) using membrane-bound vesicles.</p><p><strong>(Choice B)</strong>&amp;nbsp; Lysosomes are organelles containing proteins that can digest a diverse range of biomolecules, cellular debris, and engulfed pathogens.&amp;nbsp; Lysosomal storage disorders (eg, mucopolysaccharidoses, sphingolipidoses) result in intracellular accumulation of glycosaminoglycans, glycoproteins, or glycolipids.</p><p><strong>(Choice C)</strong>&amp;nbsp; Mitochondria are responsible for beta-oxidation of most fatty acids, ketogenesis, the TCA cycle and electron transport chain, and the first 2 steps of the urea cycle.&amp;nbsp; Dysfunction can lead to various types of encephalomyopathy.&amp;nbsp; Mitochondria do not metabolize VLCFAs or fatty acids with branch points at odd-numbered carbons.</p><p><strong>(Choice E)</strong>&amp;nbsp; Proteasomes degrade unneeded or defective intracellular proteins into small polypeptides or amino acids.&amp;nbsp; Impaired proteasome function can lead to cell cycle dysregulation, protein aggregation, and defective processing of peptide antigens.</p><p><strong>(Choice F)</strong>&amp;nbsp; The rough endoplasmic reticulum is the site of synthesis for most secretory proteins, integral membrane proteins of the nucleus and cell membrane, and lysosomal proteins.</p><p><strong>Educational objective:</strong><br>The buildup of very long chain and branched-chain (eg, phytanic) fatty acids due to impaired oxidation is the hallmark of peroxisomal disorders (eg, Zellweger syndrome, adrenoleukodystrophy).&amp;nbsp; Accumulation of these fatty acids in the brain results in permanent neurologic dysfunction.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><div id=\"div_t35643\" class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" class=\"table-default-style\"><tbody><tr><td colspan=\"2\"><p align=\"center\"><strong>&nbsp;Functions of cellular organelles</strong></p></td></tr><tr><td><p align=\"center\"><strong>Golgi apparatus</strong></p></td><td><ul><li>Protein modification, sorting &amp; transport</li></ul></td></tr><tr><td><p align=\"center\"><strong>Lysosomes</strong></p></td><td><ul><li>Digestion of cellular debris &amp; pathogens</li></ul></td></tr><tr><td><p align=\"center\"><strong>Mitochondria</strong></p></td><td><ul><li>TCA cycle &amp; fatty acid oxidation</li><li>Electron transport chain (ATP synthesis)</li><li>Apoptosis</li></ul></td></tr><tr><td><p align=\"center\"><strong>Peroxisomes</strong></p></td><td><ul><li>Oxidation of very long &amp; branched-chain fatty acids</li><li>Hydrogen peroxide degradation</li></ul></td></tr><tr><td><p align=\"center\"><strong>Proteasomes</strong></p></td><td><ul><li>Degradation of ubiquitinated proteins</li></ul></td></tr><tr><td><p align=\"center\"><strong>Rough</strong>&nbsp;<strong>endoplasmic reticulum</strong></p></td><td><ul><li>Synthesis of integral membrane &amp; secretory proteins</li></ul></td></tr><tr><td><p align=\"center\"><strong>Smooth endoplasmic reticulum</strong></p></td><td><ul><li>Drug detoxification</li><li>Lipid, phospholipid &amp; steroid synthesis</li></ul></td></tr></tbody></table></div><p class=\"ng-scope\">Peroxisomal disorders are rare inborn errors of metabolism in which <strong>peroxisomes</strong> are either absent or nonfunctional.&nbsp; <strong>Very long chain fatty acids</strong> (VLCFAs) and certain <strong>branched-chain fatty acids</strong> (eg, phytanic acid) cannot undergo mitochondrial beta-oxidation; these fatty acids are metabolized by a special form of beta-oxidation (VLCFAs) or by alpha-oxidation (branched-chain fatty acids) within peroxisomes.&nbsp; In peroxisomal disorders, these fatty acids build up in various tissues.&nbsp; Accumulation in neuronal cell membranes leads to <strong>neurologic defects</strong> due to improper neuronal migration, myelination, and degeneration.</p><p class=\"ng-scope\">This infant most likely has <strong>Zellweger syndrome</strong>, which is caused by defective peroxisomal biogenesis.&nbsp; It typically presents in early infancy with craniofacial abnormalities (eg, widened sutures, large anterior fontanelle), hepatomegaly, and profound neurologic defects (eg, hypotonia, seizures, developmental delay).&nbsp; Death usually occurs within months of initial presentation.</p><p class=\"ng-scope\">Another common peroxisomal disease is <strong>X-linked adrenoleukodystrophy</strong>, which is characterized by defective transport of VLCFAs into peroxisomes.&nbsp; Patients present in childhood or adulthood with neurologic deterioration and adrenal insufficiency as VLCFAs accumulate in the central nervous system and adrenal glands.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; The Golgi apparatus is involved in posttranslational modification of proteins from the rough endoplasmic reticulum.&nbsp; It also routes proteins to their ultimate location (eg, lysosomes, extracellular space) using membrane-bound vesicles.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Lysosomes are organelles containing proteins that can digest a diverse range of biomolecules, cellular debris, and engulfed pathogens.&nbsp; Lysosomal storage disorders (eg, mucopolysaccharidoses, sphingolipidoses) result in intracellular accumulation of glycosaminoglycans, glycoproteins, or glycolipids.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Mitochondria are responsible for beta-oxidation of most fatty acids, ketogenesis, the TCA cycle and electron transport chain, and the first 2 steps of the urea cycle.&nbsp; Dysfunction can lead to various types of encephalomyopathy.&nbsp; Mitochondria do not metabolize VLCFAs or fatty acids with branch points at odd-numbered carbons.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Proteasomes degrade unneeded or defective intracellular proteins into small polypeptides or amino acids.&nbsp; Impaired proteasome function can lead to cell cycle dysregulation, protein aggregation, and defective processing of peptide antigens.</p><p class=\"ng-scope\"><strong>(Choice F)</strong>&nbsp; The rough endoplasmic reticulum is the site of synthesis for most secretory proteins, integral membrane proteins of the nucleus and cell membrane, and lysosomal proteins.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>The buildup of very long chain and branched-chain (eg, phytanic) fatty acids due to impaired oxidation is the hallmark of peroxisomal disorders (eg, Zellweger syndrome, adrenoleukodystrophy).&nbsp; Accumulation of these fatty acids in the brain results in permanent neurologic dysfunction.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www