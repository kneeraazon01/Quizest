ants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;4639&quot; src=&quot;https://www.uworld.com/media/L25396.jpg&quot; ></p><p><strong>Calcium-sensing receptors</strong> (CaSRs) are transmembrane <strong>G<sub>q</sub> protein&amp;ndash;coupled</strong> (metabotropic) receptors that regulate the secretion of parathyroid hormone (PTH) in response to changes in circulating calcium levels.&amp;nbsp; Binding of calcium to CaSRs leads to the inhibition of PTH release, whereas low calcium levels allow increased PTH release.</p><p><strong>Familial hypocalciuric hypercalcemia</strong> (FHH) is a benign autosomal dominant disorder caused by defective CaSRs in the parathyroid gland and kidneys.&amp;nbsp; In FHH, higher serum calcium levels are required to suppress the secretion of PTH.&amp;nbsp; This raises the set point of calcium-induced regulation of PTH secretion.&amp;nbsp; Patients with FHH have mild asymptomatic hypercalcemia, reduced urinary excretion of calcium, and high normal or mildly elevated PTH.</p><p><strong>(Choice A)</strong>&amp;nbsp; Steroid hormones, thyroid hormone, and vitamin D act by binding to intracellular receptors with DNA-binding domains that interact with the regulatory DNA sequences of target genes.</p><p><strong>(Choice C)</strong>&amp;nbsp; Transmembrane ligand-gated ion channels (ionotropic receptors) allow a regulated flux of calcium, sodium, potassium, and chloride ions across the cell membrane.&amp;nbsp; Neurotransmitters that work via ion channel&amp;ndash;linked receptors include acetylcholine, serotonin, N-methyl-D-aspartate, and gamma-aminobutyric acid.</p><p><strong>(Choice D)</strong>&amp;nbsp; Insulin and insulin-like growth factor work by stimulating transmembrane receptors with intrinsic tyrosine kinase activity in the intracellular domain, initiating a downstream phosphorylation cascade.</p><p><strong>(Choice E)</strong>&amp;nbsp; Janus kinase (JAK) is a cytoplasmic protein activated by ligand binding to transmembrane receptors.&amp;nbsp; JAKs activate cytoplasmic transcription factors called signal transducers and activators of transcription (STAT), which enter the nucleus to promote gene transcription.&amp;nbsp; Examples of hormones using a JAK/STAT messenger system include <a ng-click=&quot;showexhibit('18281',true);&quot;>erythropoietin</a>, growth hormone, and prolactin.</p><p><strong>Educational objective:</strong><br>Calcium-sensing receptors are G protein&amp;ndash;coupled receptors that regulate the secretion of parathyroid hormone in response to changes in circulating calcium levels.&amp;nbsp; Familial hypocalciuric hypercalcemia is a benign autosomal dominant disorder caused by defective calcium-sensing receptors in the parathyroid gland and kidneys.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"4639\" src=\"image/L25396.jpg\"></p><p class=\"ng-scope\"><strong>Calcium-sensing receptors</strong> (CaSRs) are transmembrane <strong>G<sub>q</sub> protein\u2013coupled</strong> (metabotropic) receptors that regulate the secretion of parathyroid hormone (PTH) in response to changes in circulating calcium levels.&nbsp; Binding of calcium to CaSRs leads to the inhibition of PTH release, whereas low calcium levels allow increased PTH release.</p><p class=\"ng-scope\"><strong>Familial hypocalciuric hypercalcemia</strong> (FHH) is a benign autosomal dominant disorder caused by defective CaSRs in the parathyroid gland and kidneys.&nbsp; In FHH, higher serum calcium levels are required to suppress the secretion of PTH.&nbsp; This raises the set point of calcium-induced regulation of PTH secretion.&nbsp; Patients with FHH have mild asymptomatic hypercalcemia, reduced urinary excretion of calcium, and high normal or mildly elevated PTH.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Steroid hormones, thyroid hormone, and vitamin D act by binding to intracellular receptors with DNA-binding domains that interact with the regulatory DNA sequences of target genes.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Transmembrane ligand-gated ion channels (ionotropic receptors) allow a regulated flux of calcium, sodium, potassium, and chloride ions across the cell membrane.&nbsp; Neurotransmitters that work via ion channel\u2013linked receptors include acetylcholine, serotonin, N-methyl-D-aspartate, and gamma-aminobutyric acid.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Insulin and insulin-like growth factor work by stimulating transmembrane receptors with intrinsic tyrosine kinase activity in the intracellular domain, initiating a downstream phosphorylation cascade.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Janus kinase (JAK) is a cytoplasmic protein activated by ligand binding to transmembrane receptors.&nbsp; JAKs activate cytoplasmic transcription factors called signal transducers and activators of transcription (STAT), which enter the nucleus to promote gene transcription.&nbsp; Examples of hormones using a JAK/STAT messenger system include <a ng-click=\"showexhibit('18281',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L43076.jpg&quot;,&quot;title&quot;:&quot;L43076&quot;}]\">erythropoietin</a>, growth hormone, and prolactin.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Calcium-sensing receptors are G protein\u2013coupled receptors that regulate the secretion of parathyroid hormone in response to changes in circulating calcium levels.&nbsp; Familial hypocalciuric hypercalcemia is a benign autosomal dominant disorder caused by defective calcium-sensing receptors in the parathyroid gland and kidneys.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/21986511\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/21986511