Info.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><div id=&quot;div_t70819&quot;><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; class=&quot;table-default-style table-header-footer-style&quot;><tbody><tr><td colspan=&quot;5&quot;><p align=&quot;center&quot;><strong>Mucopolysaccharidoses*</strong></p></td></tr><tr><td><p align=&quot;center&quot;><strong>Diagnosis</strong></p></td><td><p align=&quot;center&quot;><strong>Inheritance</strong></p></td><td><p align=&quot;center&quot;><strong>Deficiency</strong></p></td><td><p align=&quot;center&quot;><strong>Accumulated<br />substrate</strong></p></td><td><p align=&quot;center&quot;><strong>Key features</strong></p></td></tr><tr><td><p align=&quot;center&quot;><strong>Hurler<br />syndrome</strong></p></td><td><p align=&quot;center&quot;>AR</p></td><td><p align=&quot;center&quot;>&amp;alpha;-L-iduronidase</p></td><td rowspan=&quot;2&quot;><p align=&quot;center&quot;>Dermatan &amp;amp; heparan sulfate</p></td><td><ul><li>Severe developmental delay &amp;amp; intellectual disability</li><li>Coarse facial features</li><li>Corneal clouding</li><li>Hepatosplenomegaly</li><li>Death at age &amp;lt;10 (cardiac complications)</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Hunter<br />syndrome</strong></p></td><td><p align=&quot;center&quot;>XLR</p></td><td><p align=&quot;center&quot;>Iduronate sulfatase</p></td><td><ul><li>Milder features than Hurler:<ul style=&quot;list-style-type:circle;&quot;><li>(+) Aggressive behavior</li><li>(&amp;minus;) Corneal clouding</li></ul></li><li>Death in adolescence/early adulthood</li></ul></td></tr><tr><td colspan=&quot;5&quot;>* Subtype of lysosomal storage disorder.<p><strong>AR</strong> = autosomal recessive; <strong>XLR</strong> = X-linked recessive</p></td></tr></tbody></table></div><p>This patient most likely had <strong>Hurler syndrome</strong> (type I mucopolysaccharidosis), an autosomal recessive disorder characterized by severe <strong>intellectual disability</strong>, <strong>corneal clouding</strong>, and <strong>coarse facial features</strong> (eg, frontal bossing, broad nose, flat midface).&amp;nbsp; Hepatosplenomegaly and heart disease are also typical findings.</p><p><strong>Mucopolysaccharidoses</strong> are lysosomal storage disorders resulting from the <strong>defective metabolism of glycosaminoglycans</strong> (GAGs).&amp;nbsp; GAGs are long, unbranched polysaccharides that are an important component of the ground substance within the extracellular matrix of connective tissues.&amp;nbsp; Hurler syndrome is caused by a <strong>deficiency</strong> of <strong>alpha-L-iduronidase</strong>, which hydrolyzes dermatan and heparan sulfate.&amp;nbsp; Hunter syndrome, a less severe disease than Hurler syndrome, also results in an accumulation of dermatan and heparan sulfate but characteristically does not cause corneal clouding.</p><p>Patients with Hurler syndrome have a shortened life expectancy.&amp;nbsp; Death, typically in early childhood, is usually caused by <strong>cardiac complications</strong> (eg, myocardial infarction); this child&amp;#39;s death from coronary artery disease was likely due to <strong>deposition of dermatan and heparan sulfate</strong> within the vessels.</p><p><strong>(Choice A)</strong>&amp;nbsp; Premature coronary artery disease is seen with familial hypercholesterolemia.&amp;nbsp; Cholesterol deposits can lead to tendon xanthomas, xanthelasmas (ie, eyelid plaques), and/or corneal arcus (ie, gray ring around the cornea) but not coarse facial features, intellectual disability, or a diffuse corneal haze.</p><p><strong>(Choice B)</strong>&amp;nbsp; Accumulation of glucocerebroside within mononuclear cells is seen in Gaucher disease, an autosomal recessive defect in beta-glucocerebrosidase.&amp;nbsp; This condition is characterized by bruising due to thrombocytopenia, hepatosplenomegaly, progressive neurologic deterioration, and severe bone pain.&amp;nbsp; Premature coronary artery disease does not occur.</p><p><strong>(Choice C)</strong>&amp;nbsp; Glycogen storage diseases cause defective metabolism of glycogen in the liver and/or muscles.&amp;nbsp; The presentation can include hypoglycemia, lactic acidosis, hepatomegaly, growth retardation, and/or muscle fatigue/cramping.&amp;nbsp; Cardiomegaly can occur with Pompe disease, but corneal clouding is not a feature.</p><p><strong>(Choice E)</strong>&amp;nbsp; Niemann-Pick disease results from a deficiency of sphingomyelinase, which leads to the accumulation of sphingomyelin within phagocytes.&amp;nbsp; This disease is characterized by hepatosplenomegaly and progressive neurologic deterioration, but eye findings include a cherry-red spot on the macula, not corneal clouding.</p><p><strong>Educational objective:</strong><br>Hurler syndrome is a mucopolysaccharidosis caused by a deficiency of alpha-L-iduronidase, which hydrolyzes dermatan and heparan sulfate.&amp;nbsp; Accumulation of these substances results in the characteristic features of intellectual disability, coarse facial features, corneal clouding, and hepatosplenomegaly.&amp;nbsp; Early death due to cardiac complications (eg, myocardial infarction) is expected.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><div id=\"div_t70819\" class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" class=\"table-default-style table-header-footer-style\"><tbody><tr><td colspan=\"5\"><p align=\"center\"><strong>Mucopolysaccharidoses*</strong></p></td></tr><tr><td><p align=\"center\"><strong>Diagnosis</strong></p></td><td><p align=\"center\"><strong>Inheritance</strong></p></td><td><p align=\"center\"><strong>Deficiency</strong></p></td><td><p align=\"center\"><strong>Accumulated<br>substrate</strong></p></td><td><p align=\"center\"><strong>Key features</strong></p></td></tr><tr><td><p align=\"center\"><strong>Hurler<br>syndrome</strong></p></td><td><p align=\"center\">AR</p></td><td><p align=\"center\">\u03b1-L-iduronidase</p></td><td rowspan=\"2\"><p align=\"center\">Dermatan &amp; heparan sulfate</p></td><td><ul><li>Severe developmental delay &amp; intellectual disability</li><li>Coarse facial features</li><li>Corneal clouding</li><li>Hepatosplenomegaly</li><li>Death at age &lt;10 (cardiac complications)</li></ul></td></tr><tr><td><p align=\"center\"><strong>Hunter<br>syndrome</strong></p></td><td><p align=\"center\">XLR</p></td><td><p align=\"center\">Iduronate sulfatase</p></td><td><ul><li>Milder features than Hurler:<ul style=\"list-style-type:circle;\"><li>(+) Aggressive behavior</li><li>(\u2212) Corneal clouding</li></ul></li><li>Death in adolescence/early adulthood</li></ul></td></tr><tr><td colspan=\"5\">* Subtype of lysosomal storage disorder.<p><strong>AR</strong> = autosomal recessive; <strong>XLR</strong> = X-linked recessive</p></td></tr></tbody></table></div><p class=\"ng-scope\">This patient most likely had <strong>Hurler syndrome</strong> (type I mucopolysaccharidosis), an autosomal recessive disorder characterized by severe <strong>intellectual disability</strong>, <strong>corneal clouding</strong>, and <strong>coarse facial features</strong> (eg, frontal bossing, broad nose, flat midface).&nbsp; Hepatosplenomegaly and heart disease are also typical findings.</p><p class=\"ng-scope\"><strong>Mucopolysaccharidoses</strong> are lysosomal storage disorders resulting from the <strong>defective metabolism of glycosaminoglycans</strong> (GAGs).&nbsp; GAGs are long, unbranched polysaccharides that are an important component of the ground substance within the extracellular matrix of connective tissues.&nbsp; Hurler syndrome is caused by a <strong>deficiency</strong> of <strong>alpha-L-iduronidase</strong>, which hydrolyzes dermatan and heparan sulfate.&nbsp; Hunter syndrome, a less severe disease than Hurler syndrome, also results in an accumulation of dermatan and heparan sulfate but characteristically does not cause corneal clouding.</p><p class=\"ng-scope\">Patients with Hurler syndrome have a shortened life expectancy.&nbsp; Death, typically in early childhood, is usually caused by <strong>cardiac complications</strong> (eg, myocardial infarction); this child's death from coronary artery disease was likely due to <strong>deposition of dermatan and heparan sulfate</strong> within the vessels.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Premature coronary artery disease is seen with familial hypercholesterolemia.&nbsp; Cholesterol deposits can lead to tendon xanthomas, xanthelasmas (ie, eyelid plaques), and/or corneal arcus (ie, gray ring around the cornea) but not coarse facial features, intellectual disability, or a diffuse corneal haze.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Accumulation of glucocerebroside within mononuclear cells is seen in Gaucher disease, an autosomal recessive defect in beta-glucocerebrosidase.&nbsp; This condition is characterized by bruising due to thrombocytopenia, hepatosplenomegaly, progressive neurologic deterioration, and severe bone pain.&nbsp; Premature coronary artery disease does not occur.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Glycogen storage diseases cause defective metabolism of glycogen in the liver and/or muscles.&nbsp; The presentation can include hypoglycemia, lactic acidosis, hepatomegaly, growth retardation, and/or muscle fatigue/cramping.&nbsp; Cardiomegaly can occur with Pompe disease, but corneal clouding is not a feature.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Niemann-Pick disease results from a deficiency of sphingomyelinase, which leads to the accumulation of sphingomyelin within phagocytes.&nbsp; This disease is characterized by hepatosplenomegaly and progressive neurologic deterioration, but eye findings include a cherry-red spot on the macula, not corneal clouding.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Hurler syndrome is a mucopolysaccharidosis caused by a deficiency of alpha-L-iduronidase, which hydrolyzes dermatan and heparan sulfate.&nbsp; Accumulation of these substances results in the characteristic features of intellectual disability, coarse facial features, corneal clouding, and hepatosplenomegaly.&nbsp; Early death due to cardiac complications (eg, myocardial infarction) is expected.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.que