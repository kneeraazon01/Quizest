              </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;34239&quot; src=&quot;https://www.uworld.com/media/L60109.jpg&quot; ></p><p>This patient who sustained trauma to the head now has hearing loss and <strong>hemotympanum</strong>, which is concerning for a <strong>temporal bone fracture</strong>.&amp;nbsp; Contained within the temporal bone are many structures involved in hearing, including the external auditory canal, tympanic membrane, ossicular chain, cochlea, and vestibulocochlear nerve (CN VIII).&amp;nbsp; Injury or disruption to any of these structures can cause <strong>hearing loss</strong>, as in this patient.</p><p>The temporal bone also contains the <strong>facial nerve (CN VII)</strong>, which originates in the brainstem and then enters the temporal bone at the internal auditory meatus.&amp;nbsp; It <strong>travels</strong> through the internal auditory canal with the vestibulocochlear nerve (CN VIII) and then takes a complicated route <strong>through the temporal bone</strong> itself, traveling near the otic capsule, through the middle ear, and then through the mastoid portion to exit at the stylomastoid foramen.</p><p>Because of its location, the facial nerve is at risk of injury in temporal bone fractures.&amp;nbsp; Deficits can be diverse and significant because the facial nerve has many functions, including:</p><ul><li>Movement of the face, including eye closure</li><li>Taste from the anterior 2/3 of the tongue</li><li>Parasympathetic innervation of submandibular and lacrimal glands</li><li>Sound dampening (due to innervation of the stapedius muscle)</li></ul><p><strong>(Choice A)</strong>&amp;nbsp; Axons of chemoreceptor cells make up the olfactory nerve (CN I) rootlets, which project through the cribriform plate of the ethmoid bone.&amp;nbsp; Acceleration-deceleration head injuries can lead to <a ng-click=&quot;showexhibit('21379',true);&quot;>avulsion</a> of the olfactory nerve rootlets, but this would not be associated with hearing loss or hemotympanum.</p><p><strong>(Choice B)</strong>&amp;nbsp; The oculomotor nerve (CN III) exits the skull base from the superior <a ng-click=&quot;showexhibit('33563',true);&quot;>orbital fissure</a> and is at risk with trauma to the orbit or anterior face.</p><p><strong>(Choice C)</strong>&amp;nbsp; The trigeminal nerve (CN V) divides into V1, V2, and <a ng-click=&quot;showexhibit('32641',true);&quot;>V3</a>, which exit the skull base from the superior orbital fissure, foramen rotundum, and foramen ovale, respectively, which are all located in the sphenoid bone.&amp;nbsp; These branches are at risk with trauma to the orbit or anterior face.</p><p><strong>(Choice E)</strong>&amp;nbsp; The hypoglossal nerve (CN XII) exits the skull base through the hypoglossal canal of the occipital bone.&amp;nbsp; It could be injured by fractures involving the occipital bone, but it is more commonly injured due to penetrating neck trauma.</p><p><strong>Educational objective:</strong><br>In addition to much of the hearing apparatus (eg, ossicles, cochlea, vestibulocochlear nerve), the temporal bone also contains the facial nerve, which can be injured in temporal bone fractures.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"34239\" src=\"image/L60109.jpg\"></p><p class=\"ng-scope\">This patient who sustained trauma to the head now has hearing loss and <strong>hemotympanum</strong>, which is concerning for a <strong>temporal bone fracture</strong>.&nbsp; Contained within the temporal bone are many structures involved in hearing, including the external auditory canal, tympanic membrane, ossicular chain, cochlea, and vestibulocochlear nerve (CN VIII).&nbsp; Injury or disruption to any of these structures can cause <strong>hearing loss</strong>, as in this patient.</p><p class=\"ng-scope\">The temporal bone also contains the <strong>facial nerve (CN VII)</strong>, which originates in the brainstem and then enters the temporal bone at the internal auditory meatus.&nbsp; It <strong>travels</strong> through the internal auditory canal with the vestibulocochlear nerve (CN VIII) and then takes a complicated route <strong>through the temporal bone</strong> itself, traveling near the otic capsule, through the middle ear, and then through the mastoid portion to exit at the stylomastoid foramen.</p><p class=\"ng-scope\">Because of its location, the facial nerve is at risk of injury in temporal bone fractures.&nbsp; Deficits can be diverse and significant because the facial nerve has many functions, including:</p><ul class=\"ng-scope\"><li>Movement of the face, including eye closure</li><li>Taste from the anterior 2/3 of the tongue</li><li>Parasympathetic innervation of submandibular and lacrimal glands</li><li>Sound dampening (due to innervation of the stapedius muscle)</li></ul><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Axons of chemoreceptor cells make up the olfactory nerve (CN I) rootlets, which project through the cribriform plate of the ethmoid bone.&nbsp; Acceleration-deceleration head injuries can lead to <a ng-click=\"showexhibit('21379',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L46576.jpg&quot;,&quot;title&quot;:&quot;L46576&quot;}]\">avulsion</a> of the olfactory nerve rootlets, but this would not be associated with hearing loss or hemotympanum.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; The oculomotor nerve (CN III) exits the skull base from the superior <a ng-click=\"showexhibit('33563',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L50964.jpg&quot;,&quot;title&quot;:&quot;L50964&quot;}]\">orbital fissure</a> and is at risk with trauma to the orbit or anterior face.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; The trigeminal nerve (CN V) divides into V1, V2, and <a ng-click=\"showexhibit('32641',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L58641.jpg&quot;,&quot;title&quot;:&quot;L58641&quot;}]\">V3</a>, which exit the skull base from the superior orbital fissure, foramen rotundum, and foramen ovale, respectively, which are all located in the sphenoid bone.&nbsp; These branches are at risk with trauma to the orbit or anterior face.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; The hypoglossal nerve (CN XII) exits the skull base through the hypoglossal canal of the occipital bone.&nbsp; It could be injured by fractures involving the occipital bone, but it is more commonly injured due to penetrating neck trauma.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>In addition to much of the hearing apparatus (eg, ossicles, cochlea, vestibulocochlear nerve), the temporal bone also contains the facial nerve, which can be injured in temporal bone fractures.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/32119456\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/32119456\"><span compiledata=\"\" htmlcontent=\"Facial Nerve Anatomy and Clinical Applications\" class=\"ng-scope\"><p class=\"ng-scope\">Facial Nerve Anatomy and Clinical Applications</p></span></a><!-- end ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal -->\n                                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal -->\n                                </li><!-- end ngRepeat: questionReference in currentQuestion.questionMappingReferencesList -->\n                            </ul>\n                        </div>\n                    </div><!-- end ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Anatomy</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Nervous System</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Skull fractures</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
