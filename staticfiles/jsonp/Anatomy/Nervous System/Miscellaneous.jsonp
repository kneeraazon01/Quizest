500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot;  draggable=&quot;false&quot; id=&quot;870&quot; src=&quot;https://www.uworld.com/media/highresdefault/L15834.jpg&quot;  ></p><p>This patient&amp;#39;s smoking history, unilateral upper limb pain, and areflexic weakness with ipsilateral <strong>Horner syndrome (HS)</strong> (eg, ptosis, miosis) are highly suggestive of Pancoast tumor.&amp;nbsp; Pancoast tumors are usually non&amp;ndash;small cell lung cancers (eg, squamous cell carcinoma, adenocarcinoma) that arise near the superior sulcus (groove produced by the subclavian artery).&amp;nbsp; Clinical manifestations are determined by the extent of local spread:</p><ul><li><p>Compression and invasion of the <strong>brachial plexus</strong> can cause ipsilateral shoulder pain, upper limb paresthesia, and areflexic arm weakness (ie, lower motor neuron pattern due to damage of a peripheral nerve).</p></li><li><p>Involvement of the <strong>cervical sympathetic ganglia</strong>, an <strong>autonomic ganglion</strong> in the lower neck that carries <a ng-click=&quot;showexhibit('51606',true);&quot;>sympathetic innervation to the eye</a>, may lead to partial ptosis (denervation of superior tarsal muscle) and miosis (interruption of sympathetic fibers to the dilator pupillae muscle).&amp;nbsp; <a ng-click=&quot;showexhibit('32895',true);&quot;>Pupil asymmetry</a> is more prominent in dim light because the increased sympathetic tone exacerbates the defect.&amp;nbsp; Anhidrosis (loss of sweating) completes the classic HS triad.</p></li></ul><p><strong>(Choice A)</strong>&amp;nbsp; The first-order neuron of the sympathetic pathway to the eye begins in the hypothalamus, so a cortical lesion would not cause HS.&amp;nbsp; Although cortical damage could cause unilateral weakness, it would have an upper motor neuron pattern (ie, increased deep tendon reflexes).</p><p><strong>(Choice B)</strong>&amp;nbsp; Although a lesion in the hypothalamus (which is part of the subcortical gray matter) could conceivably lead to HS, it would not explain this patient&amp;#39;s arm pain and weakness.</p><p><strong>(Choice C)</strong>&amp;nbsp; Brainstem lesions can lead to HS because first-order sympathetic neurons descend in the lateral brainstem.&amp;nbsp; However, brainstem lesions typically result in crossed signs (eg, contralateral body hemiparesis or sensory loss with ipsilateral cranial nerve deficits).&amp;nbsp; In addition, upper (not lower) motor neuron weakness would be seen in the extremities.</p><p><strong>(Choice D)</strong>&amp;nbsp; Although a spinal cord lesion could cause HS and unilateral weakness, the weakness would have an upper motor neuron pattern (ie, increased deep tendon reflexes) and likely impact all neurologic function below the level of the lesion (ie, the leg would also be impacted).</p><p><strong>(Choice F)</strong>&amp;nbsp; A vagus nerve (CN X) lesion may cause ipsilateral paralysis of the soft palate, pharynx, and larynx, producing hoarseness, dysarthria, dysphagia, and loss of the gag reflex (efferent limb).&amp;nbsp; CN X does not provide innervation to the eye or arm.</p><p><strong>Educational objective:</strong><br>A tumor of the lung apex (Pancoast tumor) can compress both the brachial plexus and the stellate ganglion of the sympathetic chain, leading to lower motor neuron arm weakness and ipsilateral Horner syndrome.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"870\" src=\"image/L15834.jpg\"></p><p class=\"ng-scope\">This patient's smoking history, unilateral upper limb pain, and areflexic weakness with ipsilateral <strong>Horner syndrome (HS)</strong> (eg, ptosis, miosis) are highly suggestive of Pancoast tumor.&nbsp; Pancoast tumors are usually non\u2013small cell lung cancers (eg, squamous cell carcinoma, adenocarcinoma) that arise near the superior sulcus (groove produced by the subclavian artery).&nbsp; Clinical manifestations are determined by the extent of local spread:</p><ul class=\"ng-scope\"><li><p>Compression and invasion of the <strong>brachial plexus</strong> can cause ipsilateral shoulder pain, upper limb paresthesia, and areflexic arm weakness (ie, lower motor neuron pattern due to damage of a peripheral nerve).</p></li><li><p>Involvement of the <strong>cervical sympathetic ganglia</strong>, an <strong>autonomic ganglion</strong> in the lower neck that carries <a ng-click=\"showexhibit('51606',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L80539.jpg&quot;,&quot;title&quot;:&quot;L80539&quot;}]\">sympathetic innervation to the eye</a>, may lead to partial ptosis (denervation of superior tarsal muscle) and miosis (interruption of sympathetic fibers to the dilator pupillae muscle).&nbsp; <a ng-click=\"showexhibit('32895',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L54842.jpg&quot;,&quot;title&quot;:&quot;L54842&quot;}]\">Pupil asymmetry</a> is more prominent in dim light because the increased sympathetic tone exacerbates the defect.&nbsp; Anhidrosis (loss of sweating) completes the classic HS triad.</p></li></ul><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; The first-order neuron of the sympathetic pathway to the eye begins in the hypothalamus, so a cortical lesion would not cause HS.&nbsp; Although cortical damage could cause unilateral weakness, it would have an upper motor neuron pattern (ie, increased deep tendon reflexes).</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Although a lesion in the hypothalamus (which is part of the subcortical gray matter) could conceivably lead to HS, it would not explain this patient's arm pain and weakness.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Brainstem lesions can lead to HS because first-order sympathetic neurons descend in the lateral brainstem.&nbsp; However, brainstem lesions typically result in crossed signs (eg, contralateral body hemiparesis or sensory loss with ipsilateral cranial nerve deficits).&nbsp; In addition, upper (not lower) motor neuron weakness would be seen in the extremities.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Although a spinal cord lesion could cause HS and unilateral weakness, the weakness would have an upper motor neuron pattern (ie, increased deep tendon reflexes) and likely impact all neurologic function below the level of the lesion (ie, the leg would also be impacted).</p><p class=\"ng-scope\"><strong>(Choice F)</strong>&nbsp; A vagus nerve (CN X) lesion may cause ipsilateral paralysis of the soft palate, pharynx, and larynx, producing hoarseness, dysarthria, dysphagia, and loss of the gag reflex (efferent limb).&nbsp; CN X does not provide innervation to the eye or arm.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>A tumor of the lung apex (Pancoast tumor) can compress both the brachial plexus and the stellate ganglion of the sympathetic chain, leading to lower motor neuron arm weakness and ipsilateral Horner syndrome.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Anatomy</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Nervous System</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Lung cancer</div>\n                                   