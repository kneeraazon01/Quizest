                   </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot;  draggable=&quot;false&quot; id=&quot;47575&quot; src=&quot;https://www.uworld.com/media/highresdefault/L74656.jpg&quot; ></p><p>This patient has a mass compressing the <strong>inferior pons</strong> (eg, pontine glioma).&amp;nbsp; Major nuclei in the pons include the following:</p><ul><li><p>CN V:&amp;nbsp; trigeminal motor nucleus (innervation to the muscles of mastication) and the main trigeminal nucleus (sensory innervation to the face and jaw)</p></li><li><p>CN VI:&amp;nbsp; abducens nucleus (motor innervation to the lateral rectus muscle)</p></li><li><p>CN VII:&amp;nbsp; facial nucleus (motor innervation to the muscles of facial expression)</p></li><li><p><strong>CN VIII</strong>:&amp;nbsp; dorsal and ventral cochlear nuclei (hearing) and the <strong>vestibular nucleus</strong> (head position and movement)</p></li></ul><p>The vestibular portion of CN VIII conveys information about head position and movement and is involved in the vestibuloocular reflex (VOR), which <strong>stabilizes the gaze during head movement</strong>.&amp;nbsp; Therefore, damage to the vestibular nuclei of CN VIII can lead to <strong>nystagmus</strong> due to impairment of the VOR.</p><p><strong>(Choice A)</strong>&amp;nbsp; Asymmetric pupils can occur with damage to nuclei of the oculomotor nerve (CN III).&amp;nbsp; The oculomotor nucleus provides motor innervation to the extraocular muscles, and the accessory nucleus (ie, Edinger-Westphal) provides parasympathetic input to the iris and lens (eg, pupillary constriction, iris accommodation).&amp;nbsp; Both nuclei are located in the midbrain rather than the pons.</p><p><strong>(Choice C)</strong>&amp;nbsp; Tongue atrophy can occur with damage to the nucleus of the hypoglossal nerve (CN XII).&amp;nbsp; The hypoglossal nucleus is located in the medulla.</p><p><strong>(Choice D)</strong>&amp;nbsp; The trapezius muscle is innervated by the accessory nerve (CN XI), which has fibers coming from the spinal accessory nucleus (located in the spinal cord) and the nucleus ambiguus (located in the medulla).</p><p><strong>(Choice E)</strong>&amp;nbsp; Vertical diplopia can occur with damage to the nucleus of the trochlear nerve (CN IV), which innervates the superior oblique, or the oculomotor nucleus (CN III).&amp;nbsp; Both nuclei are located in the midbrain.</p><p><strong>Educational objective:</strong><br>The nuclei of the vestibulocochlear nerve (CN VIII) are located in the pons.&amp;nbsp; The vestibular portion of CN VIII conveys information about head position and movement and helps to stabilize the gaze during head movement; damage can lead to nystagmus.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"47575\" src=\"image/L74656.jpg\"></p><p class=\"ng-scope\">This patient has a mass compressing the <strong>inferior pons</strong> (eg, pontine glioma).&nbsp; Major nuclei in the pons include the following:</p><ul class=\"ng-scope\"><li><p>CN V:&nbsp; trigeminal motor nucleus (innervation to the muscles of mastication) and the main trigeminal nucleus (sensory innervation to the face and jaw)</p></li><li><p>CN VI:&nbsp; abducens nucleus (motor innervation to the lateral rectus muscle)</p></li><li><p>CN VII:&nbsp; facial nucleus (motor innervation to the muscles of facial expression)</p></li><li><p><strong>CN VIII</strong>:&nbsp; dorsal and ventral cochlear nuclei (hearing) and the <strong>vestibular nucleus</strong> (head position and movement)</p></li></ul><p class=\"ng-scope\">The vestibular portion of CN VIII conveys information about head position and movement and is involved in the vestibuloocular reflex (VOR), which <strong>stabilizes the gaze during head movement</strong>.&nbsp; Therefore, damage to the vestibular nuclei of CN VIII can lead to <strong>nystagmus</strong> due to impairment of the VOR.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Asymmetric pupils can occur with damage to nuclei of the oculomotor nerve (CN III).&nbsp; The oculomotor nucleus provides motor innervation to the extraocular muscles, and the accessory nucleus (ie, Edinger-Westphal) provides parasympathetic input to the iris and lens (eg, pupillary constriction, iris accommodation).&nbsp; Both nuclei are located in the midbrain rather than the pons.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Tongue atrophy can occur with damage to the nucleus of the hypoglossal nerve (CN XII).&nbsp; The hypoglossal nucleus is located in the medulla.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; The trapezius muscle is innervated by the accessory nerve (CN XI), which has fibers coming from the spinal accessory nucleus (located in the spinal cord) and the nucleus ambiguus (located in the medulla).</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Vertical diplopia can occur with damage to the nucleus of the trochlear nerve (CN IV), which innervates the superior oblique, or the oculomotor nucleus (CN III).&nbsp; Both nuclei are located in the midbrain.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>The nuclei of the vestibulocochlear nerve (CN VIII) are located in the pons.&nbsp; The vestibular portion of CN VIII conveys information about head position and movement and helps to stabilize the gaze during head movement; damage can lead to nystagmus.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Anatomy</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Nervous System</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Brain tumors</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
