ShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;37864&quot; src=&quot;https://www.uworld.com/media/L19097.jpg&quot; ></p><p>This patient with recurrent headaches and seizures has multiple flow voids in the left temporoparietal lobe demonstrated by MRI, consistent with a <strong>cerebral arteriovenous malformation</strong> (AVM).</p><p>AVMs are vascular malformations in which blood courses <strong>directly from arteries to veins</strong>, without passing through an intervening capillary bed.&amp;nbsp; These tangled vessels with turbulent flow can lead to aneurysm development, bleeding, and damage to the surrounding brain.&amp;nbsp; Microscopy of AVMs shows <strong>abnormal vessels</strong>, including irregular arteries and large veins with <a ng-click=&quot;showexhibit('37865',true);&quot;>thickened walls</a>, and gliotic brain tissue.</p><p>AVMs frequently present in children and young adults with <strong>headaches</strong> and <strong>seizures</strong> as well as <strong>intracranial hemorrhage</strong> (eg, intraparenchymal, subarachnoid hemorrhage).&amp;nbsp; Some patients are initially asymptomatic and diagnosed incidentally by imaging.&amp;nbsp; A nest of abnormal vessels with <strong>&amp;#x22;bag of worms&amp;#x22;</strong> appearance may be seen on angiography.&amp;nbsp; MRI may reveal prior hemorrhage and multiple dark <strong>flow voids</strong>, indicative of swift blood flow.</p><p><strong>(Choice A)</strong>&amp;nbsp; Dense perivascular aggregates of atypical lymphoid cells are a histologic feature of <a ng-click=&quot;showexhibit('37866',true);&quot;>primary CNS lymphoma</a>, which is typically a diffuse large B-cell lymphoma.&amp;nbsp; Although clinical symptoms may include seizures, it usually presents in older adults or immunocompromised patients.&amp;nbsp; MRI would likely reveal an <a ng-click=&quot;showexhibit('37867',true);&quot;>enhancing mass</a>.</p><p><strong>(Choice B)</strong>&amp;nbsp; Lipid-laden macrophages with phagocytized myelin debris are seen in the demyelinating plaques of multiple sclerosis.&amp;nbsp; Symptoms are variable but commonly include unilateral visual disturbance (eg, optic neuritis) and focal sensory and motor deficits.&amp;nbsp; MRI typically reveals periventricular <a ng-click=&quot;showexhibit('37868',true);&quot;>white matter lesions</a>.</p><p><strong>(Choice C)</strong>&amp;nbsp; Histologic findings in cerebral <a ng-click=&quot;showexhibit('20188',true);&quot;><em>Toxoplasma gondii</em></a> infection include necrotic debris with surrounding inflammation, tachyzoites, and pseudocysts of bradyzoites.&amp;nbsp; Although seizures and headaches are a frequent manifestation, CNS toxoplasmosis usually occurs in immunocompromised patients.&amp;nbsp; Imaging would likely reveal <a ng-click=&quot;showexhibit('14594',true);&quot;>ring-enhancing lesions</a>.</p><p><strong>(Choice D)</strong>&amp;nbsp; Necrosis surrounded by malignant cells (pseudopalisading necrosis) and microvascular proliferation are features of <a ng-click=&quot;showexhibit('37869',true);&quot;>glioblastoma</a>, a high-grade infiltrating astrocytoma.&amp;nbsp; It commonly presents with seizures, headache, and neurologic deficits in middle-aged or older adults (rather than children).&amp;nbsp; MRI would likely reveal a <a ng-click=&quot;showexhibit('37870',true);&quot;>mass</a> with central hemorrhagic necrosis, vasogenic edema, and possible extension to the contralateral hemisphere.</p><p><strong>Educational objective:</strong><br>Cerebral arteriovenous malformations (AVMs) are vascular malformations histologically characterized by a tangle of abnormal vessels, including large veins with thickened walls and irregular arteries.&amp;nbsp; AVMs may present with intracranial hemorrhage, headache, and/or seizures.&amp;nbsp; Radiologic findings include abnormal vasculature with a &amp;#x22;bag of worms&amp;#x22; appearance and multiple dark flow voids.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"37864\" src=\"image/L19097.jpg\"></p><p class=\"ng-scope\">This patient with recurrent headaches and seizures has multiple flow voids in the left temporoparietal lobe demonstrated by MRI, consistent with a <strong>cerebral arteriovenous malformation</strong> (AVM).</p><p class=\"ng-scope\">AVMs are vascular malformations in which blood courses <strong>directly from arteries to veins</strong>, without passing through an intervening capillary bed.&nbsp; These tangled vessels with turbulent flow can lead to aneurysm development, bleeding, and damage to the surrounding brain.&nbsp; Microscopy of AVMs shows <strong>abnormal vessels</strong>, including irregular arteries and large veins with <a ng-click=\"showexhibit('37865',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L27228.jpg&quot;,&quot;title&quot;:&quot;L27228&quot;}]\">thickened walls</a>, and gliotic brain tissue.</p><p class=\"ng-scope\">AVMs frequently present in children and young adults with <strong>headaches</strong> and <strong>seizures</strong> as well as <strong>intracranial hemorrhage</strong> (eg, intraparenchymal, subarachnoid hemorrhage).&nbsp; Some patients are initially asymptomatic and diagnosed incidentally by imaging.&nbsp; A nest of abnormal vessels with <strong>\"bag of worms\"</strong> appearance may be seen on angiography.&nbsp; MRI may reveal prior hemorrhage and multiple dark <strong>flow voids</strong>, indicative of swift blood flow.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Dense perivascular aggregates of atypical lymphoid cells are a histologic feature of <a ng-click=\"showexhibit('37866',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L63173.jpg&quot;,&quot;title&quot;:&quot;L63173&quot;}]\">primary CNS lymphoma</a>, which is typically a diffuse large B-cell lymphoma.&nbsp; Although clinical symptoms may include seizures, it usually presents in older adults or immunocompromised patients.&nbsp; MRI would likely reveal an <a ng-click=\"showexhibit('37867',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L471.jpg&quot;,&quot;title&quot;:&quot;L471&quot;}]\">enhancing mass</a>.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Lipid-laden macrophages with phagocytized myelin debris are seen in the demyelinating plaques of multiple sclerosis.&nbsp; Symptoms are variable but commonly include unilateral visual disturbance (eg, optic neuritis) and focal sensory and motor deficits.&nbsp; MRI typically reveals periventricular <a ng-click=\"showexhibit('37868',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L764.jpg&quot;,&quot;title&quot;:&quot;L764&quot;}]\">white matter lesions</a>.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Histologic findings in cerebral <a ng-click=\"showexhibit('20188',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L26960.jpg&quot;,&quot;title&quot;:&quot;L26960&quot;}]\"><em>Toxoplasma gondii</em></a> infection include necrotic debris with surrounding inflammation, tachyzoites, and pseudocysts of bradyzoites.&nbsp; Although seizures and headaches are a frequent manifestation, CNS toxoplasmosis usually occurs in immunocompromised patients.&nbsp; Imaging would likely reveal <a ng-click=\"showexhibit('14594',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L850.jpg&quot;,&quot;title&quot;:&quot;L850&quot;}]\">ring-enhancing lesions</a>.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Necrosis surrounded by malignant cells (pseudopalisading necrosis) and microvascular proliferation are features of <a ng-click=\"showexhibit('37869',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L62936.jpg&quot;,&quot;title&quot;:&quot;L62936&quot;}]\">glioblastoma</a>, a high-grade infiltrating astrocytoma.&nbsp; It commonly presents with seizures, headache, and neurologic deficits in middle-aged or older adults (rather than children).&nbsp; MRI would likely reveal a <a ng-click=\"showexhibit('37870',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L28216.jpg&quot;,&quot;title&quot;:&quot;L28216&quot;}]\">mass</a> with central hemorrhagic necrosis, vasogenic edema, and possible extension to the contralateral hemisphere.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Cerebral arteriovenous malformations (AVMs) are vascular malformations histologically characterized by a tangle of abnormal vessels, including large veins with thickened walls and irregular arteries.&nbsp; AVMs may present with intracranial hemorrhage, headache, and/or seizures.&nbsp; Radiologic findings include abnormal vasculature with a \"bag of worms\" appearance and multiple dark flow voids.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Anatomy</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Nervous System</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Av malformation</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
