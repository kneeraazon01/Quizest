>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>E.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight5\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Tensor fascia lata</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;36151&quot; src=&quot;https://www.uworld.com/media/L57376.jpg&quot; ></p><p>The major muscles responsible for <strong>hip flexion</strong> are the <strong>rectus femoris, iliopsoas</strong>, and <strong>sartorius</strong>.&amp;nbsp; These muscles originate on the pelvis and spinal column and cross the hip joint anteriorly, giving them the capacity to carry out this motion.&amp;nbsp; This patient has impairment not only with hip flexion but also with knee extension.&amp;nbsp; Of these muscles, only the sartorius and rectus femoris (part of quadriceps femoris) also cross the knee and therefore can influence both hip and knee movements.</p><p>The <strong>rectus femoris</strong> originates from the anterior inferior iliac spine and the ilium and inserts at the base of the patella via the quadriceps tendon, allowing it to flex the hip and <strong>also extend the knee</strong> upon contraction.&amp;nbsp; The rectus femoris (and quadriceps as a whole) also provides stability and control during ambulation, deceleration (eg, landing a jump), and rapid changes in movement (eg, cutting in soccer).</p><p>In contrast, the sartorius originates from the anterior superior iliac spine and crosses the anterior thigh to insert at the superomedial surface of the tibia.&amp;nbsp; Because of its oblique orientation across the anterior thigh, the <strong>sartorius</strong> is responsible for hip flexion, external rotation, and abduction as well as <strong>knee flexion</strong> (not extension) <strong>(Choice D)</strong>.&amp;nbsp; This combination of movements is needed to sit in a cross-legged position.</p><p><strong>(Choice A)</strong>&amp;nbsp; The biceps femoris, along with semitendinosus and semimembranosus, form the <a ng-click=&quot;showexhibit('37517',true);&quot;>hamstring muscle group</a> in the posterior thigh that is responsible for <em>hip extension</em> and <em>knee flexion</em>.&amp;nbsp; These movements are made possible because the long head of biceps femoris originates at the ischial tuberosity and inserts at the fibular head, crossing both the hip and knee joints.</p><p><strong>(Choice B)</strong>&amp;nbsp; The psoas muscle originates at T12-L4 vertebrae and inserts at the lesser trochanter of the femur, enabling it to function as a hip flexor.&amp;nbsp; Because it does not cross the knee, it does not contribute to leg movement.</p><p><strong>(Choice E)</strong>&amp;nbsp; <a ng-click=&quot;showexhibit('36152',true);&quot;>Tensor fascia lata</a> is a small muscle along the lateral thigh that originates from the anterior superior iliac spine and inserts in the lateral condyle of the tibia via the iliotibial tract.&amp;nbsp; It works with the <a ng-click=&quot;showexhibit('36153',true);&quot;>gluteus medius and gluteus minimus</a> to abduct and medially rotate the hip.</p><p><strong>Educational objective:</strong><br>The rectus femoris, iliopsoas, and sartorius are the major hip flexors.&amp;nbsp; Of these, only the rectus femoris and sartorius also affect knee movement.&amp;nbsp; The sartorius flexes the knee while the rectus femoris extends the knee.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"36151\" src=\"image/L57376.jpg\"></p><p class=\"ng-scope\">The major muscles responsible for <strong>hip flexion</strong> are the <strong>rectus femoris, iliopsoas</strong>, and <strong>sartorius</strong>.&nbsp; These muscles originate on the pelvis and spinal column and cross the hip joint anteriorly, giving them the capacity to carry out this motion.&nbsp; This patient has impairment not only with hip flexion but also with knee extension.&nbsp; Of these muscles, only the sartorius and rectus femoris (part of quadriceps femoris) also cross the knee and therefore can influence both hip and knee movements.</p><p class=\"ng-scope\">The <strong>rectus femoris</strong> originates from the anterior inferior iliac spine and the ilium and inserts at the base of the patella via the quadriceps tendon, allowing it to flex the hip and <strong>also extend the knee</strong> upon contraction.&nbsp; The rectus femoris (and quadriceps as a whole) also provides stability and control during ambulation, deceleration (eg, landing a jump), and rapid changes in movement (eg, cutting in soccer).</p><p class=\"ng-scope\">In contrast, the sartorius originates from the anterior superior iliac spine and crosses the anterior thigh to insert at the superomedial surface of the tibia.&nbsp; Because of its oblique orientation across the anterior thigh, the <strong>sartorius</strong> is responsible for hip flexion, external rotation, and abduction as well as <strong>knee flexion</strong> (not extension) <strong>(Choice D)</strong>.&nbsp; This combination of movements is needed to sit in a cross-legged position.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; The biceps femoris, along with semitendinosus and semimembranosus, form the <a ng-click=\"showexhibit('37517',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L62892.jpg&quot;,&quot;title&quot;:&quot;L62892&quot;}]\">hamstring muscle group</a> in the posterior thigh that is responsible for <em>hip extension</em> and <em>knee flexion</em>.&nbsp; These movements are made possible because the long head of biceps femoris originates at the ischial tuberosity and inserts at the fibular head, crossing both the hip and knee joints.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; The psoas muscle originates at T12-L4 vertebrae and inserts at the lesser trochanter of the femur, enabling it to function as a hip flexor.&nbsp; Because it does not cross the knee, it does not contribute to leg movement.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; <a ng-click=\"showexhibit('36152',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L14543.jpg&quot;,&quot;title&quot;:&quot;L14543&quot;}]\">Tensor fascia lata</a> is a small muscle along the lateral thigh that originates from the anterior superior iliac spine and inserts in the lateral condyle of the tibia via the iliotibial tract.&nbsp; It works with the <a ng-click=\"showexhibit('36153',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L57373.jpg&quot;,&quot;title&quot;:&quot;L57373&quot;}]\">gluteus medius and gluteus minimus</a> to abduct and medially rotate the hip.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>The rectus femoris, iliopsoas, and sartorius are the major hip flexors.&nbsp; Of these, only the rectus femoris and sartorius also affect knee movement.&nbsp; The sartorius flexes the knee while the rectus femoris extends the knee.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Anatomy</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Rheumatology/Orthopedics &amp; Sports</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Quadriceps muscle and tendon injuries</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                   