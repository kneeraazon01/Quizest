userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;797&quot; src=&quot;https://www.uworld.com/media/L7456.jpg&quot; ></p><p>This patient has <strong>Osgood-Schlatter disease</strong> (OSD), a common cause of knee pain in adolescents due to overuse of the <strong>quadriceps</strong> muscle group (ie, rectus femoris, vastus intermedius, vastus lateralis, vastus medialis).&amp;nbsp; The quadriceps is located in the anterior thigh and is responsible for <strong>knee extension</strong>.&amp;nbsp; Its tendon initially inserts at the superior pole of the patella.&amp;nbsp; The tendon envelops the patella and continues as the patellar ligament, which then inserts at the <strong>tibial tubercle</strong>.&amp;nbsp; As a result, contraction of the quadriceps (eg, hiking, running) creates traction on the tibial tubercle.</p><p>In skeletally immature children, the tibial tubercle forms as a <strong>secondary ossification center</strong> (apophysis) of the tibia, and the cartilaginous nature of the developing tibial tuberosity makes it more prone to injury compared to fully ossified bone in adults.&amp;nbsp; Repetitive quadriceps contraction results in chronic <a ng-click=&quot;showexhibit('656',true);&quot;>avulsion/fragmentation of the tubercle</a>, with corresponding tenderness and swelling, as seen in this patient.</p><p><strong>(Choice A)</strong>&amp;nbsp; The biceps femoris, along with the semitendinosus and semimembranosus, forms the <a ng-click=&quot;showexhibit('37517',true);&quot;>hamstring muscle group</a> in the posterior thigh; it is responsible for hip extension and knee flexion.&amp;nbsp; The long head of the biceps femoris originates from the ischial tuberosity, whereas the short head originates from the inferior third of the linea aspera.&amp;nbsp; Both insert at the head of the fibula.</p><p><strong>(Choice B)</strong>&amp;nbsp; The popliteus originates from the lateral condyle of the femur and lateral meniscus and inserts at the proximal shaft of the tibia.&amp;nbsp; It flexes and medially rotates the leg (unlocks an extended knee).</p><p><strong>(Choice D)</strong>&amp;nbsp; The <a ng-click=&quot;showexhibit('36151',true);&quot;>sartorius</a> originates from the anterior superior iliac spine and inserts at the superomedial surface of the tibia.&amp;nbsp; Because of its oblique orientation across the anterior thigh, the sartorius is responsible for hip flexion, external rotation, and abduction, as well as knee flexion.&amp;nbsp; This combination of movements is needed to sit in a cross-legged position.</p><p><strong>(Choice E)</strong>&amp;nbsp; The <a ng-click=&quot;showexhibit('39106',true);&quot;>tibialis anterior</a> originates from the lateral condyle of the tibia, the upper half of the lateral tibia, and the interosseus membrane.&amp;nbsp; It inserts at the cuneiform and first metatarsal.&amp;nbsp; It is responsible for ankle dorsiflexion and foot inversion.</p><p><strong>Educational objective:</strong><br>Repetitive traction at the tibial tubercle due to quadriceps contraction can lead to avulsion and fragmentation of the tibial tubercle in skeletally immature children, causing Osgood-Schlatter disease.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"797\" src=\"image/L7456.jpg\"></p><p class=\"ng-scope\">This patient has <strong>Osgood-Schlatter disease</strong> (OSD), a common cause of knee pain in adolescents due to overuse of the <strong>quadriceps</strong> muscle group (ie, rectus femoris, vastus intermedius, vastus lateralis, vastus medialis).&nbsp; The quadriceps is located in the anterior thigh and is responsible for <strong>knee extension</strong>.&nbsp; Its tendon initially inserts at the superior pole of the patella.&nbsp; The tendon envelops the patella and continues as the patellar ligament, which then inserts at the <strong>tibial tubercle</strong>.&nbsp; As a result, contraction of the quadriceps (eg, hiking, running) creates traction on the tibial tubercle.</p><p class=\"ng-scope\">In skeletally immature children, the tibial tubercle forms as a <strong>secondary ossification center</strong> (apophysis) of the tibia, and the cartilaginous nature of the developing tibial tuberosity makes it more prone to injury compared to fully ossified bone in adults.&nbsp; Repetitive quadriceps contraction results in chronic <a ng-click=\"showexhibit('656',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L6675.jpg&quot;,&quot;title&quot;:&quot;L6675&quot;}]\">avulsion/fragmentation of the tubercle</a>, with corresponding tenderness and swelling, as seen in this patient.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; The biceps femoris, along with the semitendinosus and semimembranosus, forms the <a ng-click=\"showexhibit('37517',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L62892.jpg&quot;,&quot;title&quot;:&quot;L62892&quot;}]\">hamstring muscle group</a> in the posterior thigh; it is responsible for hip extension and knee flexion.&nbsp; The long head of the biceps femoris originates from the ischial tuberosity, whereas the short head originates from the inferior third of the linea aspera.&nbsp; Both insert at the head of the fibula.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; The popliteus originates from the lateral condyle of the femur and lateral meniscus and inserts at the proximal shaft of the tibia.&nbsp; It flexes and medially rotates the leg (unlocks an extended knee).</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; The <a ng-click=\"showexhibit('36151',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L57376.jpg&quot;,&quot;title&quot;:&quot;L57376&quot;}]\">sartorius</a> originates from the anterior superior iliac spine and inserts at the superomedial surface of the tibia.&nbsp; Because of its oblique orientation across the anterior thigh, the sartorius is responsible for hip flexion, external rotation, and abduction, as well as knee flexion.&nbsp; This combination of movements is needed to sit in a cross-legged position.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; The <a ng-click=\"showexhibit('39106',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L65075.jpg&quot;,&quot;title&quot;:&quot;L65075&quot;}]\">tibialis anterior</a> originates from the lateral condyle of the tibia, the upper half of the lateral tibia, and the interosseus membrane.&nbsp; It inserts at the cuneiform and first metatarsal.&nbsp; It is responsible for ankle dorsiflexion and foot inversion.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Repetitive traction at the tibial tubercle due to quadriceps contraction can lead to avulsion and fragmentation of the tibial tubercle in skeletally immature children, causing Osgood-Schlatter disease.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/31083875\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/31083875\"><span compiledata=\"\" htmlcontent=\"Apophysitis and osteochondrosis: common causes of pain in growing bones\" class=\"ng-scope\"><p class=\"ng-scope\">Apophysitis and osteochondrosis: common causes of pain in growing bones</p></span></a><!-- end ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal -->\n                                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal -->\n                                </li><!-- end ngRepeat: questionReference in currentQuestion.questionMappingReferencesList -->\n                            </ul>\n                        </div>\n                    </div><!-- end ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions