onstants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;7432&quot; src=&quot;https://www.uworld.com/media/L10015.jpg&quot; ></p><p>This patient&amp;#39;s upright chest x-ray demonstrates <strong>free air</strong> under the diaphragm (<strong>pneumoperitoneum</strong>), likely due to <strong>perforation</strong> of a hollow abdominal viscus.&amp;nbsp; The most common cause of pneumoperitoneum is a perforated duodenal ulcer, but perforation can occur anywhere along the gastrointestinal (or female reproductive) tract due to ulcers, tumors, trauma, or iatrogenic causes (eg, endoscopy, surgery).</p><p>Contamination of the sterile peritoneal cavity with bowel contents leads to chemical (acid or chyme-related) or bacterial <strong>peritonitis</strong>, which, if left untreated, can progress to sepsis and death.&amp;nbsp; Diffuse irritation of the parietal peritoneum results in severe abdominal pain with <strong>rigidity </strong>and <strong>rebound tenderness</strong>.&amp;nbsp; <strong>Shoulder pain</strong> is another frequent finding and reflects referred pain from diaphragmatic irritation.&amp;nbsp; Referred pain is due to convergence of multiple nociceptors (visceral and somatic) onto a single secondary neuron within the spinal cord, which causes the brain to misinterpret visceral pain as arising from a body surface.</p><p><strong>(Choice A)</strong>&amp;nbsp; Other common causes of abdominal pain with associated diaphragmatic irritation and referred shoulder pain include acute cholecystitis/biliary colic and splenic lacerations.&amp;nbsp; However, these would not be associated with intraabdominal free air.</p><p><strong>(Choice B)</strong>&amp;nbsp; Acute pancreatitis causes epigastric abdominal pain that may be referred to the back.&amp;nbsp; X-ray findings are often normal, although a localized ileus near the inflammed pancreas (sentinel loop sign) may be seen.</p><p><strong>(Choice C)</strong>&amp;nbsp; <a ng-click=&quot;showexhibit('15599',true);&quot;>Nephrolithiasis</a> typically presents with colicky flank pain that is referred to the groin.&amp;nbsp; Radiopaque stones (calcium, struvite, and cystine) may be visualized on plain radiography.</p><p><strong>(Choice E)</strong>&amp;nbsp; <a ng-click=&quot;showexhibit('15600',true);&quot;>Small bowel obstruction</a> causes poorly localized, constant abdominal pain, which can be referred to the epigastrium.&amp;nbsp; Plain radiography demonstrates dilation of the small bowel with air fluid levels; free air is not seen in the absence of perforation.</p><p><strong>Educational objective:</strong><br>Pneumoperitoneum is air or gas in the peritoneal cavity; it can be seen as free air under the diaphragm in an upright chest x-ray.&amp;nbsp; The most common cause is a perforated duodenal ulcer, but perforation can occur anywhere along the gastrointestinal (or female reproductive) tract.&amp;nbsp; Subsequent peritonitis can lead to diffuse abdominal pain with rebound and guarding, sometimes with referred shoulder pain due to diaphragmatic irritation.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"7432\" src=\"image/L10015.jpg\"></p><p class=\"ng-scope\">This patient's upright chest x-ray demonstrates <strong>free air</strong> under the diaphragm (<strong>pneumoperitoneum</strong>), likely due to <strong>perforation</strong> of a hollow abdominal viscus.&nbsp; The most common cause of pneumoperitoneum is a perforated duodenal ulcer, but perforation can occur anywhere along the gastrointestinal (or female reproductive) tract due to ulcers, tumors, trauma, or iatrogenic causes (eg, endoscopy, surgery).</p><p class=\"ng-scope\">Contamination of the sterile peritoneal cavity with bowel contents leads to chemical (acid or chyme-related) or bacterial <strong>peritonitis</strong>, which, if left untreated, can progress to sepsis and death.&nbsp; Diffuse irritation of the parietal peritoneum results in severe abdominal pain with <strong>rigidity </strong>and <strong>rebound tenderness</strong>.&nbsp; <strong>Shoulder pain</strong> is another frequent finding and reflects referred pain from diaphragmatic irritation.&nbsp; Referred pain is due to convergence of multiple nociceptors (visceral and somatic) onto a single secondary neuron within the spinal cord, which causes the brain to misinterpret visceral pain as arising from a body surface.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Other common causes of abdominal pain with associated diaphragmatic irritation and referred shoulder pain include acute cholecystitis/biliary colic and splenic lacerations.&nbsp; However, these would not be associated with intraabdominal free air.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Acute pancreatitis causes epigastric abdominal pain that may be referred to the back.&nbsp; X-ray findings are often normal, although a localized ileus near the inflammed pancreas (sentinel loop sign) may be seen.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; <a ng-click=\"showexhibit('15599',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L7608.jpg&quot;,&quot;title&quot;:&quot;L7608&quot;}]\">Nephrolithiasis</a> typically presents with colicky flank pain that is referred to the groin.&nbsp; Radiopaque stones (calcium, struvite, and cystine) may be visualized on plain radiography.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; <a ng-click=\"showexhibit('15600',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/U37399.png&quot;,&quot;title&quot;:&quot;U37399&quot;}]\">Small bowel obstruction</a> causes poorly localized, constant abdominal pain, which can be referred to the epigastrium.&nbsp; Plain radiography demonstrates dilation of the small bowel with air fluid levels; free air is not seen in the absence of perforation.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Pneumoperitoneum is air or gas in the peritoneal cavity; it can be seen as free air under the diaphragm in an upright chest x-ray.&nbsp; The most common cause is a perforated duodenal ulcer, but perforation can occur anywhere along the gastrointestinal (or female reproductive) tract.&nbsp; Subsequent peritonitis can lead to diffuse abdominal pain with rebound and guarding, sometimes with referred shoulder pain due to diaphragmatic irritation.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Anatomy</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Gastrointestinal &amp; Nutrition</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Perforated viscus</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
