                      </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot;  draggable=&quot;false&quot; id=&quot;42512&quot; src=&quot;https://www.uworld.com/media/highresdefault/L68045.jpg&quot; ></p><p>This patient&amp;#39;s recurrent facial swelling is most likely due to <strong>hereditary angioedema</strong>, which is characterized by a <strong>deficiency</strong> or dysfunction of <strong>C1 inhibitor</strong> (previously referred to as C1 esterase inhibitor).&amp;nbsp; Poor C1 inhibitor function leads to elevated bradykinin, a peptide that causes vasodilation and increased vascular permeability, resulting in edema.</p><p>Presentation is typically in childhood or adolescence with episodes of <strong>swelling</strong> affecting the skin (eg, face, extremities) and mucosa of the gastrointestinal tract and/or larynx.&amp;nbsp; Attacks are often precipitated by minor trauma (eg, dental procedure) or emotional stress and are <strong>not associated</strong> with itching or urticaria.</p><p>Initial evaluation of hereditary angioedema is with <a ng-click=&quot;showexhibit('47058',true);&quot;>complement</a> testing.&amp;nbsp; <strong>Low C4</strong> is characteristic because, in the absence of C1 inhibitor, unregulated activation of C1 leads to <strong>excess activated C1</strong> and, in turn, unchecked cleavage of C4.&amp;nbsp; Diagnosis is confirmed by a decrease in functional C1 inhibitor level.</p><p><strong>(Choices A and E)</strong>&amp;nbsp; Elevated eosinophils and IgE are associated with allergic conditions.&amp;nbsp; In contrast to bradykinin-mediated hereditary angioedema, allergic angioedema (eg, anaphylaxis) is due to histamine release from activated mast cells and presents with pruritis and urticaria in addition to swelling.</p><p><strong>(Choice C)</strong>&amp;nbsp; C8 is involved in the formation of the membrane attack complex, which, when deficient, results in increased susceptibility to <em>Neisseria</em> infections.&amp;nbsp; Deficiency of terminal complement components (C5, C6, C7, C8, C9) does not occur with hereditary angioedema because the upstream complement fragments&amp;nbsp;(C2b, C4b) are rapidly inactivated in the plasma.</p><p><strong>(Choice D)</strong>&amp;nbsp; Selective IgA deficiency increases risk for anaphylaxis, which can cause angioedema.&amp;nbsp; However, this risk is only with transfusion of blood products and would present with other organ system involvement (eg, bronchospasm, hypotension, urticaria).&amp;nbsp; IgA deficiency is also associated with certain autoimmune conditions (eg, celiac disease, systemic lupus erythematosus), but none of these would present with isolated facial swelling.</p><p><strong>Educational objective:</strong><br>Hereditary angioedema is characterized by recurrent episodes of cutaneous and/or mucosal swelling due to C1 inhibitor deficiency.&amp;nbsp; C4 levels are low due to uninhibited cleavage of C4 by excess activated C1.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"42512\" src=\"image/L68045.jpg\"></p><p class=\"ng-scope\">This patient's recurrent facial swelling is most likely due to <strong>hereditary angioedema</strong>, which is characterized by a <strong>deficiency</strong> or dysfunction of <strong>C1 inhibitor</strong> (previously referred to as C1 esterase inhibitor).&nbsp; Poor C1 inhibitor function leads to elevated bradykinin, a peptide that causes vasodilation and increased vascular permeability, resulting in edema.</p><p class=\"ng-scope\">Presentation is typically in childhood or adolescence with episodes of <strong>swelling</strong> affecting the skin (eg, face, extremities) and mucosa of the gastrointestinal tract and/or larynx.&nbsp; Attacks are often precipitated by minor trauma (eg, dental procedure) or emotional stress and are <strong>not associated</strong> with itching or urticaria.</p><p class=\"ng-scope\">Initial evaluation of hereditary angioedema is with <a ng-click=\"showexhibit('47058',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L73760.jpg&quot;,&quot;title&quot;:&quot;L73760&quot;}]\">complement</a> testing.&nbsp; <strong>Low C4</strong> is characteristic because, in the absence of C1 inhibitor, unregulated activation of C1 leads to <strong>excess activated C1</strong> and, in turn, unchecked cleavage of C4.&nbsp; Diagnosis is confirmed by a decrease in functional C1 inhibitor level.</p><p class=\"ng-scope\"><strong>(Choices A and E)</strong>&nbsp; Elevated eosinophils and IgE are associated with allergic conditions.&nbsp; In contrast to bradykinin-mediated hereditary angioedema, allergic angioedema (eg, anaphylaxis) is due to histamine release from activated mast cells and presents with pruritis and urticaria in addition to swelling.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; C8 is involved in the formation of the membrane attack complex, which, when deficient, results in increased susceptibility to <em>Neisseria</em> infections.&nbsp; Deficiency of terminal complement components (C5, C6, C7, C8, C9) does not occur with hereditary angioedema because the upstream complement fragments&nbsp;(C2b, C4b) are rapidly inactivated in the plasma.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Selective IgA deficiency increases risk for anaphylaxis, which can cause angioedema.&nbsp; However, this risk is only with transfusion of blood products and would present with other organ system involvement (eg, bronchospasm, hypotension, urticaria).&nbsp; IgA deficiency is also associated with certain autoimmune conditions (eg, celiac disease, systemic lupus erythematosus), but none of these would present with isolated facial swelling.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Hereditary angioedema is characterized by recurrent episodes of cutaneous and/or mucosal swelling due to C1 inhibitor deficiency.&nbsp; C4 levels are low due to uninhibited cleavage of C4 by excess activated C1.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Immunology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Allergy &amp; Immunology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Angioedema</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
        "correctAnswerIndex": 1,
