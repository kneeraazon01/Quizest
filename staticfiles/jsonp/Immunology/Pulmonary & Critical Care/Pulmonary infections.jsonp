                        <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>E.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight5\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Interferon-alpha</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>F.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight6\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Interferon-gamma</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;1967&quot; src=&quot;https://www.uworld.com/media/L7519.jpg&quot; ></p><p>This patient has cough, fever, weight loss, and an upper lobe lesion on chest x-ray, raising strong suspicion for <strong>active pulmonary tuberculosis</strong>.&amp;nbsp; <em>Mycobacterium tuberculosis</em> cannot be visualized on Gram stain due to the high lipid content (mycolic acid) of its cell wall.&amp;nbsp; Most cases are diagnosed by acid-fast sputum testing and culture, but tissue microscopy typically shows granulomas characterized by <strong>epithelioid histiocytes</strong> and <strong>multinucleated Langhans giant cells</strong>.</p><p><strong>Granulomas</strong> often form after tissue macrophages encounter pathogens or substances that cannot be easily digested or removed.&amp;nbsp; <em>M tuberculosis</em> can <a ng-click=&quot;showexhibit('16236',true);&quot;>evade intracellular killing</a> by macrophages and reproduce within phagolysosomes.&amp;nbsp; Infected macrophages present mycobacterial antigens to na&amp;iuml;ve CD4 helper T cells in pulmonary lymph nodes and secrete <a ng-click=&quot;showexhibit('6714',true);&quot;>IL-12</a>, which induces activated T helper cells to differentiate into <strong>T helper subtype 1 (Th1) cells</strong>.</p><p>Proliferating Th1 cells migrate to sites of infection, where they release <strong>interferon-gamma</strong>, which <strong>activates macrophages</strong>, improves intracellular killing of ingested mycobacteria, and recruits additional macrophages by increasing production of <strong>tumor necrosis factor-alpha</strong>.&amp;nbsp; Activated macrophages can also limit the spread of mycobacteria by differentiating into epithelioid and giant cells that surround residual foci of mycobacteria, trapping them inside the necrotic, cheese-like area of a caseating granuloma.</p><p><strong>(Choice A)</strong>&amp;nbsp; C3a is a split product anaphylatoxin derived from C3.&amp;nbsp; It stimulates the release of histamine by mast cells, which increases vascular permeability and vasodilation.</p><p><strong>(Choice B)</strong>&amp;nbsp; Fibroblast growth factor has a variety of proliferative effects, including stimulation of angiogenesis and fibroblast proliferation during formation of <a ng-click=&quot;showexhibit('29849',true);&quot;>granulation tissue</a>.&amp;nbsp; It does not have a primary role in the formation of granulomas.</p><p><strong>(Choice C)</strong>&amp;nbsp; Granulocyte-macrophage colony-stimulating factor induces stem cell differentiation into monocytes and other granulocytes (eg, neutrophils, eosinophils).&amp;nbsp; However, it does not induce granuloma formation.</p><p><strong>(Choice D)</strong>&amp;nbsp; IL-4 is a cytokine that stimulates the differentiation of na&amp;iuml;ve CD4 T cells into the Th2 subset, which plays an important role in humoral immunity and allergic reactions by stimulating B-cell proliferation and isotype switching to IgE.</p><p><strong>(Choice E)</strong>&amp;nbsp; Interferon-alpha is a primary mediator of the immune response to virus-infected cells.&amp;nbsp; However, in contrast to interferon-gamma, it limits macrophage proliferation and does not drive granuloma formation.</p><p><strong>Educational objective:</strong><br>T helper subtype 1 cells release interferon-gamma, leading to the activation of macrophages, a process critical for control of <em>Mycobacterium tuberculosis</em> infection.&amp;nbsp; Activated macrophages form mature phagolysosomes that destroy phagocytosed mycobacteria and can differentiate into epithelioid and Langhans giant cells to wall off extracellular mycobacteria within caseating granulomas.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"1967\" src=\"image/L7519.jpg\"></p><p class=\"ng-scope\">This patient has cough, fever, weight loss, and an upper lobe lesion on chest x-ray, raising strong suspicion for <strong>active pulmonary tuberculosis</strong>.&nbsp; <em>Mycobacterium tuberculosis</em> cannot be visualized on Gram stain due to the high lipid content (mycolic acid) of its cell wall.&nbsp; Most cases are diagnosed by acid-fast sputum testing and culture, but tissue microscopy typically shows granulomas characterized by <strong>epithelioid histiocytes</strong> and <strong>multinucleated Langhans giant cells</strong>.</p><p class=\"ng-scope\"><strong>Granulomas</strong> often form after tissue macrophages encounter pathogens or substances that cannot be easily digested or removed.&nbsp; <em>M tuberculosis</em> can <a ng-click=\"showexhibit('16236',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L40988.jpg&quot;,&quot;title&quot;:&quot;L40988&quot;}]\">evade intracellular killing</a> by macrophages and reproduce within phagolysosomes.&nbsp; Infected macrophages present mycobacterial antigens to na\u00efve CD4 helper T cells in pulmonary lymph nodes and secrete <a ng-click=\"showexhibit('6714',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L6907.jpg&quot;,&quot;title&quot;:&quot;L6907&quot;}]\">IL-12</a>, which induces activated T helper cells to differentiate into <strong>T helper subtype 1 (Th1) cells</strong>.</p><p class=\"ng-scope\">Proliferating Th1 cells migrate to sites of infection, where they release <strong>interferon-gamma</strong>, which <strong>activates macrophages</strong>, improves intracellular killing of ingested mycobacteria, and recruits additional macrophages by increasing production of <strong>tumor necrosis factor-alpha</strong>.&nbsp; Activated macrophages can also limit the spread of mycobacteria by differentiating into epithelioid and giant cells that surround residual foci of mycobacteria, trapping them inside the necrotic, cheese-like area of a caseating granuloma.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; C3a is a split product anaphylatoxin derived from C3.&nbsp; It stimulates the release of histamine by mast cells, which increases vascular permeability and vasodilation.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Fibroblast growth factor has a variety of proliferative effects, including stimulation of angiogenesis and fibroblast proliferation during formation of <a ng-click=\"showexhibit('29849',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L1152.jpg&quot;,&quot;title&quot;:&quot;L1152&quot;}]\">granulation tissue</a>.&nbsp; It does not have a primary role in the formation of granulomas.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Granulocyte-macrophage colony-stimulating factor induces stem cell differentiation into monocytes and other granulocytes (eg, neutrophils, eosinophils).&nbsp; However, it does not induce granuloma formation.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; IL-4 is a cytokine that stimulates the differentiation of na\u00efve CD4 T cells into the Th2 subset, which plays an important role in humoral immunity and allergic reactions by stimulating B-cell proliferation and isotype switching to IgE.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Interferon-alpha is a primary mediator of the immune response to virus-infected cells.&nbsp; However, in contrast to interferon-gamma, it limits macrophage proliferation and does not drive granuloma formation.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>T helper subtype 1 cells release interferon-gamma, leading to the activation of macrophages, a process critical for control of <em>Mycobacterium tuberculosis</em> infection.&nbsp; Activated macrophages form mature phagolysosomes that destroy phagocytosed mycobacteria and can differentiate into epithelioid and Langhans giant cells to wall off extracellular mycobacteria within caseating granulomas.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.n