== 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;16979&quot; src=&quot;https://www.uworld.com/media/L41542.jpg&quot; ></p><p>There are 2 classes of CD4<font size=&quot;2&quot;><sup>+</sup></font> T-helper cells, Th1 cells and Th2 cells.&amp;nbsp; Th1 cells contribute to cell-mediated adaptive immunity (targeting intracellular pathogens) and type IV (delayed-type) hypersensitivity reactions.&amp;nbsp; On the other hand, <strong>Th2 cells</strong> play a prominent role in allergic response and <strong>type I hypersensitivity reactions</strong>.</p><p>One hypothesis for the <strong>pathogenesis of asthma</strong> is an excess of Th2 cell activity relative to Th1 cell activity, resulting in excessive IgE production, an abnormal propensity for type I hypersensitivity reactions, and associated chronic eosinophilic bronchitis.&amp;nbsp; In the asthma <strong>sensitization phase</strong>, inhaled antigens stimulate Th2 cells to secrete <strong>IL-4</strong> and other lymphokines to stimulate B-cell antibody production as part of humoral adaptive immunity.&amp;nbsp; Th2 cells also release <strong>IL-13</strong>, which, together with IL-4, promotes B-cell immunoglobulin <strong>class switching to IgE</strong> and leads to mast cell priming.</p><p>Repeat exposure to inhaled antigens leads to mast cell degranulation of inflammatory substances (eg, histamine, leukotrienes) and further activation of eosinophils with release of tissue-damaging substances (eg, major basic protein).</p><p><strong>(Choice A)</strong>&amp;nbsp; IL-1 is predominantly involved in the Th1 response and stimulates fever and acute inflammation.&amp;nbsp; It also induces lymphokine secretion to recruit other leukocytes, including lymphocytes.</p><p><strong>(Choice B)</strong>&amp;nbsp; IL-3 is secreted by both Th1 and Th2 cells and promotes the growth and differentiation of bone marrow stem cells.</p><p><strong>(Choice D)</strong>&amp;nbsp; IL-5 is secreted by activated Th2 cells and stimulates the growth and differentiation of eosinophils.&amp;nbsp; However, IL-5 promotes the class switching of B-cell immunoglobulin synthesis to IgA rather than to IgE and is a less important therapeutic target for asthma than IL-4.</p><p><strong>(Choice E)</strong>&amp;nbsp; Interferon-gamma secreted by Th1 cells activates macrophages and, along with IL-2, stimulates CD8<font size=&quot;2&quot;><sup>+</sup></font> cytotoxic T cells.&amp;nbsp; It also inhibits the differentiation of Th2 cells.</p><p><strong>(Choice F)</strong>&amp;nbsp; Transforming growth factor-beta (TGF-&amp;#946;) is involved in tissue regeneration and repair.&amp;nbsp; It can be produced by T lymphocytes, platelets, macrophages, endothelial cells, smooth muscle cells, fibroblasts, and keratinocytes.</p><p><strong>Educational objective:</strong><br>An excess of Th2 cell activity relative to Th1 cell activity may underlie the pathogenesis of asthma.&amp;nbsp; In the asthma sensitization phase, inhaled antigens stimulate Th2 cells to secrete IL-4 and IL-13, which together promote B-lymphocyte class switching for IgE synthesis, leading to mast cell priming.&amp;nbsp; Th2 cells also secrete IL-5, which activates eosinophils.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"16979\" src=\"image/L41542.jpg\"></p><p class=\"ng-scope\">There are 2 classes of CD4<font size=\"2\"><sup>+</sup></font> T-helper cells, Th1 cells and Th2 cells.&nbsp; Th1 cells contribute to cell-mediated adaptive immunity (targeting intracellular pathogens) and type IV (delayed-type) hypersensitivity reactions.&nbsp; On the other hand, <strong>Th2 cells</strong> play a prominent role in allergic response and <strong>type I hypersensitivity reactions</strong>.</p><p class=\"ng-scope\">One hypothesis for the <strong>pathogenesis of asthma</strong> is an excess of Th2 cell activity relative to Th1 cell activity, resulting in excessive IgE production, an abnormal propensity for type I hypersensitivity reactions, and associated chronic eosinophilic bronchitis.&nbsp; In the asthma <strong>sensitization phase</strong>, inhaled antigens stimulate Th2 cells to secrete <strong>IL-4</strong> and other lymphokines to stimulate B-cell antibody production as part of humoral adaptive immunity.&nbsp; Th2 cells also release <strong>IL-13</strong>, which, together with IL-4, promotes B-cell immunoglobulin <strong>class switching to IgE</strong> and leads to mast cell priming.</p><p class=\"ng-scope\">Repeat exposure to inhaled antigens leads to mast cell degranulation of inflammatory substances (eg, histamine, leukotrienes) and further activation of eosinophils with release of tissue-damaging substances (eg, major basic protein).</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; IL-1 is predominantly involved in the Th1 response and stimulates fever and acute inflammation.&nbsp; It also induces lymphokine secretion to recruit other leukocytes, including lymphocytes.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; IL-3 is secreted by both Th1 and Th2 cells and promotes the growth and differentiation of bone marrow stem cells.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; IL-5 is secreted by activated Th2 cells and stimulates the growth and differentiation of eosinophils.&nbsp; However, IL-5 promotes the class switching of B-cell immunoglobulin synthesis to IgA rather than to IgE and is a less important therapeutic target for asthma than IL-4.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Interferon-gamma secreted by Th1 cells activates macrophages and, along with IL-2, stimulates CD8<font size=\"2\"><sup>+</sup></font> cytotoxic T cells.&nbsp; It also inhibits the differentiation of Th2 cells.</p><p class=\"ng-scope\"><strong>(Choice F)</strong>&nbsp; Transforming growth factor-beta (TGF-\u03b2) is involved in tissue regeneration and repair.&nbsp; It can be produced by T lymphocytes, platelets, macrophages, endothelial cells, smooth muscle cells, fibroblasts, and keratinocytes.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>An excess of Th2 cell activity relative to Th1 cell activity may underlie the pathogenesis of asthma.&nbsp; In the asthma sensitization phase, inhaled antigens stimulate Th2 cells to secrete IL-4 and IL-13, which together promote B-lymphocyte class switching for IgE synthesis, leading to mast cell priming.&nbsp; Th2 cells also secrete IL-5, which activates eosinophils.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Immunology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pulmonary &amp; Critical Care</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Asthma</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n             