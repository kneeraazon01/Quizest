data = [
    {
        "question": "<div class=\"questioncontent ui-resizable widthWithoutAbstract\" id=\"questionContent\" ng-show=\"showQuestion\" ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat, 'suggested-highlights-visible' : userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; currentQuestion.frequentHighlights != null &amp;&amp; (suggestedHighlightsPreference == 1 || suggestedHighlightsPreference == 2), 'only-suggested-highlights-visible' : userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; suggestedHighlightsPreference == 1 &amp;&amp; currentQuestion.frequentHighlights != null}\">\n            <div style=\"display:none;\" class=\"suggested-highlights-switch-buttons ng-hide\" ng-show=\"userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; currentQuestion.frequentHighlights != null\">\n                <span class=\"fa-stack\" ng-click=\"showUserHighlights()\" title=\"My Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fas\" ng-class=\"{'fas' : suggestedHighlightsPreference == 0, 'fal' : suggestedHighlightsPreference != 0}\"></i>\n                    <i class=\"fal fa-highlighter fa-stack-1x fa-inverse\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 0}\"></i>\n                </span>\n                <span class=\"fa-stack ng-hide\" ng-hide=\"currentQuestion.frequentHighlights == null\" ng-click=\"showSuggestedHighlights()\" title=\"Suggested Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fal\" ng-class=\"{'fas' : suggestedHighlightsPreference == 1, 'fal' : suggestedHighlightsPreference != 1}\"></i>\n                    <i class=\"fas fa-highlighter fa-stack-1x\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 1}\"></i>\n                </span>\n                <span class=\"fa-stack highlight-preference-unavailable highlight-button-with-popover\" ng-show=\"currentQuestion.frequentHighlights == null\" title=\"\" data-toggle=\"popover\" data-placement=\"bottom\" data-content=\"Collecting Data\" data-trigger=\"hover\" data-original-title=\"Suggested Highlights\">\n                    <i class=\"fal fa-circle fa-stack-2x\"></i>\n                    <i class=\"fas fa-highlighter fa-stack-1x\"></i>\n                </span>\n                <span class=\"fa-stack ng-hide\" ng-hide=\"currentQuestion.frequentHighlights == null\" ng-click=\"showUserAndSuggestedHighlights()\" title=\"Duo Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fal\" ng-class=\"{'fas' : suggestedHighlightsPreference == 2, 'fal' : suggestedHighlightsPreference != 2}\"></i>\n                    <i class=\"fad fa-highlighter fa-stack-1x\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 2}\"></i>\n                </span>\n                <span class=\"fa-stack highlight-preference-unavailable highlight-button-with-popover\" ng-show=\"currentQuestion.frequentHighlights == null\" title=\"\" data-toggle=\"popover\" data-placement=\"bottom\" data-content=\"Collecting Data\" data-trigger=\"hover\" data-original-title=\"Duo Highlights\">\n                    <i class=\"fal fa-circle fa-stack-2x\"></i>\n                    <i class=\"fad fa-highlighter fa-stack-1x\"></i>\n                </span>\n            </div>\n            <!-- ngIf: launchTestOptions.launchCode == 'nbome' && userPreferences.testPreferences.theme != clientConstants.themes.nbome -->\n            <div id=\"question\">\n                <!-- ngIf: settings.hints && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && launchTestOptions.canShowHint -->\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && (currentQuestion.qbankId != clientConstants.qbankIds.stats_faculty || currentQuestion.qbankId != clientConstants.qbankIds.stats) -->\n                <!-- ngIf: (currentQuestion.mobileExcerpt != '' && currentQuestion.mobileExcerpt != null ) -->\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat -->\n                <!--Periodic table button for MCAT -->\n                <!--<div class=\"text-center periodicTableDiv\" ng-show=\"(launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.subjectId != constants.noPeriodicTable.cars) && (currentQuestion.subjectId != constants.noPeriodicTable.pyschology) && (currentQuestion.subjectId != constants.noPeriodicTable.sociology)\">\n                    <button class=\"btn btn-default btn--custom\" style=\"color: black; padding: 4px 3px;\" ng-click=\"showresources()\">Periodic table&nbsp;<img src=\"img/mcat/Periodic_icon.png\" height=\"12\" style=\"margin-top: -2px; cursor: pointer\" title=\"Periodic Table\" /></button>\n                </div>-->\n                \n                <!--Inactive message for questions in usmle, boards and nclex and do not show for EXPERIMENTAL_INACTIVE hence the check !=3 -->\n                <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.nclex) && !currentQuestion.isActive && currentQuestion.activeStatusId != 3 -->\n                <div id=\"questionText\" class=\"questionText ng-scope\" ng-mouseup=\"invokeHighlights('questionText',constants.flashCardType.selection,$event)\" compiledata=\"\" htmlcontent=\"<p>Molecular biologists studying the properties of hemoglobin are investigating the structural changes associated with oxygen loading and unloading.&amp;nbsp; During the transition from point 1 to point 2 on the graph shown below, hemoglobin molecules are most likely to release which of the following?</p><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;6825&quot; src=&quot;https://www.uworld.com/media/L14405.jpg&quot; ></p>\"><p class=\"ng-scope\">Molecular biologists studying the properties of hemoglobin are investigating the structural changes associated with oxygen loading and unloading.&nbsp; During the transition from point 1 to point 2 on the graph shown below, hemoglobin molecules are most likely to release which of the following?</p><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"6825\" src=\"image/L14405.jpg\"></p></div>\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && (currentQuestion.formatTypeId == constants.questionFormatType.fillInTheBlank ||currentQuestion.formatTypeId == constants.questionFormatType.fillInTheBlankRange )&& currentQuestion.sectionId == constants.collegeprepSectionIds.math -->\n                <!-- change to mediatype object -->\n                <!-- ngIf: currentQuestion.questionMedia.typeId == constants.questionMediaType.drugad || currentQuestion.questionTypeId == constants.questionType.drugad -->\n                <!-- ngIf: currentQuestion.questionMedia.typeId == constants.questionMediaType.audio || currentQuestion.questionMedia.typeId == constants.questionMediaType.video || currentQuestion.questionTypeId == constants.questionType.media -->\n\n                <div id=\"answerContainer\"><div class=\"options\">\n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>A.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight1\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Chloride</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>B.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight2\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Heme</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>C.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight3\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Oxygen</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>D.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight4\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Phosphate</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>E.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight5\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Protons</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;2395&quot; src=&quot;https://www.uworld.com/media/L12691.jpg&quot; ></p><p>The transition from point 1 to 2 on the graph represents the loading of O<font size=&quot;2&quot;><sub>2</sub></font> onto partially deoxygenated hemoglobin as occurs in the <strong>lungs</strong>.&amp;nbsp; At very low partial pressure of O<font size=&quot;2&quot;><sub>2</sub></font> (pO<font size=&quot;2&quot;><sub>2</sub></font>), hemoglobin is mostly deoxygenated, and binding of O<font size=&quot;2&quot;><sub>2</sub></font> molecules is relatively difficult (as indicated by the early flatness of the curve).&amp;nbsp; As the partial pressure of O<font size=&quot;2&quot;><sub>2</sub></font> increases, O<font size=&quot;2&quot;><sub>2</sub></font> binds to 1 of the 4 binding sites on hemoglobin and <strong>increases the O<font size=&quot;2&quot;><sub>2</sub></font>-binding affinity</strong> of the other available binding sites (steepening of the curve), a phenomenon known as <strong>cooperative binding</strong>.&amp;nbsp; Subsequently, additional O<font size=&quot;2&quot;><sub>2</sub></font> molecules bind more easily as the pO<font size=&quot;2&quot;><sub>2</sub></font> increases until hemoglobin becomes nearly fully saturated.</p><p>The binding of O<font size=&quot;2&quot;><sub>2</sub></font> molecules to hemoglobin in the lungs has two consequences, known as the <strong>Haldane effect</strong>:</p><ul><li><p>The affinity of hemoglobin for CO<font size=&quot;2&quot;><sub>2</sub></font> is decreased, resulting in unloading of CO<font size=&quot;2&quot;><sub>2</sub></font> from hemoglobin (this accounts for a small percentage of overall CO<font size=&quot;2&quot;><sub>2</sub></font> in the blood and is not pictured above).</p></li><li><p>The acidity of the hemoglobin molecule is increased; in response, <strong>protons (H<font size=&quot;2&quot;><sup>+</sup></font> ions) are released</strong> from the hemoglobin binding sites.</p></li></ul><p>The H<font size=&quot;2&quot;><sup>+</sup></font> ions combine with bicarbonate ions (the primary form of CO<sub>2</sub> in the blood) in the lungs to facilitate the production of water (H<font size=&quot;2&quot;><sub>2</sub></font>O) and CO<font size=&quot;2&quot;><sub>2</sub></font>.&amp;nbsp; The CO<font size=&quot;2&quot;><sub>2</sub></font> is then transferred to the alveoli and expired while oxygen is taken up by hemoglobin.</p><p>In the peripheral tissues, high levels of CO<font size=&quot;2&quot;><sub>2</sub></font> create an increase in ambient acidity that shifts the <a ng-click=&quot;showexhibit('1588',true);&quot;>hemoglobin dissociation curve</a> to the right and facilitates the unloading of O<font size=&quot;2&quot;><sub>2</sub></font> (<strong>Bohr effect</strong>).&amp;nbsp; The CO<font size=&quot;2&quot;><sub>2</sub></font> (and water) are converted into H<font size=&quot;2&quot;><sup>+</sup></font> and HCO<font size=&quot;2&quot;><sub>3</sub></font><font size=&quot;2&quot;><sup>&amp;#8722;</sup></font>.&amp;nbsp; The H<font size=&quot;2&quot;><sup>+</sup></font> ions are carried by hemoglobin while the HCO<font size=&quot;2&quot;><sub>3</sub></font><font size=&quot;2&quot;><sup>&amp;#8722;</sup></font> is transferred to the plasma for transport back to the lungs.</p><p><strong>(Choice A)</strong>&amp;nbsp; In the lungs, chloride ions shift out of red blood cells and into the plasma in exchange for HCO<font size=&quot;2&quot;><sub>3</sub></font><font size=&quot;2&quot;><sup>&amp;#8722;</sup></font>.&amp;nbsp; These chloride ions are not bound to or released by hemoglobin.</p><p><strong>(Choice B)</strong>&amp;nbsp; Heme is not released from hemoglobin during O<font size=&quot;2&quot;><sub>2</sub></font> loading or unloading.&amp;nbsp; However, it is released during the normal destruction of aged red blood cells in the spleen.</p><p><strong>(Choice C)</strong>&amp;nbsp; O<font size=&quot;2&quot;><sub>2</sub></font> molecules bind to hemoglobin to increase hemoglobin saturation.</p><p><strong>(Choice D)</strong>&amp;nbsp; 2,3-diphosphoglycerate contains 2 phosphate groups and binds to hemoglobin to facilitate unloading of O<font size=&quot;2&quot;><sub>2</sub></font> in the tissues.&amp;nbsp; However, phosphate molecules are not directly bound or released by hemoglobin.</p><p><strong>Educational objective:</strong><br>The binding of O<font size=&quot;2&quot;><sub>2</sub></font> to hemoglobin increases the affinity for binding of subsequent O<font size=&quot;2&quot;><sub>2</sub></font> molecules (cooperative binding).&amp;nbsp; In the lungs, the binding of O<font size=&quot;2&quot;><sub>2</sub></font> to hemoglobin drives the release of H<font size=&quot;2&quot;><sup>+</sup></font> and CO<font size=&quot;2&quot;><sub>2</sub></font> from hemoglobin (Haldane effect).&amp;nbsp; In the peripheral tissues, high concentrations of CO<font size=&quot;2&quot;><sub>2</sub></font> and H<font size=&quot;2&quot;><sup>+</sup></font> facilitate O<font size=&quot;2&quot;><sub>2</sub></font> unloading from hemoglobin (Bohr effect).</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"2395\" src=\"image/L12691.jpg\"></p><p class=\"ng-scope\">The transition from point 1 to 2 on the graph represents the loading of O<font size=\"2\"><sub>2</sub></font> onto partially deoxygenated hemoglobin as occurs in the <strong>lungs</strong>.&nbsp; At very low partial pressure of O<font size=\"2\"><sub>2</sub></font> (pO<font size=\"2\"><sub>2</sub></font>), hemoglobin is mostly deoxygenated, and binding of O<font size=\"2\"><sub>2</sub></font> molecules is relatively difficult (as indicated by the early flatness of the curve).&nbsp; As the partial pressure of O<font size=\"2\"><sub>2</sub></font> increases, O<font size=\"2\"><sub>2</sub></font> binds to 1 of the 4 binding sites on hemoglobin and <strong>increases the O<font size=\"2\"><sub>2</sub></font>-binding affinity</strong> of the other available binding sites (steepening of the curve), a phenomenon known as <strong>cooperative binding</strong>.&nbsp; Subsequently, additional O<font size=\"2\"><sub>2</sub></font> molecules bind more easily as the pO<font size=\"2\"><sub>2</sub></font> increases until hemoglobin becomes nearly fully saturated.</p><p class=\"ng-scope\">The binding of O<font size=\"2\"><sub>2</sub></font> molecules to hemoglobin in the lungs has two consequences, known as the <strong>Haldane effect</strong>:</p><ul class=\"ng-scope\"><li><p>The affinity of hemoglobin for CO<font size=\"2\"><sub>2</sub></font> is decreased, resulting in unloading of CO<font size=\"2\"><sub>2</sub></font> from hemoglobin (this accounts for a small percentage of overall CO<font size=\"2\"><sub>2</sub></font> in the blood and is not pictured above).</p></li><li><p>The acidity of the hemoglobin molecule is increased; in response, <strong>protons (H<font size=\"2\"><sup>+</sup></font> ions) are released</strong> from the hemoglobin binding sites.</p></li></ul><p class=\"ng-scope\">The H<font size=\"2\"><sup>+</sup></font> ions combine with bicarbonate ions (the primary form of CO<sub>2</sub> in the blood) in the lungs to facilitate the production of water (H<font size=\"2\"><sub>2</sub></font>O) and CO<font size=\"2\"><sub>2</sub></font>.&nbsp; The CO<font size=\"2\"><sub>2</sub></font> is then transferred to the alveoli and expired while oxygen is taken up by hemoglobin.</p><p class=\"ng-scope\">In the peripheral tissues, high levels of CO<font size=\"2\"><sub>2</sub></font> create an increase in ambient acidity that shifts the <a ng-click=\"showexhibit('1588',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L14561.jpg&quot;,&quot;title&quot;:&quot;L14561&quot;}]\">hemoglobin dissociation curve</a> to the right and facilitates the unloading of O<font size=\"2\"><sub>2</sub></font> (<strong>Bohr effect</strong>).&nbsp; The CO<font size=\"2\"><sub>2</sub></font> (and water) are converted into H<font size=\"2\"><sup>+</sup></font> and HCO<font size=\"2\"><sub>3</sub></font><font size=\"2\"><sup>\u2212</sup></font>.&nbsp; The H<font size=\"2\"><sup>+</sup></font> ions are carried by hemoglobin while the HCO<font size=\"2\"><sub>3</sub></font><font size=\"2\"><sup>\u2212</sup></font> is transferred to the plasma for transport back to the lungs.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; In the lungs, chloride ions shift out of red blood cells and into the plasma in exchange for HCO<font size=\"2\"><sub>3</sub></font><font size=\"2\"><sup>\u2212</sup></font>.&nbsp; These chloride ions are not bound to or released by hemoglobin.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Heme is not released from hemoglobin during O<font size=\"2\"><sub>2</sub></font> loading or unloading.&nbsp; However, it is released during the normal destruction of aged red blood cells in the spleen.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; O<font size=\"2\"><sub>2</sub></font> molecules bind to hemoglobin to increase hemoglobin saturation.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; 2,3-diphosphoglycerate contains 2 phosphate groups and binds to hemoglobin to facilitate unloading of O<font size=\"2\"><sub>2</sub></font> in the tissues.&nbsp; However, phosphate molecules are not directly bound or released by hemoglobin.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>The binding of O<font size=\"2\"><sub>2</sub></font> to hemoglobin increases the affinity for binding of subsequent O<font size=\"2\"><sub>2</sub></font> molecules (cooperative binding).&nbsp; In the lungs, the binding of O<font size=\"2\"><sub>2</sub></font> to hemoglobin drives the release of H<font size=\"2\"><sup>+</sup></font> and CO<font size=\"2\"><sub>2</sub></font> from hemoglobin (Haldane effect).&nbsp; In the peripheral tissues, high concentrations of CO<font size=\"2\"><sub>2</sub></font> and H<font size=\"2\"><sup>+</sup></font> facilitate O<font size=\"2\"><sub>2</sub></font> unloading from hemoglobin (Bohr effect).</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Biochemistry</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pulmonary &amp; Critical Care</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Respiratory physiology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
