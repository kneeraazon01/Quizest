estModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;2707&quot; src=&quot;https://www.uworld.com/media/L23798.jpg&quot; ></p><p><strong>Von Willebrand factor (vWF)</strong> is an important hemostatic glycoprotein (GP) synthesized by endothelial cells and megakaryocytes.&amp;nbsp; Following endothelial damage, vWF <strong>binds GP Ib receptors</strong> on the platelet membrane and mediates <strong>platelet adhesion</strong> to subendothelial collagen.&amp;nbsp; Deficiency of vWF impairs platelet adhesion and can lead to easy bruising and <strong>prolonged mucocutaneous bleeding</strong> (eg, gingival bleeding, heavy menses).</p><p>Laboratory workup in vWF deficiency reveals a normal platelet count and an <strong>abnormal ristocetin cofactor assay</strong>.&amp;nbsp; The ristocetin cofactor assay measures in-vitro, vWF-dependent platelet agglutination (indicative of impaired platelet adhesion in-vivo).&amp;nbsp; Ristocetin activates GP Ib receptors on platelets and makes them available for vWF binding; when the vWF level is decreased, there is <strong>poor platelet agglutination in the presence of ristocetin</strong>.&amp;nbsp; When normal plasma that contains vWF is added, appropriate platelet agglutination occurs.</p><p>VWF also serves as a <strong>carrier for factor VIII</strong> to prolong its half-life, and vWF deficiency can lead to functional deficiency of factor VIII that further contributes to bleeding complications.&amp;nbsp; PTT may be normal or prolonged depending on the degree of factor VIII impairment.</p><p>Combined oral contraceptives are often used for treatment of menorrhagia due to vWF deficiency.&amp;nbsp; Patients can also be treated with desmopressin, which stimulates vWF release from endothelium.</p><p><strong>(Choice A)</strong>&amp;nbsp; Bernard-Soulier syndrome (hereditary deficiency of GP Ib receptors) is characterized by thrombocytopenia, enlarged platelets, and mucocutaneous bleeding.&amp;nbsp; Platelet agglutination to ristocetin will be abnormal and, because the deficiency is with GP Ib receptors and not vWF, the addition of normal plasma will not correct the agglutination.</p><p><strong>(Choice B)</strong>&amp;nbsp; Hereditary deficiency of GP IIb/IIIa receptors occurs in Glanzmann thrombasthenia, which manifests with mucocutaneous bleeding.&amp;nbsp; Platelet agglutination in response to ristocetin is normal because the levels of vWF and GP Ib receptors are normal.</p><p><strong>(Choice C)</strong>&amp;nbsp; Congenital deficiency of factor XII (Hageman) causes marked PTT prolongation; however, it does not cause clinical bleeding complications.&amp;nbsp; Instead, patients may have a tendency for thromboembolic complications for unclear reasons.</p><p><strong>(Choice D)</strong>&amp;nbsp; Thromboxane A2 deficiency is associated with aspirin treatment due to irreversible inactivation of cyclooxygenase in platelets.&amp;nbsp; The major effect is on GP IIb/IIIa-mediated platelet aggregation rather than GP Ib-mediated platelet adhesion; therefore, platelet agglutination to ristocetin is normal.</p><p><strong>Educational objective:</strong><br>Following endothelial damage, von Willebrand factor (vWF) binds glycoprotein Ib receptors on platelets to mediate platelet adherence.&amp;nbsp; The ristocetin cofactor assay measures platelet agglutination via binding of glycoprotein Ib receptors to vWF; it will be abnormal in vWF deficiency but will correct with the addition of normal (vWF-containing) plasma.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"2707\" src=\"image/L23798.jpg\"></p><p class=\"ng-scope\"><strong>Von Willebrand factor (vWF)</strong> is an important hemostatic glycoprotein (GP) synthesized by endothelial cells and megakaryocytes.&nbsp; Following endothelial damage, vWF <strong>binds GP Ib receptors</strong> on the platelet membrane and mediates <strong>platelet adhesion</strong> to subendothelial collagen.&nbsp; Deficiency of vWF impairs platelet adhesion and can lead to easy bruising and <strong>prolonged mucocutaneous bleeding</strong> (eg, gingival bleeding, heavy menses).</p><p class=\"ng-scope\">Laboratory workup in vWF deficiency reveals a normal platelet count and an <strong>abnormal ristocetin cofactor assay</strong>.&nbsp; The ristocetin cofactor assay measures in-vitro, vWF-dependent platelet agglutination (indicative of impaired platelet adhesion in-vivo).&nbsp; Ristocetin activates GP Ib receptors on platelets and makes them available for vWF binding; when the vWF level is decreased, there is <strong>poor platelet agglutination in the presence of ristocetin</strong>.&nbsp; When normal plasma that contains vWF is added, appropriate platelet agglutination occurs.</p><p class=\"ng-scope\">VWF also serves as a <strong>carrier for factor VIII</strong> to prolong its half-life, and vWF deficiency can lead to functional deficiency of factor VIII that further contributes to bleeding complications.&nbsp; PTT may be normal or prolonged depending on the degree of factor VIII impairment.</p><p class=\"ng-scope\">Combined oral contraceptives are often used for treatment of menorrhagia due to vWF deficiency.&nbsp; Patients can also be treated with desmopressin, which stimulates vWF release from endothelium.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Bernard-Soulier syndrome (hereditary deficiency of GP Ib receptors) is characterized by thrombocytopenia, enlarged platelets, and mucocutaneous bleeding.&nbsp; Platelet agglutination to ristocetin will be abnormal and, because the deficiency is with GP Ib receptors and not vWF, the addition of normal plasma will not correct the agglutination.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Hereditary deficiency of GP IIb/IIIa receptors occurs in Glanzmann thrombasthenia, which manifests with mucocutaneous bleeding.&nbsp; Platelet agglutination in response to ristocetin is normal because the levels of vWF and GP Ib receptors are normal.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Congenital deficiency of factor XII (Hageman) causes marked PTT prolongation; however, it does not cause clinical bleeding complications.&nbsp; Instead, patients may have a tendency for thromboembolic complications for unclear reasons.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Thromboxane A2 deficiency is associated with aspirin treatment due to irreversible inactivation of cyclooxygenase in platelets.&nbsp; The major effect is on GP IIb/IIIa-mediated platelet aggregation rather than GP Ib-mediated platelet adhesion; therefore, platelet agglutination to ristocetin is normal.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Following endothelial damage, von Willebrand factor (vWF) binds glycoprotein Ib receptors on platelets to mediate platelet adherence.&nbsp; The ristocetin cofactor assay measures platelet agglutination via binding of glycoprotein Ib receptors to vWF; it will be abnormal in vWF deficiency but will correct with the addition of normal (vWF-containing) plasma.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/19961139\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/19961139\"><span compiledata=\"\" htmlcontent=\"Diagnosis and management of von Willebrand disease: guidelines for primary care.\" class=\"ng-scope\"><p class=\"ng-scope\">Diagnosis and management of von Willebrand disease: guidelines for primary care.</p></span></a><!-- end ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal -->\n                                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal -->\n                                </li><!-- end ngRepeat: questionReference in currentQuestion.questionMappi