></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;3909&quot; src=&quot;https://www.uworld.com/media/L27548.jpg&quot; ></p><p>This patient&amp;#39;s limb shortening with normal torso length, specific craniofacial features (frontal bossing, midface hypoplasia), and risk of passing the condition to her offspring (50%) are characteristic of <strong>achondroplasia</strong>.&amp;nbsp; The condition is caused by an autosomal dominant point mutation in the fibroblast growth factor receptor 3 (<em>FGFR3</em>) gene; 90% are de novo (unaffected parents) and 10% are inherited.&amp;nbsp; Because the mutation is <strong>autosomal dominant</strong>, the fetus of an affected and an unaffected parent has a 50% chance of inheriting the mutation.</p><p>Achondroplasia affects <strong>endochondral ossification</strong>, the process that makes <strong>long bones</strong> (eg, humerus, femur, phalanges) and portions of the skull and face.&amp;nbsp; In endochondral bone formation, mesenchymal cells differentiate into chondrocytes that secrete cartilage matrix.&amp;nbsp; These chondrocytes continue to proliferate through childhood, forming an elongating cartilage template that progressively becomes calcified and later invaded by osteoblasts, osteoclasts, and blood vessels originating from the periosteum.&amp;nbsp; Deposition of osteoid matrix over the septa of calcified cartilage matrix forms woven bone, which then undergoes remodeling into compact bone.</p><p><strong>FGFR3</strong> is normally responsible for limiting chondrocyte proliferation during endochondral ossification.&amp;nbsp; In achondroplasia, FGFR3 becomes constitutively activated, causing exaggerated <strong>inhibition of chondrocyte proliferation</strong> that results in the characteristic bone shortening and craniofacial abnormalities.</p><p><strong>(Choices B and E)</strong>&amp;nbsp; Hypothalamic neurosecretory cells produce growth hormone-releasing hormone and anterior pituitary somatotrophs produce growth hormone (GH).&amp;nbsp; Hypothalamic or pituitary insults (eg, tumor, ischemia) can result in defective release of either hormone.&amp;nbsp; GH deficiency can lead to proportional short stature (both long and flat bones affected) as opposed to the disproportionate short stature seen in achondroplasia (only long bones affected).</p><p><strong>(Choice C)</strong>&amp;nbsp; The flat bones (eg, most of the skull, pelvis) grow primarily via intramembranous (not endochondral) ossification.&amp;nbsp; In intramembranous ossification, mesenchymal cells differentiate directly into osteoblasts and begin osteoid matrix secretion without the formation of cartilaginous bones.&amp;nbsp; As a result, most flat bones remain unaffected in achondroplasia.</p><p><strong>(Choice D)</strong>&amp;nbsp; Osteoclasts are specialized macrophages that function to resorb bone.&amp;nbsp; <a ng-click=&quot;showexhibit('2768',true);&quot;>Paget disease</a>, characterized by disordered bone mass, initially begins with excessive activity of abnormally large osteoclasts followed by osteoblastic activity and eventual sclerosis.&amp;nbsp; Patients are usually elderly and can present with focal bone pain and pathologic fractures.</p><p><strong>Educational objective:</strong><br>Unlike the process of intramembranous ossification that forms flat bones, endochondral ossification proceeds along a cartilage template and is responsible for the formation of long bones.&amp;nbsp; Achondroplasia is characterized by an exaggerated inhibition of chondrocyte proliferation in the growth plates of long bones and manifests with proximal limb shortening, midface hypoplasia, and macrocephaly.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"3909\" src=\"image/L27548.jpg\"></p><p class=\"ng-scope\">This patient's limb shortening with normal torso length, specific craniofacial features (frontal bossing, midface hypoplasia), and risk of passing the condition to her offspring (50%) are characteristic of <strong>achondroplasia</strong>.&nbsp; The condition is caused by an autosomal dominant point mutation in the fibroblast growth factor receptor 3 (<em>FGFR3</em>) gene; 90% are de novo (unaffected parents) and 10% are inherited.&nbsp; Because the mutation is <strong>autosomal dominant</strong>, the fetus of an affected and an unaffected parent has a 50% chance of inheriting the mutation.</p><p class=\"ng-scope\">Achondroplasia affects <strong>endochondral ossification</strong>, the process that makes <strong>long bones</strong> (eg, humerus, femur, phalanges) and portions of the skull and face.&nbsp; In endochondral bone formation, mesenchymal cells differentiate into chondrocytes that secrete cartilage matrix.&nbsp; These chondrocytes continue to proliferate through childhood, forming an elongating cartilage template that progressively becomes calcified and later invaded by osteoblasts, osteoclasts, and blood vessels originating from the periosteum.&nbsp; Deposition of osteoid matrix over the septa of calcified cartilage matrix forms woven bone, which then undergoes remodeling into compact bone.</p><p class=\"ng-scope\"><strong>FGFR3</strong> is normally responsible for limiting chondrocyte proliferation during endochondral ossification.&nbsp; In achondroplasia, FGFR3 becomes constitutively activated, causing exaggerated <strong>inhibition of chondrocyte proliferation</strong> that results in the characteristic bone shortening and craniofacial abnormalities.</p><p class=\"ng-scope\"><strong>(Choices B and E)</strong>&nbsp; Hypothalamic neurosecretory cells produce growth hormone-releasing hormone and anterior pituitary somatotrophs produce growth hormone (GH).&nbsp; Hypothalamic or pituitary insults (eg, tumor, ischemia) can result in defective release of either hormone.&nbsp; GH deficiency can lead to proportional short stature (both long and flat bones affected) as opposed to the disproportionate short stature seen in achondroplasia (only long bones affected).</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; The flat bones (eg, most of the skull, pelvis) grow primarily via intramembranous (not endochondral) ossification.&nbsp; In intramembranous ossification, mesenchymal cells differentiate directly into osteoblasts and begin osteoid matrix secretion without the formation of cartilaginous bones.&nbsp; As a result, most flat bones remain unaffected in achondroplasia.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Osteoclasts are specialized macrophages that function to resorb bone.&nbsp; <a ng-click=\"showexhibit('2768',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L24976.jpg&quot;,&quot;title&quot;:&quot;L24976&quot;}]\">Paget disease</a>, characterized by disordered bone mass, initially begins with excessive activity of abnormally large osteoclasts followed by osteoblastic activity and eventual sclerosis.&nbsp; Patients are usually elderly and can present with focal bone pain and pathologic fractures.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Unlike the process of intramembranous ossification that forms flat bones, endochondral ossification proceeds along a cartilage template and is responsible for the formation of long bones.&nbsp; Achondroplasia is characterized by an exaggerated inhibition of chondrocyte proliferation in the growth plates of long bones and manifests with proximal limb shortening, midface hypoplasia, and macrocephaly.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/23330244\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/23330244\"><span compiledata=\"\" htmlcontent=\"Pediatric aspects of skeletal dysplasia.\" class=\"ng-scope\"><p class=\"ng-scope\">Pediatric aspects of skeletal dysplasia.</p></span></a><!-- end ngI