e.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot;  draggable=&quot;false&quot; id=&quot;4731&quot; src=&quot;https://www.uworld.com/media/highresdefault/L29331.jpg&quot;  ></p><p><strong>Cystic fibrosis</strong> (CF) is a multisystem disorder in which a <strong>defective chloride channel</strong> (<a ng-click=&quot;showexhibit('4194',true);&quot;>CF transmembrane conductance regulator</a>) prevents the normal hydration of mucus.&amp;nbsp; Accumulation of <strong>thick, viscous secretions</strong> in the lungs leads to mucus plugging and progressive bronchiectasis, which manifest as obstructive lung disease (eg, hyperinflated lungs, barrel chest) and chronic hypoxia (eg, <a ng-click=&quot;showexhibit('51595',true);&quot;>digital clubbing</a>).&amp;nbsp; Gastrointestinal involvement can also lead to malabsorption with decreased body weight.</p><p>The abnormal airway secretions in patients with CF impair mucociliary clearance and allow <strong>colonization</strong> of bacteria such as <em>Haemophilus influenzae</em>, <em>Staphylococcus aureus</em>, and, over time, <strong>gram-negative rods</strong> such as <em><strong>Pseudomonas aeruginosa</strong></em> and <em>Burkholderia cepacia</em> complex.</p><p>The microenvironment is particularly suitable to mucoid <em>P aeruginosa</em> because localized hypoxia within the airway mucus causes the bacterium to lose motility and produce alginate, a polysaccharide involved in <strong>biofilm formation</strong>.&amp;nbsp; The biofilm acts as a <strong>protective matrix</strong> for the development of <em>Pseudomonas</em>&amp;nbsp;<strong>macrocolonies</strong>, which are difficult to eradicate and cause recurrent and <strong>persistent infection</strong>.</p><p><strong>(Choice A)</strong>&amp;nbsp; Natural killer cell deficiency is associated with severe, disseminated viral infections (eg, herpes simplex virus, cytomegalovirus).&amp;nbsp; This patient&amp;#39;s infiltrate in the right lower lobe and Gram stain findings are consistent with bacterial pneumonia, and she has no history of severe or recurrent viral infection.</p><p><strong>(Choice C)</strong>&amp;nbsp; Neutrophil free radical formation is impaired in chronic granulomatous disease, which is characterized by recurrent multiorgan (eg, lungs, liver, lymph nodes, skin) infections by catalase-positive organisms (eg, <em>Aspergillus</em>, <em>S aureus</em>).&amp;nbsp; In contrast, this patient has isolated, chronic pulmonary findings.&amp;nbsp; Moreover, CF results in excessive neutrophil free radical formation due to persistent pulmonary infection, which contributes to the progressive lung damage seen in these patients.</p><p><strong>(Choice D)</strong>&amp;nbsp; Inability to form the membrane attack complex occurs with terminal complement deficiency.&amp;nbsp; These patients have recurrent <em>Neisseria</em> meningitis, not lung infections.</p><p><strong>(Choice E)</strong>&amp;nbsp; Bacterial colonization in the lungs of patients with CF induces a massive infiltration of neutrophils, which results in the release of elastase, a protease that promotes bronchiectasis.&amp;nbsp; Therefore, chronic lung disease in CF is characterized by excessive, not reduced, endobronchial protease activity.</p><p><strong>Educational objective:</strong><br>Patients with cystic fibrosis have thick, viscous mucus that accumulates in the airways and promotes colonization of bacteria, particularly <em>Pseudomonas</em>.&amp;nbsp; This gram-negative rod can produce biofilms, which act as a protective matrix that allows bacterial macrocolonies to develop, causing persistent infection.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"4731\" src=\"image/L29331.jpg\"></p><p class=\"ng-scope\"><strong>Cystic fibrosis</strong> (CF) is a multisystem disorder in which a <strong>defective chloride channel</strong> (<a ng-click=\"showexhibit('4194',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L28038.jpg&quot;,&quot;title&quot;:&quot;L28038&quot;}]\">CF transmembrane conductance regulator</a>) prevents the normal hydration of mucus.&nbsp; Accumulation of <strong>thick, viscous secretions</strong> in the lungs leads to mucus plugging and progressive bronchiectasis, which manifest as obstructive lung disease (eg, hyperinflated lungs, barrel chest) and chronic hypoxia (eg, <a ng-click=\"showexhibit('51595',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L34235.png&quot;,&quot;title&quot;:&quot;L34235&quot;}]\">digital clubbing</a>).&nbsp; Gastrointestinal involvement can also lead to malabsorption with decreased body weight.</p><p class=\"ng-scope\">The abnormal airway secretions in patients with CF impair mucociliary clearance and allow <strong>colonization</strong> of bacteria such as <em>Haemophilus influenzae</em>, <em>Staphylococcus aureus</em>, and, over time, <strong>gram-negative rods</strong> such as <em><strong>Pseudomonas aeruginosa</strong></em> and <em>Burkholderia cepacia</em> complex.</p><p class=\"ng-scope\">The microenvironment is particularly suitable to mucoid <em>P aeruginosa</em> because localized hypoxia within the airway mucus causes the bacterium to lose motility and produce alginate, a polysaccharide involved in <strong>biofilm formation</strong>.&nbsp; The biofilm acts as a <strong>protective matrix</strong> for the development of <em>Pseudomonas</em>&nbsp;<strong>macrocolonies</strong>, which are difficult to eradicate and cause recurrent and <strong>persistent infection</strong>.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Natural killer cell deficiency is associated with severe, disseminated viral infections (eg, herpes simplex virus, cytomegalovirus).&nbsp; This patient's infiltrate in the right lower lobe and Gram stain findings are consistent with bacterial pneumonia, and she has no history of severe or recurrent viral infection.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Neutrophil free radical formation is impaired in chronic granulomatous disease, which is characterized by recurrent multiorgan (eg, lungs, liver, lymph nodes, skin) infections by catalase-positive organisms (eg, <em>Aspergillus</em>, <em>S aureus</em>).&nbsp; In contrast, this patient has isolated, chronic pulmonary findings.&nbsp; Moreover, CF results in excessive neutrophil free radical formation due to persistent pulmonary infection, which contributes to the progressive lung damage seen in these patients.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Inability to form the membrane attack complex occurs with terminal complement deficiency.&nbsp; These patients have recurrent <em>Neisseria</em> meningitis, not lung infections.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Bacterial colonization in the lungs of patients with CF induces a massive infiltration of neutrophils, which results in the release of elastase, a protease that promotes bronchiectasis.&nbsp; Therefore, chronic lung disease in CF is characterized by excessive, not reduced, endobronchial protease activity.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Patients with cystic fibrosis have thick, viscous mucus that accumulates in the airways and promotes colonization of bacteria, particularly <em>Pseudomonas</em>.&nbsp; This gram-negative rod can produce biofilms, which act as a protective matrix that allows bacterial macrocolonies to develop, causing persistent infection.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pathophysiology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pulmonary &amp; Critical Care</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Cystic fibrosis</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
