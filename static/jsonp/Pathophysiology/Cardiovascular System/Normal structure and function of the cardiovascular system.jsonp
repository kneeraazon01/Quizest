   <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;20069&quot; src=&quot;https://www.uworld.com/media/L45630.jpg&quot; ></p><p>This patient with a harsh, <strong>holosystolic murmur</strong> along the <strong>lower left sternal border</strong> most likely has a <strong>ventricular septal defect</strong> (VSD), an abnormal connection between the right ventricle (RV) and the left ventricle (LV).&amp;nbsp; Deoxygenated blood is normally delivered to the right atrium (RA) from the systemic circulation, and oxygenated blood is delivered to the left atrium (LA) via the pulmonary circulation.&amp;nbsp; Because of increased pressure on the left side of the heart compared to the right, a VSD allows for <strong>left-to-right shunting</strong> of blood.</p><p>The delivery of <strong>oxygenated blood to the RV</strong> via a VSD results in <strong>increased RV oxygen saturation</strong> (SaO<font size=&quot;2&quot;><sub>2</sub></font>) compared to that of a patient with an anatomically normal heart.&amp;nbsp; The SaO<font size=&quot;2&quot;><sub>2</sub></font> in the other cardiac chambers and systemic circulation remains unchanged, and patients are often&amp;nbsp;<strong>asymptomatic</strong> (eg, no shortness of breath, cyanosis, or clubbing).</p><p><strong>(Choice A)</strong>&amp;nbsp; In <a ng-click=&quot;showexhibit('784',true);&quot;>tricuspid atresia</a> (absent communication between the RA and the RV), an atrial septal defect (ASD) is always present, allowing deoxygenated blood to flow from the RA to the LA.&amp;nbsp; This decreases SaO<font size=&quot;2&quot;><sub>2</sub></font> in the LA, LV, and systemic circulation, causing neonatal cyanosis.&amp;nbsp; The little blood that reaches the lungs usually does so through a VSD, causing a relative increase in RV SaO<font size=&quot;2&quot;><sub>2</sub></font>.</p><p><strong>(Choice B)</strong>&amp;nbsp; The pattern of increased right-sided and decreased left-sided SaO<font size=&quot;2&quot;><sub>2 </sub></font>is seen in <a ng-click=&quot;showexhibit('785',true);&quot;>total anomalous pulmonary venous return</a>, a condition in which the pulmonary veins (ie, oxygenated blood) drain into the RA rather than the LA.</p><p><strong>(Choice C)</strong>&amp;nbsp; An <a ng-click=&quot;showexhibit('4472',true);&quot;>ASD</a> typically causes left-to-right shunting of oxygenated blood between the atria, increasing SaO<font size=&quot;2&quot;><sub>2</sub></font> in both the RA and RV.&amp;nbsp; In contrast to a VSD, an ASD typically causes a systolic ejection murmur at the left upper sternal border (pulmonic flow murmur).</p><p><strong>(Choice E)</strong>&amp;nbsp; Over time, increased pulmonary blood flow from left-to-right shunting via a VSD can reverse to right-to-left shunting (ie, <a ng-click=&quot;showexhibit('36026',true);&quot;>Eisenmenger syndrome</a>).&amp;nbsp; SaO<font size=&quot;2&quot;><sub>2</sub></font> on the right side will be normal, whereas that in the LV and systemic circulation is decreased because of mixing with deoxygenated blood from the RV.&amp;nbsp; However, Eisenmenger syndrome takes years to develop and typically presents with signs of right heart failure and cyanosis in adolescents/adults.</p><p><strong>Educational objective:</strong><br>A ventricular septal defect, classically characterized by a harsh, holosystolic murmur at the left lower sternal border, causes left-to-right shunting of oxygenated blood from the left ventricle to the right ventricle.&amp;nbsp; Therefore, right ventricular oxygen saturation is increased compared to normal.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"20069\" src=\"image/L45630.jpg\"></p><p class=\"ng-scope\">This patient with a harsh, <strong>holosystolic murmur</strong> along the <strong>lower left sternal border</strong> most likely has a <strong>ventricular septal defect</strong> (VSD), an abnormal connection between the right ventricle (RV) and the left ventricle (LV).&nbsp; Deoxygenated blood is normally delivered to the right atrium (RA) from the systemic circulation, and oxygenated blood is delivered to the left atrium (LA) via the pulmonary circulation.&nbsp; Because of increased pressure on the left side of the heart compared to the right, a VSD allows for <strong>left-to-right shunting</strong> of blood.</p><p class=\"ng-scope\">The delivery of <strong>oxygenated blood to the RV</strong> via a VSD results in <strong>increased RV oxygen saturation</strong> (SaO<font size=\"2\"><sub>2</sub></font>) compared to that of a patient with an anatomically normal heart.&nbsp; The SaO<font size=\"2\"><sub>2</sub></font> in the other cardiac chambers and systemic circulation remains unchanged, and patients are often&nbsp;<strong>asymptomatic</strong> (eg, no shortness of breath, cyanosis, or clubbing).</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; In <a ng-click=\"showexhibit('784',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L14167.jpg&quot;,&quot;title&quot;:&quot;L14167&quot;}]\">tricuspid atresia</a> (absent communication between the RA and the RV), an atrial septal defect (ASD) is always present, allowing deoxygenated blood to flow from the RA to the LA.&nbsp; This decreases SaO<font size=\"2\"><sub>2</sub></font> in the LA, LV, and systemic circulation, causing neonatal cyanosis.&nbsp; The little blood that reaches the lungs usually does so through a VSD, causing a relative increase in RV SaO<font size=\"2\"><sub>2</sub></font>.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; The pattern of increased right-sided and decreased left-sided SaO<font size=\"2\"><sub>2 </sub></font>is seen in <a ng-click=\"showexhibit('785',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L14172.jpg&quot;,&quot;title&quot;:&quot;L14172&quot;}]\">total anomalous pulmonary venous return</a>, a condition in which the pulmonary veins (ie, oxygenated blood) drain into the RA rather than the LA.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; An <a ng-click=\"showexhibit('4472',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L28491.jpg&quot;,&quot;title&quot;:&quot;L28491&quot;}]\">ASD</a> typically causes left-to-right shunting of oxygenated blood between the atria, increasing SaO<font size=\"2\"><sub>2</sub></font> in both the RA and RV.&nbsp; In contrast to a VSD, an ASD typically causes a systolic ejection murmur at the left upper sternal border (pulmonic flow murmur).</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Over time, increased pulmonary blood flow from left-to-right shunting via a VSD can reverse to right-to-left shunting (ie, <a ng-click=\"showexhibit('36026',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L29097.jpg&quot;,&quot;title&quot;:&quot;L29097&quot;}]\">Eisenmenger syndrome</a>).&nbsp; SaO<font size=\"2\"><sub>2</sub></font> on the right side will be normal, whereas that in the LV and systemic circulation is decreased because of mixing with deoxygenated blood from the RV.&nbsp; However, Eisenmenger syndrome takes years to develop and typically presents with signs of right heart failure and cyanosis in adolescents/adults.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>A ventricular septal defect, classically characterized by a harsh, holosystolic murmur at the left lower sternal border, causes left-to-right shunting of oxygenated blood from the left ventricle to the right ventricle.&nbsp; Therefore, right ventricular oxygen saturation is increased compared to normal.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/29261884\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/29261884\"><span compiledata=\"\" htmlcontent=\"Ventricular septal defect.\" class=\"ng-scope\"><p class=\"ng-scope\">Ven