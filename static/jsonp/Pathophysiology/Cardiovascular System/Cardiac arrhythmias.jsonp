         <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;21274&quot; src=&quot;https://www.uworld.com/media/L46716.png&quot; ></p><p>This elderly patient with recent lightheadedness and near-syncope has an ECG showing <strong>bradycardia</strong> with dropped P waves; these findings are consistent with <strong>sick sinus syndrome</strong>.&amp;nbsp; Sick sinus syndrome most commonly results from <strong>age-related degeneration</strong> of the <strong>sinoatrial node</strong>, which is located on the <a ng-click=&quot;showexhibit('1575',true);&quot;>right atrial wall</a> and is responsible for initiating normal cardiac conduction.&amp;nbsp; Impaired signaling from the sinoatrial node can markedly slow the rate of ventricular contraction, leading to <strong>reduced cardiac output</strong> and symptoms of dyspnea, fatigue, lightheadedness, presyncope, and syncope.</p><p>Typical ECG findings include bradycardia, <strong>sinus pauses</strong> (delayed P waves), and <strong>sinus arrest</strong> (prolonged delay of a P wave such that the P wave is dropped).&amp;nbsp; Because the atrioventricular node triggers conduction based on its own pacemaker if it does not receive a timely signal from the sinoatrial node, sinus arrest will lead to a <strong>junctional escape beat</strong> (narrow QRS complex preceded by a long pause and no P wave).&amp;nbsp; Sick sinus syndrome can also cause brief periods of atrial tachycardia (eg, atrial fibrillation) alternating with bradycardia.</p><p><strong>(Choice A)</strong>&amp;nbsp; The inferior portion of the interatrial septum contains the His bundle.&amp;nbsp; Conduction abnormalities in this region can manifest as varying degrees of atrioventricular block with prolongation of the PR interval (first degree), dropped QRS complexes (second degree), or dissociation of P waves and QRS complexes (third degree).</p><p><strong>(Choice B)</strong>&amp;nbsp; Conduction through the interventricular septum mostly involves the left and right bundle branches.&amp;nbsp; Conduction abnormalities in this region can lead to bundle branch blocks; the QRS complex is widened in <a ng-click=&quot;showexhibit('21275',true);&quot;>bundle branch block</a> due to impaired synchronization of ventricular depolarization.</p><p><strong>(Choice C)</strong>&amp;nbsp; After initiation in the sinoatrial node, conduction propagates through the cardiomyocytes of the left atrial wall to stimulate left atrial contraction.&amp;nbsp; Abnormal electrical foci can develop in the nearby pulmonary veins, which is a common trigger for <a ng-click=&quot;showexhibit('21276',true);&quot;>atrial fibrillation</a>.</p><p><strong>(Choice D)</strong>&amp;nbsp; Conduction abnormalities may originate in ischemic areas of the left ventricular free wall, leading to <a ng-click=&quot;showexhibit('21277',true);&quot;>ventricular tachycardia</a>.</p><p><strong>Educational objective:</strong><br>Sick sinus syndrome results from degeneration (usually age-related) of the sinoatrial node, leading to impaired conduction and reduced cardiac output with symptoms of dyspnea, fatigue, lightheadedness, presyncope, and syncope.&amp;nbsp; ECG typically demonstrates bradycardia with sinus pauses (delayed P waves), sinus arrest (dropped P waves), and junctional escape beats.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"21274\" src=\"image/L46716.png\"></p><p class=\"ng-scope\">This elderly patient with recent lightheadedness and near-syncope has an ECG showing <strong>bradycardia</strong> with dropped P waves; these findings are consistent with <strong>sick sinus syndrome</strong>.&nbsp; Sick sinus syndrome most commonly results from <strong>age-related degeneration</strong> of the <strong>sinoatrial node</strong>, which is located on the <a ng-click=\"showexhibit('1575',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L15146.jpg&quot;,&quot;title&quot;:&quot;L15146&quot;}]\">right atrial wall</a> and is responsible for initiating normal cardiac conduction.&nbsp; Impaired signaling from the sinoatrial node can markedly slow the rate of ventricular contraction, leading to <strong>reduced cardiac output</strong> and symptoms of dyspnea, fatigue, lightheadedness, presyncope, and syncope.</p><p class=\"ng-scope\">Typical ECG findings include bradycardia, <strong>sinus pauses</strong> (delayed P waves), and <strong>sinus arrest</strong> (prolonged delay of a P wave such that the P wave is dropped).&nbsp; Because the atrioventricular node triggers conduction based on its own pacemaker if it does not receive a timely signal from the sinoatrial node, sinus arrest will lead to a <strong>junctional escape beat</strong> (narrow QRS complex preceded by a long pause and no P wave).&nbsp; Sick sinus syndrome can also cause brief periods of atrial tachycardia (eg, atrial fibrillation) alternating with bradycardia.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; The inferior portion of the interatrial septum contains the His bundle.&nbsp; Conduction abnormalities in this region can manifest as varying degrees of atrioventricular block with prolongation of the PR interval (first degree), dropped QRS complexes (second degree), or dissociation of P waves and QRS complexes (third degree).</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Conduction through the interventricular septum mostly involves the left and right bundle branches.&nbsp; Conduction abnormalities in this region can lead to bundle branch blocks; the QRS complex is widened in <a ng-click=\"showexhibit('21275',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/U467.jpg&quot;,&quot;title&quot;:&quot;U467&quot;}]\">bundle branch block</a> due to impaired synchronization of ventricular depolarization.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; After initiation in the sinoatrial node, conduction propagates through the cardiomyocytes of the left atrial wall to stimulate left atrial contraction.&nbsp; Abnormal electrical foci can develop in the nearby pulmonary veins, which is a common trigger for <a ng-click=\"showexhibit('21276',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/U21660.png&quot;,&quot;title&quot;:&quot;U21660&quot;}]\">atrial fibrillation</a>.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Conduction abnormalities may originate in ischemic areas of the left ventricular free wall, leading to <a ng-click=\"showexhibit('21277',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/U26818.png&quot;,&quot;title&quot;:&quot;U26818&quot;}]\">ventricular tachycardia</a>.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Sick sinus syndrome results from degeneration (usually age-related) of the sinoatrial node, leading to impaired conduction and reduced cardiac output with symptoms of dyspnea, fatigue, lightheadedness, presyncope, and syncope.&nbsp; ECG typically demonstrates bradycardia with sinus pauses (delayed P waves), sinus arrest (dropped P waves), and junctional escape beats.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pathophysiology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Cardiovascular System</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Sick Sinus Syndrome</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
