n.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot;  draggable=&quot;false&quot; id=&quot;21548&quot; src=&quot;https://www.uworld.com/media/highresdefault/L47302.jpg&quot;  ></p><p>This patient&amp;#39;s red papules and pustules on the face are consistent with <strong>inflammatory acne</strong>.&amp;nbsp; The pathogenesis of acne involves the following:</p><ul><li><p><strong>Hyperkeratinization</strong> due to abnormal epithelial growth and differentiation of corneocytes leads to keratin plug formation in the pilosebaceous follicles.&amp;nbsp; These blocked follicles are referred to as comedones (ie, <a ng-click=&quot;showexhibit('45240',true);&quot;>whiteheads</a> and <a ng-click=&quot;showexhibit('51051',true);&quot;>blackheads</a>).</p></li><li><p>In response to <strong>androgen stimulation</strong> (eg, during pubertal adrenarche), <strong>sebaceous glands</strong> enlarge (not involute) and increase production of sebum, a lipid-rich substance that facilitates obstruction of pilosebaceous follicles <strong>(Choice A)</strong>.</p></li><li><p><em><strong>Cutibacterium acnes</strong></em>, an anaerobic bacteria that relies on sebum as a nutrient source, proliferates in occluded follicles, triggering an <strong>inflammatory response</strong> that results in the red papules and pustules characteristic of <a ng-click=&quot;showexhibit('51052',true);&quot;>nodulocystic acne</a>.</p></li></ul><p><strong>(Choice B)</strong>&amp;nbsp; Bacterial metabolism of apocrine secretions contributes to body odor.&amp;nbsp; Bromhidrosis is a condition characterized by excessive and offensive body odor.</p><p><strong>(Choice C)</strong>&amp;nbsp; Estrogen has antiandrogenic effects and inhibits sebum production.&amp;nbsp; For this reason, estrogen-containing oral contraceptives help to improve inflammatory acne.</p><p><strong>(Choice D)</strong>&amp;nbsp; As epithelial stem cells undergo mitosis in the basal layers of the epidermis, newer cells form and undergo differentiation as they migrate toward the surface, replacing epithelial cells that desquamate.&amp;nbsp; Increased epithelial mitosis and desquamation (ie, topical retinoids) helps extrude debris from comedones, limiting acne formation.</p><p><strong>Educational objective:</strong><br>Acne is characterized by the obstruction of pilosebaceous follicles (ie, comedones) due to hyperkeratinization and excessive sebum accumulation.&amp;nbsp; Androgens stimulate production of sebum, which serves as a nutrient source for <em>Cutibacterium acnes</em>.&amp;nbsp; Within the follicles, <em>C acnes</em> proliferation triggers an inflammatory response, resulting in formation of red papules and pustules.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"21548\" src=\"image/L47302.jpg\"></p><p class=\"ng-scope\">This patient's red papules and pustules on the face are consistent with <strong>inflammatory acne</strong>.&nbsp; The pathogenesis of acne involves the following:</p><ul class=\"ng-scope\"><li><p><strong>Hyperkeratinization</strong> due to abnormal epithelial growth and differentiation of corneocytes leads to keratin plug formation in the pilosebaceous follicles.&nbsp; These blocked follicles are referred to as comedones (ie, <a ng-click=\"showexhibit('45240',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L34133.png&quot;,&quot;title&quot;:&quot;L34133&quot;}]\">whiteheads</a> and <a ng-click=\"showexhibit('51051',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L11088.png&quot;,&quot;title&quot;:&quot;L11088&quot;}]\">blackheads</a>).</p></li><li><p>In response to <strong>androgen stimulation</strong> (eg, during pubertal adrenarche), <strong>sebaceous glands</strong> enlarge (not involute) and increase production of sebum, a lipid-rich substance that facilitates obstruction of pilosebaceous follicles <strong>(Choice A)</strong>.</p></li><li><p><em><strong>Cutibacterium acnes</strong></em>, an anaerobic bacteria that relies on sebum as a nutrient source, proliferates in occluded follicles, triggering an <strong>inflammatory response</strong> that results in the red papules and pustules characteristic of <a ng-click=\"showexhibit('51052',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L9904.png&quot;,&quot;title&quot;:&quot;L9904&quot;}]\">nodulocystic acne</a>.</p></li></ul><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Bacterial metabolism of apocrine secretions contributes to body odor.&nbsp; Bromhidrosis is a condition characterized by excessive and offensive body odor.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Estrogen has antiandrogenic effects and inhibits sebum production.&nbsp; For this reason, estrogen-containing oral contraceptives help to improve inflammatory acne.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; As epithelial stem cells undergo mitosis in the basal layers of the epidermis, newer cells form and undergo differentiation as they migrate toward the surface, replacing epithelial cells that desquamate.&nbsp; Increased epithelial mitosis and desquamation (ie, topical retinoids) helps extrude debris from comedones, limiting acne formation.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Acne is characterized by the obstruction of pilosebaceous follicles (ie, comedones) due to hyperkeratinization and excessive sebum accumulation.&nbsp; Androgens stimulate production of sebum, which serves as a nutrient source for <em>Cutibacterium acnes</em>.&nbsp; Within the follicles, <em>C acnes</em> proliferation triggers an inflammatory response, resulting in formation of red papules and pustules.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pathophysiology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;paddin