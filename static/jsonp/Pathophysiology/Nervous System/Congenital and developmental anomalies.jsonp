stModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot;  draggable=&quot;false&quot; id=&quot;52931&quot; src=&quot;https://www.uworld.com/media/highresdefault/L81422.jpg&quot;  ></p><p>This patient has <strong>myotonic dystrophy type 1</strong> (DM1), an autosomal dominant muscular dystrophy characterized by unstable expansion of cytosine-thymine-guanine <strong>(CTG) trinucleotide repeats</strong> in the dystrophia myotonica protein kinase (<em>DMPK</em>) gene.&amp;nbsp; This disorder particularly affects the brain, skeletal muscles, and myocardium, and the pathophysiology is likely due to toxic effects of mutant mRNAs with expanded CTG repeats.</p><p>Following transcription, the <strong>mutant mRNAs</strong> are not translated.&amp;nbsp; Rather, they form hairpin structures and <strong>accumulate</strong> in the nucleus.&amp;nbsp; The mutant mRNAs bind and sequester RNA-binding proteins such as muscleblind-like (MBNL) proteins.&amp;nbsp; These proteins normally play a role in mRNA splicing, and as more MBNL proteins are sequestered in the nucleus, <strong>aberrant splicing</strong> occurs, resulting in dysfunctional protein products.</p><p>Patients with DM1 demonstrate somatic mosaicism.&amp;nbsp; The CTG expansion is notably unstable in <strong>nondividing cells</strong>, including&amp;nbsp;<strong>skeletal muscle</strong>, the <strong>brain</strong>, and myocardium (repeat lengths are several-fold longer in these cells).&amp;nbsp; In addition, cellular mRNA content typically decreases with mitosis and increases when the cell is not dividing, therefore, cells that remain senescent (eg, <strong>myocardial cells</strong>) tend to accumulate more mutant mRNAs over time.&amp;nbsp; These processes help explain why patients with DM1 are more likely to develop symptoms as they age, such as cardiac conduction disturbances.</p><p><strong>(Choices A, B, C, and E)</strong>&amp;nbsp; CTG trinucleotide repeats are present in other cells in DM1 patients.&amp;nbsp; However, these proliferating cells (ie, airway basal cells, erythroblasts, intestinal epithelial cells, osteogenic cells) are not typically affected in DM1.&amp;nbsp; This may be due to having more stable and shorter CTG repeats, along with having less accumulation of mutant mRNAs compared to nondividing cells.</p><p><strong>Educational objective:</strong><br>Myotonic dystrophy type 1 is due to a cytosine-thymine-guanine trinucleotide repeat expansion in the dystrophia myotonica protein kinase (<em>DMPK</em>) gene, resulting in untranslatable mutant mRNAs.&amp;nbsp; The mutant mRNAs form hairpin structures and accumulate in the nucleus, sequestering certain RNA-binding proteins and leading&amp;nbsp;to aberrant splicing.&amp;nbsp; Nondividing cells of the brain, skeletal muscles, and myocardium are particularly affected.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"52931\" src=\"image/L81422.jpg\"></p><p class=\"ng-scope\">This patient has <strong>myotonic dystrophy type 1</strong> (DM1), an autosomal dominant muscular dystrophy characterized by unstable expansion of cytosine-thymine-guanine <strong>(CTG) trinucleotide repeats</strong> in the dystrophia myotonica protein kinase (<em>DMPK</em>) gene.&nbsp; This disorder particularly affects the brain, skeletal muscles, and myocardium, and the pathophysiology is likely due to toxic effects of mutant mRNAs with expanded CTG repeats.</p><p class=\"ng-scope\">Following transcription, the <strong>mutant mRNAs</strong> are not translated.&nbsp; Rather, they form hairpin structures and <strong>accumulate</strong> in the nucleus.&nbsp; The mutant mRNAs bind and sequester RNA-binding proteins such as muscleblind-like (MBNL) proteins.&nbsp; These proteins normally play a role in mRNA splicing, and as more MBNL proteins are sequestered in the nucleus, <strong>aberrant splicing</strong> occurs, resulting in dysfunctional protein products.</p><p class=\"ng-scope\">Patients with DM1 demonstrate somatic mosaicism.&nbsp; The CTG expansion is notably unstable in <strong>nondividing cells</strong>, including&nbsp;<strong>skeletal muscle</strong>, the <strong>brain</strong>, and myocardium (repeat lengths are several-fold longer in these cells).&nbsp; In addition, cellular mRNA content typically decreases with mitosis and increases when the cell is not dividing, therefore, cells that remain senescent (eg, <strong>myocardial cells</strong>) tend to accumulate more mutant mRNAs over time.&nbsp; These processes help explain why patients with DM1 are more likely to develop symptoms as they age, such as cardiac conduction disturbances.</p><p class=\"ng-scope\"><strong>(Choices A, B, C, and E)</strong>&nbsp; CTG trinucleotide repeats are present in other cells in DM1 patients.&nbsp; However, these proliferating cells (ie, airway basal cells, erythroblasts, intestinal epithelial cells, osteogenic cells) are not typically affected in DM1.&nbsp; This may be due to having more stable and shorter CTG repeats, along with having less accumulation of mutant mRNAs compared to nondividing cells.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Myotonic dystrophy type 1 is due to a cytosine-thymine-guanine trinucleotide repeat expansion in the dystrophia myotonica protein kinase (<em>DMPK</em>) gene, resulting in untranslatable mutant mRNAs.&nbsp; The mutant mRNAs form hairpin structures and accumulate in the nucleus, sequestering certain RNA-binding proteins and leading&nbsp;to aberrant splicing.&nbsp; Nondividing cells of the brain, skeletal muscles, and myocardium are particularly affected.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/25037086\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/25037086\"><span compiledata=\"\" htmlcontent=\"Myotonic dystrophy.\" class=\"ng-sco