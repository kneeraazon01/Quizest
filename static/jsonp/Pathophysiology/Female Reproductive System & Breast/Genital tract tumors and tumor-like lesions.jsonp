e.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><div id=&quot;div_t81119&quot;><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; class=&quot;table-default-style table-header-footer-style&quot;><tbody><tr><td colspan=&quot;2&quot;><p align=&quot;center&quot;><strong>Clinical manifestations of human papillomavirus infection</strong></p></td></tr><tr><td><p align=&quot;center&quot;><strong>HPV strains</strong></p></td><td><p align=&quot;center&quot;><strong>Presentation</strong></p></td></tr><tr><td><p align=&quot;center&quot;>1-4</p></td><td><p>Skin warts (verruca vulgaris)</p></td></tr><tr><td><p align=&quot;center&quot;>6, 11</p></td><td><p>Genital warts (condylomata acuminata)</p></td></tr><tr><td><p align=&quot;center&quot;>16, 18</p></td><td><p>Cervical, vaginal, vulvar &amp;amp; anal neoplasia</p></td></tr><tr><td colspan=&quot;2&quot;><p><strong>HPV</strong>= human papillomavirus.</p></td></tr></tbody></table></div></p><p><strong>Squamous cell carcinoma (SCC)</strong>, the most common type of cervical cancer, arises from the squamocolumnar junction of the endocervix.&amp;nbsp; <strong>Cervical intraepithelial neoplasia (CIN)</strong> is <strong>carcinoma in situ</strong> and precedes SCC.&amp;nbsp; CIN represents a wide spectrum of dysplastic changes of the cervical epithelium that occur under the influence of <strong>human papillomavirus (HPV) infection</strong>.&amp;nbsp; CIN is usually transient in young healthy women.&amp;nbsp; However, patients with persistent HPV infection or untreated high-grade CIN are at risk for invasive cervical cancer.</p><p>HPV strains 6 and 11 typically cause condylomata acuminata (genital warts) and have low malignancy potential.&amp;nbsp; However, DNA from <strong>high-risk strains (ie, 16, 18, and 31)</strong> is integrated into the human cell genome, leading to overexpression of <strong>viral oncogenes E6 and E7</strong>.&amp;nbsp; E6 binds protein p53 and increases its degradation, whereas E7 binds to the retinoblastoma (<em>RB1</em>) gene and displaces transcription factors normally bound by pRB, the tumor suppressor protein product of <em>RB</em>.</p><p>Risk factors for developing cervical cancer are related to the risk of acquiring HPV infection.&amp;nbsp; Although HPV can be transmitted by skin-to-skin contact, oncogenic strains are spread primarily by sexual contact.&amp;nbsp; Therefore, this patient&amp;#39;s most significant risk factors are a history of multiple sexual partners and the <strong>lack of barrier contraceptive</strong> (eg, condom) use.</p><p><strong>(Choice A)</strong>&amp;nbsp; Alcohol consumption increases the risk of liver and breast cancers, not cervical cancer.</p><p><strong>(Choices B, F, and G)</strong>&amp;nbsp; Nulliparity, early menarche, and obesity increase the risk of endometrial cancer.&amp;nbsp; These factors are associated with increased estrogen stimulation of the endometrium.</p><p><strong>(Choice C)</strong>&amp;nbsp; Family history of breast cancer is a risk factor for developing breast malignancy.&amp;nbsp; Breast cancer is not associated with cervical cancer, as the latter is usually due to a persistent, oncogenic HPV infection.</p><p><strong>(Choice D)</strong>&amp;nbsp; A plantar wart is usually caused by HPV strains 1-4; however, these strains are not typically carcinogenic and are not associated with cervical dysplasia and invasive cervical cancer.</p><p><strong>(Choice H)</strong>&amp;nbsp; Douching is not a risk factor for HPV infection or cervical dysplasia/cancer.&amp;nbsp; However, it is a risk factor for bacterial vaginosis.&amp;nbsp; Douching disturbs the vaginal ecosystem, resulting in overgrowth of <em>Gardnerella vaginalis</em>.</p><p><strong>Educational objective:</strong><br>Consistent use of barrier contraceptives is extremely important for preventing sexually transmitted infections, including human papillomavirus (HPV).&amp;nbsp; Patients infected with carcinogenic strains of HPV (ie, 16, 18, and 31) are at risk of squamous cell carcinoma of the cervix.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"></p><div id=\"div_t81119\" class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" class=\"table-default-style table-header-footer-style\"><tbody><tr><td colspan=\"2\"><p align=\"center\"><strong>Clinical manifestations of human papillomavirus infection</strong></p></td></tr><tr><td><p align=\"center\"><strong>HPV strains</strong></p></td><td><p align=\"center\"><strong>Presentation</strong></p></td></tr><tr><td><p align=\"center\">1-4</p></td><td><p>Skin warts (verruca vulgaris)</p></td></tr><tr><td><p align=\"center\">6, 11</p></td><td><p>Genital warts (condylomata acuminata)</p></td></tr><tr><td><p align=\"center\">16, 18</p></td><td><p>Cervical, vaginal, vulvar &amp; anal neoplasia</p></td></tr><tr><td colspan=\"2\"><p><strong>HPV</strong>= human papillomavirus.</p></td></tr></tbody></table></div><p class=\"ng-scope\"></p><p class=\"ng-scope\"><strong>Squamous cell carcinoma (SCC)</strong>, the most common type of cervical cancer, arises from the squamocolumnar junction of the endocervix.&nbsp; <strong>Cervical intraepithelial neoplasia (CIN)</strong> is <strong>carcinoma in situ</strong> and precedes SCC.&nbsp; CIN represents a wide spectrum of dysplastic changes of the cervical epithelium that occur under the influence of <strong>human papillomavirus (HPV) infection</strong>.&nbsp; CIN is usually transient in young healthy women.&nbsp; However, patients with persistent HPV infection or untreated high-grade CIN are at risk for invasive cervical cancer.</p><p class=\"ng-scope\">HPV strains 6 and 11 typically cause condylomata acuminata (genital warts) and have low malignancy potential.&nbsp; However, DNA from <strong>high-risk strains (ie, 16, 18, and 31)</strong> is integrated into the human cell genome, leading to overexpression of <strong>viral oncogenes E6 and E7</strong>.&nbsp; E6 binds protein p53 and increases its degradation, whereas E7 binds to the retinoblastoma (<em>RB1</em>) gene and displaces transcription factors normally bound by pRB, the tumor suppressor protein product of <em>RB</em>.</p><p class=\"ng-scope\">Risk factors for developing cervical cancer are related to the risk of acquiring HPV infection.&nbsp; Although HPV can be transmitted by skin-to-skin contact, oncogenic strains are spread primarily by sexual contact.&nbsp; Therefore, this patient's most significant risk factors are a history of multiple sexual partners and the <strong>lack of barrier contraceptive</strong> (eg, condom) use.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Alcohol consumption increases the risk of liver and breast cancers, not cervical cancer.</p><p class=\"ng-scope\"><strong>(Choices B, F, and G)</strong>&nbsp; Nulliparity, early menarche, and obesity increase the risk of endometrial cancer.&nbsp; These factors are associated with increased estrogen stimulation of the endometrium.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Family history of breast cancer is a risk factor for developing breast malignancy.&nbsp; Breast cancer is not associated with cervical cancer, as the latter is usually due to a persistent, oncogenic HPV infection.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; A plantar wart is usually caused by HPV strains 1-4; however, these strains are not typically carcinogenic and are not associated with cervical dysplasia and invasive cervical cancer.</p><p class=\"ng-scope\"><strong>(Choice H)</strong>&nbsp; Douching is not a risk factor for HPV infection or cervical dysplasia/cancer.&nbsp; However, it is a risk factor for bacterial vaginosis.&nbsp; Douching disturbs the vaginal ecosystem, resulting in overgrowth of <em>Gardnerella vaginalis</em>.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Consistent use of barrier contraceptives is extremely important for preventing sexually transmitted infections, including human papillomavirus (HPV).&nbsp; Patients infected with carcinogenic strains of HPV (ie, 16, 18, and 31) are at risk of squamous cell carcinoma of the cervix.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/26244052\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/26244052\"><span compiledata=\"\" htmlcontent=\"HPV infection and cervical neoplasia: associated risk factors.\" class=\"ng-scope\"><p class=\"ng-scope\">HPV infection and cervical neoplasia: associated risk factors.</p></span></a><!-- end ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal -->\n                                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal -->\n                                </li><!-- end ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/23090560\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/23090560\"><span compiledata=\"\" htmlcontent=\"ACOG Practice Bulletin No.131: Screening for cervical cancer.\" class=\"ng-scope\"><p class=\"ng-scope\">ACOG Practice Bulletin No.131: Screening for cervical cancer.</p></span></a><!-- end ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal -->\n                                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal -->\n                                </li><!-- end ngRepeat: questionReference in currentQuestion.questionMappingReferencesList -->\n                            </ul>\n                        </div>\n               