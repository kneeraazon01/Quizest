-->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><div id=&quot;div_t44037&quot;><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; class=&quot;table-default-style&quot;><tbody><tr><td><p align=&quot;center&quot;><strong>Most common pathogens causing nosocomial bloodstream infections</strong></p></td></tr><tr><td><ul><li>Coagulase-negative staphylococci</li><li><em>Staphylococcus aureus</em></li><li>Enterococci</li><li><em>Candida</em> species</li></ul></td></tr></tbody></table></div><p>Approximately 250,000 <strong>nosocomial bloodstream infections</strong> (BSIs) are estimated to occur in the United States each year.&amp;nbsp; Prior to the 1980s, the leading cause was gram-negative aerobic organisms such as <em>Escherichia coli</em> that spread from primary infections in the genitourinary or gastrointestinal tract.&amp;nbsp; However, the increasing use of <strong>intravascular catheters</strong>, which provide a portal of entry for <strong>skin flora</strong> to reach the bloodstream, has led to a shift in the underlying microbiology of BSI; most cases are now caused by skin commensals such as <strong>coagulase-negative staphylococci</strong> and <strong><em>Staphylococcus aureus</em></strong>.</p><p>BSI is most common in those who have <strong>long-term</strong> (&amp;gt;12 days) central venous access as this provides sufficient time for skin flora to colonize the internal/external lumen of the catheter, to replicate within a biofilm of host (fibrin) and bacterial (glycoprotein) components, and to subsequently spread to the bloodstream.&amp;nbsp; BSI should be suspected when a patient with an intravascular catheter develops new-onset fever or bacteremia.</p><p><strong>(Choice A)</strong>&amp;nbsp; Although <em>S aureus</em> is a leading cause of infective endocarditis in patients with artificial heart valves, bacteremia is much more commonly caused by intravascular devices.</p><p><strong>(Choice B)</strong>&amp;nbsp; The increased use of broad-spectrum antibiotics is associated with the development of drug-resistant organisms, including methicillin-resistant <em>S aureus</em> (MRSA).&amp;nbsp; Although MRSA is a common cause of skin abscess and necrotizing pneumonia, the majority of <em>S aureus</em> bacteremia is caused by intravascular catheters.</p><p><strong>(Choice C)</strong>&amp;nbsp; Injection drug use is a major risk factor for <em>S aureus</em> abscess, bacteremia, and infective endocarditis.&amp;nbsp; However, injection drug use causes far fewer BSIs than intravenous catheters.</p><p><strong>(Choice D)</strong>&amp;nbsp; Immunosuppression increases the risk of BSI due to reduced immunosurveillance.&amp;nbsp; However, BSI is far more often due to the use of intravascular catheters.</p><p><strong>(Choice E)</strong>&amp;nbsp; Indwelling urinary catheters increase the risk of urinary infection with subsequent hematogenous spread of the organism.&amp;nbsp; However, most cases are caused by gram-negative enteric organisms such as <em>E coli</em>.</p><p><strong>Educational objective:</strong><br>Nosocomial bloodstream infections are usually associated with intravascular catheters, which allow skin commensals such as <em>Staphylococcus aureus</em> and coagulase-negative staphylococci to enter the bloodstream.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><div id=\"div_t44037\" class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" class=\"table-default-style\"><tbody><tr><td><p align=\"center\"><strong>Most common pathogens causing nosocomial bloodstream infections</strong></p></td></tr><tr><td><ul><li>Coagulase-negative staphylococci</li><li><em>Staphylococcus aureus</em></li><li>Enterococci</li><li><em>Candida</em> species</li></ul></td></tr></tbody></table></div><p class=\"ng-scope\">Approximately 250,000 <strong>nosocomial bloodstream infections</strong> (BSIs) are estimated to occur in the United States each year.&nbsp; Prior to the 1980s, the leading cause was gram-negative aerobic organisms such as <em>Escherichia coli</em> that spread from primary infections in the genitourinary or gastrointestinal tract.&nbsp; However, the increasing use of <strong>intravascular catheters</strong>, which provide a portal of entry for <strong>skin flora</strong> to reach the bloodstream, has led to a shift in the underlying microbiology of BSI; most cases are now caused by skin commensals such as <strong>coagulase-negative staphylococci</strong> and <strong><em>Staphylococcus aureus</em></strong>.</p><p class=\"ng-scope\">BSI is most common in those who have <strong>long-term</strong> (&gt;12 days) central venous access as this provides sufficient time for skin flora to colonize the internal/external lumen of the catheter, to replicate within a biofilm of host (fibrin) and bacterial (glycoprotein) components, and to subsequently spread to the bloodstream.&nbsp; BSI should be suspected when a patient with an intravascular catheter develops new-onset fever or bacteremia.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Although <em>S aureus</em> is a leading cause of infective endocarditis in patients with artificial heart valves, bacteremia is much more commonly caused by intravascular devices.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; The increased use of broad-spectrum antibiotics is associated with the development of drug-resistant organisms, including methicillin-resistant <em>S aureus</em> (MRSA).&nbsp; Although MRSA is a common cause of skin abscess and necrotizing pneumonia, the majority of <em>S aureus</em> bacteremia is caused by intravascular catheters.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Injection drug use is a major risk factor for <em>S aureus</em> abscess, bacteremia, and infective endocarditis.&nbsp; However, injection drug use causes far fewer BSIs than intravenous catheters.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Immunosuppression increases the risk of BSI due to reduced immunosurveillance.&nbsp; However, BSI is far more often due to the use of intravascular catheters.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Indwelling urinary catheters increase the risk of urinary infection with subsequent hematogenous spread of the organism.&nbsp; However, most cases are caused by gram-negative enteric organisms such as <em>E coli</em>.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Nosocomial bloodstream infections are usually associated with intravascular catheters, which allow skin commensals such as <em>Staphylococcus aureus</em> and coagulase-negative staphylococci to enter the bloodstream.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Microbiology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Infectious Diseases</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Catheter related bloodstream infection</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                   