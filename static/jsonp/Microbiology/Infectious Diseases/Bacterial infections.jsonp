                                      \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><strong><em>Bacillus anthracis</em></strong> is a large, nonmotile, nonhemolytic, gram-positive rod that forms colonies in culture with multiple curled extensions at the edges that resemble a &amp;#x22;<strong>Medusa head</strong>.&amp;#x22;&amp;nbsp; Because the organism proliferates in soil contaminated with manure, pasture-grazing herbivores such as sheep, cattle, and horses are at greatest risk for infection.&amp;nbsp; However, transmission to humans occasionally occurs when <em>B anthracis</em>&amp;nbsp;<strong>spores</strong> are inhaled, ingested, or inoculated into the skin.</p><p>Inhalation of spores into the alveoli causes <strong>pulmonary anthrax</strong>.&amp;nbsp; Alveolar macrophages phagocytose the spores and bring them to mediastinal lymph nodes, where the spores germinate into vegetative rods and multiply.&amp;nbsp; The vegetative form of the bacteria contains a <strong>polypeptide capsule</strong> composed of <strong>D-glutamic acid</strong>, which is antiphagocytic.&amp;nbsp; <em>B anthracis</em> rods also produce a potent <strong>exotoxin complex</strong> composed of edema factor, lethal factor, and protective antigen, which enter host cells and cause cell death.</p><p>Initial manifestations of infection are nonspecific (eg, myalgia, fever, malaise), but proliferation of the bacteria in the mediastinum eventually causes <strong>hemorrhagic mediastinitis</strong> (often visible on chest x-ray as a widened mediastinum).&amp;nbsp; Subsequent dissemination through the bloodstream causes shock, brain edema/hemorrhage, and death.&amp;nbsp; Although pulmonary anthrax is quite rare, it can be seen in individuals who handle contaminated hides or animal hair (&amp;#x22;woolsorter&amp;#39;s disease&amp;#x22;) or those exposed to aerosolized <strong>biologic weapons</strong> (eg, opening contaminated mail).</p><p><strong>(Choice B)</strong>&amp;nbsp; Many bacteria (eg, <em>Streptococcus pneumoniae</em>) are resistant to phagocytosis due to the presence of a polysaccharide capsule.&amp;nbsp; However, <em>B anthracis</em> has a polypeptide, not polysaccharide, capsule.</p><p><strong>(Choice C)</strong>&amp;nbsp; <em>Staphylococcus aureus</em> produces protein A, which binds the Fc portion of IgG.&amp;nbsp; This inhibits opsonization and complement-mediated killing of the bacteria.</p><p><strong>(Choice D)</strong>&amp;nbsp; Intracellular polyphosphate granules are characteristic of <em>Corynebacterium diphtheriae</em>; they can be visualized on microscopy after growing the pathogen on Loeffler medium and staining it with methylene blue.</p><p><strong>(Choice E)</strong>&amp;nbsp; Peritrichous flagella (flagella distributed uniformly over the entire surface of a bacterial cell) are not present on the surface of <em>B anthracis</em> as this organism is nonmotile.&amp;nbsp; Peritrichous flagella are characteristic of highly motile organisms such as <em>Proteus mirabilis</em>.</p><p><strong>Educational objective:</strong><br><em>Bacillus anthracis</em> is a large, sporulating, gram-positive rod that is encased in an antiphagocytic polypeptide capsule composed of D-glutamic acid.&amp;nbsp; Inhalation of <em>B anthracis</em> spores can cause pulmonary anthrax, which is usually characterized by nonspecific symptoms followed by hemorrhagic mediastinitis, shock, and death.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><strong><em>Bacillus anthracis</em></strong> is a large, nonmotile, nonhemolytic, gram-positive rod that forms colonies in culture with multiple curled extensions at the edges that resemble a \"<strong>Medusa head</strong>.\"&nbsp; Because the organism proliferates in soil contaminated with manure, pasture-grazing herbivores such as sheep, cattle, and horses are at greatest risk for infection.&nbsp; However, transmission to humans occasionally occurs when <em>B anthracis</em>&nbsp;<strong>spores</strong> are inhaled, ingested, or inoculated into the skin.</p><p class=\"ng-scope\">Inhalation of spores into the alveoli causes <strong>pulmonary anthrax</strong>.&nbsp; Alveolar macrophages phagocytose the spores and bring them to mediastinal lymph nodes, where the spores germinate into vegetative rods and multiply.&nbsp; The vegetative form of the bacteria contains a <strong>polypeptide capsule</strong> composed of <strong>D-glutamic acid</strong>, which is antiphagocytic.&nbsp; <em>B anthracis</em> rods also produce a potent <strong>exotoxin complex</strong> composed of edema factor, lethal factor, and protective antigen, which enter host cells and cause cell death.</p><p class=\"ng-scope\">Initial manifestations of infection are nonspecific (eg, myalgia, fever, malaise), but proliferation of the bacteria in the mediastinum eventually causes <strong>hemorrhagic mediastinitis</strong> (often visible on chest x-ray as a widened mediastinum).&nbsp; Subsequent dissemination through the bloodstream causes shock, brain edema/hemorrhage, and death.&nbsp; Although pulmonary anthrax is quite rare, it can be seen in individuals who handle contaminated hides or animal hair (\"woolsorter's disease\") or those exposed to aerosolized <strong>biologic weapons</strong> (eg, opening contaminated mail).</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Many bacteria (eg, <em>Streptococcus pneumoniae</em>) are resistant to phagocytosis due to the presence of a polysaccharide capsule.&nbsp; However, <em>B anthracis</em> has a polypeptide, not polysaccharide, capsule.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; <em>Staphylococcus aureus</em> produces protein A, which binds the Fc portion of IgG.&nbsp; This inhibits opsonization and complement-mediated killing of the bacteria.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Intracellular polyphosphate granules are characteristic of <em>Corynebacterium diphtheriae</em>; they can be visualized on microscopy after growing the pathogen on Loeffler medium and staining it with methylene blue.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Peritrichous flagella (flagella distributed uniformly over the entire surface of a bacterial cell) are not present on the surface of <em>B anthracis</em> as this organism is nonmotile.&nbsp; Peritrichous flagella are characteristic of highly motile organisms such as <em>Proteus mirabilis</em>.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br><em>Bacillus anthracis</em> is a large, sporulating, gram-positive rod that is encased in an antiphagocytic polypeptide capsule composed of D-glutamic acid.&nbsp; Inhalation of <em>B anthracis</em> spores can cause pulmonary anthrax, which is usually characterized by nonspecific symptoms followed by hemorrhagic mediastinitis, shock, and death.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Microbiology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Infectious Diseases</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Anthrax</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
