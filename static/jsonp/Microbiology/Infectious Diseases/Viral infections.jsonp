0 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><div id=&quot;div_t31509&quot;><div align=&quot;center&quot;><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; class=&quot;table-default-style table-header-footer-style&quot;><tbody><tr><td colspan=&quot;2&quot;><p align=&quot;center&quot;><strong>Progressive multifocal leukoencephalopathy</strong></p></td></tr><tr><td><p align=&quot;center&quot;><strong>Epidemiology</strong></p></td><td><ul><li>JC virus reactivation</li><li>Severe immunosuppression (eg, untreated AIDS)</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Manifestations</strong></p></td><td><ul><li>Slowly progressive</li><li>Confusion, paresis, ataxia, seizure</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Diagnosis</strong></p></td><td><ul><li>MRI of the brain: asymmetric white matter lesions; no enhancement/edema</li><li>Lumbar puncture: CSF PCR positive for JC virus</li><li>Brain biopsy (rarely needed)</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Treatment</strong></p></td><td><ul><li>Often fatal</li><li>If HIV positive: antiretroviral therapy</li></ul></td></tr><tr><td colspan=&quot;2&quot;><p><strong>CSF</strong> = cerebrospinal fluid; <strong>PCR</strong> =&amp;nbsp;polymerase chain reaction.</p></td></tr></tbody></table></div></div><p><strong>JC virus</strong> is a polyomavirus, typically acquired in childhood, that causes life-long, latent infection of the kidney and lymphoid organs.&amp;nbsp; Most adults have serologic evidence of exposure but remain asymptomatic.&amp;nbsp; However, those with severe immunosuppression, particularly patients with <strong>advanced AIDS</strong>, are at risk for <strong>viral reactivation</strong> with spread to the brain.&amp;nbsp; The virus attacks <strong>oligodendrocytes</strong>, the cells that produce myelin, causing a severe demyelinating disease called <strong>progressive multifocal leukoencephalopathy</strong> (PML).</p><p>PML typically presents with slowly worsening confusion, ataxia, motor deficits, and seizure.&amp;nbsp; Brain MRI usually shows multifocal areas of <a ng-click=&quot;showexhibit('14552',true);&quot;>white matter demyelination</a> with <strong>no mass effect or enhancement</strong>.&amp;nbsp; Diagnosis is confirmed with JC virus PCR testing of the cerebrospinal fluid; brain biopsy is rarely required.&amp;nbsp; There is no specific treatment for PML, but patients with HIV who develop the disease are treated with antiretroviral therapy to reduce immune suppression.&amp;nbsp; This often helps stabilize neurologic symptoms.</p><p><strong>(Choice A)</strong>&amp;nbsp; Patients with advanced AIDS can occasionally develop cytomegalovirus encephalitis, which usually presents with rapidly progressive altered mental state and focal neurologic deficits.&amp;nbsp; However, this condition is highly inflammatory so brain imaging typically shows enhancing lesions.</p><p><strong>(Choice B)</strong>&amp;nbsp; Epstein-Barr virus reactivation can cause primary CNS lymphoma in patients with advanced AIDS.&amp;nbsp; Patients usually have focal neurologic symptoms and altered mental state.&amp;nbsp; However, brain MRI normally reveals a <a ng-click=&quot;showexhibit('16389',true);&quot;>single enhancing lesion</a> with mass effect.</p><p><strong>(Choice C)</strong>&amp;nbsp; Herpes simplex virus encephalitis usually causes focal neurologic deficits and altered mental status.&amp;nbsp; However, symptom onset is usually rapid (&amp;lt;1 week), most patients have fever, and brain imaging usually reveals an enhancing <a ng-click=&quot;showexhibit('16390',true);&quot;>temporal lobe lesion</a> with mass effect.</p><p><strong>(Choice E)</strong>&amp;nbsp; Measles can cause subacute, sclerosing panencephalitis several years after initial infection.&amp;nbsp; This fatal, progressive disease is characterized by slowly worsening neurologic symptoms and multifocal, enhancing, white matter lesions.</p><p><strong>(Choice F)</strong>&amp;nbsp; <em>Toxoplasma gondii</em> is usually acquired by exposure to cat feces.&amp;nbsp; Most patients are asymptomatic and have a life-long dormant infection.&amp;nbsp; However, patients with advanced AIDS can have reactivation leading to meningoencephalitis (eg, headache, confusion, fever).&amp;nbsp; Brain imaging usually shows <a ng-click=&quot;showexhibit('3142',true);&quot;>several ring-enhancing lesions</a> with surrounding edema.</p><p><strong>Educational objective:</strong><br>Patients with advanced AIDS can have reactivation of JC virus, which causes progressive multifocal leukoencephalopathy.&amp;nbsp; This condition usually presents with slowly progressive confusion, ataxia, and motor deficits.&amp;nbsp; Brain MRI reveals multifocal areas of white matter demyelination with no mass effect or enhancement.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><div id=\"div_t31509\" class=\"ng-scope\"><div align=\"center\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" class=\"table-default-style table-header-footer-style\"><tbody><tr><td colspan=\"2\"><p align=\"center\"><strong>Progressive multifocal leukoencephalopathy</strong></p></td></tr><tr><td><p align=\"center\"><strong>Epidemiology</strong></p></td><td><ul><li>JC virus reactivation</li><li>Severe immunosuppression (eg, untreated AIDS)</li></ul></td></tr><tr><td><p align=\"center\"><strong>Manifestations</strong></p></td><td><ul><li>Slowly progressive</li><li>Confusion, paresis, ataxia, seizure</li></ul></td></tr><tr><td><p align=\"center\"><strong>Diagnosis</strong></p></td><td><ul><li>MRI of the brain: asymmetric white matter lesions; no enhancement/edema</li><li>Lumbar puncture: CSF PCR positive for JC virus</li><li>Brain biopsy (rarely needed)</li></ul></td></tr><tr><td><p align=\"center\"><strong>Treatment</strong></p></td><td><ul><li>Often fatal</li><li>If HIV positive: antiretroviral therapy</li></ul></td></tr><tr><td colspan=\"2\"><p><strong>CSF</strong> = cerebrospinal fluid; <strong>PCR</strong> =&nbsp;polymerase chain reaction.</p></td></tr></tbody></table></div></div><p class=\"ng-scope\"><strong>JC virus</strong> is a polyomavirus, typically acquired in childhood, that causes life-long, latent infection of the kidney and lymphoid organs.&nbsp; Most adults have serologic evidence of exposure but remain asymptomatic.&nbsp; However, those with severe immunosuppression, particularly patients with <strong>advanced AIDS</strong>, are at risk for <strong>viral reactivation</strong> with spread to the brain.&nbsp; The virus attacks <strong>oligodendrocytes</strong>, the cells that produce myelin, causing a severe demyelinating disease called <strong>progressive multifocal leukoencephalopathy</strong> (PML).</p><p class=\"ng-scope\">PML typically presents with slowly worsening confusion, ataxia, motor deficits, and seizure.&nbsp; Brain MRI usually shows multifocal areas of <a ng-click=\"showexhibit('14552',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L487.jpg&quot;,&quot;title&quot;:&quot;L487&quot;}]\">white matter demyelination</a> with <strong>no mass effect or enhancement</strong>.&nbsp; Diagnosis is confirmed with JC virus PCR testing of the cerebrospinal fluid; brain biopsy is rarely required.&nbsp; There is no specific treatment for PML, but patients with HIV who develop the disease are treated with antiretroviral therapy to reduce immune suppression.&nbsp; This often helps stabilize neurologic symptoms.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Patients with advanced AIDS can occasionally develop cytomegalovirus encephalitis, which usually presents with rapidly progressive altered mental state and focal neurologic deficits.&nbsp; However, this condition is highly inflammatory so brain imaging typically shows enhancing lesions.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Epstein-Barr virus reactivation can cause primary CNS lymphoma in patients with advanced AIDS.&nbsp; Patients usually have focal neurologic symptoms and altered mental state.&nbsp; However, brain MRI normally reveals a <a ng-click=\"showexhibit('16389',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L472.jpg&quot;,&quot;title&quot;:&quot;L472&quot;}]\">single enhancing lesion</a> with mass effect.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Herpes simplex virus encephalitis usually causes focal neurologic deficits and altered mental status.&nbsp; However, symptom onset is usually rapid (&lt;1 week), most patients have fever, and brain imaging usually reveals an enhancing <a ng-click=\"showexhibit('16390',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L8955.jpg&quot;,&quot;title&quot;:&quot;L8955&quot;}]\">temporal lobe lesion</a> with mass effect.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Measles can cause subacute, sclerosing panencephalitis several years after initial infection.&nbsp; This fatal, progressive disease is characterized by slowly worsening neurologic symptoms and multifocal, enhancing, white matter lesions.</p><p class=\"ng-scope\"><strong>(Choice F)</strong>&nbsp; <em>Toxoplasma gondii</em> is usually acquired by exposure to cat feces.&nbsp; Most patients are asymptomatic and have a life-long dormant infection.&nbsp; However, patients with advanced AIDS can have reactivation leading to meningoencephalitis (eg, headache, confusion, fever).&nbsp; Brain imaging usually shows <a ng-click=\"showexhibit('3142',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L25958.jpg&quot;,&quot;title&quot;:&quot;L25958&quot;}]\">several ring-enhancing lesions</a> with surrounding edema.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Patients with advanced AIDS can have reactivation of JC virus, which causes progressive multifocal leukoencephalopathy.&nbsp; This condition usually presents with slowly progressive confusion, ataxia, and motor deficits.&nbsp; Brain MRI reveals multifocal areas of white matter demyelination with no mass effect or enhancement.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Microbiology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Infectious Diseases</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Progressive multifocal leukoencephalopathy</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
