         \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p>This patient, who developed a serpiginous, pruritic rash shortly after walking barefoot on a beach, likely has <strong>hookworm infection</strong>.&amp;nbsp; Hookworms spread when <strong>egg-containing feces</strong> are deposited into shady, warm, moist sand or soil.&amp;nbsp; Eggs hatch into larvae and are transmitted when they come into <strong>direct contact with human skin</strong> (eg, when walking barefoot).</p><p>Dermal penetration is usually marked by a <strong>pruritic maculopapular lesion</strong> (&amp;#x22;ground itch&amp;#x22;) at the site of larval entry (eg, foot, toe webs).&amp;nbsp; Spread of larvae through the adjacent dermal tissue can lead to the formation of migrating, reddish-brown, <strong>serpiginous tracks</strong>.&amp;nbsp; Deeper spread may occur, depending on the species of infecting hookworm, as follows:</p><ul><li><p><a ng-click=&quot;showexhibit('14433',true);&quot;>Human hookworms</a> (eg, <em>Necator americanus</em>, <em>A duodenale</em>) penetrate the basement membrane and spread from the dermis to the bloodstream.&amp;nbsp; They rupture into the alveoli and are coughed up and swallowed into the small intestine.&amp;nbsp; There, larvae mature into adult hookworms that feed on blood from the duodenal mucosa (often causing iron deficiency anemia) and shed up to 10,000 eggs/day, thereby spreading the infection to others.</p></li><li><p>Cat and dog hookworms (eg, <em>Ancylostoma braziliense</em>, <em>A caninum</em>) cause a dermal eruption in humans but are unable to penetrate the cutaneous basement membrane and spread to deeper tissues.&amp;nbsp; Therefore, larvae do not mature into adult hookworms or spread from human-to-human (humans are incidental hosts).</p></li></ul><p><strong>(Choice A)</strong>&amp;nbsp; A brown recluse spider bite usually appears as a red plaque or papule with central pallor; it may develop a dark central eschar that ulcerates due to necrosis.&amp;nbsp; Pain is often severe.</p><p><strong>(Choice C)</strong>&amp;nbsp; <em>Mycobacterium marinum</em> lesions usually form a few weeks after exposure to contaminated fresh/salt water (eg, swimming pool, fish tank).&amp;nbsp; Lesions begin as solitary papules or nodules that eventually ulcerate and scar.&amp;nbsp; Serpiginous tracts would be uncommon.</p><p><strong>(Choice D)</strong>&amp;nbsp; <em>Sporothrix schenckii</em>, a dimorphic fungus, is usually transmitted when contaminated organic material (eg, moss, soil) is inoculated into the skin or subcutaneous tissue.&amp;nbsp; A papule forms at the inoculation site and usually ulcerates; additional lesions form along the proximal route of lymphatic drainage.</p><p><strong>(Choice E)</strong>&amp;nbsp; <em>Staphylococcus aureus</em> is the most common cause of purulent cellulitis.&amp;nbsp; Lesions tend to develop over days and are characterized by painful areas of fluctuance with surrounding erythema.</p><p><strong>(Choice F)</strong>&amp;nbsp; <em>Vibrio vulnificus</em> is a free-living bacterium that grows in brackish water and marine environments.&amp;nbsp; It can contaminate wounds and cause mild cellulitis.&amp;nbsp; More severe manifestations (eg, myositis, necrotizing fasciitis, sepsis) can develop in patients with certain underlying comorbidities (eg, liver disease, hemochromatosis).</p><p><strong>Educational objective:</strong><br>Hookworm infections are transmitted via direct contact between human skin and contaminated soil/sand (eg, walking barefoot).&amp;nbsp; Dermal penetration is often characterized by an intensely pruritic papule that may form serpiginous tracks due to the subcutaneous migration of hookworm larvae.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\">This patient, who developed a serpiginous, pruritic rash shortly after walking barefoot on a beach, likely has <strong>hookworm infection</strong>.&nbsp; Hookworms spread when <strong>egg-containing feces</strong> are deposited into shady, warm, moist sand or soil.&nbsp; Eggs hatch into larvae and are transmitted when they come into <strong>direct contact with human skin</strong> (eg, when walking barefoot).</p><p class=\"ng-scope\">Dermal penetration is usually marked by a <strong>pruritic maculopapular lesion</strong> (\"ground itch\") at the site of larval entry (eg, foot, toe webs).&nbsp; Spread of larvae through the adjacent dermal tissue can lead to the formation of migrating, reddish-brown, <strong>serpiginous tracks</strong>.&nbsp; Deeper spread may occur, depending on the species of infecting hookworm, as follows:</p><ul class=\"ng-scope\"><li><p><a ng-click=\"showexhibit('14433',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L39076.jpg&quot;,&quot;title&quot;:&quot;L39076&quot;}]\">Human hookworms</a> (eg, <em>Necator americanus</em>, <em>A duodenale</em>) penetrate the basement membrane and spread from the dermis to the bloodstream.&nbsp; They rupture into the alveoli and are coughed up and swallowed into the small intestine.&nbsp; There, larvae mature into adult hookworms that feed on blood from the duodenal mucosa (often causing iron deficiency anemia) and shed up to 10,000 eggs/day, thereby spreading the infection to others.</p></li><li><p>Cat and dog hookworms (eg, <em>Ancylostoma braziliense</em>, <em>A caninum</em>) cause a dermal eruption in humans but are unable to penetrate the cutaneous basement membrane and spread to deeper tissues.&nbsp; Therefore, larvae do not mature into adult hookworms or spread from human-to-human (humans are incidental hosts).</p></li></ul><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; A brown recluse spider bite usually appears as a red plaque or papule with central pallor; it may develop a dark central eschar that ulcerates due to necrosis.&nbsp; Pain is often severe.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; <em>Mycobacterium marinum</em> lesions usually form a few weeks after exposure to contaminated fresh/salt water (eg, swimming pool, fish tank).&nbsp; Lesions begin as solitary papules or nodules that eventually ulcerate and scar.&nbsp; Serpiginous tracts would be uncommon.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; <em>Sporothrix schenckii</em>, a dimorphic fungus, is usually transmitted when contaminated organic material (eg, moss, soil) is inoculated into the skin or subcutaneous tissue.&nbsp; A papule forms at the inoculation site and usually ulcerates; additional lesions form along the proximal route of lymphatic drainage.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; <em>Staphylococcus aureus</em> is the most common cause of purulent cellulitis.&nbsp; Lesions tend to develop over days and are characterized by painful areas of fluctuance with surrounding erythema.</p><p class=\"ng-scope\"><strong>(Choice F)</strong>&nbsp; <em>Vibrio vulnificus</em> is a free-living bacterium that grows in brackish water and marine environments.&nbsp; It can contaminate wounds and cause mild cellulitis.&nbsp; More severe manifestations (eg, myositis, necrotizing fasciitis, sepsis) can develop in patients with certain underlying comorbidities (eg, liver disease, hemochromatosis).</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Hookworm infections are transmitted via direct contact between human skin and contaminated soil/sand (eg, walking barefoot).&nbsp; Dermal penetration is often characterized by an intensely pruritic papule that may form serpiginous tracks due to the subcutaneous migration of hookworm larvae.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Microbiology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Dermatology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Cutaneous larva migrans</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
