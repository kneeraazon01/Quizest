== constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p>This patient with diarrhea, abdominal pain, and fever developed a colonic perforation.&amp;nbsp; Given his histologic findings consistent with <a ng-click=&quot;showexhibit('43942',true);&quot;>pseudomembrane</a> formation, this presentation is highly suggestive of <em><strong>Clostridioides difficile</strong></em>&amp;nbsp;<strong>infection</strong> (CDI)&amp;nbsp;(formerly <em>Clostridium </em>genus).&amp;nbsp; <em>C difficile</em> is an anaerobic, spore-forming, toxigenic, gram-positive rod that causes infectious colitis.&amp;nbsp; The most significant risk factor for the development of CDI is antibiotic use.&amp;nbsp; Antibiotics (particularly clindamycin, fluoroquinolones, and cephalosporins) alter the normal bowel flora, thereby predisposing to <em>C difficile</em> overgrowth.&amp;nbsp; Other risk factors include hospitalization, older age, and proton pump inhibitor use.</p><p><em>C difficile</em> produces 2 toxins:&amp;nbsp; Toxin A (enterotoxin) and Toxin B (cytotoxin).&amp;nbsp; These toxins disrupt cellular cytoskeletons and intercellular tight junctions, leading to colonocyte apoptosis.&amp;nbsp; The colonic mucosa responds to toxin exposure by forming yellow-white, patchy <a ng-click=&quot;showexhibit('14933',true);&quot;>pseudomembranes</a>, which consist of a <strong>neutrophil-predominant</strong> inflammatory infiltrate, <strong>fibrin</strong>, bacteria, and necrotic epithelium.&amp;nbsp; Patients with severe disease may develop a nonobstructive colonic dilation known as <strong>toxic megacolon</strong>, which leads to increased risk of colonic perforation (as seen in this patient).</p><p><strong>(Choice A)</strong>&amp;nbsp; <em>Campylobacter jejuni</em> is associated with watery diarrhea that may become bloody, abdominal cramping, and fever.&amp;nbsp; Neutrophilic infiltrate and crypt abscesses are identified on histology.</p><p><strong>(Choice C)</strong>&amp;nbsp; <em>Salmonella typhi</em> causes typhoid fever, which manifests with diarrhea, abdominal pain, high fever, and salmon-colored macules located mainly on the trunk.&amp;nbsp; Histology demonstrates histiocytes, a lymphoplasmacytic inflammatory infiltrate, and associated hemorrhage.</p><p><strong>(Choice D)</strong>&amp;nbsp; <em>Shigella flexneri</em> infection is characterized by sudden onset of severe abdominal cramping, bloody diarrhea, vomiting, and high fever.&amp;nbsp; Cryptitis, ulcerations, and crypt abscesses are identified on histology.</p><p><strong>(Choice E)</strong>&amp;nbsp; <em>Vibrio cholerae</em> causes cholera, which manifests with sudden onset, voluminous, &amp;#x22;rice water&amp;#x22; diarrhea and vomiting.&amp;nbsp; Histology typically remains normal.</p><p><strong>Educational objective:</strong><br><em>Clostridioides</em> (formerly <em>Clostridium</em>) <em>difficile</em> infection is associated with yellow-white, patchy pseudomembranes on the bowel mucosa.&amp;nbsp; These pseudomembranes consist of a neutrophil-predominant inflammatory infiltrate, fibrin, bacteria, and necrotic epithelium.&amp;nbsp; Patients may develop a nonobstructive colonic dilation known as toxic megacolon, which can lead to colonic perforation.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\">This patient with diarrhea, abdominal pain, and fever developed a colonic perforation.&nbsp; Given his histologic findings consistent with <a ng-click=\"showexhibit('43942',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L27626.jpg&quot;,&quot;title&quot;:&quot;L27626&quot;}]\">pseudomembrane</a> formation, this presentation is highly suggestive of <em><strong>Clostridioides difficile</strong></em>&nbsp;<strong>infection</strong> (CDI)&nbsp;(formerly <em>Clostridium </em>genus).&nbsp; <em>C difficile</em> is an anaerobic, spore-forming, toxigenic, gram-positive rod that causes infectious colitis.&nbsp; The most significant risk factor for the development of CDI is antibiotic use.&nbsp; Antibiotics (particularly clindamycin, fluoroquinolones, and cephalosporins) alter the normal bowel flora, thereby predisposing to <em>C difficile</em> overgrowth.&nbsp; Other risk factors include hospitalization, older age, and proton pump inhibitor use.</p><p class=\"ng-scope\"><em>C difficile</em> produces 2 toxins:&nbsp; Toxin A (enterotoxin) and Toxin B (cytotoxin).&nbsp; These toxins disrupt cellular cytoskeletons and intercellular tight junctions, leading to colonocyte apoptosis.&nbsp; The colonic mucosa responds to toxin exposure by forming yellow-white, patchy <a ng-click=\"showexhibit('14933',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L29272.png&quot;,&quot;title&quot;:&quot;L29272&quot;}]\">pseudomembranes</a>, which consist of a <strong>neutrophil-predominant</strong> inflammatory infiltrate, <strong>fibrin</strong>, bacteria, and necrotic epithelium.&nbsp; Patients with severe disease may develop a nonobstructive colonic dilation known as <strong>toxic megacolon</strong>, which leads to increased risk of colonic perforation (as seen in this patient).</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; <em>Campylobacter jejuni</em> is associated with watery diarrhea that may become bloody, abdominal cramping, and fever.&nbsp; Neutrophilic infiltrate and crypt abscesses are identified on histology.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; <em>Salmonella typhi</em> causes typhoid fever, which manifests with diarrhea, abdominal pain, high fever, and salmon-colored macules located mainly on the trunk.&nbsp; Histology demonstrates histiocytes, a lymphoplasmacytic inflammatory infiltrate, and associated hemorrhage.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; <em>Shigella flexneri</em> infection is characterized by sudden onset of severe abdominal cramping, bloody diarrhea, vomiting, and high fever.&nbsp; Cryptitis, ulcerations, and crypt abscesses are identified on histology.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; <em>Vibrio cholerae</em> causes cholera, which manifests with sudden onset, voluminous, \"rice water\" diarrhea and vomiting.&nbsp; Histology typically remains normal.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br><em>Clostridioides</em> (formerly <em>Clostridium</em>) <em>difficile</em> infection is associated with yellow-white, patchy pseudomembranes on the bowel mucosa.&nbsp; These pseudomembranes consist of a neutrophil-predominant inflammatory infiltrate, fibrin, bacteria, and necrotic epithelium.&nbsp; Patients may develop a nonobstructive colonic dilation known as toxic megacolon, which can lead to colonic perforation.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Microbiology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Gastrointestinal &amp; Nutrition</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Clostridioides difficile infection</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n   