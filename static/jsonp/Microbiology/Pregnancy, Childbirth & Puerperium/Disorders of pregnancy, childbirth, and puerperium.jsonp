 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><div id=&quot;div_T52908&quot;><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; class=&quot;table-default-style table-header-footer-style&quot;><tbody><tr><td colspan=&quot;2&quot;><p align=&quot;center&quot;><strong>Key features of congenital infections</strong>*</p></td></tr><tr><td><p align=&quot;center&quot;><strong>Toxoplasmosis</strong></p></td><td><ul><li>Chorioretinitis</li><li>Hydrocephalus</li><li>Diffuse intracranial calcifications</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Syphilis</strong></p></td><td><ul><li>Rhinorrhea</li><li>Skeletal anomalies</li><li>Desquamating rash (palms/soles)</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Rubella</strong></p></td><td><ul><li>Cataracts</li><li>Heart defects (eg, PDA)</li><li>Sensorineural hearing loss</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Cytomegalovirus</strong></p></td><td><ul><li>Periventricular calcifications</li><li>Microcephaly</li><li>Sensorineural hearing loss</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Herpes simplex virus</strong></p></td><td><ul><li>Vesicular/ulcerative rash</li></ul></td></tr><tr><td colspan=&quot;2&quot;><p>*Nonspecific findings include growth restriction, jaundice, hepatosplenomegaly &amp;amp; blueberry muffin rash.</p><p><strong>PDA</strong> = patent ductus arteriosus.</p></td></tr></tbody></table></div><p>This <strong>unvaccinated</strong> woman with a recent travel history developed a low-grade fever and a <strong>maculopapular rash</strong> with <strong>cephalocaudal spread</strong>, findings characteristic of measles or rubella.&amp;nbsp; In this case, <strong>rubella</strong> is the most likely cause because polyarthritis/<strong>polyarthralgia</strong>, particularly in women, is a classic finding with acute infection.&amp;nbsp; In addition, localized occipital and/or <strong>postauricular lymphadenopathy</strong> is more often seen in rubella.</p><p>Transplacental transmission of the rubella virus to the fetus during the first trimester causes congenital rubella syndrome.&amp;nbsp; <strong>Sensorineural hearing loss</strong>, <strong>cataracts</strong>, and cardiac malformations (eg, <strong>patent ductus arteriosus</strong>) are classic manifestations in the affected infant.&amp;nbsp; <strong>Growth restriction</strong> in weight, length, and head circumference (ie, <strong>microcephaly</strong>) is also common due to diffuse fetal inflammation.</p><p>Although treatment of suspected rubella infection is supportive care alone, the diagnosis should be confirmed in patients at risk for serious sequelae, such as pregnant women or newborns with classic findings.&amp;nbsp; The presence of rubella IgM antibodies is diagnostic.</p><p><strong>(Choice A)</strong>&amp;nbsp; Epicanthic folds and brachycephaly are typical features of <a ng-click=&quot;showexhibit('2641',true);&quot;>Down syndrome</a>, a genetic disorder caused by an extra copy of chromosome 21, not by maternal infection during pregnancy.</p><p><strong>(Choice B)</strong>&amp;nbsp; Hydrocephalus and chorioretinitis are seen with congenital toxoplasmosis.&amp;nbsp; <em>Toxoplasma gondii</em> infection in pregnant women is usually asymptomatic, but a nonspecific flu-like illness (eg, fever) with a maculopapular rash and lymphadenopathy can occur.&amp;nbsp; In contrast to the diffuse onset of rash in toxoplasmosis, this patient&amp;#39;s cephalocaudal progression of rash is characteristic of rubella.&amp;nbsp; In addition, muscle aches (not joint pain) and cervical (not postauricular) lymph node swelling can occur in toxoplasmosis.</p><p><strong>(Choice C)</strong>&amp;nbsp; Meningitis and <a ng-click=&quot;showexhibit('22197',true);&quot;>malformed teeth</a> are consistent with congenital syphilis infection.&amp;nbsp; In adults, secondary syphilis can present with constitutional symptoms (eg, fever, arthralgias), lymphadenopathy, and rash.&amp;nbsp; However, lymphadenopathy is typically generalized, and the rash is classically present on the palms and soles.</p><p><strong>(Choice E)</strong>&amp;nbsp; Mucocutaneous vesicles and keratitis are manifestations of congenital herpes simplex virus (HSV) infection.&amp;nbsp; In adults with HSV, head and neck lymphadenopathy can occur, but both primary and recurrent disease are associated with vesicular, not maculopapular, lesions that typically involve the oropharynx.&amp;nbsp; Moreover, arthralgias are not seen.</p><p><strong>Educational objective:</strong><br>Maternal rubella infection classically causes a maculopapular rash with cephalocaudal progression, joint pain, and postauricular lymphadenopathy.&amp;nbsp; Transplacental transmission to the fetus leads to congenital rubella syndrome, which is characterized by sensorineural deafness, cataracts, patent ductus arteriosus, and growth restriction (eg, microcephaly).</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><div id=\"div_T52908\" class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" class=\"table-default-style table-header-footer-style\"><tbody><tr><td colspan=\"2\"><p align=\"center\"><strong>Key features of congenital infections</strong>*</p></td></tr><tr><td><p align=\"center\"><strong>Toxoplasmosis</strong></p></td><td><ul><li>Chorioretinitis</li><li>Hydrocephalus</li><li>Diffuse intracranial calcifications</li></ul></td></tr><tr><td><p align=\"center\"><strong>Syphilis</strong></p></td><td><ul><li>Rhinorrhea</li><li>Skeletal anomalies</li><li>Desquamating rash (palms/soles)</li></ul></td></tr><tr><td><p align=\"center\"><strong>Rubella</strong></p></td><td><ul><li>Cataracts</li><li>Heart defects (eg, PDA)</li><li>Sensorineural hearing loss</li></ul></td></tr><tr><td><p align=\"center\"><strong>Cytomegalovirus</strong></p></td><td><ul><li>Periventricular calcifications</li><li>Microcephaly</li><li>Sensorineural hearing loss</li></ul></td></tr><tr><td><p align=\"center\"><strong>Herpes simplex virus</strong></p></td><td><ul><li>Vesicular/ulcerative rash</li></ul></td></tr><tr><td colspan=\"2\"><p>*Nonspecific findings include growth restriction, jaundice, hepatosplenomegaly &amp; blueberry muffin rash.</p><p><strong>PDA</strong> = patent ductus arteriosus.</p></td></tr></tbody></table></div><p class=\"ng-scope\">This <strong>unvaccinated</strong> woman with a recent travel history developed a low-grade fever and a <strong>maculopapular rash</strong> with <strong>cephalocaudal spread</strong>, findings characteristic of measles or rubella.&nbsp; In this case, <strong>rubella</strong> is the most likely cause because polyarthritis/<strong>polyarthralgia</strong>, particularly in women, is a classic finding with acute infection.&nbsp; In addition, localized occipital and/or <strong>postauricular lymphadenopathy</strong> is more often seen in rubella.</p><p class=\"ng-scope\">Transplacental transmission of the rubella virus to the fetus during the first trimester causes congenital rubella syndrome.&nbsp; <strong>Sensorineural hearing loss</strong>, <strong>cataracts</strong>, and cardiac malformations (eg, <strong>patent ductus arteriosus</strong>) are classic manifestations in the affected infant.&nbsp; <strong>Growth restriction</strong> in weight, length, and head circumference (ie, <strong>microcephaly</strong>) is also common due to diffuse fetal inflammation.</p><p class=\"ng-scope\">Although treatment of suspected rubella infection is supportive care alone, the diagnosis should be confirmed in patients at risk for serious sequelae, such as pregnant women or newborns with classic findings.&nbsp; The presence of rubella IgM antibodies is diagnostic.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Epicanthic folds and brachycephaly are typical features of <a ng-click=\"showexhibit('2641',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L24588.jpg&quot;,&quot;title&quot;:&quot;L24588&quot;}]\">Down syndrome</a>, a genetic disorder caused by an extra copy of chromosome 21, not by maternal infection during pregnancy.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Hydrocephalus and chorioretinitis are seen with congenital toxoplasmosis.&nbsp; <em>Toxoplasma gondii</em> infection in pregnant women is usually asymptomatic, but a nonspecific flu-like illness (eg, fever) with a maculopapular rash and lymphadenopathy can occur.&nbsp; In contrast to the diffuse onset of rash in toxoplasmosis, this patient's cephalocaudal progression of rash is characteristic of rubella.&nbsp; In addition, muscle aches (not joint pain) and cervical (not postauricular) lymph node swelling can occur in toxoplasmosis.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Meningitis and <a ng-click=\"showexhibit('22197',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L36905.jpg&quot;,&quot;title&quot;:&quot;L36905&quot;}]\">malformed teeth</a> are consistent with congenital syphilis infection.&nbsp; In adults, secondary syphilis can present with constitutional symptoms (eg, fever, arthralgias), lymphadenopathy, and rash.&nbsp; However, lymphadenopathy is typically generalized, and the rash is classically present on the palms and soles.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Mucocutaneous vesicles and keratitis are manifestations of congenital herpes simplex virus (HSV) infection.&nbsp; In adults with HSV, head and neck lymphadenopathy can occur, but both primary and recurrent disease are associated with vesicular, not maculopapular, lesions that typically involve the oropharynx.&nbsp; Moreover, arthralgias are not seen.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Maternal rubella infection classically causes a maculopapular rash with cephalocaudal progression, joint pain, and postauricular lymphadenopathy.&nbsp; Transplacental transmission to the fetus leads to congenital rubella syndrome, which is characterized by sensorineural deafness, cataracts, patent ductus arteriosus, and growth restriction (eg, microcephaly).</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/29939656\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/29939656\"><span compiledata=\"\" htmlcontent=\"Congenital rubella.\" class=\"ng-scope\"><p class=\"ng-scope\">Congenital rubella.</p></span></a><!-- end ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal -->\n                                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal -->\n                                </li><!-- end ngRepeat: questionReference in currentQuestion.questionMappingReferencesList -->\n                            </ul>\n                        </div>\n                    </div><!-- end ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Microbiology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pregnancy, Childbirth &amp; Puerperium</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Rubella</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeed