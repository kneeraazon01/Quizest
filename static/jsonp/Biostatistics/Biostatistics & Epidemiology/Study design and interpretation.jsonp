/div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p>It is important to know how to calculate some basic measures of association when given raw data.&amp;nbsp; The first step is to organize the data into a 2&amp;times;2 (contingency) table as shown below.&amp;nbsp; The <a ng-click=&quot;showexhibit('1811',true);&quot;>standard format</a> includes the exposures in the rows (with &amp;#x22;exposure present&amp;#x22; listed above &amp;#x22;exposure absent&amp;#x22;) and the outcomes in the columns (with &amp;#x22;outcome present&amp;#x22; listed before &amp;#x22;outcome absent&amp;#x22;).</p><blockquote><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; border=&quot;1&quot; cellpadding=&quot;5&quot; cellspacing=&quot;0&quot; width=&quot;475&quot;><tbody align=&quot;center&quot;><tr><td></td><td>Coronary event</td><td>No coronary event</td><td></td></tr><tr><td align=&quot;left&quot;>ACE inhibitor</td><td>120 (a)</td><td>280 (b)</td><td>400</td></tr><tr><td align=&quot;left&quot;>No ACE inhibitor</td><td>100 (c)</td><td>200 (d)</td><td>300</td></tr><tr><td></td><td>220</td><td>480</td><td>700</td></tr></tbody></table></blockquote><p>The relative risk (RR) represents the risk of an outcome (eg, coronary event) in the exposed (eg, angiotensin-converting enzyme [ACE] inhibitor) divided by the risk of that outcome in the unexposed (eg, no ACE inhibitor).&amp;nbsp; For cohort studies, it is calculated using the following formula:</p><p>RR = [a/(a+b)] / [c/(c+d)] = (120/400)/(100/300) = 0.90</p><p>Understanding the values calculated (rather than memorizing several formulas) would prevent errors that could occur if the standard format for the 2&amp;times;2 table is not followed.&amp;nbsp; By definition, RR = risk among the exposed/risk among the unexposed.&amp;nbsp; In this example, 400 diabetic patients were taking ACE inhibitors (exposed) and 120 of those had a coronary event.&amp;nbsp; Therefore, the risk among the exposed is 120/400.&amp;nbsp; Similarly, 300 diabetic patients were not taking ACE inhibitors (unexposed) and 100 of those had a coronary event.&amp;nbsp; Therefore, the risk among the unexposed is 100/300.&amp;nbsp; The RR is the ratio of those 2 values: RR = (120/400)/(100/300).</p><p><strong>(Choice B)</strong>&amp;nbsp; The odds ratio (OR) is calculated using the following formula: OR = ad/bc.&amp;nbsp; The OR is often calculated in case-control studies where RR cannot be obtained because such studies do not follow patients over time to determine the incidence of a new outcome.</p><p><strong>Educational objective:</strong><br>The relative risk (RR) represents the risk of an outcome in the exposed divided by the risk of that outcome in the unexposed.&amp;nbsp; Applying the correct formula for RR calculations depends on the proper formatting of a 2&amp;times;2 (contingency) table.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\">It is important to know how to calculate some basic measures of association when given raw data.&nbsp; The first step is to organize the data into a 2\u00d72 (contingency) table as shown below.&nbsp; The <a ng-click=\"showexhibit('1811',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/34469.html&quot;,&quot;title&quot;:&quot;34469&quot;}]\">standard format</a> includes the exposures in the rows (with \"exposure present\" listed above \"exposure absent\") and the outcomes in the columns (with \"outcome present\" listed before \"outcome absent\").</p><blockquote class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" border=\"1\" cellpadding=\"5\" cellspacing=\"0\" width=\"475\"><tbody align=\"center\"><tr><td></td><td>Coronary event</td><td>No coronary event</td><td></td></tr><tr><td align=\"left\">ACE inhibitor</td><td>120 (a)</td><td>280 (b)</td><td>400</td></tr><tr><td align=\"left\">No ACE inhibitor</td><td>100 (c)</td><td>200 (d)</td><td>300</td></tr><tr><td></td><td>220</td><td>480</td><td>700</td></tr></tbody></table></blockquote><p class=\"ng-scope\">The relative risk (RR) represents the risk of an outcome (eg, coronary event) in the exposed (eg, angiotensin-converting enzyme [ACE] inhibitor) divided by the risk of that outcome in the unexposed (eg, no ACE inhibitor).&nbsp; For cohort studies, it is calculated using the following formula:</p><p class=\"ng-scope\">RR = [a/(a+b)] / [c/(c+d)] = (120/400)/(100/300) = 0.90</p><p class=\"ng-scope\">Understanding the values calculated (rather than memorizing several formulas) would prevent errors that could occur if the standard format for the 2\u00d72 table is not followed.&nbsp; By definition, RR = risk among the exposed/risk among the unexposed.&nbsp; In this example, 400 diabetic patients were taking ACE inhibitors (exposed) and 120 of those had a coronary event.&nbsp; Therefore, the risk among the exposed is 120/400.&nbsp; Similarly, 300 diabetic patients were not taking ACE inhibitors (unexposed) and 100 of those had a coronary event.&nbsp; Therefore, the risk among the unexposed is 100/300.&nbsp; The RR is the ratio of those 2 values: RR = (120/400)/(100/300).</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; The odds ratio (OR) is calculated using the following formula: OR = ad/bc.&nbsp; The OR is often calculated in case-control studies where RR cannot be obtained because such studies do not follow patients over time to determine the incidence of a new outcome.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>The relative risk (RR) represents the risk of an outcome in the exposed divided by the risk of that outcome in the unexposed.&nbsp; Applying the correct formula for RR calculations depends on the proper formatting of a 2\u00d72 (contingency) table.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Biostatistics</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n             