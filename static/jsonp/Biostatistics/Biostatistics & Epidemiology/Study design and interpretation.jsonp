              <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p>The odds of an event occurring are defined as the probability of that event happening divided by the probability of the event not happening.&amp;nbsp; For instance, when <a ng-click=&quot;showexhibit('4953',true);&quot;>rolling dice</a>, the probability of rolling a one is 1/6, whereas the probability of rolling any other number is 5/6.&amp;nbsp; Therefore, the odds of rolling a one are 1/5.</p><p>The <strong>odds ratio (OR)</strong> is a measure of association that compares the odds of an outcome occurring based on exposure status.&amp;nbsp; In this case, it represents the odds that the outcome (eg, major arrhythmia) occurred in the presence of the exposure (eg, beta blocker use) compared with the odds that the outcome occurred in the absence of that exposure.&amp;nbsp; The OR can be calculated using a standard contingency table (with exposures in the rows and outcomes in the columns).</p><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;1584&quot; src=&quot;https://www.uworld.com/media/L20024.jpg&quot; ></p><p>Odds of developing major arrhythmia in exposed group = 20/70<br>Odds of developing major arrhythmia in unexposed group = 30/40<br>(note that the total number of people in the exposed and unexposed groups is <strong>not</strong> used to calculate odds, unlike calculating probabilities)</p><p>Therefore, the OR of developing a major arrhythmia in patients who took beta blockers compared with those who did not is as follows:</p><blockquote><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; border=&quot;0&quot; cellpadding=&quot;0&quot; cellspacing=&quot;0&quot; width=&quot;550&quot;><tbody><tr><td align=&quot;center&quot;>(a/b) / (c/d) = (20/70) / (30/40) = <strong>(20 &amp;times; 40) / (70 &amp;times; 30)</strong> = 0.38</td></tr></tbody></table></blockquote><p>In this case, the OR calculated was the OR of disease (among exposed individuals relative to nonexposed individuals), given by (a/b) / (c/d); in some cases, the OR of exposure (among diseased individuals relative to nondiseased individuals) is calculated, given by (a/c) / (b/d).&amp;nbsp; Both OR definitions are mathematically equivalent and reduce (by a <a ng-click=&quot;showexhibit('1856',true);&quot;>mathematical shortcut</a>) to: OR = ad/bc.&amp;nbsp; Any of these formulas would give the same result.</p><p>Because the OR is &amp;lt;1, the odds of developing a major arrhythmia is lower for patients who were taking beta blockers compared with those who were not.</p><p><strong>(Choice E)</strong>&amp;nbsp; Relative risk (RR) is the probability of an outcome occurring in the exposed group compared with the probability of the outcome occurring in the unexposed group.&amp;nbsp; It is calculated using the following formula:</p><blockquote><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; border=&quot;0&quot; cellpadding=&quot;0&quot; cellspacing=&quot;0&quot; width=&quot;550&quot;><tbody><tr><td align=&quot;center&quot;>RR = [a/(a + b)] / [c/(c + d)] = (20/90) / (30/70)</td></tr></tbody></table></blockquote><p>RR often has more clinical utility than the OR since it gives the change in disease risk based on the presence or absence of risk factors.&amp;nbsp; However, RR cannot be calculated in case-control studies as patients are preselected based on disease status (cases and controls) instead of being sorted into exposure groups and followed over time to assess disease outcomes.</p><p><strong>Educational objective:</strong><br>The odds ratio (OR) is a measure of the strength of an association between an exposure and an outcome.&amp;nbsp; Specifically, it represents the odds that an outcome occurred in the presence of a particular exposure compared with the odds that the outcome occurred in the absence of that exposure.&amp;nbsp; In a standard contingency table, OR = ad/bc.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\">The odds of an event occurring are defined as the probability of that event happening divided by the probability of the event not happening.&nbsp; For instance, when <a ng-click=\"showexhibit('4953',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L29722.jpg&quot;,&quot;title&quot;:&quot;L29722&quot;}]\">rolling dice</a>, the probability of rolling a one is 1/6, whereas the probability of rolling any other number is 5/6.&nbsp; Therefore, the odds of rolling a one are 1/5.</p><p class=\"ng-scope\">The <strong>odds ratio (OR)</strong> is a measure of association that compares the odds of an outcome occurring based on exposure status.&nbsp; In this case, it represents the odds that the outcome (eg, major arrhythmia) occurred in the presence of the exposure (eg, beta blocker use) compared with the odds that the outcome occurred in the absence of that exposure.&nbsp; The OR can be calculated using a standard contingency table (with exposures in the rows and outcomes in the columns).</p><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"1584\" src=\"image/L20024.jpg\"></p><p class=\"ng-scope\">Odds of developing major arrhythmia in exposed group = 20/70<br>Odds of developing major arrhythmia in unexposed group = 30/40<br>(note that the total number of people in the exposed and unexposed groups is <strong>not</strong> used to calculate odds, unlike calculating probabilities)</p><p class=\"ng-scope\">Therefore, the OR of developing a major arrhythmia in patients who took beta blockers compared with those who did not is as follows:</p><blockquote class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" border=\"0\" cellpadding=\"0\" cellspacing=\"0\" width=\"550\"><tbody><tr><td align=\"center\">(a/b) / (c/d) = (20/70) / (30/40) = <strong>(20 \u00d7 40) / (70 \u00d7 30)</strong> = 0.38</td></tr></tbody></table></blockquote><p class=\"ng-scope\">In this case, the OR calculated was the OR of disease (among exposed individuals relative to nonexposed individuals), given by (a/b) / (c/d); in some cases, the OR of exposure (among diseased individuals relative to nondiseased individuals) is calculated, given by (a/c) / (b/d).&nbsp; Both OR definitions are mathematically equivalent and reduce (by a <a ng-click=\"showexhibit('1856',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L21566.jpg&quot;,&quot;title&quot;:&quot;L21566&quot;}]\">mathematical shortcut</a>) to: OR = ad/bc.&nbsp; Any of these formulas would give the same result.</p><p class=\"ng-scope\">Because the OR is &lt;1, the odds of developing a major arrhythmia is lower for patients who were taking beta blockers compared with those who were not.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Relative risk (RR) is the probability of an outcome occurring in the exposed group compared with the probability of the outcome occurring in the unexposed group.&nbsp; It is calculated using the following formula:</p><blockquote class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" border=\"0\" cellpadding=\"0\" cellspacing=\"0\" width=\"550\"><tbody><tr><td align=\"center\">RR = [a/(a + b)] / [c/(c + d)] = (20/90) / (30/70)</td></tr></tbody></table></blockquote><p class=\"ng-scope\">RR often has more clinical utility than the OR since it gives the change in disease risk based on the presence or absence of risk factors.&nbsp; However, RR cannot be calculated in case-control studies as patients are preselected based on disease status (cases and controls) instead of being sorted into exposure groups and followed over time to assess disease outcomes.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>The odds ratio (OR) is a measure of the strength of an association between an exposure and an outcome.&nbsp; Specifically, it represents the odds that an outcome occurred in the presence of a particular exposure compared with the odds that the outcome occurred in the absence of that exposure.&nbsp; In a standard contingency table, OR = ad/bc.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Biostatistics</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Biostatistics &amp; Epidemiology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Odds ratio</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
