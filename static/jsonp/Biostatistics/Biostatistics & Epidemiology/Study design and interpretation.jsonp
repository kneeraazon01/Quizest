           <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>E.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight5\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Recall bias</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><strong>Statistical power</strong> represents a study&amp;#39;s <strong>strength to detect a difference</strong> (ie, effect size) between treatment groups when one truly exists.&amp;nbsp; It <strong>depends</strong> on <strong>sample size</strong> (among other factors): studies with <strong>greater sample sizes</strong> have <strong>greater power</strong> than studies with smaller sample sizes.&amp;nbsp; An excessively large sample size may determine that a clinically irrelevant difference between groups is statistically significant (ie, <em>p</em> &amp;lt; 0.05), while an inappropriately small sample may fail to determine that a clinically relevant difference between groups is statistically significant (ie, <em>p</em> &amp;gt; 0.05).</p><p>In this case, the rate of relapse in the group receiving the new cream is 25% and the rate in the group receiving the emollient alone is 50%.&amp;nbsp; This represents a <strong>relative risk reduction (RRR) of 50%</strong> (ie, [50% &amp;#8722; 25%] / 50%); therefore, the new cream reduced the risk of relapse by 50% (ie, effect size) compared to emollient alone.&amp;nbsp; A 50% risk reduction may be considered <strong>clinically relevant</strong>; however, it was <strong>not statistically significant</strong> (ie, <em>p</em> = 0.14 &amp;gt; 0.05) in the study.</p><p>The most likely explanation is that the sample size (ie, 15 per group) provided <strong>insufficient power</strong> to detect the observed difference between groups (ie, effect size, RRR = 50%).&amp;nbsp; A larger sample size would increase the power of the study (ie, its ability to detect the difference), and the <em>p</em>-value would reach statistical significance (ie, <em>p</em> &amp;lt; 0.05).</p><p><strong>(Choice A)</strong>&amp;nbsp; Ascertainment bias occurs when the results of a study are distorted by awareness of treatment assignment, as in an unblind study.&amp;nbsp; This study is double-blinded, thereby minimizing potential ascertainment bias.</p><p><strong>(Choice B)</strong>&amp;nbsp; Confounding distorts the relationship between exposures (eg, treatments) and the outcome (eg, disease) of interest, and can wholly or partially account for observed effects.&amp;nbsp; This study is a randomized trial, thereby minimizing potential confounding bias by generating groups that are comparable with respect to known and unknown confounding variables.</p><p><strong>(Choice C)</strong>&amp;nbsp; Ecological fallacy occurs when conclusions are made about individuals based on studies where the unit of analysis is a group (ie, the conclusions of a study&amp;nbsp;assessing population groups do not necessarily apply to individuals).&amp;nbsp; In this study, the unit of analysis is the individual, not the group.</p><p><strong>(Choice E)</strong>&amp;nbsp; Recall bias results from the inaccurate recollection of past exposure status.&amp;nbsp; It is a potential problem for case-control studies, particularly when questionnaires are used to inquire about distant past exposure.&amp;nbsp; However, this study is an experiment in which patients are exposed to treatments and then assessed for the outcome.</p><p><strong>Educational objective:</strong><br>An inappropriately small sample will fail to identify important clinically significant differences as statistically significant because of a lack of sufficient statistical power.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><strong>Statistical power</strong> represents a study's <strong>strength to detect a difference</strong> (ie, effect size) between treatment groups when one truly exists.&nbsp; It <strong>depends</strong> on <strong>sample size</strong> (among other factors): studies with <strong>greater sample sizes</strong> have <strong>greater power</strong> than studies with smaller sample sizes.&nbsp; An excessively large sample size may determine that a clinically irrelevant difference between groups is statistically significant (ie, <em>p</em> &lt; 0.05), while an inappropriately small sample may fail to determine that a clinically relevant difference between groups is statistically significant (ie, <em>p</em> &gt; 0.05).</p><p class=\"ng-scope\">In this case, the rate of relapse in the group receiving the new cream is 25% and the rate in the group receiving the emollient alone is 50%.&nbsp; This represents a <strong>relative risk reduction (RRR) of 50%</strong> (ie, [50% \u2212 25%] / 50%); therefore, the new cream reduced the risk of relapse by 50% (ie, effect size) compared to emollient alone.&nbsp; A 50% risk reduction may be considered <strong>clinically relevant</strong>; however, it was <strong>not statistically significant</strong> (ie, <em>p</em> = 0.14 &gt; 0.05) in the study.</p><p class=\"ng-scope\">The most likely explanation is that the sample size (ie, 15 per group) provided <strong>insufficient power</strong> to detect the observed difference between groups (ie, effect size, RRR = 50%).&nbsp; A larger sample size would increase the power of the study (ie, its ability to detect the difference), and the <em>p</em>-value would reach statistical significance (ie, <em>p</em> &lt; 0.05).</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Ascertainment bias occurs when the results of a study are distorted by awareness of treatment assignment, as in an unblind study.&nbsp; This study is double-blinded, thereby minimizing potential ascertainment bias.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Confounding distorts the relationship between exposures (eg, treatments) and the outcome (eg, disease) of interest, and can wholly or partially account for observed effects.&nbsp; This study is a randomized trial, thereby minimizing potential confounding bias by generating groups that are comparable with respect to known and unknown confounding variables.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Ecological fallacy occurs when conclusions are made about individuals based on studies where the unit of analysis is a group (ie, the conclusions of a study&nbsp;assessing population groups do not necessarily apply to individuals).&nbsp; In this study, the unit of analysis is the individual, not the group.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Recall bias results from the inaccurate recollection of past exposure status.&nbsp; It is a potential problem for case-control studies, particularly when questionnaires are used to inquire about distant past exposure.&nbsp; However, this study is an experiment in which patients are exposed to treatments and then assessed for the outcome.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>An inappropriately small sample will fail to identify important clinically significant differences as statistically significant because of a lack of sufficient statistical power.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Biostatistics</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Biostatistics &amp; Epidemiology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Power and sample size</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
