egeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; id=&quot;9151&quot;  src=&quot;https://www.uworld.com/media/cardiacAPd.gif&quot; draggable=&quot;false&quot; ><P>The action potential of ventricular and non-nodal conduction cells within the heart consists of four phases.</P><OL><LI><STRONG>Phase 4: Resting potential (diastole).</STRONG>&amp;nbsp; Resting potential is determined largely by membrane permeability to K<FONT size=3><SUP>+</SUP></FONT> ions when in the resting state.&amp;nbsp; The resting potential of cardiac myocytes is approximately -90mV, while the resting potential of skeletal myocytes is approximately -75mV.&amp;nbsp; The highly negative resting potential of cardiac myocytes reduces the risk of arrhythmias, as a larger stimulus is needed to excite the cells.</LI><LI><STRONG>Phase 0: Rapid depolarization.</STRONG> As in skeletal myocytes, the action potential onset occurs when voltage-gated Na<FONT size=3><SUP>+</SUP></FONT> channels open and Na<FONT size=3><SUP>+</SUP></FONT> ions rush into the cell.</LI><LI><STRONG>Phase 1: Initial rapid repolarization.</STRONG> This phase is associated with the rapid closure of Na<FONT size=3><SUP>+</SUP></FONT> channels.</LI><LI><STRONG>Phase 2: Plateau.</STRONG> This phase is a distinctive feature of the cardiac action potential not seen in other tissues.&amp;nbsp; It is characterized by the opening of L-type dihydropyridine-sensitive Ca<FONT size=3><SUP>++</SUP></FONT> channels and the closure of some K<FONT size=3><SUP>+</SUP></FONT> channels.&amp;nbsp; Consequently, the membrane becomes highly permeable to Ca<FONT size=3><SUP>++</SUP></FONT> ions and minimally permeable to K<FONT size=3><SUP>+</SUP></FONT> ions.</LI><LI><STRONG>Phase 3: Late rapid repolarization.</STRONG> This phase features closure of Ca<FONT size=3><SUP>++</SUP></FONT> channels and the opening of K<FONT size=3><SUP>+</SUP></FONT> channels.&amp;nbsp; Efflux of K<FONT size=3><SUP>+</SUP></FONT> from the cell restores the membrane resting potential.</LI></OL><P>In sum, the cardiac myocyte action potential includes the following three key events: Na<FONT size=3><SUP>+</SUP></FONT> ion influx (corresponding to the red curve on the ion conductivity graph); Ca<FONT size=3><SUP>++</SUP></FONT> ion influx (corresponding to the blue curve on the ion conductivity graph); and decreased K<FONT size=3><SUP>+</SUP></FONT> permeability during the action potential upstroke (corresponding to the green curve on the ion conductivity graph).</P><P><STRONG>(Choices A, C, D, and E)</STRONG> The other ion sequences listed do not correlate with the action potential phases observed in cardiac myocytes.</P><P><STRONG>Educational Objective:</STRONG><BR>The cardiac myocyte action potential consists of rapid depolarization (phase 0), initial rapid repolarization (phase 1), plateau (phase 2), late rapid repolarization (phase 3), and resting potential (phase 4).&amp;nbsp; The action potential is associated with increased membrane permeability to Na<FONT size=3><SUP>+</SUP></FONT> and Ca<FONT size=3><SUP>++</SUP></FONT> and decreased permeability to K<FONT size=3><SUP>+</SUP></FONT>.</P>\"><p class=\"ng-scope\"></p><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><img class=\"imgWidthHelper ng-scope\" ng-click=\"decideClickAction( $event )\" id=\"9151\" src=\"image/cardiacAPd.gif\" draggable=\"false\"><p class=\"ng-scope\">The action potential of ventricular and non-nodal conduction cells within the heart consists of four phases.</p><ol class=\"ng-scope\"><li><strong>Phase 4: Resting potential (diastole).</strong>&nbsp; Resting potential is determined largely by membrane permeability to K<font size=\"3\"><sup>+</sup></font> ions when in the resting state.&nbsp; The resting potential of cardiac myocytes is approximately -90mV, while the resting potential of skeletal myocytes is approximately -75mV.&nbsp; The highly negative resting potential of cardiac myocytes reduces the risk of arrhythmias, as a larger stimulus is needed to excite the cells.</li><li><strong>Phase 0: Rapid depolarization.</strong> As in skeletal myocytes, the action potential onset occurs when voltage-gated Na<font size=\"3\"><sup>+</sup></font> channels open and Na<font size=\"3\"><sup>+</sup></font> ions rush into the cell.</li><li><strong>Phase 1: Initial rapid repolarization.</strong> This phase is associated with the rapid closure of Na<font size=\"3\"><sup>+</sup></font> channels.</li><li><strong>Phase 2: Plateau.</strong> This phase is a distinctive feature of the cardiac action potential not seen in other tissues.&nbsp; It is characterized by the opening of L-type dihydropyridine-sensitive Ca<font size=\"3\"><sup>++</sup></font> channels and the closure of some K<font size=\"3\"><sup>+</sup></font> channels.&nbsp; Consequently, the membrane becomes highly permeable to Ca<font size=\"3\"><sup>++</sup></font> ions and minimally permeable to K<font size=\"3\"><sup>+</sup></font> ions.</li><li><strong>Phase 3: Late rapid repolarization.</strong> This phase features closure of Ca<font size=\"3\"><sup>++</sup></font> channels and the opening of K<font size=\"3\"><sup>+</sup></font> channels.&nbsp; Efflux of K<font size=\"3\"><sup>+</sup></font> from the cell restores the membrane resting potential.</li></ol><p class=\"ng-scope\">In sum, the cardiac myocyte action potential includes the following three key events: Na<font size=\"3\"><sup>+</sup></font> ion influx (corresponding to the red curve on the ion conductivity graph); Ca<font size=\"3\"><sup>++</sup></font> ion influx (corresponding to the blue curve on the ion conductivity graph); and decreased K<font size=\"3\"><sup>+</sup></font> permeability during the action potential upstroke (corresponding to the green curve on the ion conductivity graph).</p><p class=\"ng-scope\"><strong>(Choices A, C, D, and E)</strong> The other ion sequences listed do not correlate with the action potential phases observed in cardiac myocytes.</p><p class=\"ng-scope\"><strong>Educational Objective:</strong><br>The cardiac myocyte action potential consists of rapid depolarization (phase 0), initial rapid repolarization (phase 1), plateau (phase 2), late rapid repolarization (phase 3), and resting potential (phase 4).&nbsp; The action potential is associated with increased membrane permeability to Na<font size=\"3\"><sup>+</sup></font> and Ca<font size=\"3\"><sup>++</sup></font> and decreased permeability to K<font size=\"3\"><sup>+</sup></font>.</p><p class=\"ng-scope\"></p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Physiology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Cardiovascular System</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Resting membrane potential and action potential</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
