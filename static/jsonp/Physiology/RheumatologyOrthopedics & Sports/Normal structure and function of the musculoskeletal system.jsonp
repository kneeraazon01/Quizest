        <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>E.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight5\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Uncoordinated contraction of myofibrils within affected muscle fibers</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;12289&quot; src=&quot;https://www.uworld.com/media/L36834.jpg&quot; ></p><p><strong>Transverse tubules</strong> (T-tubules) are <strong>invaginations</strong> of the muscle cell membrane (<strong>sarcolemma</strong>) located in close proximity to the terminal cisterns of the sarcoplasmic reticulum.&amp;nbsp; They are found in striated (eg, cardiac, skeletal) muscle and contain voltage-gated L-type calcium channels (dihydropyridine receptors) adjacent to the ryanodine receptors on the terminal cisterns.&amp;nbsp; During muscle contraction, T-tubules allow the <strong>depolarization</strong> impulse to <strong>rapidly propagate</strong> through the interior of the muscle fiber.&amp;nbsp; This ensures that <a ng-click=&quot;showexhibit('3898',true);&quot;>calcium release</a> from the sarcoplasmic reticulum occurs uniformly throughout the fiber, allowing for <strong>synchronized contraction of myofibrils</strong> in each muscle cell.</p><p>This patient most likely has a form of limb girdle muscular dystrophy due to a mutated sarcolemma protein (eg, caveolin) affecting excitation-contraction coupling.&amp;nbsp; Decreased numbers of functional T-tubules in affected muscle fibers lead to <strong>uncoordinated</strong> contraction of myofibrils, which manifests as muscle weakness.</p><p><strong>(Choice A)</strong>&amp;nbsp; Decremental force generated on repeated stimulation characterizes myasthenia gravis, an autoantibody disorder inhibiting postsynaptic acetylcholine receptors in the neuromuscular junction.&amp;nbsp; A constant low response to repeated nerve stimulation is more likely if T-tubules are lacking.</p><p><strong>(Choice B)</strong>&amp;nbsp; ATP fuels myosin movement along actin filaments (causing contraction) and drives the pumps that transport calcium back into the sarcoplasmic reticulum (causing relaxation).&amp;nbsp; McArdle disease is one cause of impaired ATP production and occurs due to a failure of muscle glycogen breakdown.</p><p><strong>(Choice C)</strong>&amp;nbsp; Impaired relaxation after sustained contraction occurs in myotonic dystrophy due to a trinucleotide repeat expansion altering myotonin-protein kinase.&amp;nbsp; This protein facilitates myosin head detachment from the actin filament to enable muscle relaxation; T-tubules do not play a role.</p><p><strong>(Choice D)</strong>&amp;nbsp; A mutation in <a ng-click=&quot;showexhibit('3899',true);&quot;>troponin C</a> may block its response to intracellular calcium and prevent muscle contraction.&amp;nbsp; However, muscle fibers with decreased numbers of T-tubules will maintain limited contractility.</p><p><strong>Educational objective:</strong><br>Transverse tubules (T-tubules) are invaginations of the sarcolemma that transmit depolarization signals to the sarcoplasmic reticulum to trigger the release of calcium and induce muscle contraction.&amp;nbsp; The uniform distribution of T-tubules in striated muscle fibers ensures that each myofibril contracts at the same time, which is necessary for efficient contraction.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"12289\" src=\"image/L36834.jpg\"></p><p class=\"ng-scope\"><strong>Transverse tubules</strong> (T-tubules) are <strong>invaginations</strong> of the muscle cell membrane (<strong>sarcolemma</strong>) located in close proximity to the terminal cisterns of the sarcoplasmic reticulum.&nbsp; They are found in striated (eg, cardiac, skeletal) muscle and contain voltage-gated L-type calcium channels (dihydropyridine receptors) adjacent to the ryanodine receptors on the terminal cisterns.&nbsp; During muscle contraction, T-tubules allow the <strong>depolarization</strong> impulse to <strong>rapidly propagate</strong> through the interior of the muscle fiber.&nbsp; This ensures that <a ng-click=\"showexhibit('3898',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L26823.jpg&quot;,&quot;title&quot;:&quot;L26823&quot;}]\">calcium release</a> from the sarcoplasmic reticulum occurs uniformly throughout the fiber, allowing for <strong>synchronized contraction of myofibrils</strong> in each muscle cell.</p><p class=\"ng-scope\">This patient most likely has a form of limb girdle muscular dystrophy due to a mutated sarcolemma protein (eg, caveolin) affecting excitation-contraction coupling.&nbsp; Decreased numbers of functional T-tubules in affected muscle fibers lead to <strong>uncoordinated</strong> contraction of myofibrils, which manifests as muscle weakness.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Decremental force generated on repeated stimulation characterizes myasthenia gravis, an autoantibody disorder inhibiting postsynaptic acetylcholine receptors in the neuromuscular junction.&nbsp; A constant low response to repeated nerve stimulation is more likely if T-tubules are lacking.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; ATP fuels myosin movement along actin filaments (causing contraction) and drives the pumps that transport calcium back into the sarcoplasmic reticulum (causing relaxation).&nbsp; McArdle disease is one cause of impaired ATP production and occurs due to a failure of muscle glycogen breakdown.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Impaired relaxation after sustained contraction occurs in myotonic dystrophy due to a trinucleotide repeat expansion altering myotonin-protein kinase.&nbsp; This protein facilitates myosin head detachment from the actin filament to enable muscle relaxation; T-tubules do not play a role.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; A mutation in <a ng-click=\"showexhibit('3899',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L9720.jpg&quot;,&quot;title&quot;:&quot;L9720&quot;}]\">troponin C</a> may block its response to intracellular calcium and prevent muscle contraction.&nbsp; However, muscle fibers with decreased numbers of T-tubules will maintain limited contractility.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Transverse tubules (T-tubules) are invaginations of the sarcolemma that transmit depolarization signals to the sarcoplasmic reticulum to trigger the release of calcium and induce muscle contraction.&nbsp; The uniform distribution of T-tubules in striated muscle fibers ensures that each myofibril contracts at the same time, which is necessary for efficient contraction.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standar