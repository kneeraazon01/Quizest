    {
        "question": "<div class=\"questioncontent ui-resizable widthWithoutAbstract\" id=\"questionContent\" ng-show=\"showQuestion\" ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat, 'suggested-highlights-visible' : userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; currentQuestion.frequentHighlights != null &amp;&amp; (suggestedHighlightsPreference == 1 || suggestedHighlightsPreference == 2), 'only-suggested-highlights-visible' : userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; suggestedHighlightsPreference == 1 &amp;&amp; currentQuestion.frequentHighlights != null}\">\n            <div style=\"display:none;\" class=\"suggested-highlights-switch-buttons ng-hide\" ng-show=\"userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; currentQuestion.frequentHighlights != null\">\n                <span class=\"fa-stack\" ng-click=\"showUserHighlights()\" title=\"My Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fas\" ng-class=\"{'fas' : suggestedHighlightsPreference == 0, 'fal' : suggestedHighlightsPreference != 0}\"></i>\n                    <i class=\"fal fa-highlighter fa-stack-1x fa-inverse\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 0}\"></i>\n                </span>\n                <span class=\"fa-stack ng-hide\" ng-hide=\"currentQuestion.frequentHighlights == null\" ng-click=\"showSuggestedHighlights()\" title=\"Suggested Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fal\" ng-class=\"{'fas' : suggestedHighlightsPreference == 1, 'fal' : suggestedHighlightsPreference != 1}\"></i>\n                    <i class=\"fas fa-highlighter fa-stack-1x\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 1}\"></i>\n                </span>\n                <span class=\"fa-stack highlight-preference-unavailable highlight-button-with-popover\" ng-show=\"currentQuestion.frequentHighlights == null\" title=\"\" data-toggle=\"popover\" data-placement=\"bottom\" data-content=\"Collecting Data\" data-trigger=\"hover\" data-original-title=\"Suggested Highlights\">\n                    <i class=\"fal fa-circle fa-stack-2x\"></i>\n                    <i class=\"fas fa-highlighter fa-stack-1x\"></i>\n                </span>\n                <span class=\"fa-stack ng-hide\" ng-hide=\"currentQuestion.frequentHighlights == null\" ng-click=\"showUserAndSuggestedHighlights()\" title=\"Duo Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fal\" ng-class=\"{'fas' : suggestedHighlightsPreference == 2, 'fal' : suggestedHighlightsPreference != 2}\"></i>\n                    <i class=\"fad fa-highlighter fa-stack-1x\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 2}\"></i>\n                </span>\n                <span class=\"fa-stack highlight-preference-unavailable highlight-button-with-popover\" ng-show=\"currentQuestion.frequentHighlights == null\" title=\"\" data-toggle=\"popover\" data-placement=\"bottom\" data-content=\"Collecting Data\" data-trigger=\"hover\" data-original-title=\"Duo Highlights\">\n                    <i class=\"fal fa-circle fa-stack-2x\"></i>\n                    <i class=\"fad fa-highlighter fa-stack-1x\"></i>\n                </span>\n            </div>\n            <!-- ngIf: launchTestOptions.launchCode == 'nbome' && userPreferences.testPreferences.theme != clientConstants.themes.nbome -->\n            <div id=\"question\">\n                <!-- ngIf: settings.hints && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && launchTestOptions.canShowHint -->\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && (currentQuestion.qbankId != clientConstants.qbankIds.stats_faculty || currentQuestion.qbankId != clientConstants.qbankIds.stats) -->\n                <!-- ngIf: (currentQuestion.mobileExcerpt != '' && currentQuestion.mobileExcerpt != null ) -->\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat -->\n                <!--Periodic table button for MCAT -->\n                <!--<div class=\"text-center periodicTableDiv\" ng-show=\"(launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.subjectId != constants.noPeriodicTable.cars) && (currentQuestion.subjectId != constants.noPeriodicTable.pyschology) && (currentQuestion.subjectId != constants.noPeriodicTable.sociology)\">\n                    <button class=\"btn btn-default btn--custom\" style=\"color: black; padding: 4px 3px;\" ng-click=\"showresources()\">Periodic table&nbsp;<img src=\"img/mcat/Periodic_icon.png\" height=\"12\" style=\"margin-top: -2px; cursor: pointer\" title=\"Periodic Table\" /></button>\n                </div>-->\n                \n                <!--Inactive message for questions in usmle, boards and nclex and do not show for EXPERIMENTAL_INACTIVE hence the check !=3 -->\n                <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.nclex) && !currentQuestion.isActive && currentQuestion.activeStatusId != 3 -->\n                <div id=\"questionText\" class=\"questionText ng-scope\" ng-mouseup=\"invokeHighlights('questionText',constants.flashCardType.selection,$event)\" compiledata=\"\" htmlcontent=\"<p>A 4-year-old girl is brought to the office after her parents noticed dark red blood on her toilet tissue after a bowel movement.&amp;nbsp; She has had no abdominal pain, dysuria, or discomfort with defecation.&amp;nbsp; The girl has no known medical conditions and takes no medications.&amp;nbsp; Temperature is 36.7 C (98.1 F), blood pressure is 100/40 mm Hg, and pulse is 112/min.&amp;nbsp; Examination shows a soft and nontender abdomen, and bowel sounds are present.&amp;nbsp; Hemoglobin is 8 g/dL, and platelet count is 215,000/mm<font size=&quot;2&quot;><sup>3</sup></font>.&amp;nbsp; Coagulation studies are normal.&amp;nbsp; Sequential imaging using <font size=&quot;2&quot;><sup>99m</sup></font>Tc-pertechnetate scintigraphy demonstrates focal radiotracer accumulation in the right lower quadrant.&amp;nbsp; Which of the following embryologic processes most likely resulted in this patient&amp;#39;s condition?</p>\"><p class=\"ng-scope\">A 4-year-old girl is brought to the office after her parents noticed dark red blood on her toilet tissue after a bowel movement.&nbsp; She has had no abdominal pain, dysuria, or discomfort with defecation.&nbsp; The girl has no known medical conditions and takes no medications.&nbsp; Temperature is 36.7 C (98.1 F), blood pressure is 100/40 mm Hg, and pulse is 112/min.&nbsp; Examination shows a soft and nontender abdomen, and bowel sounds are present.&nbsp; Hemoglobin is 8 g/dL, and platelet count is 215,000/mm<font size=\"2\"><sup>3</sup></font>.&nbsp; Coagulation studies are normal.&nbsp; Sequential imaging using <font size=\"2\"><sup>99m</sup></font>Tc-pertechnetate scintigraphy demonstrates focal radiotracer accumulation in the right lower quadrant.&nbsp; Which of the following embryologic processes most likely resulted in this patient's condition?</p></div>\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && (currentQuestion.formatTypeId == constants.questionFormatType.fillInTheBlank ||currentQuestion.formatTypeId == constants.questionFormatType.fillInTheBlankRange )&& currentQuestion.sectionId == constants.collegeprepSectionIds.math -->\n                <!-- change to mediatype object -->\n                <!-- ngIf: currentQuestion.questionMedia.typeId == constants.questionMediaType.drugad || currentQuestion.questionTypeId == constants.questionType.drugad -->\n                <!-- ngIf: currentQuestion.questionMedia.typeId == constants.questionMediaType.audio || currentQuestion.questionMedia.typeId == constants.questionMediaType.video || currentQuestion.questionTypeId == constants.questionType.media -->\n\n                <div id=\"answerContainer\"><div class=\"options\">\n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>A.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight1\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Arrested hindgut descent along the inferior mesenteric artery</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>B.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight2\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Defective neural crest cell migration into the bowel wall</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>C.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight3\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Disrupted vascular flow to the ileum</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>D.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight4\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Failed obliteration of the vitelline duct</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>E.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight5\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Partial midgut rotation around the superior mesenteric artery</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><div id=&quot;div_t45286&quot;><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; class=&quot;table-default-style&quot;><tbody><tr><td colspan=&quot;2&quot;><p align=&quot;center&quot;><strong>Meckel diverticulum</strong></p></td></tr><tr><td><p align=&quot;center&quot;><strong>Pathogenesis</strong></p></td><td><ul><li>Persistent vitelline (omphalomesenteric) duct</li><li>Often contains ectopic gastric mucosa &amp;rarr; intestinal ulceration &amp;amp; bleeding</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Epidemiology</strong></p></td><td><p>Rule of 2s:</p><ul><li>Presentation often by age 2 (but can occur at any age)</li><li>Usually &amp;lt;2 inches long</li><li>Location within 2 feet of ileocecal valve</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Clinical presentation</strong></p></td><td><ul><li>May be asymptomatic (incidental finding)</li><li>Painless rectal bleeding &amp;plusmn; iron deficiency anemia</li><li>Acute abdominal pain due to complications:<ul style=&quot;list-style-type:circle;&quot;><li>Intussusception (recurrent, atypical)</li><li>Diverticulitis (mimics appendicitis)</li><li>Bowel obstruction, perforation (peritoneal signs)</li></ul></li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Diagnosis</strong></p></td><td><ul><li>Technetium-99m pertechnetate (Meckel) scan detects gastric mucosa</li></ul></td></tr></tbody></table></div><p>This patient with painless rectal bleeding has imaging findings consistent with a <strong>Meckel diverticulum</strong>, a congenital intestinal abnormality that results from failed obliteration of the <strong>vitelline (omphalomesenteric) duct</strong>.&amp;nbsp; The <a ng-click=&quot;showexhibit('504',true);&quot;>vitelline duct</a> is an embryologic remnant that connects the yolk sac to the primitive midgut, and duct closure normally occurs in early gestation.&amp;nbsp; Incomplete closure results in an ileal outpouching with a fibrous connection to the umbilicus.</p><p>Meckel diverticulum often contains heterotopic <strong>gastric mucosa</strong>.&amp;nbsp; Secretion of hydrochloric acid by gastric parietal cells causes ulceration and bleeding of local small bowel, resulting in typical findings of <strong>painless lower gastrointestinal (GI) bleeding</strong> and <strong>anemia</strong>, as seen in this patient.&amp;nbsp; Less commonly, Meckel diverticulum can present with acute abdominal pain; for example, it can serve as a lead point for intussusception, causing colicky pain and currant jelly stools.&amp;nbsp; Although presentation of Meckel diverticulum is most common at age &amp;lt;2, it can occur at any age.</p><p>Diagnosis is made with <a ng-click=&quot;showexhibit('4010',true);&quot;><font size=&quot;2&quot;><sup>99m</sup></font>Tc-pertechnetate scan</a> (ie, Meckel scan).&amp;nbsp; The radioisotope <font size=&quot;2&quot;><sup>99m</sup></font>Tc-pertechnetate has an affinity for <strong>parietal cells</strong> of gastric mucosa, and increased uptake of the tracer in the periumbilical/right lower quadrant area is characteristic.</p><p><strong>(Choice A)</strong>&amp;nbsp; The hindgut, supplied by the inferior mesenteric artery, gives rise to the distal colon, rectum, and upper part of the anal canal.&amp;nbsp; Arrest of hindgut descent in utero leads to an imperforate anus, which presents in neonates with failure to pass stool, not painless rectal bleeding.</p><p><strong>(Choices B and C)</strong>&amp;nbsp; Failure of craniocaudal neural crest cell migration results in an aganglionic sigmoid colon/rectum that is narrow and inactive (ie, <a ng-click=&quot;showexhibit('4011',true);&quot;>Hirschsprung disease</a>), and disrupted vascular flow to the ileum results in ileal atresia.&amp;nbsp; Both conditions typically manifest with signs of obstruction (eg, abdominal distension, bilious emesis, failure to pass meconium) in a neonate, not painless GI bleeding in a 4-year-old.</p><p><strong>(Choice E)</strong>&amp;nbsp; Abnormal midgut rotation around the superior mesenteric artery describes intestinal malrotation.&amp;nbsp; Patients may have hematochezia because the narrow mesenteric base seen in malrotation permits the small bowel to twist with subsequent ischemia (ie, <a ng-click=&quot;showexhibit('24431',true);&quot;>volvulus</a>).&amp;nbsp; However, abdominal pain, vomiting, and signs of peritonitis are also typically present.&amp;nbsp; Moreover, <font size=&quot;2&quot;><sup>99m</sup></font>Tc-pertechnetate scintigraphy would not show abnormal uptake.</p><p><strong>Educational objective:</strong><br>Meckel diverticulum results from incomplete obliteration of the vitelline duct and usually presents with painless lower gastrointestinal bleeding.&amp;nbsp; <font size=&quot;2&quot;><sup>99m</sup></font>Tc-pertechnetate localizes to heterotopic gastric mucosa that is often contained within a Meckel diverticulum, and uptake in the periumbilical area or right lower quadrant is diagnostic.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><div id=\"div_t45286\" class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" class=\"table-default-style\"><tbody><tr><td colspan=\"2\"><p align=\"center\"><strong>Meckel diverticulum</strong></p></td></tr><tr><td><p align=\"center\"><strong>Pathogenesis</strong></p></td><td><ul><li>Persistent vitelline (omphalomesenteric) duct</li><li>Often contains ectopic gastric mucosa \u2192 intestinal ulceration &amp; bleeding</li></ul></td></tr><tr><td><p align=\"center\"><strong>Epidemiology</strong></p></td><td><p>Rule of 2s:</p><ul><li>Presentation often by age 2 (but can occur at any age)</li><li>Usually &lt;2 inches long</li><li>Location within 2 feet of ileocecal valve</li></ul></td></tr><tr><td><p align=\"center\"><strong>Clinical presentation</strong></p></td><td><ul><li>May be asymptomatic (incidental finding)</li><li>Painless rectal bleeding \u00b1 iron deficiency anemia</li><li>Acute abdominal pain due to complications:<ul style=\"list-style-type:circle;\"><li>Intussusception (recurrent, atypical)</li><li>Diverticulitis (mimics appendicitis)</li><li>Bowel obstruction, perforation (peritoneal signs)</li></ul></li></ul></td></tr><tr><td><p align=\"center\"><strong>Diagnosis</strong></p></td><td><ul><li>Technetium-99m pertechnetate (Meckel) scan detects gastric mucosa</li></ul></td></tr></tbody></table></div><p class=\"ng-scope\">This patient with painless rectal bleeding has imaging findings consistent with a <strong>Meckel diverticulum</strong>, a congenital intestinal abnormality that results from failed obliteration of the <strong>vitelline (omphalomesenteric) duct</strong>.&nbsp; The <a ng-click=\"showexhibit('504',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L7316.jpg&quot;,&quot;title&quot;:&quot;L7316&quot;}]\">vitelline duct</a> is an embryologic remnant that connects the yolk sac to the primitive midgut, and duct closure normally occurs in early gestation.&nbsp; Incomplete closure results in an ileal outpouching with a fibrous connection to the umbilicus.</p><p class=\"ng-scope\">Meckel diverticulum often contains heterotopic <strong>gastric mucosa</strong>.&nbsp; Secretion of hydrochloric acid by gastric parietal cells causes ulceration and bleeding of local small bowel, resulting in typical findings of <strong>painless lower gastrointestinal (GI) bleeding</strong> and <strong>anemia</strong>, as seen in this patient.&nbsp; Less commonly, Meckel diverticulum can present with acute abdominal pain; for example, it can serve as a lead point for intussusception, causing colicky pain and currant jelly stools.&nbsp; Although presentation of Meckel diverticulum is most common at age &lt;2, it can occur at any age.</p><p class=\"ng-scope\">Diagnosis is made with <a ng-click=\"showexhibit('4010',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L27865.jpg&quot;,&quot;title&quot;:&quot;L27865&quot;}]\"><font size=\"2\"><sup>99m</sup></font>Tc-pertechnetate scan</a> (ie, Meckel scan).&nbsp; The radioisotope <font size=\"2\"><sup>99m</sup></font>Tc-pertechnetate has an affinity for <strong>parietal cells</strong> of gastric mucosa, and increased uptake of the tracer in the periumbilical/right lower quadrant area is characteristic.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; The hindgut, supplied by the inferior mesenteric artery, gives rise to the distal colon, rectum, and upper part of the anal canal.&nbsp; Arrest of hindgut descent in utero leads to an imperforate anus, which presents in neonates with failure to pass stool, not painless rectal bleeding.</p><p class=\"ng-scope\"><strong>(Choices B and C)</strong>&nbsp; Failure of craniocaudal neural crest cell migration results in an aganglionic sigmoid colon/rectum that is narrow and inactive (ie, <a ng-click=\"showexhibit('4011',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L24344.jpg&quot;,&quot;title&quot;:&quot;L24344&quot;}]\">Hirschsprung disease</a>), and disrupted vascular flow to the ileum results in ileal atresia.&nbsp; Both conditions typically manifest with signs of obstruction (eg, abdominal distension, bilious emesis, failure to pass meconium) in a neonate, not painless GI bleeding in a 4-year-old.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Abnormal midgut rotation around the superior mesenteric artery describes intestinal malrotation.&nbsp; Patients may have hematochezia because the narrow mesenteric base seen in malrotation permits the small bowel to twist with subsequent ischemia (ie, <a ng-click=\"showexhibit('24431',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L29144.jpg&quot;,&quot;title&quot;:&quot;L29144&quot;}]\">volvulus</a>).&nbsp; However, abdominal pain, vomiting, and signs of peritonitis are also typically present.&nbsp; Moreover, <font size=\"2\"><sup>99m</sup></font>Tc-pertechnetate scintigraphy would not show abnormal uptake.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Meckel diverticulum results from incomplete obliteration of the vitelline duct and usually presents with painless lower gastrointestinal bleeding.&nbsp; <font size=\"2\"><sup>99m</sup></font>Tc-pertechnetate localizes to heterotopic gastric mucosa that is often contained within a Meckel diverticulum, and uptake in the periumbilical area or right lower quadrant is diagnostic.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepe