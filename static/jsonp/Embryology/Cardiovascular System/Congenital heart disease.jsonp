nswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; id=&quot;752&quot;  src=&quot;https://www.uworld.com/media/L1458.jpg&quot; draggable=&quot;false&quot; ></p><p>This patient&amp;#39;s clinical presentation (<strong>cyanotic spells</strong> that improve with <strong>squatting</strong>, prominent right ventricular impulse, <strong>systolic murmur</strong>) is consistent with tetralogy of Fallot (TOF).&amp;nbsp; Abnormal neural crest cell migration leads to anterior and cephalad deviation of the infundibular septum during embryologic development, resulting in a malaligned VSD and an overriding aorta.&amp;nbsp; TOF is characterized by 4 distinct anatomic abnormalities:</p><ul><li>Ventricular septal defect (VSD)</li><li>Overriding aorta over the right and left ventricles</li><li>Right ventricular outflow tract (RVOT) obstruction</li><li>Right ventricular hypertrophy</li></ul><p>Cyanosis occurs due to the presence of right-to-left shunt in patients with severe or worsening <strong>RVOT obstruction</strong>.&amp;nbsp; The typical harsh, systolic ejection murmur over the mid-to-left upper sternal border is due to the presence of RVOT obstruction (subvalvular, pulmonary valve stenosis or supravalvular narrowing in the main pulmonary artery).&amp;nbsp; Squatting increases the peripheral systemic vascular resistance (afterload) and decreases the degree of right-to-left shunting across the VSD, thereby improving cyanosis.</p><p><strong>(Choice A)</strong>&amp;nbsp; In <a ng-click=&quot;showexhibit('785',true);&quot;>anomalous pulmonary venous return</a>, blood from both the pulmonary (oxygenated) and systemic (deoxygenated) venous systems flow into the right atrium, leading to right atrial and ventricular dilation.&amp;nbsp; Patients also have obligatory right-to-left atrial shunting.</p><p><strong>(Choice B)</strong>&amp;nbsp; Aortic arch constriction results in <a ng-click=&quot;showexhibit('922',true);&quot;>coarctation of the aorta</a> and is most commonly located just distal to the left subclavian artery (juxtaductal).&amp;nbsp; Common manifestations include brachial-femoral pulse delay and blood pressure discrepancy between the upper and lower extremities.</p><p><strong>(Choice D)</strong>&amp;nbsp; Failed fusion of the superior and inferior endocardial cushions (eg, endocardial cushion defect) results in defects of the atrioventricular septum and valves (mitral and tricuspid valves).&amp;nbsp; These typically manifest initially as atrial defects and/or VSDs with left-to-right shunting of blood.&amp;nbsp; Over time, the increased right-sided blood flow leads to pulmonary hypertension with reversal of blood flow through the shunt and development of cyanosis (<a ng-click=&quot;showexhibit('2170',true);&quot;>Eisenmenger syndrome</a>).&amp;nbsp; This patient has had cyanotic symptoms since birth that would not be expected with an isolated endocardial cushion defect.</p><p><strong>(Choice E)</strong>&amp;nbsp; <a ng-click=&quot;showexhibit('921',true);&quot;>Transposition of the great arteries</a> results from linear, rather than spiral, development of the aorticopulmonary septum in utero.</p><p><strong>Educational objective:</strong><br>Tetralogy of Fallot results from anterior and cephalad deviation of the infundibular septum during embryologic development, resulting in a malaligned ventricular septal defect (VSD) with an overriding aorta.&amp;nbsp; As a result, the patient has right ventricular outflow obstruction (resulting in a systolic murmur) and squats to increase the peripheral systemic vascular resistance (afterload) and decrease right-to-left shunting across the VSD.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" id=\"752\" src=\"image/L1458.jpg\" draggable=\"false\"></p><p class=\"ng-scope\">This patient's clinical presentation (<strong>cyanotic spells</strong> that improve with <strong>squatting</strong>, prominent right ventricular impulse, <strong>systolic murmur</strong>) is consistent with tetralogy of Fallot (TOF).&nbsp; Abnormal neural crest cell migration leads to anterior and cephalad deviation of the infundibular septum during embryologic development, resulting in a malaligned VSD and an overriding aorta.&nbsp; TOF is characterized by 4 distinct anatomic abnormalities:</p><ul class=\"ng-scope\"><li>Ventricular septal defect (VSD)</li><li>Overriding aorta over the right and left ventricles</li><li>Right ventricular outflow tract (RVOT) obstruction</li><li>Right ventricular hypertrophy</li></ul><p class=\"ng-scope\">Cyanosis occurs due to the presence of right-to-left shunt in patients with severe or worsening <strong>RVOT obstruction</strong>.&nbsp; The typical harsh, systolic ejection murmur over the mid-to-left upper sternal border is due to the presence of RVOT obstruction (subvalvular, pulmonary valve stenosis or supravalvular narrowing in the main pulmonary artery).&nbsp; Squatting increases the peripheral systemic vascular resistance (afterload) and decreases the degree of right-to-left shunting across the VSD, thereby improving cyanosis.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; In <a ng-click=\"showexhibit('785',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L14172.jpg&quot;,&quot;title&quot;:&quot;L14172&quot;}]\">anomalous pulmonary venous return</a>, blood from both the pulmonary (oxygenated) and systemic (deoxygenated) venous systems flow into the right atrium, leading to right atrial and ventricular dilation.&nbsp; Patients also have obligatory right-to-left atrial shunting.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Aortic arch constriction results in <a ng-click=\"showexhibit('922',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L6361.jpg&quot;,&quot;title&quot;:&quot;L6361&quot;}]\">coarctation of the aorta</a> and is most commonly located just distal to the left subclavian artery (juxtaductal).&nbsp; Common manifestations include brachial-femoral pulse delay and blood pressure discrepancy between the upper and lower extremities.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Failed fusion of the superior and inferior endocardial cushions (eg, endocardial cushion defect) results in defects of the atrioventricular septum and valves (mitral and tricuspid valves).&nbsp; These typically manifest initially as atrial defects and/or VSDs with left-to-right shunting of blood.&nbsp; Over time, the increased right-sided blood flow leads to pulmonary hypertension with reversal of blood flow through the shunt and development of cyanosis (<a ng-click=\"showexhibit('2170',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L18474.jpg&quot;,&quot;title&quot;:&quot;L18474&quot;}]\">Eisenmenger syndrome</a>).&nbsp; This patient has had cyanotic symptoms since birth that would not be expected with an isolated endocardial cushion defect.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; <a ng-click=\"showexhibit('921',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L16503.jpg&quot;,&quot;title&quot;:&quot;L16503&quot;}]\">Transposition of the great arteries</a> results from linear, rather than spiral, development of the aorticopulmonary septum in utero.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Tetralogy of Fallot results from anterior and cephalad deviation of the infundibular septum during embryologic development, resulting in a malaligned ventricular septal defect (VSD) with an overriding aorta.&nbsp; As a result, the patient has right ventricular outflow obstruction (resulting in a systolic murmur) and squats to increase the peripheral systemic vascular resistance (afterload) and decrease right-to-left shunting across the VSD.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/22010618\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubm