    },
    {
        "question": "<div class=\"questioncontent ui-resizable widthWithoutAbstract\" id=\"questionContent\" ng-show=\"showQuestion\" ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat, 'suggested-highlights-visible' : userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; currentQuestion.frequentHighlights != null &amp;&amp; (suggestedHighlightsPreference == 1 || suggestedHighlightsPreference == 2), 'only-suggested-highlights-visible' : userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; suggestedHighlightsPreference == 1 &amp;&amp; currentQuestion.frequentHighlights != null}\">\n            <div style=\"display:none;\" class=\"suggested-highlights-switch-buttons ng-hide\" ng-show=\"userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; currentQuestion.frequentHighlights != null\">\n                <span class=\"fa-stack\" ng-click=\"showUserHighlights()\" title=\"My Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fas\" ng-class=\"{'fas' : suggestedHighlightsPreference == 0, 'fal' : suggestedHighlightsPreference != 0}\"></i>\n                    <i class=\"fal fa-highlighter fa-stack-1x fa-inverse\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 0}\"></i>\n                </span>\n                <span class=\"fa-stack ng-hide\" ng-hide=\"currentQuestion.frequentHighlights == null\" ng-click=\"showSuggestedHighlights()\" title=\"Suggested Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fal\" ng-class=\"{'fas' : suggestedHighlightsPreference == 1, 'fal' : suggestedHighlightsPreference != 1}\"></i>\n                    <i class=\"fas fa-highlighter fa-stack-1x\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 1}\"></i>\n                </span>\n                <span class=\"fa-stack highlight-preference-unavailable highlight-button-with-popover\" ng-show=\"currentQuestion.frequentHighlights == null\" title=\"\" data-toggle=\"popover\" data-placement=\"bottom\" data-content=\"Collecting Data\" data-trigger=\"hover\" data-original-title=\"Suggested Highlights\">\n                    <i class=\"fal fa-circle fa-stack-2x\"></i>\n                    <i class=\"fas fa-highlighter fa-stack-1x\"></i>\n                </span>\n                <span class=\"fa-stack ng-hide\" ng-hide=\"currentQuestion.frequentHighlights == null\" ng-click=\"showUserAndSuggestedHighlights()\" title=\"Duo Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fal\" ng-class=\"{'fas' : suggestedHighlightsPreference == 2, 'fal' : suggestedHighlightsPreference != 2}\"></i>\n                    <i class=\"fad fa-highlighter fa-stack-1x\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 2}\"></i>\n                </span>\n                <span class=\"fa-stack highlight-preference-unavailable highlight-button-with-popover\" ng-show=\"currentQuestion.frequentHighlights == null\" title=\"\" data-toggle=\"popover\" data-placement=\"bottom\" data-content=\"Collecting Data\" data-trigger=\"hover\" data-original-title=\"Duo Highlights\">\n                    <i class=\"fal fa-circle fa-stack-2x\"></i>\n                    <i class=\"fad fa-highlighter fa-stack-1x\"></i>\n                </span>\n            </div>\n            <!-- ngIf: launchTestOptions.launchCode == 'nbome' && userPreferences.testPreferences.theme != clientConstants.themes.nbome -->\n            <div id=\"question\">\n                <!-- ngIf: settings.hints && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && launchTestOptions.canShowHint -->\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && (currentQuestion.qbankId != clientConstants.qbankIds.stats_faculty || currentQuestion.qbankId != clientConstants.qbankIds.stats) -->\n                <!-- ngIf: (currentQuestion.mobileExcerpt != '' && currentQuestion.mobileExcerpt != null ) -->\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat -->\n                <!--Periodic table button for MCAT -->\n                <!--<div class=\"text-center periodicTableDiv\" ng-show=\"(launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.subjectId != constants.noPeriodicTable.cars) && (currentQuestion.subjectId != constants.noPeriodicTable.pyschology) && (currentQuestion.subjectId != constants.noPeriodicTable.sociology)\">\n                    <button class=\"btn btn-default btn--custom\" style=\"color: black; padding: 4px 3px;\" ng-click=\"showresources()\">Periodic table&nbsp;<img src=\"img/mcat/Periodic_icon.png\" height=\"12\" style=\"margin-top: -2px; cursor: pointer\" title=\"Periodic Table\" /></button>\n                </div>-->\n                \n                <!--Inactive message for questions in usmle, boards and nclex and do not show for EXPERIMENTAL_INACTIVE hence the check !=3 -->\n                <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.nclex) && !currentQuestion.isActive && currentQuestion.activeStatusId != 3 -->\n                <div id=\"questionText\" class=\"questionText ng-scope\" ng-mouseup=\"invokeHighlights('questionText',constants.flashCardType.selection,$event)\" compiledata=\"\" htmlcontent=\"<p>A 77-year-old man comes to the emergency department due to cough, shortness of breath, sharp chest pains, night sweats, and fatigue for 4 days.&amp;nbsp; Temperature is 38.3 C (101 F), blood pressure is 110/62 mm Hg, pulse is 106/min, and respirations are 24/min.&amp;nbsp; The patient appears ill.&amp;nbsp; Neck veins are nondistended.&amp;nbsp; There is decreased chest expansion on the right side.&amp;nbsp; Auscultation reveals decreased breath sounds over the right lung base.&amp;nbsp; Chest imaging shows a right lower lobe infiltrate and a small, free-flowing pleural effusion on the right side.&amp;nbsp; Thoracentesis is performed.&amp;nbsp; Laboratory results are as follows:</p><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; border=&quot;0&quot; cellpadding=&quot;0&quot; cellspacing=&quot;0&quot;><tbody><tr><td colspan=&quot;2&quot; style=&quot;vertical-align:middle;&quot;>Pleural fluid analysis</td></tr><tr><td style=&quot;vertical-align:middle;&quot;>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;Leukocytes</td><td style=&quot;vertical-align:middle;&quot;>620/mm<font size=&quot;2&quot;><sup>3</sup></font></td></tr><tr><td style=&quot;vertical-align:middle;&quot;>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;pH</td><td style=&quot;vertical-align:middle;&quot;>normal</td></tr><tr><td style=&quot;vertical-align:middle;&quot;>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;Glucose</td><td style=&quot;vertical-align:middle;&quot;>normal</td></tr><tr><td style=&quot;vertical-align:middle;&quot;>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;Pleural fluid/plasma lactate dehydrogenase ratio</td><td style=&quot;vertical-align:middle;&quot;>0.7</td></tr></tbody></table><p>Which of the following processes best explains this patient&amp;#39;s pleural fluid findings?</p>\"><p class=\"ng-scope\">A 77-year-old man comes to the emergency department due to cough, shortness of breath, sharp chest pains, night sweats, and fatigue for 4 days.&nbsp; Temperature is 38.3 C (101 F), blood pressure is 110/62 mm Hg, pulse is 106/min, and respirations are 24/min.&nbsp; The patient appears ill.&nbsp; Neck veins are nondistended.&nbsp; There is decreased chest expansion on the right side.&nbsp; Auscultation reveals decreased breath sounds over the right lung base.&nbsp; Chest imaging shows a right lower lobe infiltrate and a small, free-flowing pleural effusion on the right side.&nbsp; Thoracentesis is performed.&nbsp; Laboratory results are as follows:</p><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" border=\"0\" cellpadding=\"0\" cellspacing=\"0\" class=\"ng-scope\"><tbody><tr><td colspan=\"2\" style=\"vertical-align:middle;\">Pleural fluid analysis</td></tr><tr><td style=\"vertical-align:middle;\">&nbsp;&nbsp;&nbsp;&nbsp;Leukocytes</td><td style=\"vertical-align:middle;\">620/mm<font size=\"2\"><sup>3</sup></font></td></tr><tr><td style=\"vertical-align:middle;\">&nbsp;&nbsp;&nbsp;&nbsp;pH</td><td style=\"vertical-align:middle;\">normal</td></tr><tr><td style=\"vertical-align:middle;\">&nbsp;&nbsp;&nbsp;&nbsp;Glucose</td><td style=\"vertical-align:middle;\">normal</td></tr><tr><td style=\"vertical-align:middle;\">&nbsp;&nbsp;&nbsp;&nbsp;Pleural fluid/plasma lactate dehydrogenase ratio</td><td style=\"vertical-align:middle;\">0.7</td></tr></tbody></table><p class=\"ng-scope\">Which of the following processes best explains this patient's pleural fluid findings?</p></div>\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && (currentQuestion.formatTypeId == constants.questionFormatType.fillInTheBlank ||currentQuestion.formatTypeId == constants.questionFormatType.fillInTheBlankRange )&& currentQuestion.sectionId == constants.collegeprepSectionIds.math -->\n                <!-- change to mediatype object -->\n                <!-- ngIf: currentQuestion.questionMedia.typeId == constants.questionMediaType.drugad || currentQuestion.questionTypeId == constants.questionType.drugad -->\n                <!-- ngIf: currentQuestion.questionMedia.typeId == constants.questionMediaType.audio || currentQuestion.questionMedia.typeId == constants.questionMediaType.video || currentQuestion.questionTypeId == constants.questionType.media -->\n\n                <div id=\"answerContainer\"><div class=\"options\">\n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>A.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight1\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Elevated pulmonary capillary hydrostatic pressure</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>B.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight2\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Movement of sterile exudate into the pleural space</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>C.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight3\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Obstruction of lymphatic pleural fluid drainage</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>D.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight4\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Reduced pleural capillary oncotic pressure</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><div id=&quot;div_T63781&quot;><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; class=&quot;table-default-style&quot; style=&quot;width:576px&quot;><tbody><tr><td colspan=&quot;3&quot;><p align=&quot;center&quot;><strong>Parapneumonic effusions</strong></p></td></tr><tr><td></td><td><p align=&quot;center&quot;><strong>Uncomplicated</strong></p></td><td><p align=&quot;center&quot;><strong>Complicated*</strong></p></td></tr><tr><td><p align=&quot;center&quot;><strong>Etiology</strong></p></td><td><p align=&quot;center&quot;>Sterile exudate in<br>pleural space</p></td><td><p align=&quot;center&quot;>Bacterial invasion of<br>pleural space</p></td></tr><tr><td><p align=&quot;center&quot;><strong>Radiologic<br>appearance</strong></p></td><td><p align=&quot;center&quot;>Small to moderate &amp;amp;<br>free flowing</p></td><td><p align=&quot;center&quot;>Moderate to large,<br>free flowing or loculated</p></td></tr><tr><td><p align=&quot;center&quot;><strong>Pleural fluid<br>characteristics</strong></p></td><td><ul><li>pH &amp;#8805;7.2</li><li>Glucose &amp;#8805;60 mg/dL</li><li>WBCs &amp;#8804;50,000/mm<sup>3</sup></li></ul></td><td><ul><li>pH &amp;lt;7.2</li><li>Glucose &amp;lt;60 mg/dL</li><li>WBCs &amp;gt;50,000/mm<sup>3</sup></li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Treatment</strong></p></td><td><p align=&quot;center&quot;>Antibiotics</p></td><td><p align=&quot;center&quot;>Antibiotics + drainage</p></td></tr><tr><td colspan=&quot;3&quot;><p>*Empyema represents advanced progression of a complicated effusion.<br><strong>WBC</strong> = white blood cell.</p></td></tr></tbody></table></div><p>This patient&amp;#39;s presentation with fever and several days of cough, shortness of breath, and sharp chest pain is consistent with <strong>community-acquired pneumonia</strong> (CAP).&amp;nbsp; The right lower lobe infiltrate on chest x-ray supports the diagnosis of CAP and reveals an associated <strong>parapneumonic effusion</strong>, which develops in approximately 40% of bacterial pneumonia cases.</p><p>Parapneumonic effusions result from an <a ng-click=&quot;showexhibit('22505',true);&quot;>inflammatory increase</a> in vascular membrane permeability and therefore are <strong>exudative</strong> by <a ng-click=&quot;showexhibit('18479',true);&quot;>Light criteria</a>, which is confirmed by this patient&amp;#39;s pleural fluid/serum lactate dehydrogenase ratio &amp;gt;0.6.&amp;nbsp; Most parapneumonic effusions are <strong>uncomplicated</strong>, resulting from <strong>movement of sterile exudate</strong> into the pleural space and exhibiting <strong>normal pH and glucose concentration</strong> (near that of serum) as well as relatively <strong>low leukocyte count</strong> (eg, &amp;lt;50,000/mm<font size=&quot;2&quot;><sup>3</sup></font>) (as in this patient).&amp;nbsp; Complicated parapneumonic effusions involve inflammatory disruption of the visceral pleura membrane with bacterial seeding of the pleural space.&amp;nbsp; These effusions are recognized by low pH and glucose levels (due to bacteria metabolizing glucose and producing acidic wastes) and high leukocyte count.</p><p><strong>(Choices A and D)</strong>&amp;nbsp; Changes in hydrostatic or oncotic pressure drive the development of transudative (not exudative) pleural effusions.&amp;nbsp; Elevated pulmonary capillary hydrostatic pressure is the cause of pleural effusion in decompensated heart failure, and reduced pleural capillary oncotic pressure contributes to pleural effusion in conditions that cause hypoalbuminemia (eg, malnutrition, nephrotic syndrome).&amp;nbsp; The pleural fluid/serum lactate dehydrogenase ratio is &amp;lt;0.6 with transudative effusions.</p><p><strong>(Choice C)</strong>&amp;nbsp; Fluid drains from the pleural cavity via lymphatics on the parietal surface; therefore, lymphatic obstruction (eg, due to malignant spread within the pleural space) can cause a pleural effusion.&amp;nbsp; These effusions are usually exudative by Light criteria due to malignancy-associated inflammation.&amp;nbsp; However, this patient&amp;#39;s clinical presentation is more consistent with CAP.</p><p><strong>Educational objective:</strong><br>Parapneumonic effusions are exudative by Light criteria and are classified into uncomplicated and complicated.&amp;nbsp; Uncomplicated parapneumonic effusions result from movement of sterile exudate into the pleural space, whereas complicated parapneumonic effusions involve inflammatory disruption of the visceral pleural membrane with bacterial translocation into the pleural space.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><div id=\"div_T63781\" class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" class=\"table-default-style\" style=\"width:576px\"><tbody><tr><td colspan=\"3\"><p align=\"center\"><strong>Parapneumonic effusions</strong></p></td></tr><tr><td></td><td><p align=\"center\"><strong>Uncomplicated</strong></p></td><td><p align=\"center\"><strong>Complicated*</strong></p></td></tr><tr><td><p align=\"center\"><strong>Etiology</strong></p></td><td><p align=\"center\">Sterile exudate in<br>pleural space</p></td><td><p align=\"center\">Bacterial invasion of<br>pleural space</p></td></tr><tr><td><p align=\"center\"><strong>Radiologic<br>appearance</strong></p></td><td><p align=\"center\">Small to moderate &amp;<br>free flowing</p></td><td><p align=\"center\">Moderate to large,<br>free flowing or loculated</p></td></tr><tr><td><p align=\"center\"><strong>Pleural fluid<br>characteristics</strong></p></td><td><ul><li>pH \u22657.2</li><li>Glucose \u226560 mg/dL</li><li>WBCs \u226450,000/mm<sup>3</sup></li></ul></td><td><ul><li>pH &lt;7.2</li><li>Glucose &lt;60 mg/dL</li><li>WBCs &gt;50,000/mm<sup>3</sup></li></ul></td></tr><tr><td><p align=\"center\"><strong>Treatment</strong></p></td><td><p align=\"center\">Antibiotics</p></td><td><p align=\"center\">Antibiotics + drainage</p></td></tr><tr><td colspan=\"3\"><p>*Empyema represents advanced progression of a complicated effusion.<br><strong>WBC</strong> = white blood cell.</p></td></tr></tbody></table></div><p class=\"ng-scope\">This patient's presentation with fever and several days of cough, shortness of breath, and sharp chest pain is consistent with <strong>community-acquired pneumonia</strong> (CAP).&nbsp; The right lower lobe infiltrate on chest x-ray supports the diagnosis of CAP and reveals an associated <strong>parapneumonic effusion</strong>, which develops in approximately 40% of bacterial pneumonia cases.</p><p class=\"ng-scope\">Parapneumonic effusions result from an <a ng-click=\"showexhibit('22505',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L48965.jpg&quot;,&quot;title&quot;:&quot;L48965&quot;}]\">inflammatory increase</a> in vascular membrane permeability and therefore are <strong>exudative</strong> by <a ng-click=\"showexhibit('18479',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/43194.html&quot;,&quot;title&quot;:&quot;43194&quot;}]\">Light criteria</a>, which is confirmed by this patient's pleural fluid/serum lactate dehydrogenase ratio &gt;0.6.&nbsp; Most parapneumonic effusions are <strong>uncomplicated</strong>, resulting from <strong>movement of sterile exudate</strong> into the pleural space and exhibiting <strong>normal pH and glucose concentration</strong> (near that of serum) as well as relatively <strong>low leukocyte count</strong> (eg, &lt;50,000/mm<font size=\"2\"><sup>3</sup></font>) (as in this patient).&nbsp; Complicated parapneumonic effusions involve inflammatory disruption of the visceral pleura membrane with bacterial seeding of the pleural space.&nbsp; These effusions are recognized by low pH and glucose levels (due to bacteria metabolizing glucose and producing acidic wastes) and high leukocyte count.</p><p class=\"ng-scope\"><strong>(Choices A and D)</strong>&nbsp; Changes in hydrostatic or oncotic pressure drive the development of transudative (not exudative) pleural effusions.&nbsp; Elevated pulmonary capillary hydrostatic pressure is the cause of pleural effusion in decompensated heart failure, and reduced pleural capillary oncotic pressure contributes to pleural effusion in conditions that cause hypoalbuminemia (eg, malnutrition, nephrotic syndrome).&nbsp; The pleural fluid/serum lactate dehydrogenase ratio is &lt;0.6 with transudative effusions.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Fluid drains from the pleural cavity via lymphatics on the parietal surface; therefore, lymphatic obstruction (eg, due to malignant spread within the pleural space) can cause a pleural effusion.&nbsp; These effusions are usually exudative by Light criteria due to malignancy-associated inflammation.&nbsp; However, this patient's clinical presentation is more consistent with CAP.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Parapneumonic effusions are exudative by Light criteria and are classified into uncomplicated and complicated.&nbsp; Uncomplicated parapneumonic effusions result from movement of sterile exudate into the pleural space, whereas complicated parapneumonic effusions involve inflammatory disruption of the visceral pleural membrane with bacterial translocation into the pleural space.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/16623208\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/16623208\"><span compiledata=\"\" htmlcontent=\"Diagnostic approach to pleural effusion in adults.\" class=\"ng-scope\"><p class=\"ng-scope\">Diagnostic approach to pleural effusion in adults.</p></span></a><!-- end ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal -->\n                                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal -->\n                                </li><!-- end ngRepeat: questionReference in currentQuestion.questionM