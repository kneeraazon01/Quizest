estMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><div id=&quot;div_T48541&quot;><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; class=&quot;table-default-style table-header-footer-style&quot;><tbody><tr><td colspan=&quot;2&quot;><p align=&quot;center&quot;><strong>Sj&amp;ouml;gren syndrome</strong></p></td></tr><tr><td><p align=&quot;center&quot;><strong>Pathogenesis</strong></p></td><td><ul><li>Immune-mediated destruction of the lacrimal &amp;amp; salivary glands</li><li>Can occur as primary disease or secondary with other autoimmune disorders (eg, SLE, RA)</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Clinical features</strong></p></td><td><ul><li>Dry eyes (keratoconjunctivitis sicca)</li><li>Dry mouth (xerostomia), salivary hypertrophy</li><li>Dry skin (xerosis)</li><li>Raynaud phenomenon</li><li>Cutaneous vasculitis</li><li>Positive anti-Ro (SSA) &amp;amp;/or anti-La (SSB)</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Complications</strong></p></td><td><ul><li>Non-Hodgkin lymphoma</li><li>Corneal damage, dental caries</li></ul></td></tr><tr><td colspan=&quot;2&quot;><p><strong>RA</strong> = rheumatoid arthritis; <strong>SLE</strong> = systemic lupus erythematosus; <strong>SSA/SSB</strong> = Sj&amp;ouml;gren syndrome (antibody) A/B.</p></td></tr></tbody></table></div><p>This patient with chronic dry mouth and palpable salivary gland enlargement has extensive <strong>lymphocytic infiltrate</strong> with a <a ng-click=&quot;showexhibit('22313',true);&quot;>germinal center</a> on salivary gland biopsy, findings consistent with <strong>Sj&amp;ouml;gren syndrome</strong> (SS).&amp;nbsp; SS is an autoimmune disorder characterized by a lymphocytic inflammatory infiltrate in the <strong>exocrine glands</strong>.&amp;nbsp; It presents with <strong>dry eyes</strong> (keratoconjunctivitis sicca) and <strong>dry mouth</strong> (xerostomia).&amp;nbsp; Extraglandular manifestations such as <strong>arthralgia</strong> and cutaneous vasculitis can also occur due to the deposition of circulating immune complexes.</p><p>Complications of SS can include corneal ulcerations due to diminished lacrimal gland function and an increase in dental caries due to diminished salivary gland function.&amp;nbsp; The chronic B-lymphocyte stimulation in SS also predisposes patients to malignant transformation and resulting&amp;nbsp;<strong>non-Hodgkin lymphoma</strong> (eg, marginal zone lymphoma, diffuse large B-cell lymphoma).</p><p><strong>(Choice A)</strong>&amp;nbsp; Although inflammatory bowel disease (eg, ulcerative colitis) can be associated with spondyloarthropathies (eg, low back pain, peripheral arthritis), dry mouth is not a typical feature; <a ng-click=&quot;showexhibit('54832',true);&quot;>Crohn disease</a> can occasionally involve the salivary glands, but biopsy would show noncaseating granulomas.</p><p><strong>(Choice C)</strong>&amp;nbsp; Portal hypertension can be a complication of&amp;nbsp;<a ng-click=&quot;showexhibit('54833',true);&quot;>hemochromatosis</a>, which is characterized by iron accumulation in the liver (eg, hepatomegaly, cirrhosis), skin (eg, hyperpigmentation), pancreas (eg, diabetes mellitus), and joints (eg, arthralgia); dry mouth would not be expected.</p><p><strong>(Choice D)</strong>&amp;nbsp; Renal artery stenosis (occlusion) is most commonly seen in the context of advanced atherosclerosis or fibromuscular dysplasia and often causes treatment-resistant hypertension.</p><p><strong>(Choice E)</strong>&amp;nbsp; Restrictive cardiomyopathy is often idiopathic or can be associated with infiltrative disorders (eg, <a ng-click=&quot;showexhibit('52090',true);&quot;>amyloidosis</a>, sarcoidosis, hemochromatosis).&amp;nbsp; Cardiac manifestations of SS are rare and do not include restrictive disease.</p><p><strong>Educational objective:</strong><br>Sj&amp;ouml;gren syndrome is an autoimmune disorder characterized by lymphocytic inflammation in exocrine glands (eg, lacrimal, salivary).&amp;nbsp; It presents with dry eyes (keratoconjunctivitis sicca) and dry mouth (xerostomia).&amp;nbsp; Chronic B-cell proliferation increases the risk for non-Hodgkin lymphoma.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><div id=\"div_T48541\" class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" class=\"table-default-style table-header-footer-style\"><tbody><tr><td colspan=\"2\"><p align=\"center\"><strong>Sj\u00f6gren syndrome</strong></p></td></tr><tr><td><p align=\"center\"><strong>Pathogenesis</strong></p></td><td><ul><li>Immune-mediated destruction of the lacrimal &amp; salivary glands</li><li>Can occur as primary disease or secondary with other autoimmune disorders (eg, SLE, RA)</li></ul></td></tr><tr><td><p align=\"center\"><strong>Clinical features</strong></p></td><td><ul><li>Dry eyes (keratoconjunctivitis sicca)</li><li>Dry mouth (xerostomia), salivary hypertrophy</li><li>Dry skin (xerosis)</li><li>Raynaud phenomenon</li><li>Cutaneous vasculitis</li><li>Positive anti-Ro (SSA) &amp;/or anti-La (SSB)</li></ul></td></tr><tr><td><p align=\"center\"><strong>Complications</strong></p></td><td><ul><li>Non-Hodgkin lymphoma</li><li>Corneal damage, dental caries</li></ul></td></tr><tr><td colspan=\"2\"><p><strong>RA</strong> = rheumatoid arthritis; <strong>SLE</strong> = systemic lupus erythematosus; <strong>SSA/SSB</strong> = Sj\u00f6gren syndrome (antibody) A/B.</p></td></tr></tbody></table></div><p class=\"ng-scope\">This patient with chronic dry mouth and palpable salivary gland enlargement has extensive <strong>lymphocytic infiltrate</strong> with a <a ng-click=\"showexhibit('22313',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L1804.png&quot;,&quot;title&quot;:&quot;L1804&quot;}]\">germinal center</a> on salivary gland biopsy, findings consistent with <strong>Sj\u00f6gren syndrome</strong> (SS).&nbsp; SS is an autoimmune disorder characterized by a lymphocytic inflammatory infiltrate in the <strong>exocrine glands</strong>.&nbsp; It presents with <strong>dry eyes</strong> (keratoconjunctivitis sicca) and <strong>dry mouth</strong> (xerostomia).&nbsp; Extraglandular manifestations such as <strong>arthralgia</strong> and cutaneous vasculitis can also occur due to the deposition of circulating immune complexes.</p><p class=\"ng-scope\">Complications of SS can include corneal ulcerations due to diminished lacrimal gland function and an increase in dental caries due to diminished salivary gland function.&nbsp; The chronic B-lymphocyte stimulation in SS also predisposes patients to malignant transformation and resulting&nbsp;<strong>non-Hodgkin lymphoma</strong> (eg, marginal zone lymphoma, diffuse large B-cell lymphoma).</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Although inflammatory bowel disease (eg, ulcerative colitis) can be associated with spondyloarthropathies (eg, low back pain, peripheral arthritis), dry mouth is not a typical feature; <a ng-click=\"showexhibit('54832',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L84834.png&quot;,&quot;title&quot;:&quot;L84834&quot;}]\">Crohn disease</a> can occasionally involve the salivary glands, but biopsy would show noncaseating granulomas.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Portal hypertension can be a complication of&nbsp;<a ng-click=\"showexhibit('54833',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L84835.png&quot;,&quot;title&quot;:&quot;L84835&quot;}]\">hemochromatosis</a>, which is characterized by iron accumulation in the liver (eg, hepatomegaly, cirrhosis), skin (eg, hyperpigmentation), pancreas (eg, diabetes mellitus), and joints (eg, arthralgia); dry mouth would not be expected.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Renal artery stenosis (occlusion) is most commonly seen in the context of advanced atherosclerosis or fibromuscular dysplasia and often causes treatment-resistant hypertension.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Restrictive cardiomyopathy is often idiopathic or can be associated with infiltrative disorders (eg, <a ng-click=\"showexhibit('52090',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L62718.png&quot;,&quot;title&quot;:&quot;L62718&quot;}]\">amyloidosis</a>, sarcoidosis, hemochromatosis).&nbsp; Cardiac manifestations of SS are rare and do not include restrictive disease.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Sj\u00f6gren syndrome is an autoimmune disorder characterized by lymphocytic inflammation in exocrine glands (eg, lacrimal, salivary).&nbsp; It presents with dry eyes (keratoconjunctivitis sicca) and dry mouth (xerostomia).&nbsp; Chronic B-cell proliferation increases the risk for non-Hodgkin lymphoma.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pathology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Rheumatology/Orthopedics &amp; Sports</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Sjogren syndrome</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQues