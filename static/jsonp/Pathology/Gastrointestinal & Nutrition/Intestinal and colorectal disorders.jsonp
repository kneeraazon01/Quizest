              </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;14893&quot; src=&quot;https://www.uworld.com/media/L39667.png&quot; ></p><p><strong>Crohn disease</strong> (CD) is an inflammatory bowel disease characterized by patchy inflammation that can occur throughout the entire gastrointestinal (GI) tract; patients commonly complain of relapsing and remitting abdominal pain, diarrhea (with or without blood), and weight loss.&amp;nbsp; CD typically involves the ileum; however, up to 20% of patients have only colonic involvement.&amp;nbsp; Unlike ulcerative colitis, which is characterized by continuous inflammation of the colon limited to the mucosa and submucosa, inflammation in CD is <strong>transmural</strong> (full thickness) and <strong>segmental</strong> in nature.&amp;nbsp; This leads to recognizable manifestations on gross pathology, including:</p><ul><li><p><a ng-click=&quot;showexhibit('15207',true);&quot;>Skip lesions</a> &amp;ndash; sharply delineated ulcerations surrounded by normal bowel</p></li><li><p><strong>Cobblestone</strong>&amp;nbsp;appearance &amp;ndash; serpiginous (ie, wavy, snakelike) depressed ulcerations that separate elevated islands of healthy tissue (giving the mucosa an appearance of a cobblestone street)</p></li><li><p><strong>Bowel wall thickening</strong> &amp;ndash; due to inflammation involving all layers of the bowel wall leading to edema, hypertrophy of the muscularis mucosa, and fibrosis</p></li><li><p><strong>Creeping fat</strong> &amp;ndash; mesenteric fat becomes wrapped around the bowel, possibly in response to intestinal bacteria translocation</p></li></ul><p>In addition, the transmural inflammation frequently leads to complications such as abscesses, <strong>strictures</strong>, and <strong>fistulas</strong>.</p><p><strong>(Choice A)</strong>&amp;nbsp; Amoebic dysentery caused by <em>Entamoeba histolytica</em> causes hematochezia, abdominal pain, and ulcerations; however, the ulcers are classically flask-shaped and smaller than those seen with inflammatory bowel disease.</p><p><strong>(Choice B)</strong>&amp;nbsp; Collagenous colitis can cause chronic watery diarrhea (&amp;gt;5-10 episodes daily) but is commonly seen in middle-aged women and is not associated with bleeding, ulcerations, inflammation, or abdominal pain.</p><p><strong>(Choice D)</strong>&amp;nbsp; Graft-versus-host disease of the GI tract can cause abdominal pain and ulcerations, but it typically causes severe diarrhea (up to 10 L daily) and occurs as a complication of allogeneic stem cell transplant.</p><p><strong>(Choice E)</strong>&amp;nbsp; <a ng-click=&quot;showexhibit('14933',true);&quot;>Pseudomembranous colitis</a> occurs from <em>Clostridioides difficile</em> infection (CDI).&amp;nbsp; It causes abdominal pain and diarrhea that can result in toxic megacolon.&amp;nbsp; A white/yellow, patchy pseudomembrane, composed of inflammatory cells, fibrin, and cellular debris, is often visualized.&amp;nbsp; In addition, CDI is typically associated with recent antibiotic use.</p><p><strong>(Choice F)</strong>&amp;nbsp; <a ng-click=&quot;showexhibit('42061',true);&quot;>Ulcerative colitis</a> is characterized by contiguous inflammation that extends from the rectum.&amp;nbsp; Ulcerations are shallow and involve only the mucosa and submucosa; pseudopolyps (areas of inflamed mucosa and/or granulation tissue that appear polyp-like due to surrounding ulcerations) are seen; bowel wall thickening is not as prominent as with CD.</p><p><strong>Educational objective:</strong><br>Crohn disease is an inflammatory bowel disease characterized by patchy inflammation that can occur throughout the entire gastrointestinal tract.&amp;nbsp; Gross pathology demonstrates skip lesions, cobblestoning of the mucosa, bowel wall thickening, and creeping fat.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"14893\" src=\"image/L39667.png\"></p><p class=\"ng-scope\"><strong>Crohn disease</strong> (CD) is an inflammatory bowel disease characterized by patchy inflammation that can occur throughout the entire gastrointestinal (GI) tract; patients commonly complain of relapsing and remitting abdominal pain, diarrhea (with or without blood), and weight loss.&nbsp; CD typically involves the ileum; however, up to 20% of patients have only colonic involvement.&nbsp; Unlike ulcerative colitis, which is characterized by continuous inflammation of the colon limited to the mucosa and submucosa, inflammation in CD is <strong>transmural</strong> (full thickness) and <strong>segmental</strong> in nature.&nbsp; This leads to recognizable manifestations on gross pathology, including:</p><ul class=\"ng-scope\"><li><p><a ng-click=\"showexhibit('15207',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L7024.png&quot;,&quot;title&quot;:&quot;L7024&quot;}]\">Skip lesions</a> \u2013 sharply delineated ulcerations surrounded by normal bowel</p></li><li><p><strong>Cobblestone</strong>&nbsp;appearance \u2013 serpiginous (ie, wavy, snakelike) depressed ulcerations that separate elevated islands of healthy tissue (giving the mucosa an appearance of a cobblestone street)</p></li><li><p><strong>Bowel wall thickening</strong> \u2013 due to inflammation involving all layers of the bowel wall leading to edema, hypertrophy of the muscularis mucosa, and fibrosis</p></li><li><p><strong>Creeping fat</strong> \u2013 mesenteric fat becomes wrapped around the bowel, possibly in response to intestinal bacteria translocation</p></li></ul><p class=\"ng-scope\">In addition, the transmural inflammation frequently leads to complications such as abscesses, <strong>strictures</strong>, and <strong>fistulas</strong>.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Amoebic dysentery caused by <em>Entamoeba histolytica</em> causes hematochezia, abdominal pain, and ulcerations; however, the ulcers are classically flask-shaped and smaller than those seen with inflammatory bowel disease.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Collagenous colitis can cause chronic watery diarrhea (&gt;5-10 episodes daily) but is commonly seen in middle-aged women and is not associated with bleeding, ulcerations, inflammation, or abdominal pain.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Graft-versus-host disease of the GI tract can cause abdominal pain and ulcerations, but it typically causes severe diarrhea (up to 10 L daily) and occurs as a complication of allogeneic stem cell transplant.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; <a ng-click=\"showexhibit('14933',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L29272.png&quot;,&quot;title&quot;:&quot;L29272&quot;}]\">Pseudomembranous colitis</a> occurs from <em>Clostridioides difficile</em> infection (CDI).&nbsp; It causes abdominal pain and diarrhea that can result in toxic megacolon.&nbsp; A white/yellow, patchy pseudomembrane, composed of inflammatory cells, fibrin, and cellular debris, is often visualized.&nbsp; In addition, CDI is typically associated with recent antibiotic use.</p><p class=\"ng-scope\"><strong>(Choice F)</strong>&nbsp; <a ng-click=\"showexhibit('42061',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L66358.png&quot;,&quot;title&quot;:&quot;L66358&quot;}]\">Ulcerative colitis</a> is characterized by contiguous inflammation that extends from the rectum.&nbsp; Ulcerations are shallow and involve only the mucosa and submucosa; pseudopolyps (areas of inflamed mucosa and/or granulation tissue that appear polyp-like due to surrounding ulcerations) are seen; bowel wall thickening is not as prominent as with CD.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Crohn disease is an inflammatory bowel disease characterized by patchy inflammation that can occur throughout the entire gastrointestinal tract.&nbsp; Gross pathology demonstrates skip lesions, cobblestoning of the mucosa, bowel wall thickening, and creeping fat.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pathology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Gastrointestinal &amp; Nutrition</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Inflammatory bowel disease</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
