answer-radio\" type=\"radio\">\n                            <strong>G.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight7\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Wolff-Parkinson-White syndrome</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; id=&quot;2795&quot;  src=&quot;https://www.uworld.com/media/L13550.jpg&quot; draggable=&quot;false&quot; ></p><p>The autopsy findings of this patient&amp;#39;s left ventricle (LV) are consistent with <strong>concentric hypertrophy</strong> most likely due to chronic hypertension.</p><p>LV hypertrophy is generally defined as an increase in the left ventricular mass due to <strong>increased wall thickness</strong> (concentric) or cavity size (eccentric).&amp;nbsp; Concentric hypertrophy is characterized by <strong>uniform</strong> thickening of the ventricular wall with the outer dimensions of the ventricle remaining almost unchanged, resulting in a narrowed ventricular cavity size.&amp;nbsp; It is due to chronic elevation of ventricular pressures during systole, which is usually caused by <strong>long-standing hypertension</strong> or aortic stenosis (<strong>increased LV afterload</strong>).&amp;nbsp; Patients may develop diastolic dysfunction with left atrial enlargement and congestive heart failure due to impaired ventricular compliance and filling.</p><p><strong>(Choice A)</strong>&amp;nbsp; An atrial septal defect causes a shunting of blood flow from the left to right atrium.&amp;nbsp; This typically results in volume overload of the right ventricle, producing right atrial enlargement and eccentric (dilated) right ventricular hypertrophy.</p><p><strong>(Choice C)</strong>&amp;nbsp; Mitral insufficiency causes LV volume overload as regurgitant flow through the mitral valve during systole increases the amount of blood returning to the LV during diastole.&amp;nbsp; The net structural consequence of mitral insufficiency is enlargement of the LV cavity and eccentric hypertrophy.</p><p><strong>(Choice D)</strong>&amp;nbsp; Mitral stenosis leads to left atrial enlargement due to elevated pressure in the left atrium.&amp;nbsp; The LV cavity is typically small/normal in size with normal wall thickness due to restriction of blood flow across the mitral valve.</p><p><strong>(Choice E)</strong>&amp;nbsp; Aging is associated with a decrease in LV chamber size, predominantly through shortening of the apex-to-base dimension accompanied by a leftward bowing of the lower interventricular (sigmoid) septum.&amp;nbsp; The aortic valve may also become calcified, resulting in aortic stenosis; however, structural cardiac changes due to aging are generally not prominent before age 65.</p><p><strong>(Choice F)</strong>&amp;nbsp; The ventricular remodeling that occurs after an acute myocardial infarction involves expansion, thinning, and fibrous healing of the infarcted zone of myocardium.&amp;nbsp; Regional dysfunction of the infarcted myocardium causes volume overload for the remaining viable myocardium.&amp;nbsp; The net result is usually eccentric hypertrophy, with enlargement of the LV cavity.</p><p><strong>(Choice G)</strong>&amp;nbsp; Patients with Wolff-Parkinson-White syndrome have an accessory pathway that bypasses the atrioventricular node and directly connects the atria and ventricles.&amp;nbsp; This results in ventricular preexcitation and the development of tachyarrhythmias.&amp;nbsp; The LV usually appears grossly normal.</p><p><strong>Educational objective:</strong><br>Concentric hypertrophy is characterized by uniform thickening of the ventricular wall and narrowing of the ventricular cavity due to increased afterload (eg, chronic hypertension, aortic stenosis).&amp;nbsp; Eccentric hypertrophy is characterized by reduced ventricular wall thickness with an associated increase in chamber size due to volume overload.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" id=\"2795\" src=\"image/L13550.jpg\" draggable=\"false\"></p><p class=\"ng-scope\">The autopsy findings of this patient's left ventricle (LV) are consistent with <strong>concentric hypertrophy</strong> most likely due to chronic hypertension.</p><p class=\"ng-scope\">LV hypertrophy is generally defined as an increase in the left ventricular mass due to <strong>increased wall thickness</strong> (concentric) or cavity size (eccentric).&nbsp; Concentric hypertrophy is characterized by <strong>uniform</strong> thickening of the ventricular wall with the outer dimensions of the ventricle remaining almost unchanged, resulting in a narrowed ventricular cavity size.&nbsp; It is due to chronic elevation of ventricular pressures during systole, which is usually caused by <strong>long-standing hypertension</strong> or aortic stenosis (<strong>increased LV afterload</strong>).&nbsp; Patients may develop diastolic dysfunction with left atrial enlargement and congestive heart failure due to impaired ventricular compliance and filling.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; An atrial septal defect causes a shunting of blood flow from the left to right atrium.&nbsp; This typically results in volume overload of the right ventricle, producing right atrial enlargement and eccentric (dilated) right ventricular hypertrophy.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Mitral insufficiency causes LV volume overload as regurgitant flow through the mitral valve during systole increases the amount of blood returning to the LV during diastole.&nbsp; The net structural consequence of mitral insufficiency is enlargement of the LV cavity and eccentric hypertrophy.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Mitral stenosis leads to left atrial enlargement due to elevated pressure in the left atrium.&nbsp; The LV cavity is typically small/normal in size with normal wall thickness due to restriction of blood flow across the mitral valve.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Aging is associated with a decrease in LV chamber size, predominantly through shortening of the apex-to-base dimension accompanied by a leftward bowing of the lower interventricular (sigmoid) septum.&nbsp; The aortic valve may also become calcified, resulting in aortic stenosis; however, structural cardiac changes due to aging are generally not prominent before age 65.</p><p class=\"ng-scope\"><strong>(Choice F)</strong>&nbsp; The ventricular remodeling that occurs after an acute myocardial infarction involves expansion, thinning, and fibrous healing of the infarcted zone of myocardium.&nbsp; Regional dysfunction of the infarcted myocardium causes volume overload for the remaining viable myocardium.&nbsp; The net result is usually eccentric hypertrophy, with enlargement of the LV cavity.</p><p class=\"ng-scope\"><strong>(Choice G)</strong>&nbsp; Patients with Wolff-Parkinson-White syndrome have an accessory pathway that bypasses the atrioventricular node and directly connects the atria and ventricles.&nbsp; This results in ventricular preexcitation and the development of tachyarrhythmias.&nbsp; The LV usually appears grossly normal.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Concentric hypertrophy is characterized by uniform thickening of the ventricular wall and narrowing of the ventricular cavity due to increased afterload (eg, chronic hypertension, aortic stenosis).&nbsp; Eccentric hypertrophy is characterized by reduced ventricular wall thickness with an associated increase in chamber size due to volume overload.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/21263005\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/21263005\"><span compiledata=\"\" htmlcontent=\"The progression of hypertensive heart disease.\" class=\"ng-scope\"><p class=\"ng-scope\">The progression of hypertensive heart disease.</p></span></a><!-- end ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal -->\n                                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal -->\n                                </li><!-- end ngRepeat: questionReference in currentQuestion.questionMappingReferencesList -->\n                            </ul>\n                        </div>\n                    </div><!-- end ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pathology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Cardiovascular System</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Primary hypertension</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
        "correctAnswerIndex": 1,
        "category": "Hypertension"
