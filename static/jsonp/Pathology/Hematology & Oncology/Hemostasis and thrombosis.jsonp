    },
    {
        "question": "<div class=\"questioncontent ui-resizable widthWithoutAbstract\" id=\"questionContent\" ng-show=\"showQuestion\" ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat, 'suggested-highlights-visible' : userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; currentQuestion.frequentHighlights != null &amp;&amp; (suggestedHighlightsPreference == 1 || suggestedHighlightsPreference == 2), 'only-suggested-highlights-visible' : userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; suggestedHighlightsPreference == 1 &amp;&amp; currentQuestion.frequentHighlights != null}\">\n            <div style=\"display:none;\" class=\"suggested-highlights-switch-buttons ng-hide\" ng-show=\"userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; currentQuestion.frequentHighlights != null\">\n                <span class=\"fa-stack\" ng-click=\"showUserHighlights()\" title=\"My Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fas\" ng-class=\"{'fas' : suggestedHighlightsPreference == 0, 'fal' : suggestedHighlightsPreference != 0}\"></i>\n                    <i class=\"fal fa-highlighter fa-stack-1x fa-inverse\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 0}\"></i>\n                </span>\n                <span class=\"fa-stack ng-hide\" ng-hide=\"currentQuestion.frequentHighlights == null\" ng-click=\"showSuggestedHighlights()\" title=\"Suggested Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fal\" ng-class=\"{'fas' : suggestedHighlightsPreference == 1, 'fal' : suggestedHighlightsPreference != 1}\"></i>\n                    <i class=\"fas fa-highlighter fa-stack-1x\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 1}\"></i>\n                </span>\n                <span class=\"fa-stack highlight-preference-unavailable highlight-button-with-popover\" ng-show=\"currentQuestion.frequentHighlights == null\" title=\"\" data-toggle=\"popover\" data-placement=\"bottom\" data-content=\"Collecting Data\" data-trigger=\"hover\" data-original-title=\"Suggested Highlights\">\n                    <i class=\"fal fa-circle fa-stack-2x\"></i>\n                    <i class=\"fas fa-highlighter fa-stack-1x\"></i>\n                </span>\n                <span class=\"fa-stack ng-hide\" ng-hide=\"currentQuestion.frequentHighlights == null\" ng-click=\"showUserAndSuggestedHighlights()\" title=\"Duo Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fal\" ng-class=\"{'fas' : suggestedHighlightsPreference == 2, 'fal' : suggestedHighlightsPreference != 2}\"></i>\n                    <i class=\"fad fa-highlighter fa-stack-1x\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 2}\"></i>\n                </span>\n                <span class=\"fa-stack highlight-preference-unavailable highlight-button-with-popover\" ng-show=\"currentQuestion.frequentHighlights == null\" title=\"\" data-toggle=\"popover\" data-placement=\"bottom\" data-content=\"Collecting Data\" data-trigger=\"hover\" data-original-title=\"Duo Highlights\">\n                    <i class=\"fal fa-circle fa-stack-2x\"></i>\n                    <i class=\"fad fa-highlighter fa-stack-1x\"></i>\n                </span>\n            </div>\n            <!-- ngIf: launchTestOptions.launchCode == 'nbome' && userPreferences.testPreferences.theme != clientConstants.themes.nbome -->\n            <div id=\"question\">\n                <!-- ngIf: settings.hints && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && launchTestOptions.canShowHint -->\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && (currentQuestion.qbankId != clientConstants.qbankIds.stats_faculty || currentQuestion.qbankId != clientConstants.qbankIds.stats) -->\n                <!-- ngIf: (currentQuestion.mobileExcerpt != '' && currentQuestion.mobileExcerpt != null ) -->\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat -->\n                <!--Periodic table button for MCAT -->\n                <!--<div class=\"text-center periodicTableDiv\" ng-show=\"(launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.subjectId != constants.noPeriodicTable.cars) && (currentQuestion.subjectId != constants.noPeriodicTable.pyschology) && (currentQuestion.subjectId != constants.noPeriodicTable.sociology)\">\n                    <button class=\"btn btn-default btn--custom\" style=\"color: black; padding: 4px 3px;\" ng-click=\"showresources()\">Periodic table&nbsp;<img src=\"img/mcat/Periodic_icon.png\" height=\"12\" style=\"margin-top: -2px; cursor: pointer\" title=\"Periodic Table\" /></button>\n                </div>-->\n                \n                <!--Inactive message for questions in usmle, boards and nclex and do not show for EXPERIMENTAL_INACTIVE hence the check !=3 -->\n                <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.nclex) && !currentQuestion.isActive && currentQuestion.activeStatusId != 3 -->\n                <div id=\"questionText\" class=\"questionText ng-scope\" ng-mouseup=\"invokeHighlights('questionText',constants.flashCardType.selection,$event)\" compiledata=\"\" htmlcontent=\"<p>A 24-year-old woman, gravida 1 para 0, at 28 weeks gestation comes to the labor and delivery unit due to intense abdominal pain, vaginal bleeding, and absent fetal movement.&amp;nbsp; Temperature is 36.7 C (98.1 F), blood pressure is 170/96 mm Hg, pulse is 108/min, and respirations are 22/min.&amp;nbsp; Pulse oximetry shows 98% on room air.&amp;nbsp; Physical examination shows a tense abdomen and a firm, tender uterus.&amp;nbsp; Ultrasound shows a hematoma between the placenta and the uterine wall and no fetal cardiac activity.&amp;nbsp; Laboratory results are as follows:</p><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; border=&quot;0&quot; cellpadding=&quot;0&quot; cellspacing=&quot;0&quot;><tbody><tr><td style=&quot;vertical-align:middle;&quot;>Hemoglobin</td><td style=&quot;vertical-align:middle;&quot;>9.2 g/dL</td></tr><tr><td style=&quot;vertical-align:middle;&quot;>Platelets</td><td style=&quot;vertical-align:middle;&quot;>60,000/mm<font size=&quot;2&quot;><sup>3</sup></font></td></tr><tr><td style=&quot;vertical-align:middle;&quot;>Aspartate aminotransferase (SGOT)</td><td style=&quot;vertical-align:middle;&quot;>12 U/L</td></tr><tr><td style=&quot;vertical-align:middle;&quot;>Alanine aminotransferase (SGPT)</td><td style=&quot;vertical-align:middle;&quot;>24 U/L</td></tr></tbody></table><p>The patient is hospitalized for further management.&amp;nbsp; Once hospitalized, she begins to experience bleeding from her gums and intravenous catheter sites.&amp;nbsp; Blood is also noted in the urinary catheter.&amp;nbsp; Vital signs are unchanged, and she reports no shortness of breath.&amp;nbsp; Which of the following is the most likely underlying cause of this patient&amp;#39;s hospital symptoms?</p>\"><p class=\"ng-scope\">A 24-year-old woman, gravida 1 para 0, at 28 weeks gestation comes to the labor and delivery unit due to intense abdominal pain, vaginal bleeding, and absent fetal movement.&nbsp; Temperature is 36.7 C (98.1 F), blood pressure is 170/96 mm Hg, pulse is 108/min, and respirations are 22/min.&nbsp; Pulse oximetry shows 98% on room air.&nbsp; Physical examination shows a tense abdomen and a firm, tender uterus.&nbsp; Ultrasound shows a hematoma between the placenta and the uterine wall and no fetal cardiac activity.&nbsp; Laboratory results are as follows:</p><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" border=\"0\" cellpadding=\"0\" cellspacing=\"0\" class=\"ng-scope\"><tbody><tr><td style=\"vertical-align:middle;\">Hemoglobin</td><td style=\"vertical-align:middle;\">9.2 g/dL</td></tr><tr><td style=\"vertical-align:middle;\">Platelets</td><td style=\"vertical-align:middle;\">60,000/mm<font size=\"2\"><sup>3</sup></font></td></tr><tr><td style=\"vertical-align:middle;\">Aspartate aminotransferase (SGOT)</td><td style=\"vertical-align:middle;\">12 U/L</td></tr><tr><td style=\"vertical-align:middle;\">Alanine aminotransferase (SGPT)</td><td style=\"vertical-align:middle;\">24 U/L</td></tr></tbody></table><p class=\"ng-scope\">The patient is hospitalized for further management.&nbsp; Once hospitalized, she begins to experience bleeding from her gums and intravenous catheter sites.&nbsp; Blood is also noted in the urinary catheter.&nbsp; Vital signs are unchanged, and she reports no shortness of breath.&nbsp; Which of the following is the most likely underlying cause of this patient's hospital symptoms?</p></div>\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && (currentQuestion.formatTypeId == constants.questionFormatType.fillInTheBlank ||currentQuestion.formatTypeId == constants.questionFormatType.fillInTheBlankRange )&& currentQuestion.sectionId == constants.collegeprepSectionIds.math -->\n                <!-- change to mediatype object -->\n                <!-- ngIf: currentQuestion.questionMedia.typeId == constants.questionMediaType.drugad || currentQuestion.questionTypeId == constants.questionType.drugad -->\n                <!-- ngIf: currentQuestion.questionMedia.typeId == constants.questionMediaType.audio || currentQuestion.questionMedia.typeId == constants.questionMediaType.video || currentQuestion.questionTypeId == constants.questionType.media -->\n\n                <div id=\"answerContainer\"><div class=\"options\">\n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>A.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight1\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Embolization of amniotic fluid into the maternal circulation</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>B.</strong>\n                            <span>\n                                