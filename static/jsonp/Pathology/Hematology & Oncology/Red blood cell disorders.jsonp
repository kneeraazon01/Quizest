>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;4944&quot; src=&quot;https://www.uworld.com/media/L8254.jpg&quot; ></p><p>The fragmented erythrocytes shown on the slide above are called <strong>schistocytes</strong> or helmet cells.&amp;nbsp; They are formed by <strong>mechanical trauma</strong> to erythrocytes as they circulate through the vasculature.&amp;nbsp; This patient&amp;#39;s schistocytes are most likely due to erythrocyte damage caused by his <strong>aortic valve prosthesis</strong>.&amp;nbsp; Artificial (mechanical) valves are more traumatic for RBCs than porcine prostheses and frequently cause hemolysis.&amp;nbsp; Schistocytes can also be formed from narrowing of the microvascular spaces, as seen in disseminated intravascular coagulation, hemolytic-uremic syndrome, and thrombotic thrombocytopenic purpura.</p><p><strong>Hemolytic anemia</strong> due to intravascular erythrocyte destruction results in laboratory findings similar to those in other types of hemolytic anemias, including an increase in serum indirect bilirubin.&amp;nbsp; Intravascular erythrocyte damage also results in free hemoglobin in serum (hemoglobinemia) and urine (hemoglobinuria) as well as increased serum lactate dehydrogenase (LDH).&amp;nbsp; Haptoglobin is a serum protein that binds to free hemoglobin and promotes its uptake by the reticuloendothelial system.&amp;nbsp; <strong>Haptoglobin levels decrease</strong> when significant quantities of hemoglobin are released into the circulation, as occurs with intravascular hemolysis.</p><p><strong>(Choice A)</strong>&amp;nbsp; Increased serum iron occurs in hemochromatosis most classically, but increased serum iron can also be iatrogenic.&amp;nbsp; Hemochromatosis does not cause anemia, but it is associated with cirrhosis and increased incidence of hepatocellular cancer.&amp;nbsp; Hemolysis does not usually have a significant effect on serum iron because the iron released from lysed RBCs remains bound to hemoglobin.</p><p><strong>(Choice C)</strong>&amp;nbsp; Increased mean corpuscular volume (MCV) occurs in&amp;nbsp;<a ng-click=&quot;showexhibit('41381',true);&quot;>megaloblastic anemia</a> (eg, folate/vitamin B12 deficiency) due to the presence of enlarged ovoid erythrocytes.&amp;nbsp; Increased MCV may also occur with other forms of anemia due to a reactive increase&amp;nbsp;in reticulocytes (large, immature RBCs); however, anemia caused by mechanical erythrocyte trauma usually results in decreased MCV due to the presence of small RBC fragments (schistocytes).</p><p><strong>(Choice D)</strong>&amp;nbsp; A decreased reticulocyte count in the presence of anemia is characteristic of aplastic anemia.&amp;nbsp; In hemolytic anemias, the reticulocyte count is increased to compensate for the increased destruction of RBCs.</p><p><strong>(Choice E)</strong>&amp;nbsp; A decreased serum albumin level is associated with cirrhosis (decreased production), nephrotic syndrome (urinary loss), and protein-wasting enteropathy (bowel loss).</p><p><strong>Educational objective:</strong><br>Schistocytes (helmet cells) are fragmented erythrocytes.&amp;nbsp; They occur secondary to mechanical trauma from microangiopathic hemolytic anemias or prosthetic cardiac valves (macroangiopathic).&amp;nbsp; Intravascular hemolytic anemias are characterized by decreased serum haptoglobin levels as well as increased LDH and bilirubin.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"4944\" src=\"image/L8254.jpg\"></p><p class=\"ng-scope\">The fragmented erythrocytes shown on the slide above are called <strong>schistocytes</strong> or helmet cells.&nbsp; They are formed by <strong>mechanical trauma</strong> to erythrocytes as they circulate through the vasculature.&nbsp; This patient's schistocytes are most likely due to erythrocyte damage caused by his <strong>aortic valve prosthesis</strong>.&nbsp; Artificial (mechanical) valves are more traumatic for RBCs than porcine prostheses and frequently cause hemolysis.&nbsp; Schistocytes can also be formed from narrowing of the microvascular spaces, as seen in disseminated intravascular coagulation, hemolytic-uremic syndrome, and thrombotic thrombocytopenic purpura.</p><p class=\"ng-scope\"><strong>Hemolytic anemia</strong> due to intravascular erythrocyte destruction results in laboratory findings similar to those in other types of hemolytic anemias, including an increase in serum indirect bilirubin.&nbsp; Intravascular erythrocyte damage also results in free hemoglobin in serum (hemoglobinemia) and urine (hemoglobinuria) as well as increased serum lactate dehydrogenase (LDH).&nbsp; Haptoglobin is a serum protein that binds to free hemoglobin and promotes its uptake by the reticuloendothelial system.&nbsp; <strong>Haptoglobin levels decrease</strong> when significant quantities of hemoglobin are released into the circulation, as occurs with intravascular hemolysis.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Increased serum iron occurs in hemochromatosis most classically, but increased serum iron can also be iatrogenic.&nbsp; Hemochromatosis does not cause anemia, but it is associated with cirrhosis and increased incidence of hepatocellular cancer.&nbsp; Hemolysis does not usually have a significant effect on serum iron because the iron released from lysed RBCs remains bound to hemoglobin.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Increased mean corpuscular volume (MCV) occurs in&nbsp;<a ng-click=\"showexhibit('41381',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L65104.jpg&quot;,&quot;title&quot;:&quot;L65104&quot;}]\">megaloblastic anemia</a> (eg, folate/vitamin B12 deficiency) due to the presence of enlarged ovoid erythrocytes.&nbsp; Increased MCV may also occur with other forms of anemia due to a reactive increase&nbsp;in reticulocytes (large, immature RBCs); however, anemia caused by mechanical erythrocyte trauma usually results in decreased MCV due to the presence of small RBC fragments (schistocytes).</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; A decreased reticulocyte count in the presence of anemia is characteristic of aplastic anemia.&nbsp; In hemolytic anemias, the reticulocyte count is increased to compensate for the increased destruction of RBCs.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; A decreased serum albumin level is associated with cirrhosis (decreased production), nephrotic syndrome (urinary loss), and protein-wasting enteropathy (bowel loss).</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Schistocytes (helmet cells) are fragmented erythrocytes.&nbsp; They occur secondary to mechanical trauma from microangiopathic hemolytic anemias or prosthetic cardiac valves (macroangiopathic).&nbsp; Intravascular hemolytic anemias are characterized by decreased serum haptoglobin levels as well as increased LDH and bilirubin.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards !