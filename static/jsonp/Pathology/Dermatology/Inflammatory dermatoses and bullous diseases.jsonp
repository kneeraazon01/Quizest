testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><div id=&quot;div_T35861&quot;><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; class=&quot;table-default-style table-header-footer-style&quot;><tbody><tr><td colspan=&quot;2&quot;><p align=&quot;center&quot;><strong>Atopic dermatitis</strong></p></td></tr><tr><td style=&quot;width: 25%;&quot;><p align=&quot;center&quot;><strong>Pathogenesis</strong></p></td><td style=&quot;width: 75%;&quot;><ul><li>Mutation in epidermal barrier proteins (eg, filaggrin) leads to &amp;#8595; water content, &amp;#8593; permeability, &amp;#8593; inflammation</li><li>Immune dysfunction (ie, Th2-skewed response)</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Clinical<br>findings</strong></p></td><td><ul><li>Acute: pruritic, erythematous patches &amp;amp; papules<ul style=&quot;list-style-type:circle;&quot;><li>Infants: red extensor surfaces, trunk &amp;amp; face</li><li>Children &amp;amp; adults: flexor surfaces</li></ul></li><li>Chronic: lichenified plaques</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Associated<br>conditions</strong></p></td><td><ul><li>Allergic rhinitis</li><li>Asthma</li><li>Food allergy</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Laboratory<br>findings</strong></p></td><td><ul><li>&amp;#8593; IgE</li><li>Eosinophilia</li></ul></td></tr><tr><td colspan=&quot;2&quot;><p><strong>Th2</strong> = T-helper cell type 2.</p></td></tr></tbody></table></div><p><strong>Atopic dermatitis</strong> (AD), or <a ng-click=&quot;showexhibit('4272',true);&quot;>eczema</a>, is a <strong>chronic</strong> inflammatory skin disorder common in children.&amp;nbsp; The disease typically occurs at <strong>age &amp;lt;5</strong>, with many patients developing symptoms in infancy.&amp;nbsp; AD typically presents as erythematous papules and plaques involving the face, scalp, trunk, and extensor surfaces.&amp;nbsp; The diaper area is usually spared.&amp;nbsp; In older children and adults, it often manifests as <a ng-click=&quot;showexhibit('49023',true);&quot;>lichenification</a> in a flexural distribution (eg, neck, wrists, antecubital and popliteal fossae).&amp;nbsp; Eczema has a chronic course marked by exacerbations and remissions, and its severity tends to lessen with age.&amp;nbsp; Intense <strong>pruritus</strong> is a hallmark of AD.</p><p>AD can be associated with mutations affecting epidermal barrier proteins such as filaggrin, resulting in <strong>impaired skin&amp;nbsp;barrier function</strong>.&amp;nbsp; This increases immunologic exposure to environmental allergens and microbial antigens, leading to <strong>immune hypersensitivity</strong>.&amp;nbsp; Affected patients usually have high serum IgE levels and peripheral eosinophilia.&amp;nbsp; Children with AD often have a family history of atopy and are at risk of developing other atopic diseases, such as <strong>allergic rhinitis</strong> and <strong>asthma</strong> (allergic <strong>triad</strong>).</p><p><strong>(Choice A)</strong>&amp;nbsp; Celiac disease is associated with <a ng-click=&quot;showexhibit('46193',true);&quot;>dermatitis herpetiformis</a>, which presents with pruritic vesicles and plaques on the buttocks and extensor surfaces of the extremities.&amp;nbsp; In addition, this skin disorder is extremely uncommon in children.</p><p><strong>(Choice C)</strong>&amp;nbsp; Langerhans cell histiocytosis is a dendritic cell disorder that can present in infancy or early childhood with a severe, refractory, erythematous or ulcerative rash on the scalp, trunk, or groin.&amp;nbsp; This patient&amp;#39;s rash flares and resolves without intervention.</p><p><strong>(Choice D)</strong>&amp;nbsp; Psoriatic arthritis is associated with <a ng-click=&quot;showexhibit('53197',true);&quot;>psoriasis</a>.&amp;nbsp; Manifestations of psoriasis include erythematous, well-demarcated papules and plaques with a silvery scale, which are not seen in this patient.</p><p><strong>(Choice E)</strong>&amp;nbsp; Ulcerative colitis, a type of inflammatory bowel disease, can be associated with dermatologic manifestations, such as <a ng-click=&quot;showexhibit('2097',true);&quot;>pyoderma gangrenosum</a> (ulcerative rash) or <a ng-click=&quot;showexhibit('13117',true);&quot;>erythema nodosum</a> (painful raised red nodules).&amp;nbsp; Both are uncommon in children, and neither is seen in this case.</p><p><strong>Educational objective:</strong><br>Atopic dermatitis (eczema) is a common, chronic inflammatory disorder caused by impairment of the skin&amp;#39;s barrier function.&amp;nbsp; Eczema presents with pruritus and erythematous papules and plaques and is associated with other atopic diseases, such as allergic rhinitis and asthma.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><div id=\"div_T35861\" class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" class=\"table-default-style table-header-footer-style\"><tbody><tr><td colspan=\"2\"><p align=\"center\"><strong>Atopic dermatitis</strong></p></td></tr><tr><td style=\"width: 25%;\"><p align=\"center\"><strong>Pathogenesis</strong></p></td><td style=\"width: 75%;\"><ul><li>Mutation in epidermal barrier proteins (eg, filaggrin) leads to \u2193 water content, \u2191 permeability, \u2191 inflammation</li><li>Immune dysfunction (ie, Th2-skewed response)</li></ul></td></tr><tr><td><p align=\"center\"><strong>Clinical<br>findings</strong></p></td><td><ul><li>Acute: pruritic, erythematous patches &amp; papules<ul style=\"list-style-type:circle;\"><li>Infants: red extensor surfaces, trunk &amp; face</li><li>Children &amp; adults: flexor surfaces</li></ul></li><li>Chronic: lichenified plaques</li></ul></td></tr><tr><td><p align=\"center\"><strong>Associated<br>conditions</strong></p></td><td><ul><li>Allergic rhinitis</li><li>Asthma</li><li>Food allergy</li></ul></td></tr><tr><td><p align=\"center\"><strong>Laboratory<br>findings</strong></p></td><td><ul><li>\u2191 IgE</li><li>Eosinophilia</li></ul></td></tr><tr><td colspan=\"2\"><p><strong>Th2</strong> = T-helper cell type 2.</p></td></tr></tbody></table></div><p class=\"ng-scope\"><strong>Atopic dermatitis</strong> (AD), or <a ng-click=\"showexhibit('4272',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L28431.jpg&quot;,&quot;title&quot;:&quot;L28431&quot;}]\">eczema</a>, is a <strong>chronic</strong> inflammatory skin disorder common in children.&nbsp; The disease typically occurs at <strong>age &lt;5</strong>, with many patients developing symptoms in infancy.&nbsp; AD typically presents as erythematous papules and plaques involving the face, scalp, trunk, and extensor surfaces.&nbsp; The diaper area is usually spared.&nbsp; In older children and adults, it often manifests as <a ng-click=\"showexhibit('49023',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L9278.png&quot;,&quot;title&quot;:&quot;L9278&quot;}]\">lichenification</a> in a flexural distribution (eg, neck, wrists, antecubital and popliteal fossae).&nbsp; Eczema has a chronic course marked by exacerbations and remissions, and its severity tends to lessen with age.&nbsp; Intense <strong>pruritus</strong> is a hallmark of AD.</p><p class=\"ng-scope\">AD can be associated with mutations affecting epidermal barrier proteins such as filaggrin, resulting in <strong>impaired skin&nbsp;barrier function</strong>.&nbsp; This increases immunologic exposure to environmental allergens and microbial antigens, leading to <strong>immune hypersensitivity</strong>.&nbsp; Affected patients usually have high serum IgE levels and peripheral eosinophilia.&nbsp; Children with AD often have a family history of atopy and are at risk of developing other atopic diseases, such as <strong>allergic rhinitis</strong> and <strong>asthma</strong> (allergic <strong>triad</strong>).</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Celiac disease is associated with <a ng-click=\"showexhibit('46193',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L7992.png&quot;,&quot;title&quot;:&quot;L7992&quot;}]\">dermatitis herpetiformis</a>, which presents with pruritic vesicles and plaques on the buttocks and extensor surfaces of the extremities.&nbsp; In addition, this skin disorder is extremely uncommon in children.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Langerhans cell histiocytosis is a dendritic cell disorder that can present in infancy or early childhood with a severe, refractory, erythematous or ulcerative rash on the scalp, trunk, or groin.&nbsp; This patient's rash flares and resolves without intervention.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Psoriatic arthritis is associated with <a ng-click=\"showexhibit('53197',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L10704.png&quot;,&quot;title&quot;:&quot;L10704&quot;}]\">psoriasis</a>.&nbsp; Manifestations of psoriasis include erythematous, well-demarcated papules and plaques with a silvery scale, which are not seen in this patient.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Ulcerative colitis, a type of inflammatory bowel disease, can be associated with dermatologic manifestations, such as <a ng-click=\"showexhibit('2097',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L21233.png&quot;,&quot;title&quot;:&quot;L21233&quot;}]\">pyoderma gangrenosum</a> (ulcerative rash) or <a ng-click=\"showexhibit('13117',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L35385.jpg&quot;,&quot;title&quot;:&quot;L35385&quot;}]\">erythema nodosum</a> (painful raised red nodules).&nbsp; Both are uncommon in children, and neither is seen in this case.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Atopic dermatitis (eczema) is a common, chronic inflammatory disorder caused by impairment of the skin's barrier function.&nbsp; Eczema presents with pruritus and erythematous papules and plaques and is associated with other atopic diseases, such as allergic rhinitis and asthma.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLev