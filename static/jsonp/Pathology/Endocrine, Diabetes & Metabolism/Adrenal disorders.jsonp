               </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><div id=&quot;div_T53245&quot;><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; class=&quot;table-default-style&quot;><tbody><tr><td colspan=&quot;2&quot;><p align=&quot;center&quot;><strong>Primary adrenal insufficiency</strong></p></td></tr><tr><td><p align=&quot;center&quot;><strong>Etiology</strong></p></td><td><ul><li><strong>Autoimmune adrenalitis</strong> (most common)</li><li>Infection (eg, tuberculosis)</li><li>Hemorrhagic infarction</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Clinical features</strong></p></td><td><ul><li>Fatigue, weakness, anorexia/weight loss</li><li>Nausea/vomiting/abdominal pain</li><li>Postural hypotension</li><li>Hyperpigmentation</li><li><strong>Acute adrenal crisis:</strong> confusion, shock</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Laboratory findings</strong></p></td><td><ul><li>Hyponatremia, hyperkalemia</li><li>Low morning cortisol, high ACTH</li><li>Eosinophilia</li></ul></td></tr></tbody></table></div><p>This patient with a three month history of weight loss, abdominal pain, and fatigue developed cardiovascular collapse during a medical procedure (endoscopy), which suggests <strong>chronic adrenal insufficiency</strong> complicated by <strong>acute adrenal crisis</strong>.&amp;nbsp; Severe physiologic stress (eg, infection, medical procedures) normally induces increased glucocorticoid production; however, patients with adrenal insufficiency are unable to increase production.&amp;nbsp; Adrenal crisis (eg, hypotension, shock) is rapidly fatal if unrecognized.</p><p>Adrenal insufficiency can be caused by dysfunction of either the adrenal glands (primary) or pituitary (secondary).&amp;nbsp; This patient&amp;#39;s concomitant&amp;nbsp;<strong>skin hyperpigmentation</strong> is highly suggestive of chronic <strong>primary adrenal insufficiency</strong> (PAI), as the resulting cortisol deficiency leads to increased ACTH secretion.&amp;nbsp; ACTH is derived from proopiomelanocortin (POMC), the same prohormone from which melanocyte-stimulating hormone (MSH) is derived.&amp;nbsp; Increased ACTH also leads to increased MSH secretion, which results in hyperpigmentation.</p><p><strong>Autoimmune adrenalitis</strong> is the most common cause (&amp;gt;90%) of chronic PAI and occurs due to autoantibody formation, resulting in <strong>atrophy of the bilateral adrenal glands</strong>.&amp;nbsp; It can occur as an isolated disorder or in association with other autoimmune syndromes (eg, hypothyroidism, vitiligo).</p><p><strong>(Choice B)</strong>&amp;nbsp; Bilateral adrenal hemorrhage (Waterhouse-Friderichsen syndrome [WFS]) can cause PAI and adrenal crisis but is typically seen <em>acutely </em>in the setting of bacterial sepsis (eg, <em>Neisseria meningitidis</em>).&amp;nbsp; However, hyperpigmentation occurs in <em>chronic </em>adrenal insufficiency and therefore would not be expected with WFS.</p><p><strong>(Choices C and D)</strong>&amp;nbsp; Ischemic necrosis of the pituitary (Sheehan syndrome) most commonly occurs due to hypoperfusion of the enlarged pituitary gland during childbirth.&amp;nbsp; Acute pituitary hemorrhage (pituitary apoplexy) typically occurs in patients with pituitary adenomas.&amp;nbsp; These conditions can cause central (secondary) adrenal insufficiency; however, hyperpigmentation is not seen because ACTH levels are low, and POMC&amp;nbsp;levels are not increased.</p><p><strong>(Choice E)</strong>&amp;nbsp; A cortisol-producing adrenal tumor can cause contralateral adrenal atrophy due to suppression of ACTH secretion; however, it would not be associated with hyperpigmentation and would manifest as <a ng-click=&quot;showexhibit('4734',true);&quot;>hypercortisolism</a> rather than adrenal insufficiency.</p><p><strong>Educational objective:</strong><br>Primary adrenal insufficiency (PAI) is characterized by weight loss, abdominal pain, fatigue, and hyperpigmentation.&amp;nbsp; Physiologic stress in patients with PAI can trigger acute adrenal crisis (eg, fever, shock, mental status changes).&amp;nbsp; Autoimmune adrenalitis is the most common cause of PAI and leads to bilateral adrenal atrophy.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><div id=\"div_T53245\" class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" class=\"table-default-style\"><tbody><tr><td colspan=\"2\"><p align=\"center\"><strong>Primary adrenal insufficiency</strong></p></td></tr><tr><td><p align=\"center\"><strong>Etiology</strong></p></td><td><ul><li><strong>Autoimmune adrenalitis</strong> (most common)</li><li>Infection (eg, tuberculosis)</li><li>Hemorrhagic infarction</li></ul></td></tr><tr><td><p align=\"center\"><strong>Clinical features</strong></p></td><td><ul><li>Fatigue, weakness, anorexia/weight loss</li><li>Nausea/vomiting/abdominal pain</li><li>Postural hypotension</li><li>Hyperpigmentation</li><li><strong>Acute adrenal crisis:</strong> confusion, shock</li></ul></td></tr><tr><td><p align=\"center\"><strong>Laboratory findings</strong></p></td><td><ul><li>Hyponatremia, hyperkalemia</li><li>Low morning cortisol, high ACTH</li><li>Eosinophilia</li></ul></td></tr></tbody></table></div><p class=\"ng-scope\">This patient with a three month history of weight loss, abdominal pain, and fatigue developed cardiovascular collapse during a medical procedure (endoscopy), which suggests <strong>chronic adrenal insufficiency</strong> complicated by <strong>acute adrenal crisis</strong>.&nbsp; Severe physiologic stress (eg, infection, medical procedures) normally induces increased glucocorticoid production; however, patients with adrenal insufficiency are unable to increase production.&nbsp; Adrenal crisis (eg, hypotension, shock) is rapidly fatal if unrecognized.</p><p class=\"ng-scope\">Adrenal insufficiency can be caused by dysfunction of either the adrenal glands (primary) or pituitary (secondary).&nbsp; This patient's concomitant&nbsp;<strong>skin hyperpigmentation</strong> is highly suggestive of chronic <strong>primary adrenal insufficiency</strong> (PAI), as the resulting cortisol deficiency leads to increased ACTH secretion.&nbsp; ACTH is derived from proopiomelanocortin (POMC), the same prohormone from which melanocyte-stimulating hormone (MSH) is derived.&nbsp; Increased ACTH also leads to increased MSH secretion, which results in hyperpigmentation.</p><p class=\"ng-scope\"><strong>Autoimmune adrenalitis</strong> is the most common cause (&gt;90%) of chronic PAI and occurs due to autoantibody formation, resulting in <strong>atrophy of the bilateral adrenal glands</strong>.&nbsp; It can occur as an isolated disorder or in association with other autoimmune syndromes (eg, hypothyroidism, vitiligo).</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Bilateral adrenal hemorrhage (Waterhouse-Friderichsen syndrome [WFS]) can cause PAI and adrenal crisis but is typically seen <em>acutely </em>in the setting of bacterial sepsis (eg, <em>Neisseria meningitidis</em>).&nbsp; However, hyperpigmentation occurs in <em>chronic </em>adrenal insufficiency and therefore would not be expected with WFS.</p><p class=\"ng-scope\"><strong>(Choices C and D)</strong>&nbsp; Ischemic necrosis of the pituitary (Sheehan syndrome) most commonly occurs due to hypoperfusion of the enlarged pituitary gland during childbirth.&nbsp; Acute pituitary hemorrhage (pituitary apoplexy) typically occurs in patients with pituitary adenomas.&nbsp; These conditions can cause central (secondary) adrenal insufficiency; however, hyperpigmentation is not seen because ACTH levels are low, and POMC&nbsp;levels are not increased.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; A cortisol-producing adrenal tumor can cause contralateral adrenal atrophy due to suppression of ACTH secretion; however, it would not be associated with hyperpigmentation and would manifest as <a ng-click=\"showexhibit('4734',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L29332.jpg&quot;,&quot;title&quot;:&quot;L29332&quot;}]\">hypercortisolism</a> rather than adrenal insufficiency.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Primary adrenal insufficiency (PAI) is characterized by weight loss, abdominal pain, fatigue, and hyperpigmentation.&nbsp; Physiologic stress in patients with PAI can trigger acute adrenal crisis (eg, fever, shock, mental status changes).&nbsp; Autoimmune adrenalitis is the most common cause of PAI and leads to bilateral adrenal atrophy.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/24424183\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/24424183\"><span compiledata=\"\" htmlcontent=\"Diagnosis and classification of Addison&amp;#39;s disease (autoimmune adrenalitis).\" class=\"ng-scope\"><p class=\"ng-scope\">Diagnosis and classification of Addison's disease (autoimmune adrenalitis).</p></span></a><!-- end ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal -->\n                                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal -->\n                                </li><!-- end ngRepeat: questionReference in currentQuestion.questionMappingReferencesList -->\n                            </ul>\n                        </div>\n                    </div><!-- end ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" 