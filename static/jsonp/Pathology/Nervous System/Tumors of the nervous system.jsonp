edTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;22669&quot; src=&quot;https://www.uworld.com/media/L49102.png&quot; ></p><p>This patient with <strong>progressive headaches</strong> has an enhancing ventricular mass on MRI, consistent with a ventricular <strong>ependymoma</strong>.&amp;nbsp; Ependymomas arise from <strong>ependymal cells</strong>, which line the walls of the ventricular system and the central canal of the spinal cord and produce cerebrospinal fluid.&amp;nbsp; Choroid plexus tumors (derived from modified ependymal cells) can also be found in the lateral ventricles and have a similar appearance on MRI.</p><p>Intraventricular tumors can cause <strong>obstructive (noncommunicating) hydrocephalus</strong> due to impaired flow of cerebrospinal fluid through the interventricular foramina.&amp;nbsp; Hydrocephalus typically manifests with headache, signs of <strong>increased intracranial pressure</strong> (eg, nausea, papilledema, blurred vision), and dilated ventricles on neuroimaging.&amp;nbsp; The diagnosis of ependymoma can be confirmed on histopathologic examination, which shows <a ng-click=&quot;showexhibit('43974',true);&quot;>perivascular pseudorosettes</a>.</p><p><strong>(Choice A)</strong>&amp;nbsp; Astrocytes are the most prevalent type of glial cell in the CNS.&amp;nbsp; Tumors derived from these cells (eg, astrocytoma, <a ng-click=&quot;showexhibit('3246',true);&quot;>glioblastoma</a>) can cause hydrocephalus due to mass effect but are typically found in the brain parenchyma as opposed to the ventricular system.</p><p><strong>(Choice C)</strong>&amp;nbsp; Microglial cells are the phagocytic cells in the CNS.&amp;nbsp; They are not the originators of any major types of CNS tumors.</p><p><strong>(Choice D)</strong>&amp;nbsp; Brain tumors that arise from neuronal cells are rare, usually found in the brain parenchyma, and more likely to be of mixed neuronal-glial cell origin.</p><p><strong>(Choice E)</strong>&amp;nbsp; Oligodendrocytes are glial cells that are responsible for the myelination of axons in the CNS.&amp;nbsp; Well-differentiated brain tumors arising from oligodendrocytes are referred to as oligodendrogliomas.&amp;nbsp; These tumors, like astrocytomas, occur in the brain parenchyma and not in the ventricular system.</p><p><strong>(Choice F)</strong>&amp;nbsp; Schwann cells form myelin in the peripheral nervous system and serve as the cell of origin for schwannomas, which most commonly occur on upper extremity nerves.&amp;nbsp; Schwannomas that arise from cranial nerves (eg, CN VIII, <a ng-click=&quot;showexhibit('6812',true);&quot;>acoustic neuromas</a>) can cause hydrocephalus from mass effect due to their central location but would not be associated with an intraventricular mass.</p><p><strong>(Choice G)</strong>&amp;nbsp; <a ng-click=&quot;showexhibit('43975',true);&quot;>Hemangioblastomas</a> originate from vascular endothelial cells in the cerebellum or spinal cord.&amp;nbsp; They characteristically appear as an enhancing nodule with a cystic component.&amp;nbsp; These tumors often bleed, which can cause hydrocephalus due to dysfunction in cerebrospinal reabsorption at the arachnoid villi, but they do not originate from the ventricular wall.</p><p><strong>Educational objective:</strong><br>Ependymomas originate from the ependymal cells that line the ventricular system and the central canal of the spinal cord.&amp;nbsp; Ventricular ependymomas can block the flow of cerebrospinal fluid through the interventricular foramina, causing signs of increased intracranial pressure (eg, headache, papilledema) due to obstructive (noncommunicating) hydrocephalus.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"22669\" src=\"image/L49102.png\"></p><p class=\"ng-scope\">This patient with <strong>progressive headaches</strong> has an enhancing ventricular mass on MRI, consistent with a ventricular <strong>ependymoma</strong>.&nbsp; Ependymomas arise from <strong>ependymal cells</strong>, which line the walls of the ventricular system and the central canal of the spinal cord and produce cerebrospinal fluid.&nbsp; Choroid plexus tumors (derived from modified ependymal cells) can also be found in the lateral ventricles and have a similar appearance on MRI.</p><p class=\"ng-scope\">Intraventricular tumors can cause <strong>obstructive (noncommunicating) hydrocephalus</strong> due to impaired flow of cerebrospinal fluid through the interventricular foramina.&nbsp; Hydrocephalus typically manifests with headache, signs of <strong>increased intracranial pressure</strong> (eg, nausea, papilledema, blurred vision), and dilated ventricles on neuroimaging.&nbsp; The diagnosis of ependymoma can be confirmed on histopathologic examination, which shows <a ng-click=\"showexhibit('43974',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L65995.jpg&quot;,&quot;title&quot;:&quot;L65995&quot;}]\">perivascular pseudorosettes</a>.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Astrocytes are the most prevalent type of glial cell in the CNS.&nbsp; Tumors derived from these cells (eg, astrocytoma, <a ng-click=\"showexhibit('3246',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L445.png&quot;,&quot;title&quot;:&quot;L445&quot;}]\">glioblastoma</a>) can cause hydrocephalus due to mass effect but are typically found in the brain parenchyma as opposed to the ventricular system.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Microglial cells are the phagocytic cells in the CNS.&nbsp; They are not the originators of any major types of CNS tumors.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Brain tumors that arise from neuronal cells are rare, usually found in the brain parenchyma, and more likely to be of mixed neuronal-glial cell origin.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Oligodendrocytes are glial cells that are responsible for the myelination of axons in the CNS.&nbsp; Well-differentiated brain tumors arising from oligodendrocytes are referred to as oligodendrogliomas.&nbsp; These tumors, like astrocytomas, occur in the brain parenchyma and not in the ventricular system.</p><p class=\"ng-scope\"><strong>(Choice F)</strong>&nbsp; Schwann cells form myelin in the peripheral nervous system and serve as the cell of origin for schwannomas, which most commonly occur on upper extremity nerves.&nbsp; Schwannomas that arise from cranial nerves (eg, CN VIII, <a ng-click=\"showexhibit('6812',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L469.png&quot;,&quot;title&quot;:&quot;L469&quot;}]\">acoustic neuromas</a>) can cause hydrocephalus from mass effect due to their central location but would not be associated with an intraventricular mass.</p><p class=\"ng-scope\"><strong>(Choice G)</strong>&nbsp; <a ng-click=\"showexhibit('43975',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L65424.png&quot;,&quot;title&quot;:&quot;L65424&quot;}]\">Hemangioblastomas</a> originate from vascular endothelial cells in the cerebellum or spinal cord.&nbsp; They characteristically appear as an enhancing nodule with a cystic component.&nbsp; These tumors often bleed, which can cause hydrocephalus due to dysfunction in cerebrospinal reabsorption at the arachnoid villi, but they do not originate from the ventricular wall.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Ependymomas originate from the ependymal cells that line the ventricular system and the central canal of the spinal cord.&nbsp; Ventricular ependymomas can block the flow of cerebrospinal fluid through the interventricular foramina, causing signs of increased intracranial pressure (eg, headache, papilledema) due to obstructive (noncommunicating) hydrocephalus.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pathology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Nervous System</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Brain tumors</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
