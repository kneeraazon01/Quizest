6\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Transient neural compression</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;22510&quot; src=&quot;https://www.uworld.com/media/L47785.jpg&quot; ></p><p>Neurons are the major functional unit of the nervous system and are characterized histologically by large, round, centrally located nuclei and basophilic cytoplasmic granules composed of rough endoplasmic reticulum (Nissl bodies).&amp;nbsp; When neurons are injured, they undergo distinct histopathologic changes depending on the <a ng-click=&quot;showexhibit('20561',true);&quot;>type of underlying injury</a>.</p><p>In response to an <strong>acute irreversible injury</strong> (eg, severe hypoglycemia, <strong>ischemia</strong>), neurons begin to display characteristic morphologic changes 12-24 hours after the inciting event, appearing as shrunken cells separated from the surrounding tissue.&amp;nbsp; The <strong>nucleus</strong> becomes <strong>pyknotic</strong> (shrunken and basophilic with loss of the nucleolus), and the basophilic <strong>Nissl bodies disappear</strong>, resulting in cytoplasm that stains deeply eosinophilic <strong>(red neurons)</strong>.</p><p>In the brain, in contrast to most other tissues, ischemia ultimately results in liquefactive necrosis due to the breakdown of neural architecture by hydrolytic enzymes released from reacting inflammatory cells and damaged neurons.&amp;nbsp; The necrotic neuronal remnants are then phagocytized by microglia, and astrocytes proliferate at the site of the injury to form a glial scar.</p><p><strong>(Choice A)</strong>&amp;nbsp; Several neurodegenerative diseases (eg, Alzheimer disease, frontotemporal lobar degeneration) are characterized by intracellular and/or extracellular accumulation of abnormal proteins in neural tissue.&amp;nbsp; Histologic examination of affected areas demonstrates proteinaceous plaques, tangles, and/or inclusions with reactive gliosis and loss of neurons.</p><p><strong>(Choice B)</strong>&amp;nbsp; Normal aging is associated with progressive atrophy and neuronal loss.&amp;nbsp; However, red neuron changes are not characteristic of age-related physiologic changes.</p><p><strong>(Choice C)</strong>&amp;nbsp; Lysosomal storage diseases (eg, Tay-Sachs, <a ng-click=&quot;showexhibit('3580',true);&quot;>Niemann-Pick</a>) cause the accumulation of improperly digested substances in lysosomes, leading to neural degeneration through free radical damage and apoptosis.&amp;nbsp; Expected histopathologic changes include enlarged neuronal cells with a foamy appearance due to accumulated undigested macromolecules.</p><p><strong>(Choice E)</strong>&amp;nbsp; Extensive axon damage or transection results in an axonal reaction characterized by cell body swelling, movement of the nucleus to the periphery, and dispersion of Nissl bodies.&amp;nbsp; This response occurs due to increased protein and lipid synthesis during axon regeneration.</p><p><strong>(Choice F)</strong>&amp;nbsp; Transient neural compression can lead to a temporary loss of function due to ischemia but would not lead to significant histopathologic changes unless the compression was sustained.&amp;nbsp; Neural compression may occur in the CNS due to increased intracranial pressure or a mass lesion.</p><p><strong>Educational objective:</strong><br>Neurons that sustain irreversible ischemic injury begin to develop characteristic histopathologic changes 12-24 hours after the inciting event.&amp;nbsp; These changes include shrinkage of the cell body, pyknosis of the nucleus, loss of Nissl bodies, and cytoplasmic eosinophilia (red neurons).</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"22510\" src=\"image/L47785.jpg\"></p><p class=\"ng-scope\">Neurons are the major functional unit of the nervous system and are characterized histologically by large, round, centrally located nuclei and basophilic cytoplasmic granules composed of rough endoplasmic reticulum (Nissl bodies).&nbsp; When neurons are injured, they undergo distinct histopathologic changes depending on the <a ng-click=\"showexhibit('20561',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/46322.html&quot;,&quot;title&quot;:&quot;46322&quot;}]\">type of underlying injury</a>.</p><p class=\"ng-scope\">In response to an <strong>acute irreversible injury</strong> (eg, severe hypoglycemia, <strong>ischemia</strong>), neurons begin to display characteristic morphologic changes 12-24 hours after the inciting event, appearing as shrunken cells separated from the surrounding tissue.&nbsp; The <strong>nucleus</strong> becomes <strong>pyknotic</strong> (shrunken and basophilic with loss of the nucleolus), and the basophilic <strong>Nissl bodies disappear</strong>, resulting in cytoplasm that stains deeply eosinophilic <strong>(red neurons)</strong>.</p><p class=\"ng-scope\">In the brain, in contrast to most other tissues, ischemia ultimately results in liquefactive necrosis due to the breakdown of neural architecture by hydrolytic enzymes released from reacting inflammatory cells and damaged neurons.&nbsp; The necrotic neuronal remnants are then phagocytized by microglia, and astrocytes proliferate at the site of the injury to form a glial scar.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Several neurodegenerative diseases (eg, Alzheimer disease, frontotemporal lobar degeneration) are characterized by intracellular and/or extracellular accumulation of abnormal proteins in neural tissue.&nbsp; Histologic examination of affected areas demonstrates proteinaceous plaques, tangles, and/or inclusions with reactive gliosis and loss of neurons.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Normal aging is associated with progressive atrophy and neuronal loss.&nbsp; However, red neuron changes are not characteristic of age-related physiologic changes.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Lysosomal storage diseases (eg, Tay-Sachs, <a ng-click=\"showexhibit('3580',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L8150.jpg&quot;,&quot;title&quot;:&quot;L8150&quot;}]\">Niemann-Pick</a>) cause the accumulation of improperly digested substances in lysosomes, leading to neural degeneration through free radical damage and apoptosis.&nbsp; Expected histopathologic changes include enlarged neuronal cells with a foamy appearance due to accumulated undigested macromolecules.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Extensive axon damage or transection results in an axonal reaction characterized by cell body swelling, movement of the nucleus to the periphery, and dispersion of Nissl bodies.&nbsp; This response occurs due to increased protein and lipid synthesis during axon regeneration.</p><p class=\"ng-scope\"><strong>(Choice F)</strong>&nbsp; Transient neural compression can lead to a temporary loss of function due to ischemia but would not lead to significant histopathologic changes unless the compression was sustained.&nbsp; Neural compression may occur in the CNS due to increased intracranial pressure or a mass lesion.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Neurons that sustain irreversible ischemic injury begin to develop characteristic histopathologic changes 12-24 hours after the inciting event.&nbsp; These changes include shrinkage of the cell body, pyknosis of the nucleus, loss of Nissl bodies, and cytoplasmic eosinophilia (red neurons).</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pathology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Nervous System</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Ischemic stroke</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
