Product.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><div id=&quot;div_t33501&quot;><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; class=&quot;table-default-style table-header-footer-style&quot;><tbody><tr><td colspan=&quot;2&quot;><p align=&quot;center&quot;><strong>Serotonin syndrome</strong></p></td></tr><tr><td><p align=&quot;center&quot;><strong>Causes</strong></p></td><td><ul><li>Serotonergic medications, especially in combination<br />(eg, SSRI/SNRI, TCA, tramadol)</li><li>Drug interactions: serotonergic medication &amp;amp; MAOI or linezolid</li><li>Intentional overdose of serotonergic medications</li><li>Serotonergic drugs of abuse (eg, MDMA)</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Clinical<br />features</strong></p></td><td><ul><li>Mental status changes (eg, anxiety, agitation, delirium)</li><li>Autonomic dysregulation (eg, diaphoresis, hypertension, tachycardia, hyperthermia, vomiting, diarrhea)</li><li>Neuromuscular hyperactivity (eg, tremor, myoclonus, hyperreflexia)</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Management</strong></p></td><td><ul><li>Discontinuation of all serotonergic medications</li><li>Supportive care, sedation with benzodiazepines</li><li>Serotonin antagonist (cyproheptadine) if supportive measures fail</li><li>Immediate sedation, paralysis &amp;amp; tracheal intubation if<br />temperature &amp;gt;41.1 C (106 F)</li></ul></td></tr><tr><td colspan=&quot;2&quot;><p><strong>MAOI</strong> = monoamine oxidase inhibitor; <strong>MDMA</strong> = 3,4-methylenedioxymethamphetamine;<br /><strong>SNRI</strong> = serotonin-norepinephrine reuptake inhibitor; <strong>SSRI </strong>= selective serotonin reuptake inhibitor; <strong>TCA</strong> = tricyclic antidepressant.</p></td></tr></tbody></table></div><p>This patient&amp;#39;s feelings of sadness and worthlessness&amp;mdash;combined with her impaired sleep, decreased appetite, and decreased enjoyment of previously pleasurable activities (eg, socializing with friends)&amp;mdash;lasting &amp;#8805;2 weeks are consistent with major depressive disorder (MDD).&amp;nbsp; First-line pharmacotherapy for MDD is a selective serotonin reuptake inhibitor (SSRI).&amp;nbsp; Her apparent overdose on the prescribed SSRI and her resultant <strong>altered mental status</strong> (eg, disorientation), <strong>autonomic hyperactivity</strong> (eg, hyperthermia, tachycardia, diaphoresis), and <strong>neuromuscular excitation</strong> (eg, hyperreflexia, clonus, tremor) are characteristic of <strong>serotonin syndrome</strong>.</p><p>Serotonin syndrome is caused by excessive serotonin activity, most commonly brought on by combining SSRIs with another serotonergic agent such as a monoamine oxidase inhibitor.&amp;nbsp; It may also occur with a single agent if taken in overdose.&amp;nbsp; <strong>Tryptophan</strong> is a precursor of <strong>serotonin</strong>, whose biosynthesis occurs via the enzymes tryptophan hydroxylase and amino acid decarboxylase.</p><p><strong>(Choice A)</strong>&amp;nbsp; Glutamic acid is a precursor for the inhibitory neurotransmitter GABA.</p><p><strong>(Choice B)</strong>&amp;nbsp; Histidine is the precursor of histamine, which plays a role in allergic (atopic) reactions.</p><p><strong>(Choice C)</strong>&amp;nbsp; Methionine is a precursor or intermediate in the synthesis of cysteine, carnitine, taurine, and lecithin.</p><p><strong>(Choice E)</strong>&amp;nbsp; Tyrosine is a precursor for thyroxine, dopamine, epinephrine, norepinephrine, and melanin.</p><p><strong>Educational objective:</strong><br>Serotonin syndrome is characterized by altered mental status, autonomic hyperactivity, and neuromuscular excitation (eg, hyperreflexia, clonus).&amp;nbsp; Causes include high doses and/or combinations of serotonergic drugs (eg, SSRI and MAOI).&amp;nbsp; Tryptophan is a precursor for serotonin.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><div id=\"div_t33501\" class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" class=\"table-default-style table-header-footer-style\"><tbody><tr><td colspan=\"2\"><p align=\"center\"><strong>Serotonin syndrome</strong></p></td></tr><tr><td><p align=\"center\"><strong>Causes</strong></p></td><td><ul><li>Serotonergic medications, especially in combination<br>(eg, SSRI/SNRI, TCA, tramadol)</li><li>Drug interactions: serotonergic medication &amp; MAOI or linezolid</li><li>Intentional overdose of serotonergic medications</li><li>Serotonergic drugs of abuse (eg, MDMA)</li></ul></td></tr><tr><td><p align=\"center\"><strong>Clinical<br>features</strong></p></td><td><ul><li>Mental status changes (eg, anxiety, agitation, delirium)</li><li>Autonomic dysregulation (eg, diaphoresis, hypertension, tachycardia, hyperthermia, vomiting, diarrhea)</li><li>Neuromuscular hyperactivity (eg, tremor, myoclonus, hyperreflexia)</li></ul></td></tr><tr><td><p align=\"center\"><strong>Management</strong></p></td><td><ul><li>Discontinuation of all serotonergic medications</li><li>Supportive care, sedation with benzodiazepines</li><li>Serotonin antagonist (cyproheptadine) if supportive measures fail</li><li>Immediate sedation, paralysis &amp; tracheal intubation if<br>temperature &gt;41.1 C (106 F)</li></ul></td></tr><tr><td colspan=\"2\"><p><strong>MAOI</strong> = monoamine oxidase inhibitor; <strong>MDMA</strong> = 3,4-methylenedioxymethamphetamine;<br><strong>SNRI</strong> = serotonin-norepinephrine reuptake inhibitor; <strong>SSRI </strong>= selective serotonin reuptake inhibitor; <strong>TCA</strong> = tricyclic antidepressant.</p></td></tr></tbody></table></div><p class=\"ng-scope\">This patient's feelings of sadness and worthlessness\u2014combined with her impaired sleep, decreased appetite, and decreased enjoyment of previously pleasurable activities (eg, socializing with friends)\u2014lasting \u22652 weeks are consistent with major depressive disorder (MDD).&nbsp; First-line pharmacotherapy for MDD is a selective serotonin reuptake inhibitor (SSRI).&nbsp; Her apparent overdose on the prescribed SSRI and her resultant <strong>altered mental status</strong> (eg, disorientation), <strong>autonomic hyperactivity</strong> (eg, hyperthermia, tachycardia, diaphoresis), and <strong>neuromuscular excitation</strong> (eg, hyperreflexia, clonus, tremor) are characteristic of <strong>serotonin syndrome</strong>.</p><p class=\"ng-scope\">Serotonin syndrome is caused by excessive serotonin activity, most commonly brought on by combining SSRIs with another serotonergic agent such as a monoamine oxidase inhibitor.&nbsp; It may also occur with a single agent if taken in overdose.&nbsp; <strong>Tryptophan</strong> is a precursor of <strong>serotonin</strong>, whose biosynthesis occurs via the enzymes tryptophan hydroxylase and amino acid decarboxylase.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Glutamic acid is a precursor for the inhibitory neurotransmitter GABA.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Histidine is the precursor of histamine, which plays a role in allergic (atopic) reactions.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Methionine is a precursor or intermediate in the synthesis of cysteine, carnitine, taurine, and lecithin.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Tyrosine is a precursor for thyroxine, dopamine, epinephrine, norepinephrine, and melanin.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Serotonin syndrome is characterized by altered mental status, autonomic hyperactivity, and neuromuscular excitation (eg, hyperreflexia, clonus).&nbsp; Causes include high doses and/or combinations of serotonergic drugs (eg, SSRI and MAOI).&nbsp; Tryptophan is a precursor for serotonin.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/27406219\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/27406219\"><span compiledata=\"\" htmlcontent=\"Conu