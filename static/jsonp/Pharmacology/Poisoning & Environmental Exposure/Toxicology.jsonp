& !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><div id=&quot;div_T58382&quot;><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; class=&quot;table-default-style&quot;><tbody><tr><td colspan=&quot;2&quot;><p align=&quot;center&quot;><strong>Organophosphate poisoning</strong></p></td></tr><tr><td><p align=&quot;center&quot;><strong>Common exposures</strong></p></td><td><ul><li>Pesticide: farmer/field worker, pediatric ingestion, suicide attempt</li><li>Nerve agent: multiple patients presenting with similar symptoms</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Manifestations</strong></p></td><td><ul><li>Muscarinic:<ul style=&quot;list-style-type:circle;&quot;><li><strong>D</strong>iarrhea/diaphoresis</li><li><strong>U</strong>rination</li><li><strong>M</strong>iosis</li><li><strong>B</strong>ronchospasms, bronchorrhea, bradycardia</li><li><strong>E</strong>mesis</li><li><strong>L</strong>acrimation</li><li><strong>S</strong>alivation</li></ul></li><li>Nicotinic: muscle weakness, paralysis, fasciculations</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Management</strong></p></td><td><ul><li>Decontamination</li><li>Atropine reverses muscarinic symptoms</li><li>Pralidoxime reverses nicotinic and muscarinic symptoms (reactivates cholinesterase)</li></ul></td></tr></tbody></table></div><p>This patient developed bradycardia, tachypnea, and excessive salivation after being exposed to a pesticide, suggesting acute <strong>organophosphate toxicity</strong>.&amp;nbsp; Organophosphates are <strong>acetylcholinesterase inhibitors</strong> that produce signs of <strong>cholinergic excess</strong> due to hyperstimulation of the muscarinic (mnemonic: DUMBELS&amp;mdash;see table) and nicotinic (ie, muscle weakness, fasciculations, paralysis) receptors.&amp;nbsp; Most exposures occur with commercial pesticides, typically in farmers/field workers or in children with access to stored chemicals.</p><p>Initial management of organophosphate toxicity includes the administration of <strong>atropine</strong>, a <strong>competitive inhibitor</strong> of acetylcholine at the <strong>muscarinic receptor</strong>, which leads to the resolution of muscarinic symptoms (eg, bradycardia, salivation).&amp;nbsp; Patients who exhibit signs of nicotinic hyperstimulation (eg, weakness) should then receive pralidoxime, a cholinesterase-reactivating agent that treats both the muscarinic and nicotinic effects of organophosphates.&amp;nbsp; However, <strong>pralidoxime </strong>should be <strong>given only after atropine</strong> because it can cause transient acetylcholinesterase inhibition, which can momentarily worsen symptoms <strong>(Choice E)</strong>.</p><p><strong>(Choice B)</strong>&amp;nbsp; Flumazenil is indicated to treat benzodiazepine overdose, which presents with sedation but would not be expected to cause bradycardia, tachypnea, or hypersalivation.</p><p><strong>(Choice C)</strong>&amp;nbsp; Naloxone is indicated for treatment of opioid intoxication, which can cause sedation and miosis but suppresses (not increases) the respiratory rate.&amp;nbsp; Opioid intoxication usually occurs via injection or ingestion, rather than external contact with aerosolized agents.</p><p><strong>(Choice D)</strong>&amp;nbsp; Neostigmine is another acetylcholinesterase inhibitor that is sometimes used to treat myasthenia gravis; its use would worsen this patient&amp;#39;s cholinergic symptoms.</p><p><strong>(Choice F)</strong>&amp;nbsp; Sodium bicarbonate is indicated to treat patients with tricyclic antidepressant (TCA) overdose who are at risk for ventricular arrhythmias.&amp;nbsp; However, TCA poisoning presents with altered mental status and anticholinergic signs (ie, flushed skin, mydriasis).</p><p><strong>Educational objective:</strong><br>Organophosphates are acetylcholinesterase inhibitors that are commonly used as agricultural pesticides.&amp;nbsp; Toxicity is characterized by signs of cholinergic excess (eg, miosis, bronchospasm, muscle fasciculations/weakness, diarrhea, vomiting, lacrimation).&amp;nbsp; First-line therapy is atropine, a competitive inhibitor of acetylcholine at the muscarinic receptor.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><div id=\"div_T58382\" class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" class=\"table-default-style\"><tbody><tr><td colspan=\"2\"><p align=\"center\"><strong>Organophosphate poisoning</strong></p></td></tr><tr><td><p align=\"center\"><strong>Common exposures</strong></p></td><td><ul><li>Pesticide: farmer/field worker, pediatric ingestion, suicide attempt</li><li>Nerve agent: multiple patients presenting with similar symptoms</li></ul></td></tr><tr><td><p align=\"center\"><strong>Manifestations</strong></p></td><td><ul><li>Muscarinic:<ul style=\"list-style-type:circle;\"><li><strong>D</strong>iarrhea/diaphoresis</li><li><strong>U</strong>rination</li><li><strong>M</strong>iosis</li><li><strong>B</strong>ronchospasms, bronchorrhea, bradycardia</li><li><strong>E</strong>mesis</li><li><strong>L</strong>acrimation</li><li><strong>S</strong>alivation</li></ul></li><li>Nicotinic: muscle weakness, paralysis, fasciculations</li></ul></td></tr><tr><td><p align=\"center\"><strong>Management</strong></p></td><td><ul><li>Decontamination</li><li>Atropine reverses muscarinic symptoms</li><li>Pralidoxime reverses nicotinic and muscarinic symptoms (reactivates cholinesterase)</li></ul></td></tr></tbody></table></div><p class=\"ng-scope\">This patient developed bradycardia, tachypnea, and excessive salivation after being exposed to a pesticide, suggesting acute <strong>organophosphate toxicity</strong>.&nbsp; Organophosphates are <strong>acetylcholinesterase inhibitors</strong> that produce signs of <strong>cholinergic excess</strong> due to hyperstimulation of the muscarinic (mnemonic: DUMBELS\u2014see table) and nicotinic (ie, muscle weakness, fasciculations, paralysis) receptors.&nbsp; Most exposures occur with commercial pesticides, typically in farmers/field workers or in children with access to stored chemicals.</p><p class=\"ng-scope\">Initial management of organophosphate toxicity includes the administration of <strong>atropine</strong>, a <strong>competitive inhibitor</strong> of acetylcholine at the <strong>muscarinic receptor</strong>, which leads to the resolution of muscarinic symptoms (eg, bradycardia, salivation).&nbsp; Patients who exhibit signs of nicotinic hyperstimulation (eg, weakness) should then receive pralidoxime, a cholinesterase-reactivating agent that treats both the muscarinic and nicotinic effects of organophosphates.&nbsp; However, <strong>pralidoxime </strong>should be <strong>given only after atropine</strong> because it can cause transient acetylcholinesterase inhibition, which can momentarily worsen symptoms <strong>(Choice E)</strong>.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Flumazenil is indicated to treat benzodiazepine overdose, which presents with sedation but would not be expected to cause bradycardia, tachypnea, or hypersalivation.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Naloxone is indicated for treatment of opioid intoxication, which can cause sedation and miosis but suppresses (not increases) the respiratory rate.&nbsp; Opioid intoxication usually occurs via injection or ingestion, rather than external contact with aerosolized agents.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Neostigmine is another acetylcholinesterase inhibitor that is sometimes used to treat myasthenia gravis; its use would worsen this patient's cholinergic symptoms.</p><p class=\"ng-scope\"><strong>(Choice F)</strong>&nbsp; Sodium bicarbonate is indicated to treat patients with tricyclic antidepressant (TCA) overdose who are at risk for ventricular arrhythmias.&nbsp; However, TCA poisoning presents with altered mental status and anticholinergic signs (ie, flushed skin, mydriasis).</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Organophosphates are acetylcholinesterase inhibitors that are commonly used as agricultural pesticides.&nbsp; Toxicity is characterized by signs of cholinergic excess (eg, miosis, bronchospasm, muscle fasciculations/weakness, diarrhea, vomiting, lacrimation).&nbsp; First-line therapy is atropine, a competitive inhibitor of acetylcholine at the muscarinic receptor.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pharmacology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Poisoning &amp; Environmental Exposure</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Organophosphate poisoning</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>