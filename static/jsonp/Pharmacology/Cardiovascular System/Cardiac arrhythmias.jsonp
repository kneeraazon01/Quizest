estPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p>This patient sustained an anterolateral ST elevation myocardial infarction (MI) with successful reperfusion following percutaneous coronary intervention.&amp;nbsp; <strong>Ventricular arrhythmias</strong> (premature beats, ventricular tachycardia/fibrillation) are common in the first 24-48 hours after MI and may be treated with antiarrhythmic medications (eg, <strong>amiodarone</strong>, <strong>lidocaine</strong>).</p><p><strong>Class IB drugs</strong> like lidocaine are the weakest sodium channel blockers (<strong>dissociate the fastest</strong>) compared to other class I antiarrhythmics.&amp;nbsp; They predominantly bind to sodium channels in the <strong>inactivated state</strong>, and dissociation from the channels occurs so rapidly that they have a negligible effect on QRS duration in normal cardiac tissues.&amp;nbsp; Ischemic myocardium has higher than normal (less negative) resting membrane potential, which delays voltage-dependent recovery of <a ng-click=&quot;showexhibit('3793',true);&quot;>sodium channels</a> from the inactivated to the resting state.&amp;nbsp; This allows increased binding of class IB agents; therefore, class IB antiarrhythmics are highly efficacious in inhibiting <strong>ischemia-induced ventricular arrhythmias</strong>.</p><p><strong>(Choice A)</strong>&amp;nbsp; Adenosine causes transient conduction delay through the atrioventricular (AV) node and is used in the acute treatment of paroxysmal supraventricular tachycardia; it is not indicated in ventricular arrhythmias.</p><p><strong>(Choice B)</strong>&amp;nbsp; Digoxin enhances vagal tone and leads to an increased effective refractory period and decreased conduction velocity through the AV node.&amp;nbsp; These actions are useful in management of patients with supraventricular tachyarrhythmias.</p><p><strong>(Choice C)</strong>&amp;nbsp; Diltiazem is a class IV antiarrhythmic drug (calcium channel blocker) that slows the sinus rate, prolongs conduction through the AV node, and depresses myocardial contractility (ie, a negative inotropic effect).&amp;nbsp; It is primarily used as a rate-control agent for the management of atrial tachyarrhythmias (eg, atrial flutter, atrial fibrillation).</p><p><strong>(Choice D)</strong>&amp;nbsp; Ibutilide is a class III antiarrhythmic drug (potassium channel blocker) and is occasionally used for acute termination of atrial flutter and fibrillation.&amp;nbsp; It prolongs the QT interval and increases the risk of polymorphic ventricular tachycardia (torsades de pointes).</p><p><strong>(Choice F)</strong>&amp;nbsp; Metoprolol is a selective beta-1 receptor antagonist (class II antiarrhythmic) that acts as a negative inotropic and chronotropic agent (decreased myocardial contractility and heart rate).&amp;nbsp; Beta blockers are frequently used in patients with acute or recent MI; they reduce the risk of ventricular arrhythmia by decreasing automaticity and reduce the size of ischemic myocardium by decreasing myocardial oxygen demand.</p><p><strong>(Choice G)</strong>&amp;nbsp; Class IA antiarrhythmic drugs (eg, procainamide, disopyramide) tend to bind to sodium channels in the open state and are more specific for suppressing arrhythmias arising from areas of normal automaticity (lidocaine is more specific to inactivated channels that are more predominant in ischemic myocardium).&amp;nbsp; Class IA agents also have significant potassium channel-blocking activity, leading to prolongation of action potential and an increased risk of ventricular arrhythmias.</p><p><strong>Educational objective:</strong><br>Lidocaine is a class IB antiarrhythmic drug that tends to bind to inactivated sodium channels and rapidly dissociates.&amp;nbsp; As a result, it is effective in suppressing ventricular tachyarrhythmias induced by rapidly depolarizing and ischemic myocardium.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\">This patient sustained an anterolateral ST elevation myocardial infarction (MI) with successful reperfusion following percutaneous coronary intervention.&nbsp; <strong>Ventricular arrhythmias</strong> (premature beats, ventricular tachycardia/fibrillation) are common in the first 24-48 hours after MI and may be treated with antiarrhythmic medications (eg, <strong>amiodarone</strong>, <strong>lidocaine</strong>).</p><p class=\"ng-scope\"><strong>Class IB drugs</strong> like lidocaine are the weakest sodium channel blockers (<strong>dissociate the fastest</strong>) compared to other class I antiarrhythmics.&nbsp; They predominantly bind to sodium channels in the <strong>inactivated state</strong>, and dissociation from the channels occurs so rapidly that they have a negligible effect on QRS duration in normal cardiac tissues.&nbsp; Ischemic myocardium has higher than normal (less negative) resting membrane potential, which delays voltage-dependent recovery of <a ng-click=\"showexhibit('3793',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L9719.jpg&quot;,&quot;title&quot;:&quot;L9719&quot;}]\">sodium channels</a> from the inactivated to the resting state.&nbsp; This allows increased binding of class IB agents; therefore, class IB antiarrhythmics are highly efficacious in inhibiting <strong>ischemia-induced ventricular arrhythmias</strong>.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Adenosine causes transient conduction delay through the atrioventricular (AV) node and is used in the acute treatment of paroxysmal supraventricular tachycardia; it is not indicated in ventricular arrhythmias.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Digoxin enhances vagal tone and leads to an increased effective refractory period and decreased conduction velocity through the AV node.&nbsp; These actions are useful in management of patients with supraventricular tachyarrhythmias.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Diltiazem is a class IV antiarrhythmic drug (calcium channel blocker) that slows the sinus rate, prolongs conduction through the AV node, and depresses myocardial contractility (ie, a negative inotropic effect).&nbsp; It is primarily used as a rate-control agent for the management of atrial tachyarrhythmias (eg, atrial flutter, atrial fibrillation).</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Ibutilide is a class III antiarrhythmic drug (potassium channel blocker) and is occasionally used for acute termination of atrial flutter and fibrillation.&nbsp; It prolongs the QT interval and increases the risk of polymorphic ventricular tachycardia (torsades de pointes).</p><p class=\"ng-scope\"><strong>(Choice F)</strong>&nbsp; Metoprolol is a selective beta-1 receptor antagonist (class II antiarrhythmic) that acts as a negative inotropic and chronotropic agent (decreased myocardial contractility and heart rate).&nbsp; Beta blockers are frequently used in patients with acute or recent MI; they reduce the risk of ventricular arrhythmia by decreasing automaticity and reduce the size of ischemic myocardium by decreasing myocardial oxygen demand.</p><p class=\"ng-scope\"><strong>(Choice G)</strong>&nbsp; Class IA antiarrhythmic drugs (eg, procainamide, disopyramide) tend to bind to sodium channels in the open state and are more specific for suppressing arrhythmias arising from areas of normal automaticity (lidocaine is more specific to inactivated channels that are more predominant in ischemic myocardium).&nbsp; Class IA agents also have significant potassium channel-blocking activity, leading to prolongation of action potential and an increased risk of ventricular arrhythmias.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Lidocaine is a class IB antiarrhythmic drug that tends to bind to inactivated sodium channels and rapidly dissociates.&nbsp; As a result, it is effective in suppressing ventricular tachyarrhythmias induced by rapidly depolarizing and ischemic myocardium.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.