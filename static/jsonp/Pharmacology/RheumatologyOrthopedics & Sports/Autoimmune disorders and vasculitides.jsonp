   </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;33528&quot; src=&quot;https://www.uworld.com/media/L59330.jpg&quot; ></p><p><strong>Azathioprine</strong> is an <strong>immunosuppressant</strong> used in the prevention of organ transplant rejection and the treatment of autoimmune diseases, including rheumatoid arthritis, inflammatory bowel disease, and autoimmune hepatitis.&amp;nbsp; It functions via inhibition of purine synthesis pathways.&amp;nbsp; Following ingestion, azathioprine is <a ng-click=&quot;showexhibit('33399',true);&quot;>converted</a> to 6-mercaptopurine, which is then converted to <strong>6-thioguanine metabolites</strong>; these metabolites are pharmacologically active and mediate both the therapeutic and adverse effects of azathioprine.</p><p>The 6-thioguanine metabolites reduce cellular proliferation via 2 methods of purine synthesis inhibition:</p><ul><li>The metabolites <strong>inhibit </strong>phosphoribosylpyrophosphate <strong>(PRPP) amidotransferase</strong>, an enzyme that catalyzes an early step in <strong>de novo purine synthesis</strong>.<br>&amp;nbsp;</li><li>The metabolites act as <strong>false nucleotides</strong>, incorporating into actively replicating DNA and RNA and rendering the growing nucleic acid strand nonfunctional.</li></ul><p>These effects are exerted preferentially on rapidly dividing cells (eg, activated lymphocytes in the setting of organ transplant or autoimmune disease) but are otherwise nonspecific and disrupt other hematologic cell lines, including neutrophils, erythrocytes, and platelets.&amp;nbsp; Therefore, <strong>pancytopenia</strong> (ie, leukopenia, anemia, and thrombocytopenia) is a common <strong>adverse effect</strong> that must be <strong>monitored</strong> with periodic <strong>complete blood count</strong> testing.</p><p><strong>(Choices B and C)</strong>&amp;nbsp; Unlike glucocorticoids, azathioprine has no adverse effects on glucose or lipid metabolism, an advantage of this medication over chronic glucocorticoid therapy.</p><p><strong>(Choice D)</strong>&amp;nbsp; Azathioprine has no known effect on lung function.&amp;nbsp; The chemotherapeutic agent bleomycin may cause pulmonary fibrosis and therefore requires periodic monitoring of pulmonary function.</p><p><strong>(Choice E)</strong>&amp;nbsp; The anemia caused by azathioprine and other antimetabolite drugs (eg, methotrexate) is typically macrocytic (due to impaired DNA synthesis), but this macrocytic effect is independent of vitamin B<font size=&quot;2&quot;><sub>12</sub></font> levels.&amp;nbsp; Proton pump inhibitors and histamine 2 blockers can disrupt absorption of vitamin B<font size=&quot;2&quot;><sub>12</sub></font> and may cause or contribute to its deficiency.</p><p><strong>Educational objective:</strong><br>Azathioprine is an immunosuppressant that functions via inhibition of purine synthesis by its pharmacologically active 6-thioguanine metabolites.&amp;nbsp; In addition to the desired effect of reduced lymphocyte proliferation, the proliferation of other hematologic cell lines (eg, neutrophils, erythrocytes, platelets) is reduced, leading to a common adverse effect of pancytopenia.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"33528\" src=\"image/L59330.jpg\"></p><p class=\"ng-scope\"><strong>Azathioprine</strong> is an <strong>immunosuppressant</strong> used in the prevention of organ transplant rejection and the treatment of autoimmune diseases, including rheumatoid arthritis, inflammatory bowel disease, and autoimmune hepatitis.&nbsp; It functions via inhibition of purine synthesis pathways.&nbsp; Following ingestion, azathioprine is <a ng-click=\"showexhibit('33399',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L59300.jpg&quot;,&quot;title&quot;:&quot;L59300&quot;}]\">converted</a> to 6-mercaptopurine, which is then converted to <strong>6-thioguanine metabolites</strong>; these metabolites are pharmacologically active and mediate both the therapeutic and adverse effects of azathioprine.</p><p class=\"ng-scope\">The 6-thioguanine metabolites reduce cellular proliferation via 2 methods of purine synthesis inhibition:</p><ul class=\"ng-scope\"><li>The metabolites <strong>inhibit </strong>phosphoribosylpyrophosphate <strong>(PRPP) amidotransferase</strong>, an enzyme that catalyzes an early step in <strong>de novo purine synthesis</strong>.<br>&nbsp;</li><li>The metabolites act as <strong>false nucleotides</strong>, incorporating into actively replicating DNA and RNA and rendering the growing nucleic acid strand nonfunctional.</li></ul><p class=\"ng-scope\">These effects are exerted preferentially on rapidly dividing cells (eg, activated lymphocytes in the setting of organ transplant or autoimmune disease) but are otherwise nonspecific and disrupt other hematologic cell lines, including neutrophils, erythrocytes, and platelets.&nbsp; Therefore, <strong>pancytopenia</strong> (ie, leukopenia, anemia, and thrombocytopenia) is a common <strong>adverse effect</strong> that must be <strong>monitored</strong> with periodic <strong>complete blood count</strong> testing.</p><p class=\"ng-scope\"><strong>(Choices B and C)</strong>&nbsp; Unlike glucocorticoids, azathioprine has no adverse effects on glucose or lipid metabolism, an advantage of this medication over chronic glucocorticoid therapy.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Azathioprine has no known effect on lung function.&nbsp; The chemotherapeutic agent bleomycin may cause pulmonary fibrosis and therefore requires periodic monitoring of pulmonary function.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; The anemia caused by azathioprine and other antimetabolite drugs (eg, methotrexate) is typically macrocytic (due to impaired DNA synthesis), but this macrocytic effect is independent of vitamin B<font size=\"2\"><sub>12</sub></font> levels.&nbsp; Proton pump inhibitors and histamine 2 blockers can disrupt absorption of vitamin B<font size=\"2\"><sub>12</sub></font> and may cause or contribute to its deficiency.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Azathioprine is an immunosuppressant that functions via inhibition of purine synthesis by its pharmacologically active 6-thioguanine metabolites.&nbsp; In addition to the desired effect of reduced lymphocyte proliferation, the proliferation of other hematologic cell lines (eg, neutrophils, erythrocytes, platelets) is reduced, leading to a common adverse effect of pancytopenia.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/8174958\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/8174958\"><span compiledata=\"\" htmlcontent=\"Bone marrow toxicity caused by azathioprine in inflammatory bowel disease: 27 years of experience.\" class=\"ng-scope\"><p class=\"ng-scope\">Bone marrow toxicity caused by azathioprine in inflammatory bowel disease: 27 years of experience.</p></span></a><!-- end ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal -->\n                                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal -->\n                                </li><!-- end ngRepeat: questionReference in currentQuestion.questionMappingReferencesList -->\n                            </ul>\n                        </div>\n                    </div><!-- end ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n