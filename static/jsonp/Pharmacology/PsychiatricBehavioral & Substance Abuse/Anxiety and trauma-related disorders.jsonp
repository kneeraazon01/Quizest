        <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;3644&quot; src=&quot;https://www.uworld.com/media/L26055.jpg&quot; ></p><p>This patient exhibits classic symptoms of a panic attack (abrupt and unexpected onset of characteristic physical symptoms and negative medical workup).&amp;nbsp; She most likely received a benzodiazepine, a medication commonly used in emergency settings because it provides rapid relief of acute anxiety.</p><p>Benzodiazepines work at the GABA<font size=&quot;2&quot;><sub>A</sub></font> (&amp;#947;-aminobutyric acid) receptor complex.&amp;nbsp; <strong>GABA</strong> is the <strong>main inhibitory neurotransmitter</strong> in the central nervous system and is synthesized from glutamate using glutamate decarboxylase.&amp;nbsp; The GABA<font size=&quot;2&quot;><sub>A</sub></font> receptor complex consists of 5 subunits and a central chloride ion channel, with different binding sites for GABA and various drugs (eg, benzodiazepines, barbiturates).&amp;nbsp; <strong>Benzodiazepines</strong> act by binding to the benzodiazepine binding site, which <strong>allosterically modulates</strong> the <strong>binding of GABA</strong>, resulting in an <strong>increased </strong>frequency of <strong>chloride ion channel opening</strong>.&amp;nbsp; The influx of chloride ions into the neurons causes neuronal hyperpolarization and inhibition of the action potential.&amp;nbsp; Barbiturates act at a separate allosteric site and work by prolonging the duration of channel openings in response to GABA and, at higher doses, as GABA<font size=&quot;2&quot;><sub>A</sub></font> receptor agonists.</p><p><strong>(Choice A)</strong>&amp;nbsp; GABA<font size=&quot;2&quot;><sub>B</sub></font> receptors mediate their actions using G proteins.&amp;nbsp; The skeletal muscle relaxant baclofen is an example of a drug that works as a GABA<font size=&quot;2&quot;><sub>B</sub></font> receptor agonist.&amp;nbsp; Benzodiazepines work at the GABA<font size=&quot;2&quot;><sub>A</sub></font> receptor.</p><p><strong>(Choice B)</strong>&amp;nbsp; Benzodiazepines do not alter GABA metabolism.&amp;nbsp; Certain anti-epileptic medications, including valproic acid and vigabatrin, function to reduce GABA catabolism.</p><p><strong>(Choice D)</strong>&amp;nbsp; Benzodiazepines do not function through direct activation of the GABA<font size=&quot;2&quot;><sub>A</sub></font> receptor.&amp;nbsp; Instead, they work as positive allosteric modulators that promote an increased frequency of channel openings in response to GABA stimulation.</p><p><strong>(Choice E)</strong>&amp;nbsp; Benzodiazepines do not function through blockage of the GABA<font size=&quot;2&quot;><sub>A</sub></font> receptor lumen.&amp;nbsp; Examples of drugs that directly block the lumen of ion channels include amiloride (which affects epithelial sodium channels in the distal portions of the nephron) and verapamil (a calcium channel blocker).</p><p><strong>(Choice F)</strong>&amp;nbsp; Prolonged benzodiazepine use has been shown to cause downregulation of GABA<font size=&quot;2&quot;><sub>A</sub></font> receptors and may be one of the mechanisms by which benzodiazepine tolerance develops.<br><br><strong>Educational objective:</strong><br>Benzodiazepines act by binding to the benzodiazepine binding site, which allosterically modulates the binding of GABA, resulting in an increased frequency of chloride ion channel opening. &amp;nbsp;The <em>influx of chloride ions</em> into the neurons causes neuronal hyperpolarization and inhibition of the action potential.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"3644\" src=\"image/L26055.jpg\"></p><p class=\"ng-scope\">This patient exhibits classic symptoms of a panic attack (abrupt and unexpected onset of characteristic physical symptoms and negative medical workup).&nbsp; She most likely received a benzodiazepine, a medication commonly used in emergency settings because it provides rapid relief of acute anxiety.</p><p class=\"ng-scope\">Benzodiazepines work at the GABA<font size=\"2\"><sub>A</sub></font> (\u03b3-aminobutyric acid) receptor complex.&nbsp; <strong>GABA</strong> is the <strong>main inhibitory neurotransmitter</strong> in the central nervous system and is synthesized from glutamate using glutamate decarboxylase.&nbsp; The GABA<font size=\"2\"><sub>A</sub></font> receptor complex consists of 5 subunits and a central chloride ion channel, with different binding sites for GABA and various drugs (eg, benzodiazepines, barbiturates).&nbsp; <strong>Benzodiazepines</strong> act by binding to the benzodiazepine binding site, which <strong>allosterically modulates</strong> the <strong>binding of GABA</strong>, resulting in an <strong>increased </strong>frequency of <strong>chloride ion channel opening</strong>.&nbsp; The influx of chloride ions into the neurons causes neuronal hyperpolarization and inhibition of the action potential.&nbsp; Barbiturates act at a separate allosteric site and work by prolonging the duration of channel openings in response to GABA and, at higher doses, as GABA<font size=\"2\"><sub>A</sub></font> receptor agonists.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; GABA<font size=\"2\"><sub>B</sub></font> receptors mediate their actions using G proteins.&nbsp; The skeletal muscle relaxant baclofen is an example of a drug that works as a GABA<font size=\"2\"><sub>B</sub></font> receptor agonist.&nbsp; Benzodiazepines work at the GABA<font size=\"2\"><sub>A</sub></font> receptor.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Benzodiazepines do not alter GABA metabolism.&nbsp; Certain anti-epileptic medications, including valproic acid and vigabatrin, function to reduce GABA catabolism.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Benzodiazepines do not function through direct activation of the GABA<font size=\"2\"><sub>A</sub></font> receptor.&nbsp; Instead, they work as positive allosteric modulators that promote an increased frequency of channel openings in response to GABA stimulation.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Benzodiazepines do not function through blockage of the GABA<font size=\"2\"><sub>A</sub></font> receptor lumen.&nbsp; Examples of drugs that directly block the lumen of ion channels include amiloride (which affects epithelial sodium channels in the distal portions of the nephron) and verapamil (a calcium channel blocker).</p><p class=\"ng-scope\"><strong>(Choice F)</strong>&nbsp; Prolonged benzodiazepine use has been shown to cause downregulation of GABA<font size=\"2\"><sub>A</sub></font> receptors and may be one of the mechanisms by which benzodiazepine tolerance develops.<br><br><strong>Educational objective:</strong><br>Benzodiazepines act by binding to the benzodiazepine binding site, which allosterically modulates the binding of GABA, resulting in an increased frequency of chloride ion channel opening. &nbsp;The <em>influx of chloride ions</em> into the neurons causes neuronal hyperpolarization and inhibition of the action potential.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/23789008\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/23789008\"><span compiledata=\"\" htmlcontent=\"Benzodiazepine pharmacology and central nervous system-mediated effects.\" class=\"ng-scope\"><p class=\"ng-scope\">Benzodiazepine pharmacology and central nervous system-mediated effects.</p></span></a><!-- end ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal -->\n                                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal -->\n                                </li><!-- end ngRepeat: questionReference in currentQuestion.questionMappingReferencesList -->\n                            </ul>\n                        </div>\n                    </div><!-- end ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pharmacology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n     