 ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><div id=&quot;div_T58674&quot;><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; class=&quot;table-default-style table-header-footer-style&quot;><tbody><tr><td colspan=&quot;2&quot;><p align=&quot;center&quot;><strong>Common medications to avoid in older adults (Beers criteria)</strong></p></td></tr><tr><td><p align=&quot;center&quot;><strong>Anticholinergic</strong></p></td><td><ul><li>First-generation antihistamines</li><li>Gastrointestinal antispasmodics</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Cardiovascular</strong></p></td><td><ul><li>Alpha-1 blockers (as antihypertensives)</li><li>Centrally acting alpha-2 agonists</li><li>Many antiarrhythmics</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>CNS</strong></p></td><td><ul><li>Tricyclic antidepressants</li><li>Antipsychotics</li><li>Barbiturates, benzodiazepines &amp;amp; other hypnotics</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Endocrine</strong></p></td><td><ul><li>Long-acting sulfonylureas</li><li>Sliding-scale insulin</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Pain</strong></p></td><td><ul><li>Nonselective NSAIDs</li><li>Skeletal muscle relaxants</li></ul></td></tr><tr><td colspan=&quot;2&quot;><p><strong>NSAIDs</strong> = nonsteroidal anti-inflammatory drugs.</p></td></tr></tbody></table></div><p>Older patients are at increased risk for adverse effects of medications due to decreased drug clearance, cognitive and sensory deficits, multiple comorbidities, drug-drug interactions, and poor drug compliance.&amp;nbsp; <strong>Polypharmacy</strong> (ie, &amp;#8805;5 prescription and/or over-the-counter medications) is of particular concern in the elderly and can lead to a prescribing cascade, in which additional drugs are given for adverse effects that are misinterpreted as a new medical condition.</p><p>The <strong>Beers criteria</strong> were developed to identify drugs requiring caution in geriatric patients.&amp;nbsp; <strong>First-generation antihistamines</strong> (eg, diphenhydramine) and other drugs with anticholinergic activity can increase the risk of confusion, hallucinations, dizziness, dry mouth, and constipation and should be avoided in this population.&amp;nbsp; Other commonly used medications that should be avoided in the elderly include benzodiazepines (and other sedating medications), antipsychotics, many antiarrhythmics (eg, digoxin), and centrally acting skeletal muscle relaxants.</p><p><strong>(Choices A and E)</strong>&amp;nbsp; Although hypotension due to excessive antihypertension medication use could cause dizziness, this patient has normal blood pressure and heart rate, making this much less likely as a cause of her dizziness.&amp;nbsp; In addition, although centrally acting alpha agonists (eg, clonidine) and alpha-1 blockers (eg, doxazosin) confer a&amp;nbsp;high risk of orthostatic hypotension in elderly patients,&amp;nbsp;thiazide diuretics (eg, hydrochlorothiazide),&amp;nbsp;calcium channel blockers (eg, amlodipine), and ACE inhibitors/angiotensin II receptor blockers are generally considered acceptable for this population.</p><p><strong>(Choice B)</strong>&amp;nbsp; Aspirin should be used with caution in older adults due to the increased risk of gastric ulcer; however, the reduced risk of cardiovascular events provided by aspirin in patients with prior transient ischemic attack or stroke likely outweighs this risk.</p><p><strong>(Choice D)</strong>&amp;nbsp; Donepezil is a cholinesterase inhibitor that is commonly used in the treatment of Alzheimer dementia.&amp;nbsp; Bradycardia can occasionally occur; however, this patient has a normal heart rate.</p><p><strong>(Choice F)</strong>&amp;nbsp; Selective serotonin reuptake inhibitors (eg, sertraline) are the preferred first-line medications for depression in older adults.&amp;nbsp; Serotonin-norepinephrine reuptake inhibitors (eg, duloxetine) may also be used, especially for patients with chronic pain.&amp;nbsp; Tricyclic antidepressants are relatively contraindicated in the elderly due to their anticholinergic effects.</p><p><strong>Educational objective:</strong><br>The Beers criteria identify drugs that should be used with caution in geriatric patients.&amp;nbsp; Common drugs to avoid include anticholinergics (eg, first-generation antihistamines), centrally acting alpha-2 agonists, tricyclic antidepressants, benzodiazepines (and other sedating medications), antipsychotics, many antiarrhythmics (eg, digoxin), and skeletal muscle relaxants.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><div id=\"div_T58674\" class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" class=\"table-default-style table-header-footer-style\"><tbody><tr><td colspan=\"2\"><p align=\"center\"><strong>Common medications to avoid in older adults (Beers criteria)</strong></p></td></tr><tr><td><p align=\"center\"><strong>Anticholinergic</strong></p></td><td><ul><li>First-generation antihistamines</li><li>Gastrointestinal antispasmodics</li></ul></td></tr><tr><td><p align=\"center\"><strong>Cardiovascular</strong></p></td><td><ul><li>Alpha-1 blockers (as antihypertensives)</li><li>Centrally acting alpha-2 agonists</li><li>Many antiarrhythmics</li></ul></td></tr><tr><td><p align=\"center\"><strong>CNS</strong></p></td><td><ul><li>Tricyclic antidepressants</li><li>Antipsychotics</li><li>Barbiturates, benzodiazepines &amp; other hypnotics</li></ul></td></tr><tr><td><p align=\"center\"><strong>Endocrine</strong></p></td><td><ul><li>Long-acting sulfonylureas</li><li>Sliding-scale insulin</li></ul></td></tr><tr><td><p align=\"center\"><strong>Pain</strong></p></td><td><ul><li>Nonselective NSAIDs</li><li>Skeletal muscle relaxants</li></ul></td></tr><tr><td colspan=\"2\"><p><strong>NSAIDs</strong> = nonsteroidal anti-inflammatory drugs.</p></td></tr></tbody></table></div><p class=\"ng-scope\">Older patients are at increased risk for adverse effects of medications due to decreased drug clearance, cognitive and sensory deficits, multiple comorbidities, drug-drug interactions, and poor drug compliance.&nbsp; <strong>Polypharmacy</strong> (ie, \u22655 prescription and/or over-the-counter medications) is of particular concern in the elderly and can lead to a prescribing cascade, in which additional drugs are given for adverse effects that are misinterpreted as a new medical condition.</p><p class=\"ng-scope\">The <strong>Beers criteria</strong> were developed to identify drugs requiring caution in geriatric patients.&nbsp; <strong>First-generation antihistamines</strong> (eg, diphenhydramine) and other drugs with anticholinergic activity can increase the risk of confusion, hallucinations, dizziness, dry mouth, and constipation and should be avoided in this population.&nbsp; Other commonly used medications that should be avoided in the elderly include benzodiazepines (and other sedating medications), antipsychotics, many antiarrhythmics (eg, digoxin), and centrally acting skeletal muscle relaxants.</p><p class=\"ng-scope\"><strong>(Choices A and E)</strong>&nbsp; Although hypotension due to excessive antihypertension medication use could cause dizziness, this patient has normal blood pressure and heart rate, making this much less likely as a cause of her dizziness.&nbsp; In addition, although centrally acting alpha agonists (eg, clonidine) and alpha-1 blockers (eg, doxazosin) confer a&nbsp;high risk of orthostatic hypotension in elderly patients,&nbsp;thiazide diuretics (eg, hydrochlorothiazide),&nbsp;calcium channel blockers (eg, amlodipine), and ACE inhibitors/angiotensin II receptor blockers are generally considered acceptable for this population.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Aspirin should be used with caution in older adults due to the increased risk of gastric ulcer; however, the reduced risk of cardiovascular events provided by aspirin in patients with prior transient ischemic attack or stroke likely outweighs this risk.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Donepezil is a cholinesterase inhibitor that is commonly used in the treatment of Alzheimer dementia.&nbsp; Bradycardia can occasionally occur; however, this patient has a normal heart rate.</p><p class=\"ng-scope\"><strong>(Choice F)</strong>&nbsp; Selective serotonin reuptake inhibitors (eg, sertraline) are the preferred first-line medications for depression in older adults.&nbsp; Serotonin-norepinephrine reuptake inhibitors (eg, duloxetine) may also be used, especially for patients with chronic pain.&nbsp; Tricyclic antidepressants are relatively contraindicated in the elderly due to their anticholinergic effects.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>The Beers criteria identify drugs that should be used with caution in geriatric patients.&nbsp; Common drugs to avoid include anticholinergics (eg, first-generation antihistamines), centrally acting alpha-2 agonists, tricyclic antidepressants, benzodiazepines (and other sedating medications), antipsychotics, many antiarrhythmics (eg, digoxin), and skeletal muscle relaxants.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testIn