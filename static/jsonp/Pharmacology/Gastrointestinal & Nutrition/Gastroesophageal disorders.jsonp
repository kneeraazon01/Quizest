|| testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><strong>Achalasia</strong> is an esophageal motility disorder characterized by the absence of esophageal peristalsis in the distal esophagus and incomplete relaxation of a <strong>hypertonic lower esophageal sphincter</strong> (LES).&amp;nbsp; Achalasia is caused by the immune-mediated <strong>destruction of inhibitory ganglion cells</strong> in the esophageal wall.&amp;nbsp; Nitric oxide&amp;ndash;producing neurons in the myenteric plexus that are responsible for esophageal smooth muscle relaxation are preferentially destroyed, whereas excitatory cholinergic neurons responsible for smooth muscle contraction are relatively spared.&amp;nbsp; This imbalance results in increased lower esophageal tone and impaired sphincter relaxation.</p><p><a ng-click=&quot;showexhibit('14919',true);&quot;>Botulinum toxin</a>, a neurotoxin formed by <em>Clostridium botulinum</em>, prevents the fusion of acetylcholine-containing synaptic vesicles with the plasma membrane.&amp;nbsp; This effectively prevents acetylcholine release into the neuromuscular synapse, resulting in muscle weakness and paralysis.&amp;nbsp; In a patient with achalasia, injection of <strong>botulinum toxin</strong> into the LES causes <strong>cholinergic blockade</strong>, leading to sphincter relaxation and symptomatic improvement.</p><p><strong>(Choices B and D)</strong>&amp;nbsp; In addition to the cholinergic neurons, there are multiple other hormones and neurotransmitters that modulate esophageal tone.&amp;nbsp; Prostaglandins promote LES relaxation, whereas alpha-adrenergic agonists, such as phenylephrine, increase LES tone.&amp;nbsp; Because corticosteroids inhibit prostaglandin synthesis, both corticosteroids and phenylephrine would worsen the patient&amp;#39;s symptoms.</p><p><strong>(Choices C and E)</strong>&amp;nbsp; Neostigmine is a cholinesterase inhibitor and pilocarpine is a cholinergic agonist.&amp;nbsp; Both of these medications increase cholinergic stimuli at the LES and would worsen the patient&amp;#39;s symptoms.</p><p><strong>Educational objective:</strong><br>Achalasia is caused by loss of inhibitory ganglion cells in the distal esophageal wall and is characterized by the absence of esophageal peristalsis and incomplete relaxation of a hypertonic lower esophageal sphincter.&amp;nbsp; Botulinum toxin prevents acetylcholine release into the neuromuscular synapse, resulting in inhibition of cholinergic neurons and lower esophageal sphincter relaxation.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><strong>Achalasia</strong> is an esophageal motility disorder characterized by the absence of esophageal peristalsis in the distal esophagus and incomplete relaxation of a <strong>hypertonic lower esophageal sphincter</strong> (LES).&nbsp; Achalasia is caused by the immune-mediated <strong>destruction of inhibitory ganglion cells</strong> in the esophageal wall.&nbsp; Nitric oxide\u2013producing neurons in the myenteric plexus that are responsible for esophageal smooth muscle relaxation are preferentially destroyed, whereas excitatory cholinergic neurons responsible for smooth muscle contraction are relatively spared.&nbsp; This imbalance results in increased lower esophageal tone and impaired sphincter relaxation.</p><p class=\"ng-scope\"><a ng-click=\"showexhibit('14919',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L39097.jpg&quot;,&quot;title&quot;:&quot;L39097&quot;}]\">Botulinum toxin</a>, a neurotoxin formed by <em>Clostridium botulinum</em>, prevents the fusion of acetylcholine-containing synaptic vesicles with the plasma membrane.&nbsp; This effectively prevents acetylcholine release into the neuromuscular synapse, resulting in muscle weakness and paralysis.&nbsp; In a patient with achalasia, injection of <strong>botulinum toxin</strong> into the LES causes <strong>cholinergic blockade</strong>, leading to sphincter relaxation and symptomatic improvement.</p><p class=\"ng-scope\"><strong>(Choices B and D)</strong>&nbsp; In addition to the cholinergic neurons, there are multiple other hormones and neurotransmitters that modulate esophageal tone.&nbsp; Prostaglandins promote LES relaxation, whereas alpha-adrenergic agonists, such as phenylephrine, increase LES tone.&nbsp; Because corticosteroids inhibit prostaglandin synthesis, both corticosteroids and phenylephrine would worsen the patient's symptoms.</p><p class=\"ng-scope\"><strong>(Choices C and E)</strong>&nbsp; Neostigmine is a cholinesterase inhibitor and pilocarpine is a cholinergic agonist.&nbsp; Both of these medications increase cholinergic stimuli at the LES and would worsen the patient's symptoms.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Achalasia is caused by loss of inhibitory ganglion cells in the distal esophageal wall and is characterized by the absence of esophageal peristalsis and incomplete relaxation of a hypertonic lower esophageal sphincter.&nbsp; Botulinum toxin prevents acetylcholine release into the neuromuscular synapse, resulting in inhibition of cholinergic neurons and lower esophageal sphincter relaxation.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pharmacology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Gastrointestinal &amp; Nutrition</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Achalasia</div>\n 