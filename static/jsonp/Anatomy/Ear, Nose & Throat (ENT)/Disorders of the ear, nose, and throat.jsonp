an>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>E.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight5\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Superior pharyngeal constrictor</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot;  draggable=&quot;false&quot; id=&quot;46490&quot; src=&quot;https://www.uworld.com/media/L72520.jpg&quot; ></p><p>This patient has a <strong>cleft lip and palate</strong>, which can lead to problems with feeding, speech, and orthognathic (ie, jaw) development.&amp;nbsp; Children with cleft palate often have <strong>recurrent otitis media</strong> due to <strong>eustachian tube dysfunction</strong>.&amp;nbsp; The muscles primarily responsible for opening the eustachian tube include the <strong>tensor veli palatini</strong> and the levator veli palatini.</p><p>The <strong>levator veli palatini</strong> arises from the petrous portion of the temporal bone and medial part of the eustachian tube.&amp;nbsp; Its fibers insert into the superior portion of the palatine aponeurosis (soft palate) and interdigitate in the midline with the contralateral muscle fibers.&amp;nbsp; It <strong>elevates the soft palate</strong> during swallowing to prevent food from refluxing into the nasopharynx; contraction also <strong>opens the eustachian tube</strong>.&amp;nbsp; With cleft palate, levator veli palatini insertion is no longer stable, so contraction does not lead to force on the eustachian tube.</p><p><strong>(Choice A)</strong>&amp;nbsp; The <a ng-click=&quot;showexhibit('45147',true);&quot;>lateral pterygoid</a> muscles depress the mandible (ie, open the jaw).&amp;nbsp; They are not impacted in cleft lip or palate.</p><p><strong>(Choice C)</strong>&amp;nbsp; The <a ng-click=&quot;showexhibit('34070',true);&quot;>orbicularis oris</a> encircles the mouth, acting as a sphincter to close the mouth and pucker the lips.&amp;nbsp; In cleft lip, it does not form a complete circle, but inserts into the base of the nose on either side of the cleft; however, it does not impact the function of the eustachian tube.</p><p><strong>(Choice D)</strong>&amp;nbsp; The <a ng-click=&quot;showexhibit('2710',true);&quot;>stapedius muscle</a> is innervated by the facial nerve (CN VII).&amp;nbsp; It arises from the wall of the tympanic cavity and inserts on the neck of the stapes bone.&amp;nbsp; It functions to stabilize the stapes.&amp;nbsp; Contraction causes dampening of sound transmission in response to loud noises.&amp;nbsp; It does not open the eustachian tube.</p><p><strong>(Choice E)</strong>&amp;nbsp; The superior pharyngeal constrictor forms portions of the posterior and lateral oropharyngeal walls.&amp;nbsp; Contraction propels food and liquid inferiorly into the esophagus during swallowing.&amp;nbsp; Together with the levator veli palatini, it elevates the palate to seal the nasal cavity during swallowing.&amp;nbsp; Although both functions are impaired in cleft palate, the superior pharyngeal constrictor does not play a direct role in opening the eustachian tube.</p><p><strong>Educational objective:</strong><br>Patients with cleft palate are at increased risk of chronic or recurrent acute otitis media, partly because of dysfunction of the muscles (eg, levator veli palatini) that contract against the soft palate to open the eustachian tube.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"46490\" src=\"image/L72520.jpg\"></p><p class=\"ng-scope\">This patient has a <strong>cleft lip and palate</strong>, which can lead to problems with feeding, speech, and orthognathic (ie, jaw) development.&nbsp; Children with cleft palate often have <strong>recurrent otitis media</strong> due to <strong>eustachian tube dysfunction</strong>.&nbsp; The muscles primarily responsible for opening the eustachian tube include the <strong>tensor veli palatini</strong> and the levator veli palatini.</p><p class=\"ng-scope\">The <strong>levator veli palatini</strong> arises from the petrous portion of the temporal bone and medial part of the eustachian tube.&nbsp; Its fibers insert into the superior portion of the palatine aponeurosis (soft palate) and interdigitate in the midline with the contralateral muscle fibers.&nbsp; It <strong>elevates the soft palate</strong> during swallowing to prevent food from refluxing into the nasopharynx; contraction also <strong>opens the eustachian tube</strong>.&nbsp; With cleft palate, levator veli palatini insertion is no longer stable, so contraction does not lead to force on the eustachian tube.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; The <a ng-click=\"showexhibit('45147',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L59352.jpg&quot;,&quot;title&quot;:&quot;L59352&quot;}]\">lateral pterygoid</a> muscles depress the mandible (ie, open the jaw).&nbsp; They are not impacted in cleft lip or palate.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; The <a ng-click=\"showexhibit('34070',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L59476.jpg&quot;,&quot;title&quot;:&quot;L59476&quot;}]\">orbicularis oris</a> encircles the mouth, acting as a sphincter to close the mouth and pucker the lips.&nbsp; In cleft lip, it does not form a complete circle, but inserts into the base of the nose on either side of the cleft; however, it does not impact the function of the eustachian tube.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; The <a ng-click=\"showexhibit('2710',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L19103.jpg&quot;,&quot;title&quot;:&quot;L19103&quot;}]\">stapedius muscle</a> is innervated by the facial nerve (CN VII).&nbsp; It arises from the wall of the tympanic cavity and inserts on the neck of the stapes bone.&nbsp; It functions to stabilize the stapes.&nbsp; Contraction causes dampening of sound transmission in response to loud noises.&nbsp; It does not open the eustachian tube.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; The superior pharyngeal constrictor forms portions of the posterior and lateral oropharyngeal walls.&nbsp; Contraction propels food and liquid inferiorly into the esophagus during swallowing.&nbsp; Together with the levator veli palatini, it elevates the palate to seal the nasal cavity during swallowing.&nbsp; Although both functions are impaired in cleft palate, the superior pharyngeal constrictor does not play a direct role in opening the eustachian tube.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Patients with cleft palate are at increased risk of chronic or recurrent acute otitis media, partly because of dysfunction of the muscles (eg, levator veli palatini) that contract against the soft palate to open the eustachian tube.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Anatomy</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Ear, Nose &amp; Throat (ENT)</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Cleft lip and cleft palate</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
