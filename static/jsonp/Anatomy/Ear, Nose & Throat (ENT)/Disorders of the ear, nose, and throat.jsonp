&& (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; id=&quot;3002&quot;  src=&quot;https://www.uworld.com/media/L7149.jpg&quot; draggable=&quot;false&quot; ></p><p>The nasal mucosa is highly vascular and easily irritated by trauma (eg, nose-picking), mucosal dryness, foreign body insertion, and rhinitis (eg, allergy, infection).&amp;nbsp; <strong>Epistaxis</strong> is very common in children and may be classified as anterior or posterior, depending on the bleeding source.&amp;nbsp; Anterior nosebleeds are by far the most common, and the vast majority occur within the vascular watershed area of the <strong>nasal septum</strong> (anteroinferior part of the nasal septal mucosa) known as <strong>Kiesselbach plexus</strong>.&amp;nbsp; Anastomosis of the following vessels occurs in this region:</p><ul><li>Septal branch of the <strong>anterior ethmoidal artery</strong></li><li>Lateral nasal branch of the <strong>sphenopalatine artery</strong></li><li>Septal branch of the <strong>superior labial artery</strong> (branch of the facial artery)</li></ul><p>Management is directed at stopping the bleeding from Kiesselbach plexus, preferably by direct <a ng-click=&quot;showexhibit('3003',true);&quot;>compression of the nasal alae</a>.&amp;nbsp; Cautery (eg, silver nitrate) of the area surrounding the bleeding site may be necessary for persistent bleeding.</p><p><strong>(Choices A and B)</strong>&amp;nbsp; The <a ng-click=&quot;showexhibit('3004',true);&quot;>lateral nasal wall</a> contains the superior, middle, and inferior turbinates (also known as conchae).&amp;nbsp; These 3 bony projections are covered with mucous membrane; they warm, humidify, and filter inspired air and expand and contract in response to environmental changes (eg, temperature, humidity, allergens).&amp;nbsp; The turbinates form corresponding meatuses that serve as drainage pathways.&amp;nbsp; The superior meatus provides drainage for the sphenoidal and posterior ethmoidal sinuses.&amp;nbsp; The middle meatus drains the frontal, maxillary, and anterior ethmoidal sinuses and is the most common site of nasal polyps.&amp;nbsp; The inferior meatus drains the nasolacrimal duct.</p><p><strong>(Choices D and E)</strong>&amp;nbsp; Branches of the sphenopalatine artery supply the posterolateral wall and posterior choanae.&amp;nbsp; These posterolateral branches are the most likely source of posterior nosebleeds.&amp;nbsp; Posterior epistaxis is usually severe and cannot be treated with cautery.</p><p><strong>Educational objective:</strong><br>Epistaxis is commonly caused by irritation of the highly vascular mucosa at the anterior nasal septum.&amp;nbsp; The anterior nasal septum contains the Kiesselbach plexus.&amp;nbsp; The anterior ethmoidal, sphenopalatine, and superior labial arteries anastomose in this region.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" id=\"3002\" src=\"image/L7149.jpg\" draggable=\"false\"></p><p class=\"ng-scope\">The nasal mucosa is highly vascular and easily irritated by trauma (eg, nose-picking), mucosal dryness, foreign body insertion, and rhinitis (eg, allergy, infection).&nbsp; <strong>Epistaxis</strong> is very common in children and may be classified as anterior or posterior, depending on the bleeding source.&nbsp; Anterior nosebleeds are by far the most common, and the vast majority occur within the vascular watershed area of the <strong>nasal septum</strong> (anteroinferior part of the nasal septal mucosa) known as <strong>Kiesselbach plexus</strong>.&nbsp; Anastomosis of the following vessels occurs in this region:</p><ul class=\"ng-scope\"><li>Septal branch of the <strong>anterior ethmoidal artery</strong></li><li>Lateral nasal branch of the <strong>sphenopalatine artery</strong></li><li>Septal branch of the <strong>superior labial artery</strong> (branch of the facial artery)</li></ul><p class=\"ng-scope\">Management is directed at stopping the bleeding from Kiesselbach plexus, preferably by direct <a ng-click=\"showexhibit('3003',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L18962.jpg&quot;,&quot;title&quot;:&quot;L18962&quot;}]\">compression of the nasal alae</a>.&nbsp; Cautery (eg, silver nitrate) of the area surrounding the bleeding site may be necessary for persistent bleeding.</p><p class=\"ng-scope\"><strong>(Choices A and B)</strong>&nbsp; The <a ng-click=\"showexhibit('3004',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L25918.jpg&quot;,&quot;title&quot;:&quot;L25918&quot;}]\">lateral nasal wall</a> contains the superior, middle, and inferior turbinates (also known as conchae).&nbsp; These 3 bony projections are covered with mucous membrane; they warm, humidify, and filter inspired air and expand and contract in response to environmental changes (eg, temperature, humidity, allergens).&nbsp; The turbinates form corresponding meatuses that serve as drainage pathways.&nbsp; The superior meatus provides drainage for the sphenoidal and posterior ethmoidal sinuses.&nbsp; The middle meatus drains the frontal, maxillary, and anterior ethmoidal sinuses and is the most common site of nasal polyps.&nbsp; The inferior meatus drains the nasolacrimal duct.</p><p class=\"ng-scope\"><strong>(Choices D and E)</strong>&nbsp; Branches of the sphenopalatine artery supply the posterolateral wall and posterior choanae.&nbsp; These posterolateral branches are the most likely source of posterior nosebleeds.&nbsp; Posterior epistaxis is usually severe and cannot be treated with cautery.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Epistaxis is commonly caused by irritation of the highly vascular mucosa at the anterior nasal septum.&nbsp; The anterior nasal septum contains the Kiesselbach plexus.&nbsp; The anterior ethmoidal, sphenopalatine, and superior labial arteries anastomose in this region.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-h