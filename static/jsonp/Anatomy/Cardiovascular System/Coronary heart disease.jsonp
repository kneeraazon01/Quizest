onstants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;16283&quot; src=&quot;https://www.uworld.com/media/L41036.jpg&quot; ></p><p><strong>Low-degree atrioventricular (AV) block</strong>, including first-degree and Mobitz type I second-degree, usually involves conduction disruption <strong>within the AV node</strong>.&amp;nbsp; This patient&amp;#39;s development of <a ng-click=&quot;showexhibit('29279',true);&quot;>Mobitz type I</a> second-degree AV block in the setting of acute <strong>myocardial infarction</strong> (MI) suggests obstruction of the <strong>right coronary artery</strong> (RCA).&amp;nbsp; In approximately 90% of patients, the <strong>RCA supplies blood to the AV node</strong> while coursing around the right side of the heart to perfuse the right ventricle and inferior portion of the left ventricle.&amp;nbsp; Because the RCA also usually supplies blood to the sinoatrial node, sinus bradycardia is also commonly seen with MI involving the RCA.</p><p>In contrast, the left anterior descending (LAD) artery <strong>(Choice A)</strong>, which courses down the anterior left ventricle, typically supplies blood to the left and right bundle branches and sometimes the bundle of His.&amp;nbsp; MI involving the LAD can lead to high-degree AV block (ie, Mobitz type II second-degree, infranodal third-degree) involving conduction disruption below the AV node.&amp;nbsp; Because the LAD does not supply blood to the AV node, low-degree AV block is not typically seen.</p><p><strong>(Choices B and C)</strong>&amp;nbsp; The left main coronary artery bifurcates into the LAD artery and the left circumflex artery, which courses around the left side of the heart to supply the lateral left ventricle.&amp;nbsp; The left main coronary artery does not directly supply blood to the conduction system.&amp;nbsp; The left circumflex artery frequently supplies blood to the sinoatrial node after coursing around to the back of the heart, but only supplies the AV node in about 10% of patients (ie, those with left-dominant coronary circulation).</p><p><strong>(Choice D)</strong>&amp;nbsp; Present in approximately 20% of the population, a ramus intermedius is a third branch off of the left main coronary artery.&amp;nbsp; It originates at the bifurcation of the LAD and left circumflex arteries and courses between them to supply the anterolateral left ventricular wall.&amp;nbsp; It does not supply blood to the conduction system.</p><p><strong>Educational objective:</strong><br>Low-grade atrioventricular block (ie, first-degree, Mobitz type I second-degree) involves conduction disruption within the AV node.&amp;nbsp; The occurrence of low-grade AV block in the setting of myocardial infarction suggests obstruction of the right coronary artery because it supplies blood to the AV node in approximately 90% of patients.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"16283\" src=\"image/L41036.jpg\"></p><p class=\"ng-scope\"><strong>Low-degree atrioventricular (AV) block</strong>, including first-degree and Mobitz type I second-degree, usually involves conduction disruption <strong>within the AV node</strong>.&nbsp; This patient's development of <a ng-click=\"showexhibit('29279',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L55308.png&quot;,&quot;title&quot;:&quot;L55308&quot;}]\">Mobitz type I</a> second-degree AV block in the setting of acute <strong>myocardial infarction</strong> (MI) suggests obstruction of the <strong>right coronary artery</strong> (RCA).&nbsp; In approximately 90% of patients, the <strong>RCA supplies blood to the AV node</strong> while coursing around the right side of the heart to perfuse the right ventricle and inferior portion of the left ventricle.&nbsp; Because the RCA also usually supplies blood to the sinoatrial node, sinus bradycardia is also commonly seen with MI involving the RCA.</p><p class=\"ng-scope\">In contrast, the left anterior descending (LAD) artery <strong>(Choice A)</strong>, which courses down the anterior left ventricle, typically supplies blood to the left and right bundle branches and sometimes the bundle of His.&nbsp; MI involving the LAD can lead to high-degree AV block (ie, Mobitz type II second-degree, infranodal third-degree) involving conduction disruption below the AV node.&nbsp; Because the LAD does not supply blood to the AV node, low-degree AV block is not typically seen.</p><p class=\"ng-scope\"><strong>(Choices B and C)</strong>&nbsp; The left main coronary artery bifurcates into the LAD artery and the left circumflex artery, which courses around the left side of the heart to supply the lateral left ventricle.&nbsp; The left main coronary artery does not directly supply blood to the conduction system.&nbsp; The left circumflex artery frequently supplies blood to the sinoatrial node after coursing around to the back of the heart, but only supplies the AV node in about 10% of patients (ie, those with left-dominant coronary circulation).</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Present in approximately 20% of the population, a ramus intermedius is a third branch off of the left main coronary artery.&nbsp; It originates at the bifurcation of the LAD and left circumflex arteries and courses between them to supply the anterolateral left ventricular wall.&nbsp; It does not supply blood to the conduction system.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Low-grade atrioventricular block (ie, first-degree, Mobitz type I second-degree) involves conduction disruption within the AV node.&nbsp; The occurrence of low-grade AV block in the setting of myocardial infarction suggests obstruction of the right coronary artery because it supplies blood to the AV node in approximately 90% of patients.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Anatomy</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Cardiovascular System</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Myocardial infarction</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
