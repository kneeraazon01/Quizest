                                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal -->\n                                </li><!-- end ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/25178467\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/25178467\"><span compiledata=\"\" htmlcontent=\"Spinal cord injury following thoracic and thoracoabdominal repairs.\" class=\"ng-scope\"><p class=\"ng-scope\">Spinal cord injury following thoracic and thoracoabdominal repairs.</p></span></a><!-- end ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal -->\n                                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal -->\n                                </li><!-- end ngRepeat: questionReference in currentQuestion.questionMappingReferencesList -->\n                            </ul>\n                        </div>\n                    </div><!-- end ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n             