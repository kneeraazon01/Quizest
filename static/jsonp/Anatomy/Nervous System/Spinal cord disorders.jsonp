}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight5\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\"><img class=\"anschs_non_ekg_img\" draggable=\"false\" id=\"4470\" src=\"image/U29074.jpg\"></span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot;  draggable=&quot;false&quot; id=&quot;4471&quot; src=&quot;https://www.uworld.com/media/highresdefault/L29069.jpg&quot;  ></p><p>This patient most likely has <strong>subacute combined degeneration</strong> of the spinal cord, a condition that results from chronically low levels of vitamin B<font size=&quot;2&quot;><sub>12</sub></font> (cobalamin).&amp;nbsp; Low cobalamin levels may occur due to pernicious anemia (autoimmune gastritis with deficient intrinsic factor), <strong>gastrectomy</strong>, ileal resections, infection by the tapeworm <em>Diphyllobothrium latum</em>, and strict vegan diets containing no animal products for a prolonged period (3-4 years).</p><p>Vitamin B<font size=&quot;2&quot;><sub>12</sub></font> is an important cofactor in DNA synthesis, and hematologic manifestations of deficiency include megaloblastic <strong>anemia</strong> and pancytopenia.&amp;nbsp; This patient&amp;#39;s pale palmar crease and easy fatigability are consistent with anemia.</p><p>Elevated levels of <a ng-click=&quot;showexhibit('3617',true);&quot;>methylmalonic acid</a>&amp;nbsp;also occur in vitamin B<font size=&quot;2&quot;><sub>12</sub></font> deficiency and can destablize myelin and damage white matter in particular regions of the spinal cord:</p><ul><li><strong>Dorsal columns</strong> (loss of position and vibration sensation, sensory ataxia [Romberg sign])</li><li><strong>Lateral corticospinal tracts</strong> (progressive weakness that can result in spastic paresis)</li><li>Spinocerebellar tracts (contributes to ataxia)</li></ul><p>Patients may also develop atrophic glossitis, which appears as a smooth, shiny, erythematous tongue.</p><p><strong>(Choice A)</strong>&amp;nbsp; Poliomyelitis and spinal muscular atrophy are associated with the destruction of lower motor neurons in the anterior horn, which causes flaccid paralysis, muscle atrophy, and fasciculations.</p><p><strong>(Choice B)</strong>&amp;nbsp; Multiple sclerosis is an inflammatory demyelinating disorder affecting the white matter of the CNS in a random and asymmetric pattern.&amp;nbsp; Patients typically have neurologic deficits disseminated in space and time.</p><p><strong>(Choice C)</strong>&amp;nbsp; Tabes dorsales (tertiary neurosyphilis) is characterized by degeneration of the dorsal root ganglia and dorsal columns.&amp;nbsp; Although patients may have impaired vibratory sensation and proprioception, pancytopenia and atrophic glossitis are not characteristic.&amp;nbsp; In addition, tabes dorsales does not typically affect the lateral corticospinal tracts.</p><p><strong>(Choice E)</strong>&amp;nbsp; Syringomyelia is a central cavitation of the cervical spinal cord that is associated with damage to the anterior white commissure of the spinothalamic tract.&amp;nbsp; Patients typically have bilateral loss of pain/temperature in the upper extremities.&amp;nbsp; Expansion into the ventral horns may result in flaccid paralysis of the hand muscles.</p><p><strong>Educational objective:</strong><br>Neurologic damage associated with vitamin B<font size=&quot;2&quot;><sub>12</sub></font> deficiency classically includes subacute combined degeneration of the dorsal columns (loss of position and vibration sensation, positive Romberg sign) and lateral corticospinal tracts (muscle weakness, spastic paresis).</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"4471\" src=\"image/L29069.jpg\"></p><p class=\"ng-scope\">This patient most likely has <strong>subacute combined degeneration</strong> of the spinal cord, a condition that results from chronically low levels of vitamin B<font size=\"2\"><sub>12</sub></font> (cobalamin).&nbsp; Low cobalamin levels may occur due to pernicious anemia (autoimmune gastritis with deficient intrinsic factor), <strong>gastrectomy</strong>, ileal resections, infection by the tapeworm <em>Diphyllobothrium latum</em>, and strict vegan diets containing no animal products for a prolonged period (3-4 years).</p><p class=\"ng-scope\">Vitamin B<font size=\"2\"><sub>12</sub></font> is an important cofactor in DNA synthesis, and hematologic manifestations of deficiency include megaloblastic <strong>anemia</strong> and pancytopenia.&nbsp; This patient's pale palmar crease and easy fatigability are consistent with anemia.</p><p class=\"ng-scope\">Elevated levels of <a ng-click=\"showexhibit('3617',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L24626.jpg&quot;,&quot;title&quot;:&quot;L24626&quot;}]\">methylmalonic acid</a>&nbsp;also occur in vitamin B<font size=\"2\"><sub>12</sub></font> deficiency and can destablize myelin and damage white matter in particular regions of the spinal cord:</p><ul class=\"ng-scope\"><li><strong>Dorsal columns</strong> (loss of position and vibration sensation, sensory ataxia [Romberg sign])</li><li><strong>Lateral corticospinal tracts</strong> (progressive weakness that can result in spastic paresis)</li><li>Spinocerebellar tracts (contributes to ataxia)</li></ul><p class=\"ng-scope\">Patients may also develop atrophic glossitis, which appears as a smooth, shiny, erythematous tongue.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Poliomyelitis and spinal muscular atrophy are associated with the destruction of lower motor neurons in the anterior horn, which causes flaccid paralysis, muscle atrophy, and fasciculations.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Multiple sclerosis is an inflammatory demyelinating disorder affecting the white matter of the CNS in a random and asymmetric pattern.&nbsp; Patients typically have neurologic deficits disseminated in space and time.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Tabes dorsales (tertiary neurosyphilis) is characterized by degeneration of the dorsal root ganglia and dorsal columns.&nbsp; Although patients may have impaired vibratory sensation and proprioception, pancytopenia and atrophic glossitis are not characteristic.&nbsp; In addition, tabes dorsales does not typically affect the lateral corticospinal tracts.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Syringomyelia is a central cavitation of the cervical spinal cord that is associated with damage to the anterior white commissure of the spinothalamic tract.&nbsp; Patients typically have bilateral loss of pain/temperature in the upper extremities.&nbsp; Expansion into the ventral horns may result in flaccid paralysis of the hand muscles.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Neurologic damage associated with vitamin B<font size=\"2\"><sub>12</sub></font> deficiency classically includes subacute combined degeneration of the dorsal columns (loss of position and vibration sensation, positive Romberg sign) and lateral corticospinal tracts (muscle weakness, spastic paresis).</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Anatomy</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Nervous System</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Vitamin b12 deficiency</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
