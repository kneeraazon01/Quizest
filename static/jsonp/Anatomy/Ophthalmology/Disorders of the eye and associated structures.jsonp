\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot;  draggable=&quot;false&quot; id=&quot;32895&quot; src=&quot;https://www.uworld.com/media/L54842.jpg&quot;  ></p><p>This patient has <strong>anisocoria</strong> (ie, pupillary asymmetry), which indicates a <strong>unilateral defect</strong> in input from either the ocular sympathetic (pupil dilation) or parasympathetic (pupil constriction) pathway.&amp;nbsp; In response to <strong>dim light</strong>, input from the <strong>oculosympathetic pathway</strong> initiates pupillary <strong>dilation</strong>, allowing more light to reach the retina.&amp;nbsp; Under bright light, parasympathetic input from the ipsilateral oculomotor nerve (CN III) initiates pupillary constriction, limiting the amount of light received by the retina.</p><p>Determining which pathway is affected in anisocoria can be accomplished by observing the change in pupillary discrepancy in both bright and dim light:</p><ul><li><p>Asymmetry that increases in dim light indicates that the smaller pupil is unable to dilate due to loss of sympathetic nerve input.&amp;nbsp; Under bright light, the asymmetry will decrease because parasympathetic input (pupillary constriction) is unaffected.</p></li><li><p>Asymmetry that increases under bright light indicates that the larger pupil is unable to constrict due to loss of parasympathetic nerve input.&amp;nbsp; In dim light, the asymmetry will decrease because sympathetic input (pupillary dilation) is unaffected.</p></li></ul><p>This patient has <strong>increased asymmetry</strong> in a <strong>dim room</strong>, indicating that the smaller (right) pupil is unable to dilate due to loss of sympathetic nerve input secondary to a <strong>lesion</strong> in the <strong>right oculosympathetic pathway</strong>.</p><p><strong>(Choices A and D)</strong>&amp;nbsp; An oculomotor nerve lesion would disrupt parasympathetic innervation of the ipsilateral pupil, resulting in anisocoria that increases under bright light because the abnormal larger pupil is unable to constrict in response to light.</p><p><strong>(Choice B)</strong>&amp;nbsp; Damage to the left oculosympathetic pathway would cause loss of sympathetic input to the left eye, resulting in a fixed, constricted pupil in the left eye (with increased asymmetry in dim light) rather than in the right.</p><p><strong>(Choices C and F)</strong>&amp;nbsp; The optic nerves carry the <a ng-click=&quot;showexhibit('347',true);&quot;>afferent portions</a> of the pupillary reflex from each eye.&amp;nbsp; A unilateral optic nerve lesion would not cause pupillary asymmetry, but instead monocular vision loss and decreased bilateral pupillary constriction in response to light entering the affected eye (ie, <a ng-click=&quot;showexhibit('26509',true);&quot;>relative afferent pupillary defect</a>).</p><p><strong>Educational objective:</strong><br>Pupillary asymmetry (ie, anisocoria) is caused by a lesion in the ocular parasympathetic (pupillary constriction) or sympathetic (pupillary dilation) pathways.&amp;nbsp; In this patient, the pupillary asymmetry increases in a dim room, indicating that the smaller right pupil is unable to dilate due to a lesion in the right oculosympathetic pathway.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"32895\" src=\"image/L54842.jpg\"></p><p class=\"ng-scope\">This patient has <strong>anisocoria</strong> (ie, pupillary asymmetry), which indicates a <strong>unilateral defect</strong> in input from either the ocular sympathetic (pupil dilation) or parasympathetic (pupil constriction) pathway.&nbsp; In response to <strong>dim light</strong>, input from the <strong>oculosympathetic pathway</strong> initiates pupillary <strong>dilation</strong>, allowing more light to reach the retina.&nbsp; Under bright light, parasympathetic input from the ipsilateral oculomotor nerve (CN III) initiates pupillary constriction, limiting the amount of light received by the retina.</p><p class=\"ng-scope\">Determining which pathway is affected in anisocoria can be accomplished by observing the change in pupillary discrepancy in both bright and dim light:</p><ul class=\"ng-scope\"><li><p>Asymmetry that increases in dim light indicates that the smaller pupil is unable to dilate due to loss of sympathetic nerve input.&nbsp; Under bright light, the asymmetry will decrease because parasympathetic input (pupillary constriction) is unaffected.</p></li><li><p>Asymmetry that increases under bright light indicates that the larger pupil is unable to constrict due to loss of parasympathetic nerve input.&nbsp; In dim light, the asymmetry will decrease because sympathetic input (pupillary dilation) is unaffected.</p></li></ul><p class=\"ng-scope\">This patient has <strong>increased asymmetry</strong> in a <strong>dim room</strong>, indicating that the smaller (right) pupil is unable to dilate due to loss of sympathetic nerve input secondary to a <strong>lesion</strong> in the <strong>right oculosympathetic pathway</strong>.</p><p class=\"ng-scope\"><strong>(Choices A and D)</strong>&nbsp; An oculomotor nerve lesion would disrupt parasympathetic innervation of the ipsilateral pupil, resulting in anisocoria that increases under bright light because the abnormal larger pupil is unable to constrict in response to light.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Damage to the left oculosympathetic pathway would cause loss of sympathetic input to the left eye, resulting in a fixed, constricted pupil in the left eye (with increased asymmetry in dim light) rather than in the right.</p><p class=\"ng-scope\"><strong>(Choices C and F)</strong>&nbsp; The optic nerves carry the <a ng-click=\"showexhibit('347',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L10921.jpg&quot;,&quot;title&quot;:&quot;L10921&quot;}]\">afferent portions</a> of the pupillary reflex from each eye.&nbsp; A unilateral optic nerve lesion would not cause pupillary asymmetry, but instead monocular vision loss and decreased bilateral pupillary constriction in response to light entering the affected eye (ie, <a ng-click=\"showexhibit('26509',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/51549.mp4&quot;,&quot;title&quot;:&quot;51549&quot;}]\">relative afferent pupillary defect</a>).</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Pupillary asymmetry (ie, anisocoria) is caused by a lesion in the ocular parasympathetic (pupillary constriction) or sympathetic (pupillary dilation) pathways.&nbsp; In this patient, the pupillary asymmetry increases in a dim room, indicating that the smaller right pupil is unable to dilate due to a lesion in the right oculosympathetic pathway.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Anatomy</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Ophthalmology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Horner syndrome</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
