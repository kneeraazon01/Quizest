\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; draggable=&quot;false&quot; id=&quot;43087&quot; src=&quot;https://www.uworld.com/media/L63299.png&quot; ></p><p>This patient with heavy menses and dysmenorrhea has <strong>adenomyosis</strong>, the abnormal presence of <a ng-click=&quot;showexhibit('43088',true);&quot;>endometrial glands and stroma</a> within the uterine myometrium (ie, uterine wall).&amp;nbsp; Adenomyosis typically occurs in women age &amp;gt;40, and risk factors include multiparity and prior uterine surgery (eg, cesarean delivery).</p><p>The pathology of adenomyosis correlates with its clinical symptoms.&amp;nbsp; Adenomyosis has a gross appearance as areas of <strong>dark red endometrial tissue within the myometrium</strong>.&amp;nbsp; With each menstrual cycle, the endometrial tissue proliferates and sheds but the bleeding is confined within the uterine wall, causing <strong>dysmenorrhea</strong>.&amp;nbsp; In addition, the ectopic endometrial glands induce <strong>myometrial hyperplasia and hypertrophy</strong>, creating a <strong>globular, uniformly enlarged uterus</strong>.&amp;nbsp; This uterine enlargement also increases the surface area of the endometrial cavity, resulting in <strong>regular, heavy menses</strong> (eg, every 28-30 days).&amp;nbsp; Definitive treatment is with hysterectomy.</p><p><strong>(Choice B)</strong>&amp;nbsp; Chronic endometritis is an intrauterine polymicrobial infection that typically causes constant pelvic pain with irregular bleeding rather than dysmenorrhea and regular, heavy menses.&amp;nbsp; Because the disease process is intracavitary, there is no associated uterine enlargement.&amp;nbsp; Gross pathology may reveal intrauterine adhesions due to chronic intrauterine inflammation.</p><p><strong>(Choice C)</strong>&amp;nbsp; A <a ng-click=&quot;showexhibit('43089',true);&quot;>complete hydatidiform mole</a> typically causes amenorrhea or irregular, first-trimester bleeding rather than regular, heavy menses.&amp;nbsp; In addition, patients typically have markedly elevated &amp;#946;-hCG levels (ie, &amp;gt;100,000 mIU/mL).&amp;nbsp; Gross pathology may reveal uterine enlargement, but there are typically numerous edematous intrauterine chorionic villi.</p><p><strong>(Choice D)</strong>&amp;nbsp; Endometriosis is a common cause of dysmenorrhea due to ectopic implantation of endometrial glands and stroma in the abdominopelvic cavity; because the ectopic implants are extrauterine, patients do not have uterine enlargement or heavy menses.&amp;nbsp; Gross pathology typically reveals black powder&amp;shy;&amp;ndash;burn lesions or ovarian cysts (ie, <a ng-click=&quot;showexhibit('42483',true);&quot;>endometrioma</a>).</p><p><strong>(Choice E)</strong>&amp;nbsp; <a ng-click=&quot;showexhibit('43090',true);&quot;>Uterine leiomyomas</a> are benign, uterine smooth muscle cell tumors that can cause regular, heavy menses.&amp;nbsp; However, gross pathology typically reveals an irregularly enlarged uterus with discrete tumors rather than a uniformly globular uterus.</p><p><strong>(Choice F)</strong>&amp;nbsp; Uterine sarcomas are malignant uterine tumors.&amp;nbsp; Affected patients typically have irregular, postmenopausal bleeding rather than regular, heavy menses.&amp;nbsp; Gross pathology typically reveals a discrete uterine mass.</p><p><strong>Educational objective:</strong><br>Adenomyosis typically presents with regular (ie, cyclic), heavy, painful menses.&amp;nbsp; Gross pathology reveals a uniformly enlarged, globular uterus with the abnormal presence of endometrial glands and stroma in the myometrium.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" draggable=\"false\" id=\"43087\" src=\"image/L63299.png\"></p><p class=\"ng-scope\">This patient with heavy menses and dysmenorrhea has <strong>adenomyosis</strong>, the abnormal presence of <a ng-click=\"showexhibit('43088',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L1842.jpg&quot;,&quot;title&quot;:&quot;L1842&quot;}]\">endometrial glands and stroma</a> within the uterine myometrium (ie, uterine wall).&nbsp; Adenomyosis typically occurs in women age &gt;40, and risk factors include multiparity and prior uterine surgery (eg, cesarean delivery).</p><p class=\"ng-scope\">The pathology of adenomyosis correlates with its clinical symptoms.&nbsp; Adenomyosis has a gross appearance as areas of <strong>dark red endometrial tissue within the myometrium</strong>.&nbsp; With each menstrual cycle, the endometrial tissue proliferates and sheds but the bleeding is confined within the uterine wall, causing <strong>dysmenorrhea</strong>.&nbsp; In addition, the ectopic endometrial glands induce <strong>myometrial hyperplasia and hypertrophy</strong>, creating a <strong>globular, uniformly enlarged uterus</strong>.&nbsp; This uterine enlargement also increases the surface area of the endometrial cavity, resulting in <strong>regular, heavy menses</strong> (eg, every 28-30 days).&nbsp; Definitive treatment is with hysterectomy.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Chronic endometritis is an intrauterine polymicrobial infection that typically causes constant pelvic pain with irregular bleeding rather than dysmenorrhea and regular, heavy menses.&nbsp; Because the disease process is intracavitary, there is no associated uterine enlargement.&nbsp; Gross pathology may reveal intrauterine adhesions due to chronic intrauterine inflammation.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; A <a ng-click=\"showexhibit('43089',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L6980.png&quot;,&quot;title&quot;:&quot;L6980&quot;}]\">complete hydatidiform mole</a> typically causes amenorrhea or irregular, first-trimester bleeding rather than regular, heavy menses.&nbsp; In addition, patients typically have markedly elevated \u03b2-hCG levels (ie, &gt;100,000 mIU/mL).&nbsp; Gross pathology may reveal uterine enlargement, but there are typically numerous edematous intrauterine chorionic villi.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Endometriosis is a common cause of dysmenorrhea due to ectopic implantation of endometrial glands and stroma in the abdominopelvic cavity; because the ectopic implants are extrauterine, patients do not have uterine enlargement or heavy menses.&nbsp; Gross pathology typically reveals black powder\u00ad\u2013burn lesions or ovarian cysts (ie, <a ng-click=\"showexhibit('42483',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L66833.jpg&quot;,&quot;title&quot;:&quot;L66833&quot;}]\">endometrioma</a>).</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; <a ng-click=\"showexhibit('43090',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L63379.jpg&quot;,&quot;title&quot;:&quot;L63379&quot;}]\">Uterine leiomyomas</a> are benign, uterine smooth muscle cell tumors that can cause regular, heavy menses.&nbsp; However, gross pathology typically reveals an irregularly enlarged uterus with discrete tumors rather than a uniformly globular uterus.</p><p class=\"ng-scope\"><strong>(Choice F)</strong>&nbsp; Uterine sarcomas are malignant uterine tumors.&nbsp; Affected patients typically have irregular, postmenopausal bleeding rather than regular, heavy menses.&nbsp; Gross pathology typically reveals a discrete uterine mass.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Adenomyosis typically presents with regular (ie, cyclic), heavy, painful menses.&nbsp; Gross pathology reveals a uniformly enlarged, globular uterus with the abnormal presence of endometrial glands and stroma in the myometrium.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                                <!-- ngRepeat: questionReference in currentQuestion.questionMappingReferencesList --><li ng-repeat=\"questionReference in currentQuestion.questionMappingReferencesList\" class=\"ng-scope\">\n                                    <!-- ngIf: launchTestOptions.topLevelProductId != constants.topLevelProduct.legal --><a ng-if=\"launchTestOptions.topLevelProductId != constants.topLevelProduct.legal\" ng-href=\"http://www.ncbi.nlm.nih.gov/pubmed/26315369\" target=\"_blank\" class=\"ng-scope\" href=\"http://www.ncbi.nlm.nih.gov/pubmed/26315369\"><span compiledata=\"\" htmlcontent=\"Adenomyosis: what is new