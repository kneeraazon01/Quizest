                <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>F.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight6\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">NADPH oxidase</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><img class=&quot;imgWidthHelper&quot; ng-click=&quot;decideClickAction( $event )&quot; id=&quot;6877&quot;  src=&quot;https://www.uworld.com/media/L24539.jpg&quot; draggable=&quot;false&quot; ></p><p>This patient&amp;#39;s history of recurrent respiratory infections and <a ng-click=&quot;showexhibit('473',true);&quot;>dextrocardia</a> on physical examination are suggestive of <strong>Kartagener syndrome</strong>, a form of <strong>primary ciliary dyskinesia </strong>(PCD).&amp;nbsp; This autosomal recessive condition is caused by a wide variety of mutations that impair ciliary structure or function.</p><p>Eukaryotic flagella and cilia are composed of a central core known as the axoneme that is surrounded by cell membrane.&amp;nbsp; The axoneme of flagella and motile cilia consists of a circular array of microtubule doublets surrounding 2 central microtubules (9+2 arrangement); the axoneme is anchored to the cell by a basal body.&amp;nbsp; Each doublet has an A and B subunit and is connected to the adjacent doublet via dynein arms.&amp;nbsp; These dynein arms contain an ATPase that generates energy to slide the microtubules past each other, producing ciliary movement.&amp;nbsp; Primary ciliary dyskinesia can result from failure of the <strong>dynein arms</strong> to develop normally.</p><p>Patients with PCD experience <strong>recurrent respiratory infections</strong> (eg, chronic sinusitis, bronchiectasis) as a result of impaired mucociliary clearance.&amp;nbsp; In addition, impaired ciliary movement during embryogenesis can cause <strong>situs inversus</strong> (reversed right/left positioning of internal organs).&amp;nbsp; <strong>Infertility</strong> in men (impaired sperm motility) and women (immobility of fallopian tube cilia) is another frequent manifestation.</p><p><strong>(Choice A)</strong>&amp;nbsp; Deficiency of the C1 complement component causes increased susceptibility to encapsulated bacteria (eg, <em>Streptococcus pneumoniae, Haemophilus influenzae, Neisseria meningitidis</em>) and also predisposes to developing systemic lupus erythematosus.</p><p><strong>(Choice B)</strong>&amp;nbsp; Epithelial cell chloride channels are defective in cystic fibrosis.&amp;nbsp; Although patients with cystic fibrosis typically have recurrent respiratory infections and infertility, dextrocardia/situs inversus are not seen.</p><p><strong>(Choice D)</strong>&amp;nbsp; Defects in T-cell IL-2 receptors can cause severe combined immunodeficiency disease (SCID).</p><p><strong>(Choice E)</strong>&amp;nbsp; Immunoglobulin gene rearrangement is impaired in hyper-IgM syndrome.&amp;nbsp; In this disorder, B cells produce high amounts of IgM but are unable to switch production to other immunoglobulin classes.</p><p><strong>(Choice F)</strong>&amp;nbsp; NADPH oxidase deficiency causes chronic granulomatous disease.</p><p><strong>Educational objective:</strong><br>Kartagener syndrome is a form of primary ciliary dyskinesia characterized by the triad of situs inversus, chronic sinusitis, and bronchiectasis.&amp;nbsp; It occurs due to mutations that impair the structure or function of cilia.&amp;nbsp; Cystic fibrosis also causes chronic respiratory infections, but it is not associated with situs inversus.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><img class=\"imgWidthHelper\" ng-click=\"decideClickAction( $event )\" id=\"6877\" src=\"image/L24539.jpg\" draggable=\"false\"></p><p class=\"ng-scope\">This patient's history of recurrent respiratory infections and <a ng-click=\"showexhibit('473',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/U86.jpg&quot;,&quot;title&quot;:&quot;U86&quot;}]\">dextrocardia</a> on physical examination are suggestive of <strong>Kartagener syndrome</strong>, a form of <strong>primary ciliary dyskinesia </strong>(PCD).&nbsp; This autosomal recessive condition is caused by a wide variety of mutations that impair ciliary structure or function.</p><p class=\"ng-scope\">Eukaryotic flagella and cilia are composed of a central core known as the axoneme that is surrounded by cell membrane.&nbsp; The axoneme of flagella and motile cilia consists of a circular array of microtubule doublets surrounding 2 central microtubules (9+2 arrangement); the axoneme is anchored to the cell by a basal body.&nbsp; Each doublet has an A and B subunit and is connected to the adjacent doublet via dynein arms.&nbsp; These dynein arms contain an ATPase that generates energy to slide the microtubules past each other, producing ciliary movement.&nbsp; Primary ciliary dyskinesia can result from failure of the <strong>dynein arms</strong> to develop normally.</p><p class=\"ng-scope\">Patients with PCD experience <strong>recurrent respiratory infections</strong> (eg, chronic sinusitis, bronchiectasis) as a result of impaired mucociliary clearance.&nbsp; In addition, impaired ciliary movement during embryogenesis can cause <strong>situs inversus</strong> (reversed right/left positioning of internal organs).&nbsp; <strong>Infertility</strong> in men (impaired sperm motility) and women (immobility of fallopian tube cilia) is another frequent manifestation.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Deficiency of the C1 complement component causes increased susceptibility to encapsulated bacteria (eg, <em>Streptococcus pneumoniae, Haemophilus influenzae, Neisseria meningitidis</em>) and also predisposes to developing systemic lupus erythematosus.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Epithelial cell chloride channels are defective in cystic fibrosis.&nbsp; Although patients with cystic fibrosis typically have recurrent respiratory infections and infertility, dextrocardia/situs inversus are not seen.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Defects in T-cell IL-2 receptors can cause severe combined immunodeficiency disease (SCID).</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Immunoglobulin gene rearrangement is impaired in hyper-IgM syndrome.&nbsp; In this disorder, B cells produce high amounts of IgM but are unable to switch production to other immunoglobulin classes.</p><p class=\"ng-scope\"><strong>(Choice F)</strong>&nbsp; NADPH oxidase deficiency causes chronic granulomatous disease.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Kartagener syndrome is a form of primary ciliary dyskinesia characterized by the triad of situs inversus, chronic sinusitis, and bronchiectasis.&nbsp; It occurs due to mutations that impair the structure or function of cilia.&nbsp; Cystic fibrosis also causes chronic respiratory infections, but it is not associated with situs inversus.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Immunology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Pathology (General Principles)</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Immotile cilia syndrome</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' 