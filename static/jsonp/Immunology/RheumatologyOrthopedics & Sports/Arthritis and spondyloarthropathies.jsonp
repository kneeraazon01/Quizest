    },
    {
        "question": "<div class=\"questioncontent ui-resizable widthWithoutAbstract\" id=\"questionContent\" ng-show=\"showQuestion\" ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat, 'suggested-highlights-visible' : userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; currentQuestion.frequentHighlights != null &amp;&amp; (suggestedHighlightsPreference == 1 || suggestedHighlightsPreference == 2), 'only-suggested-highlights-visible' : userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; suggestedHighlightsPreference == 1 &amp;&amp; currentQuestion.frequentHighlights != null}\">\n            <div style=\"display:none;\" class=\"suggested-highlights-switch-buttons ng-hide\" ng-show=\"userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; currentQuestion.frequentHighlights != null\">\n                <span class=\"fa-stack\" ng-click=\"showUserHighlights()\" title=\"My Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fas\" ng-class=\"{'fas' : suggestedHighlightsPreference == 0, 'fal' : suggestedHighlightsPreference != 0}\"></i>\n                    <i class=\"fal fa-highlighter fa-stack-1x fa-inverse\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 0}\"></i>\n                </span>\n                <span class=\"fa-stack ng-hide\" ng-hide=\"currentQuestion.frequentHighlights == null\" ng-click=\"showSuggestedHighlights()\" title=\"Suggested Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fal\" ng-class=\"{'fas' : suggestedHighlightsPreference == 1, 'fal' : suggestedHighlightsPreference != 1}\"></i>\n                    <i class=\"fas fa-highlighter fa-stack-1x\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 1}\"></i>\n                </span>\n                <span class=\"fa-stack highlight-preference-unavailable highlight-button-with-popover\" ng-show=\"currentQuestion.frequentHighlights == null\" title=\"\" data-toggle=\"popover\" data-placement=\"bottom\" data-content=\"Collecting Data\" data-trigger=\"hover\" data-original-title=\"Suggested Highlights\">\n                    <i class=\"fal fa-circle fa-stack-2x\"></i>\n                    <i class=\"fas fa-highlighter fa-stack-1x\"></i>\n                </span>\n                <span class=\"fa-stack ng-hide\" ng-hide=\"currentQuestion.frequentHighlights == null\" ng-click=\"showUserAndSuggestedHighlights()\" title=\"Duo Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fal\" ng-class=\"{'fas' : suggestedHighlightsPreference == 2, 'fal' : suggestedHighlightsPreference != 2}\"></i>\n                    <i class=\"fad fa-highlighter fa-stack-1x\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 2}\"></i>\n                </span>\n                <span class=\"fa-stack highlight-preference-unavailable highlight-button-with-popover\" ng-show=\"currentQuestion.frequentHighlights == null\" title=\"\" data-toggle=\"popover\" data-placement=\"bottom\" data-content=\"Collecting Data\" data-trigger=\"hover\" data-original-title=\"Duo Highlights\">\n                    <i class=\"fal fa-circle fa-stack-2x\"></i>\n                    <i class=\"fad fa-highlighter fa-stack-1x\"></i>\n                </span>\n            </div>\n            <!-- ngIf: launchTestOptions.launchCode == 'nbome' && userPreferences.testPreferences.theme != clientConstants.themes.nbome -->\n            <div id=\"question\">\n                <!-- ngIf: settings.hints && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && launchTestOptions.canShowHint -->\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && (currentQuestion.qbankId != clientConstants.qbankIds.stats_faculty || currentQuestion.qbankId != clientConstants.qbankIds.stats) -->\n                <!-- ngIf: (currentQuestion.mobileExcerpt != '' && currentQuestion.mobileExcerpt != null ) -->\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat -->\n                <!--Periodic table button for MCAT -->\n                <!--<div class=\"text-center periodicTableDiv\" ng-show=\"(launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.subjectId != constants.noPeriodicTable.cars) && (currentQuestion.subjectId != constants.noPeriodicTable.pyschology) && (currentQuestion.subjectId != constants.noPeriodicTable.sociology)\">\n                    <button class=\"btn btn-default btn--custom\" style=\"color: black; padding: 4px 3px;\" ng-click=\"showresources()\">Periodic table&nbsp;<img src=\"img/mcat/Periodic_icon.png\" height=\"12\" style=\"margin-top: -2px; cursor: pointer\" title=\"Periodic Table\" /></button>\n                </div>-->\n                \n                <!--Inactive message for questions in usmle, boards and nclex and do not show for EXPERIMENTAL_INACTIVE hence the check !=3 -->\n                <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.nclex) && !currentQuestion.isActive && currentQuestion.activeStatusId != 3 -->\n                <div id=\"questionText\" class=\"questionText ng-scope\" ng-mouseup=\"invokeHighlights('questionText',constants.flashCardType.selection,$event)\" compiledata=\"\" htmlcontent=\"<p>A 28-year-old man comes to the office with a 2-month history of dull low back pain and morning stiffness.&amp;nbsp; The pain had an insidious onset, and the patient does not recall any back trauma.&amp;nbsp; He attempted treatment with acetaminophen and spinal manipulation, which did not provide significant relief.&amp;nbsp; The patient had been a back sleeper, but now must lie on his side to fall asleep easily.&amp;nbsp; Past medical history is unremarkable.&amp;nbsp; Vital signs are normal.&amp;nbsp; Physical examination shows limited anterior flexion of the spine.&amp;nbsp; There is no swelling or warmth of any peripheral joints.&amp;nbsp; X-rays reveal narrowing of the sacroiliac joints.&amp;nbsp; Which of the following is most strongly associated with this patient&amp;#39;s condition?</p>\"><p class=\"ng-scope\">A 28-year-old man comes to the office with a 2-month history of dull low back pain and morning stiffness.&nbsp; The pain had an insidious onset, and the patient does not recall any back trauma.&nbsp; He attempted treatment with acetaminophen and spinal manipulation, which did not provide significant relief.&nbsp; The patient had been a back sleeper, but now must lie on his side to fall asleep easily.&nbsp; Past medical history is unremarkable.&nbsp; Vital signs are normal.&nbsp; Physical examination shows limited anterior flexion of the spine.&nbsp; There is no swelling or warmth of any peripheral joints.&nbsp; X-rays reveal narrowing of the sacroiliac joints.&nbsp; Which of the following is most strongly associated with this patient's condition?</p></div>\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && (currentQuestion.formatTypeId == constants.questionFormatType.fillInTheBlank ||currentQuestion.formatTypeId == constants.questionFormatType.fillInTheBlankRange )&& currentQuestion.sectionId == constants.collegeprepSectionIds.math -->\n                <!-- change to mediatype object -->\n                <!-- ngIf: currentQuestion.questionMedia.typeId == constants.questionMediaType.drugad || currentQuestion.questionTypeId == constants.questionType.drugad -->\n                <!-- ngIf: currentQuestion.questionMedia.typeId == constants.questionMediaType.audio || currentQuestion.questionMedia.typeId == constants.questionMediaType.video || currentQuestion.questionTypeId == constants.questionType.media -->\n\n                <div id=\"answerContainer\"><div class=\"options\">\n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>A.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight1\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">A specific human leukocyte antigen class I allele</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>B.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight2\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">A specific human leukocyte antigen class II allele</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>C.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight3\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Auto-reactive immunoglobulin M antibodies</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>D.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight4\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Deficient complement component</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>E.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight5\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">Selective immunoglobulin deficiency</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p><strong>Ankylosing spondylitis</strong> is a chronic inflammatory disorder of the sacroiliac joints and axial skeleton.&amp;nbsp; It is most common in young and middle-aged men and presents with morning stiffness and low back pain.&amp;nbsp; Ankylosing spondylitis is characterized by destruction of articular cartilage with resulting stiffness and fusion of axial joints.&amp;nbsp; The <strong>sacroiliac joints</strong> are often tender to palpation, and the spine may have decreased range of motion.&amp;nbsp; X-ray of the sacroiliac joints may reveal erosions, sclerosis, and narrowing; ultimately <a ng-click=&quot;showexhibit('2579',true);&quot;>fusion</a> of the joint spaces.&amp;nbsp; <a ng-click=&quot;showexhibit('52147',true);&quot;>Spine x-rays</a> reveal sclerosis, ligamentous calcification, and vertebral fusion (&amp;#x22;bamboo spine&amp;#x22;).</p><p>Ankylosing spondylitis, reactive arthritis, arthritis associated with inflammatory bowel disease, and psoriatic arthritis are <strong>seronegative spondyloarthropathies</strong>, so-called due to the absence of serum rheumatoid factor.&amp;nbsp; Patients with these diseases have a higher incidence of the <strong>human leukocyte antigen (HLA) B27</strong> allele, which encodes a specific HLA class I molecule, compared to the general population (although most patients with HLA B27 will not develop spondyloarthropathies).</p><p><strong>(Choice B)</strong>&amp;nbsp; Class I HLA proteins (eg, HLA B27) are expressed by all nucleated cells and present endogenous antigens to CD8<font size=&quot;2&quot;><sup>+</sup></font> cytotoxic T cells.&amp;nbsp; By contrast, <a ng-click=&quot;showexhibit('33403',true);&quot;>HLA class II</a> proteins (eg, DR, DP, DQ alleles) are expressed by antigen-presenting cells (eg, macrophages, dendritic cells) and present predominantly foreign antigens to CD4<font size=&quot;2&quot;><sup>+</sup></font> helper T cells.&amp;nbsp; Conditions associated with specific HLA class II alleles include rheumatoid arthritis, type I diabetes mellitus, and celiac disease.</p><p><strong>(Choice C)</strong>&amp;nbsp; Rheumatoid factor (IgM antibodies against self IgG) is present in the majority of patients with rheumatoid arthritis and may be seen in a variety of other autoimmune diseases as well as in some healthy individuals.</p><p><strong>(Choice D)</strong>&amp;nbsp; Complement component deficiencies are associated with recurrent infections and systemic lupus erythematosus.</p><p><strong>(Choice E)</strong>&amp;nbsp; IgA deficiency causes recurrent mucosal and respiratory infections as well as anaphylactic reactions after transfusion of blood products.</p><p><strong>Educational objective:</strong><br>The seronegative spondyloarthropathies include ankylosing spondylitis, reactive arthritis, psoriatic arthritis, and arthritis associated with inflammatory bowel disease.&amp;nbsp; Individuals expressing HLA B27 (a specific HLA class I molecule) are at increased risk for the seronegative spondyloarthropathies.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\"><strong>Ankylosing spondylitis</strong> is a chronic inflammatory disorder of the sacroiliac joints and axial skeleton.&nbsp; It is most common in young and middle-aged men and presents with morning stiffness and low back pain.&nbsp; Ankylosing spondylitis is characterized by destruction of articular cartilage with resulting stiffness and fusion of axial joints.&nbsp; The <strong>sacroiliac joints</strong> are often tender to palpation, and the spine may have decreased range of motion.&nbsp; X-ray of the sacroiliac joints may reveal erosions, sclerosis, and narrowing; ultimately <a ng-click=\"showexhibit('2579',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L504.png&quot;,&quot;title&quot;:&quot;L504&quot;}]\">fusion</a> of the joint spaces.&nbsp; <a ng-click=\"showexhibit('52147',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L163.png&quot;,&quot;title&quot;:&quot;L163&quot;}]\">Spine x-rays</a> reveal sclerosis, ligamentous calcification, and vertebral fusion (\"bamboo spine\").</p><p class=\"ng-scope\">Ankylosing spondylitis, reactive arthritis, arthritis associated with inflammatory bowel disease, and psoriatic arthritis are <strong>seronegative spondyloarthropathies</strong>, so-called due to the absence of serum rheumatoid factor.&nbsp; Patients with these diseases have a higher incidence of the <strong>human leukocyte antigen (HLA) B27</strong> allele, which encodes a specific HLA class I molecule, compared to the general population (although most patients with HLA B27 will not develop spondyloarthropathies).</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Class I HLA proteins (eg, HLA B27) are expressed by all nucleated cells and present endogenous antigens to CD8<font size=\"2\"><sup>+</sup></font> cytotoxic T cells.&nbsp; By contrast, <a ng-click=\"showexhibit('33403',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/L58961.jpg&quot;,&quot;title&quot;:&quot;L58961&quot;}]\">HLA class II</a> proteins (eg, DR, DP, DQ alleles) are expressed by antigen-presenting cells (eg, macrophages, dendritic cells) and present predominantly foreign antigens to CD4<font size=\"2\"><sup>+</sup></font> helper T cells.&nbsp; Conditions associated with specific HLA class II alleles include rheumatoid arthritis, type I diabetes mellitus, and celiac disease.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Rheumatoid factor (IgM antibodies against self IgG) is present in the majority of patients with rheumatoid arthritis and may be seen in a variety of other autoimmune diseases as well as in some healthy individuals.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Complement component deficiencies are associated with recurrent infections and systemic lupus erythematosus.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; IgA deficiency causes recurrent mucosal and respiratory infections as well as anaphylactic reactions after transfusion of blood products.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>The seronegative spondyloarthropathies include ankylosing spondylitis, reactive arthritis, psoriatic arthritis, and arthritis associated with inflammatory bowel disease.&nbsp; Individuals expressing HLA B27 (a specific HLA class I molecule) are at increased risk for the seronegative spondyloarthropathies.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Immunology</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Rheumatology/Orthopedics &amp; Sports</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Ankylosing spondylitis</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
