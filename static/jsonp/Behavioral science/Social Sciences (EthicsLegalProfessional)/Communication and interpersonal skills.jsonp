    {
        "question": "<div class=\"questioncontent ui-resizable widthWithoutAbstract\" id=\"questionContent\" ng-show=\"showQuestion\" ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat, 'suggested-highlights-visible' : userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; currentQuestion.frequentHighlights != null &amp;&amp; (suggestedHighlightsPreference == 1 || suggestedHighlightsPreference == 2), 'only-suggested-highlights-visible' : userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; suggestedHighlightsPreference == 1 &amp;&amp; currentQuestion.frequentHighlights != null}\">\n            <div style=\"display:none;\" class=\"suggested-highlights-switch-buttons ng-hide\" ng-show=\"userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; currentQuestion.frequentHighlights != null\">\n                <span class=\"fa-stack\" ng-click=\"showUserHighlights()\" title=\"My Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fas\" ng-class=\"{'fas' : suggestedHighlightsPreference == 0, 'fal' : suggestedHighlightsPreference != 0}\"></i>\n                    <i class=\"fal fa-highlighter fa-stack-1x fa-inverse\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 0}\"></i>\n                </span>\n                <span class=\"fa-stack ng-hide\" ng-hide=\"currentQuestion.frequentHighlights == null\" ng-click=\"showSuggestedHighlights()\" title=\"Suggested Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fal\" ng-class=\"{'fas' : suggestedHighlightsPreference == 1, 'fal' : suggestedHighlightsPreference != 1}\"></i>\n                    <i class=\"fas fa-highlighter fa-stack-1x\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 1}\"></i>\n                </span>\n                <span class=\"fa-stack highlight-preference-unavailable highlight-button-with-popover\" ng-show=\"currentQuestion.frequentHighlights == null\" title=\"\" data-toggle=\"popover\" data-placement=\"bottom\" data-content=\"Collecting Data\" data-trigger=\"hover\" data-original-title=\"Suggested Highlights\">\n                    <i class=\"fal fa-circle fa-stack-2x\"></i>\n                    <i class=\"fas fa-highlighter fa-stack-1x\"></i>\n                </span>\n                <span class=\"fa-stack ng-hide\" ng-hide=\"currentQuestion.frequentHighlights == null\" ng-click=\"showUserAndSuggestedHighlights()\" title=\"Duo Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fal\" ng-class=\"{'fas' : suggestedHighlightsPreference == 2, 'fal' : suggestedHighlightsPreference != 2}\"></i>\n                    <i class=\"fad fa-highlighter fa-stack-1x\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 2}\"></i>\n                </span>\n                <span class=\"fa-stack highlight-preference-unavailable highlight-button-with-popover\" ng-show=\"currentQuestion.frequentHighlights == null\" title=\"\" data-toggle=\"popover\" data-placement=\"bottom\" data-content=\"Collecting Data\" data-trigger=\"hover\" data-original-title=\"Duo Highlights\">\n                    <i class=\"fal fa-circle fa-stack-2x\"></i>\n                    <i class=\"fad fa-highlighter fa-stack-1x\"></i>\n                </span>\n            </div>\n            <!-- ngIf: launchTestOptions.launchCode == 'nbome' && userPreferences.testPreferences.theme != clientConstants.themes.nbome -->\n            <div id=\"question\">\n                <!-- ngIf: settings.hints && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && launchTestOptions.canShowHint -->\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && (currentQuestion.qbankId != clientConstants.qbankIds.stats_faculty || currentQuestion.qbankId != clientConstants.qbankIds.stats) -->\n                <!-- ngIf: (currentQuestion.mobileExcerpt != '' && currentQuestion.mobileExcerpt != null ) -->\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat -->\n                <!--Periodic table button for MCAT -->\n                <!--<div class=\"text-center periodicTableDiv\" ng-show=\"(launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.subjectId != constants.noPeriodicTable.cars) && (currentQuestion.subjectId != constants.noPeriodicTable.pyschology) && (currentQuestion.subjectId != constants.noPeriodicTable.sociology)\">\n                    <button class=\"btn btn-default btn--custom\" style=\"color: black; padding: 4px 3px;\" ng-click=\"showresources()\">Periodic table&nbsp;<img src=\"img/mcat/Periodic_icon.png\" height=\"12\" style=\"margin-top: -2px; cursor: pointer\" title=\"Periodic Table\" /></button>\n                </div>-->\n                \n                <!--Inactive message for questions in usmle, boards and nclex and do not show for EXPERIMENTAL_INACTIVE hence the check !=3 -->\n                <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.nclex) && !currentQuestion.isActive && currentQuestion.activeStatusId != 3 -->\n                <div id=\"questionText\" class=\"questionText ng-scope\" ng-mouseup=\"invokeHighlights('questionText',constants.flashCardType.selection,$event)\" compiledata=\"\" htmlcontent=\"<p>A 46-year-old woman comes to the office for evaluation of chronic fatigue, concentration difficulties, and intermittent headaches for the past 8 months.&amp;nbsp; The patient says, &amp;#x22;I had to start working part-time because I&amp;#39;m so tired and can&amp;#39;t focus, especially when I have a headache.&amp;#x22;&amp;nbsp; She pulls out a folder with several case reports on rare presentations of systemic lupus erythematosus and gives it to the physician.&amp;nbsp; The patient says, &amp;#x22;I know this is what I have, and I&amp;#39;ve included a list of all the immunological tests that you need to order.&amp;#x22;&amp;nbsp; She has already been to a few physicians for evaluation but felt they did not take her concerns seriously.&amp;nbsp; Which of the following is the most appropriate response to this patient&amp;#39;s request?</p>\"><p class=\"ng-scope\">A 46-year-old woman comes to the office for evaluation of chronic fatigue, concentration difficulties, and intermittent headaches for the past 8 months.&nbsp; The patient says, \"I had to start working part-time because I'm so tired and can't focus, especially when I have a headache.\"&nbsp; She pulls out a folder with several case reports on rare presentations of systemic lupus erythematosus and gives it to the physician.&nbsp; The patient says, \"I know this is what I have, and I've included a list of all the immunological tests that you need to order.\"&nbsp; She has already been to a few physicians for evaluation but felt they did not take her concerns seriously.&nbsp; Which of the following is the most appropriate response to this patient's request?</p></div>\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && (currentQuestion.formatTypeId == constants.questionFormatType.fillInTheBlank ||currentQuestion.formatTypeId == constants.questionFormatType.fillInTheBlankRange )&& currentQuestion.sectionId == constants.collegeprepSectionIds.math -->\n                <!-- change to mediatype object -->\n                <!-- ngIf: currentQuestion.questionMedia.typeId == constants.questionMediaType.drugad || currentQuestion.questionTypeId == constants.questionType.drugad -->\n                <!-- ngIf: currentQuestion.questionMedia.typeId == constants.questionMediaType.audio || currentQuestion.questionMedia.typeId == constants.questionMediaType.video || currentQuestion.questionTypeId == constants.questionType.media -->\n\n                <div id=\"answerContainer\"><div class=\"options\">\n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>A.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight1\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">\"Although there is some overlap between your symptoms and those of lupus, we should first consider more common and likely causes.\"</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>B.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight2\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">\"It must be difficult to deal with these symptoms every day and feel like you're not being listened to; have you been feeling anxious recently?\"</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>C.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight3\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">\"Thank you for sharing this information with me; however, the initial work-up for the symptoms you're describing do not include these immunological tests.\"</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>D.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight4\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">\"You want an explanation for the symptoms you're experiencing; since this is our first meeting, I'd like to do a thorough evaluation before we talk about testing.\"</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>E.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight5\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">\"You've read a lot about what could be causing your symptoms, but what makes you certain that this is the condition you have?\"</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p>This patient with chronic fatigue, concentration difficulties, and intermittent headaches has various <strong>nonspecific physical symptoms</strong> causing her significant distress.&amp;nbsp; Patients without a clear diagnosis can spend excessive amounts of time researching possible explanations (eg, case reports on rare presentations).&amp;nbsp; This may lead to deeply held convictions about a diagnosis (eg, &amp;#x22;I know this is what I have&amp;#x22;) and <strong>demands for specific diagnostic testing</strong> (eg, immunologic tests), which may or may not be medically indicated.</p><p>These challenging situations are often compounded by patients&amp;#39; anger and frustration with previous providers who minimized or invalidated their concerns, which lead them to seek yet another opinion.&amp;nbsp; In response to these challenging patient encounters, physicians must <strong>practice empathy</strong> and recognize patient suffering while also adhering to standard medical care (eg, perform evaluation prior to testing).</p><p>In this scenario, the most appropriate response opens with <strong>nonjudgmental validation</strong> of the patient&amp;#39;s symptoms (eg, &amp;#x22;You want an explanation for the symptoms you&amp;#39;re experiencing&amp;#x22;).&amp;nbsp; This is followed by outlining the standard evaluation process (eg, &amp;#x22;since this is our first meeting, I&amp;#39;d like to do a thorough evaluation&amp;#x22;).&amp;nbsp; In contrast to other approaches, this response recognizes the patient&amp;#39;s concerns without immediately rejecting or challenging her perspective <strong>(Choices C and E)</strong>.&amp;nbsp; This approach is more likely to establish an effective physician-patient relationship.&amp;nbsp; Adherence to a stepwise evaluation can also help <strong>avoid unnecessary testing</strong>.</p><p><strong>(Choice A)</strong>&amp;nbsp; Although physicians begin mentally constructing differential diagnoses based on symptoms alone (with greater consideration given to more common diagnoses), sharing these thoughts is premature without a complete evaluation (eg, physical examination).&amp;nbsp; In addition, this response may antagonize this patient by challenging her deeply held conviction that she has lupus.</p><p><strong>(Choice B)</strong>&amp;nbsp; Insinuating that the patient&amp;#39;s symptoms are due to an underlying psychiatric condition (&amp;#x22;have you been feeling anxious recently?&amp;#x22;) is invalidating and diagnostically premature.&amp;nbsp; Organic disease and psychiatric morbidity (eg, somatic symptom disorder) often coexist; therefore, a thorough evaluation is first performed to avoid overlooking any potential illness.</p><p><strong>Educational objective:</strong><br>Patients may come to the office for medical care with deeply held convictions about a potential diagnosis and may have premature or inappropriate requests for specific testing.&amp;nbsp; Empathic validation followed by outlining the standard evaluation process is more likely to establish an effective physician-patient relationship while avoiding unnecessary testing.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\">This patient with chronic fatigue, concentration difficulties, and intermittent headaches has various <strong>nonspecific physical symptoms</strong> causing her significant distress.&nbsp; Patients without a clear diagnosis can spend excessive amounts of time researching possible explanations (eg, case reports on rare presentations).&nbsp; This may lead to deeply held convictions about a diagnosis (eg, \"I know this is what I have\") and <strong>demands for specific diagnostic testing</strong> (eg, immunologic tests), which may or may not be medically indicated.</p><p class=\"ng-scope\">These challenging situations are often compounded by patients' anger and frustration with previous providers who minimized or invalidated their concerns, which lead them to seek yet another opinion.&nbsp; In response to these challenging patient encounters, physicians must <strong>practice empathy</strong> and recognize patient suffering while also adhering to standard medical care (eg, perform evaluation prior to testing).</p><p class=\"ng-scope\">In this scenario, the most appropriate response opens with <strong>nonjudgmental validation</strong> of the patient's symptoms (eg, \"You want an explanation for the symptoms you're experiencing\").&nbsp; This is followed by outlining the standard evaluation process (eg, \"since this is our first meeting, I'd like to do a thorough evaluation\").&nbsp; In contrast to other approaches, this response recognizes the patient's concerns without immediately rejecting or challenging her perspective <strong>(Choices C and E)</strong>.&nbsp; This approach is more likely to establish an effective physician-patient relationship.&nbsp; Adherence to a stepwise evaluation can also help <strong>avoid unnecessary testing</strong>.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Although physicians begin mentally constructing differential diagnoses based on symptoms alone (with greater consideration given to more common diagnoses), sharing these thoughts is premature without a complete evaluation (eg, physical examination).&nbsp; In addition, this response may antagonize this patient by challenging her deeply held conviction that she has lupus.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Insinuating that the patient's symptoms are due to an underlying psychiatric condition (\"have you been feeling anxious recently?\") is invalidating and diagnostically premature.&nbsp; Organic disease and psychiatric morbidity (eg, somatic symptom disorder) often coexist; therefore, a thorough evaluation is first performed to avoid overlooking any potential illness.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Patients may come to the office for medical care with deeply held convictions about a potential diagnosis and may have premature or inappropriate requests for specific testing.&nbsp; Empathic validation followed by outlining the standard evaluation process is more likely to establish an effective physician-patient relationship while avoiding unnecessary testing.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Behavioral science</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Social Sciences (Ethics/Legal/Professional)</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Physician patient communication</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
        "correctAnswerIndex": 3,
