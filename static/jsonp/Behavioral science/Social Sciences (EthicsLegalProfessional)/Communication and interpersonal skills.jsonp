iv class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>D.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight4\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">\"Unfortunately, modern antibiotics can have serious side effects.&nbsp; But I am glad to see you are doing better.\"</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>E.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight5\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">\"Unfortunately, we did not get information about your medication allergy yesterday.&nbsp; That is why you received the wrong antibiotic.\"</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><div id=&quot;div_t58818&quot;><table ng-click=&quot;showContextMenu( $event, constants.flashCardType.table )&quot; class=&quot;table-default-style&quot;><tbody><tr><td colspan=&quot;2&quot;><p align=&quot;center&quot;><strong>Reducing malpractice liability from treatment complications</strong></p></td></tr><tr><td><p align=&quot;center&quot;><strong>Initial disclosure</strong></p></td><td><ul><li>Acknowledge errors openly</li><li>Do not hide errors or other relevant factors</li><li>Express empathy &amp;amp; give apology as appropriate</li><li>Avoid blaming or denigrating other team members &amp;amp; providers</li><li>Allow adequate opportunity for patient questions</li></ul></td></tr><tr><td><p align=&quot;center&quot;><strong>Follow-up steps</strong></p></td><td><ul><li>Remain engaged in the patient&amp;#39;s care</li><li>Outline steps to prevent future occurrence</li></ul></td></tr></tbody></table></div><p>This patient experienced a severe medication allergy with respiratory distress.&amp;nbsp; Although key information did not accompany the patient at the time of transfer, this event was likely preventable.&amp;nbsp; In cases where important information is missing or incomplete, clinicians have a duty to obtain the information as soon as possible.&amp;nbsp; Because of the harm done to the patient, this clinician is at risk for a <strong>medical malpractice claim</strong>.</p><p>Most patients who experience complications, even avoidable complications, do not initiate a malpractice lawsuit.&amp;nbsp; Patients who sue providers are typically not driven by financial rewards, but are more likely to be motivated by <strong>anger</strong>, <strong>distrust</strong>, or a feeling that the <strong>clinician is not concerned</strong> for their well-being.&amp;nbsp; Often, patients report poor communication from the provider and file a lawsuit simply to get a better understanding of what happened.</p><p>When speaking with a patient following a medical error, clinicians should directly and honestly <strong>discuss that an error occurred</strong>; concealing an error can appear dishonest and generate anger if the error is later discovered.&amp;nbsp; The conversation should allow opportunity for the patient to ask <strong>questions</strong>, and answers should be given with language that the patient can understand.&amp;nbsp; A <strong>plan to rectify the error</strong> should be provided.&amp;nbsp; The provider should then convey <strong>concern</strong> for the patient and remain engaged with the patient&amp;#39;s care until the issue has been resolved.</p><p><strong>(Choice A)</strong>&amp;nbsp; Many experts recommend an apology be made to the patient following an error.&amp;nbsp; However, specific blame (including self-blame) is not necessary and may make defending a claim more difficult later.</p><p><strong>(Choice C)</strong>&amp;nbsp; This statement does not acknowledge that an error occurred and may be taken as blaming the patient.&amp;nbsp; Plans for future prevention should generally come after an open discussion of the initial error.</p><p><strong>(Choice D)</strong>&amp;nbsp; This statement conveys superficial concern but does not acknowledge the error and is likely to be seen as detached or blas&amp;eacute; on the part of the clinician.</p><p><strong>(Choice E)</strong>&amp;nbsp; Although factually correct, this statement conveys no empathy for the patient.&amp;nbsp; It also deflects blame to the referring provider, which often generates distrust toward all of the involved parties.</p><p><strong>Educational objective:</strong><br>When speaking with a patient following a medical error, clinicians should directly discuss the error.&amp;nbsp; The conversation should allow opportunity for the patient to ask questions.&amp;nbsp; A plan to rectify the error should be provided.&amp;nbsp; Finally, the provider should convey concern for the patient and remain engaged with the patient&amp;#39;s care.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><div id=\"div_t58818\" class=\"ng-scope\"><table ng-click=\"showContextMenu( $event, constants.flashCardType.table )\" class=\"table-default-style\"><tbody><tr><td colspan=\"2\"><p align=\"center\"><strong>Reducing malpractice liability from treatment complications</strong></p></td></tr><tr><td><p align=\"center\"><strong>Initial disclosure</strong></p></td><td><ul><li>Acknowledge errors openly</li><li>Do not hide errors or other relevant factors</li><li>Express empathy &amp; give apology as appropriate</li><li>Avoid blaming or denigrating other team members &amp; providers</li><li>Allow adequate opportunity for patient questions</li></ul></td></tr><tr><td><p align=\"center\"><strong>Follow-up steps</strong></p></td><td><ul><li>Remain engaged in the patient's care</li><li>Outline steps to prevent future occurrence</li></ul></td></tr></tbody></table></div><p class=\"ng-scope\">This patient experienced a severe medication allergy with respiratory distress.&nbsp; Although key information did not accompany the patient at the time of transfer, this event was likely preventable.&nbsp; In cases where important information is missing or incomplete, clinicians have a duty to obtain the information as soon as possible.&nbsp; Because of the harm done to the patient, this clinician is at risk for a <strong>medical malpractice claim</strong>.</p><p class=\"ng-scope\">Most patients who experience complications, even avoidable complications, do not initiate a malpractice lawsuit.&nbsp; Patients who sue providers are typically not driven by financial rewards, but are more likely to be motivated by <strong>anger</strong>, <strong>distrust</strong>, or a feeling that the <strong>clinician is not concerned</strong> for their well-being.&nbsp; Often, patients report poor communication from the provider and file a lawsuit simply to get a better understanding of what happened.</p><p class=\"ng-scope\">When speaking with a patient following a medical error, clinicians should directly and honestly <strong>discuss that an error occurred</strong>; concealing an error can appear dishonest and generate anger if the error is later discovered.&nbsp; The conversation should allow opportunity for the patient to ask <strong>questions</strong>, and answers should be given with language that the patient can understand.&nbsp; A <strong>plan to rectify the error</strong> should be provided.&nbsp; The provider should then convey <strong>concern</strong> for the patient and remain engaged with the patient's care until the issue has been resolved.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Many experts recommend an apology be made to the patient following an error.&nbsp; However, specific blame (including self-blame) is not necessary and may make defending a claim more difficult later.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; This statement does not acknowledge that an error occurred and may be taken as blaming the patient.&nbsp; Plans for future prevention should generally come after an open discussion of the initial error.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; This statement conveys superficial concern but does not acknowledge the error and is likely to be seen as detached or blas\u00e9 on the part of the clinician.</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; Although factually correct, this statement conveys no empathy for the patient.&nbsp; It also deflects blame to the referring provider, which often generates distrust toward all of the involved parties.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>When speaking with a patient following a medical error, clinicians should directly discuss the error.&nbsp; The conversation should allow opportunity for the patient to ask questions.&nbsp; A plan to rectify the error should be provided.&nbsp; Finally, the provider should convey concern for the patient and remain engaged with the patient's care.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Behavioral science</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Social Sciences (Ethics/Legal/Professional)</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Physician patient communication</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
