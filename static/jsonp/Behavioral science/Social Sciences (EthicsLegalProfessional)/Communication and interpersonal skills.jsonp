    },
    {
        "question": "<div class=\"questioncontent ui-resizable widthWithoutAbstract\" id=\"questionContent\" ng-show=\"showQuestion\" ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat, 'suggested-highlights-visible' : userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; currentQuestion.frequentHighlights != null &amp;&amp; (suggestedHighlightsPreference == 1 || suggestedHighlightsPreference == 2), 'only-suggested-highlights-visible' : userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; suggestedHighlightsPreference == 1 &amp;&amp; currentQuestion.frequentHighlights != null}\">\n            <div style=\"display:none;\" class=\"suggested-highlights-switch-buttons ng-hide\" ng-show=\"userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; currentQuestion.frequentHighlights != null\">\n                <span class=\"fa-stack\" ng-click=\"showUserHighlights()\" title=\"My Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fas\" ng-class=\"{'fas' : suggestedHighlightsPreference == 0, 'fal' : suggestedHighlightsPreference != 0}\"></i>\n                    <i class=\"fal fa-highlighter fa-stack-1x fa-inverse\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 0}\"></i>\n                </span>\n                <span class=\"fa-stack ng-hide\" ng-hide=\"currentQuestion.frequentHighlights == null\" ng-click=\"showSuggestedHighlights()\" title=\"Suggested Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fal\" ng-class=\"{'fas' : suggestedHighlightsPreference == 1, 'fal' : suggestedHighlightsPreference != 1}\"></i>\n                    <i class=\"fas fa-highlighter fa-stack-1x\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 1}\"></i>\n                </span>\n                <span class=\"fa-stack highlight-preference-unavailable highlight-button-with-popover\" ng-show=\"currentQuestion.frequentHighlights == null\" title=\"\" data-toggle=\"popover\" data-placement=\"bottom\" data-content=\"Collecting Data\" data-trigger=\"hover\" data-original-title=\"Suggested Highlights\">\n                    <i class=\"fal fa-circle fa-stack-2x\"></i>\n                    <i class=\"fas fa-highlighter fa-stack-1x\"></i>\n                </span>\n                <span class=\"fa-stack ng-hide\" ng-hide=\"currentQuestion.frequentHighlights == null\" ng-click=\"showUserAndSuggestedHighlights()\" title=\"Duo Highlights\">\n                    <i class=\"fa-circle fa-stack-2x fal\" ng-class=\"{'fas' : suggestedHighlightsPreference == 2, 'fal' : suggestedHighlightsPreference != 2}\"></i>\n                    <i class=\"fad fa-highlighter fa-stack-1x\" ng-class=\"{'fa-inverse' : suggestedHighlightsPreference == 2}\"></i>\n                </span>\n                <span class=\"fa-stack highlight-preference-unavailable highlight-button-with-popover\" ng-show=\"currentQuestion.frequentHighlights == null\" title=\"\" data-toggle=\"popover\" data-placement=\"bottom\" data-content=\"Collecting Data\" data-trigger=\"hover\" data-original-title=\"Duo Highlights\">\n                    <i class=\"fal fa-circle fa-stack-2x\"></i>\n                    <i class=\"fad fa-highlighter fa-stack-1x\"></i>\n                </span>\n            </div>\n            <!-- ngIf: launchTestOptions.launchCode == 'nbome' && userPreferences.testPreferences.theme != clientConstants.themes.nbome -->\n            <div id=\"question\">\n                <!-- ngIf: settings.hints && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && launchTestOptions.canShowHint -->\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && (currentQuestion.qbankId != clientConstants.qbankIds.stats_faculty || currentQuestion.qbankId != clientConstants.qbankIds.stats) -->\n                <!-- ngIf: (currentQuestion.mobileExcerpt != '' && currentQuestion.mobileExcerpt != null ) -->\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat -->\n                <!--Periodic table button for MCAT -->\n                <!--<div class=\"text-center periodicTableDiv\" ng-show=\"(launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.subjectId != constants.noPeriodicTable.cars) && (currentQuestion.subjectId != constants.noPeriodicTable.pyschology) && (currentQuestion.subjectId != constants.noPeriodicTable.sociology)\">\n                    <button class=\"btn btn-default btn--custom\" style=\"color: black; padding: 4px 3px;\" ng-click=\"showresources()\">Periodic table&nbsp;<img src=\"img/mcat/Periodic_icon.png\" height=\"12\" style=\"margin-top: -2px; cursor: pointer\" title=\"Periodic Table\" /></button>\n                </div>-->\n                \n                <!--Inactive message for questions in usmle, boards and nclex and do not show for EXPERIMENTAL_INACTIVE hence the check !=3 -->\n                <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.nclex) && !currentQuestion.isActive && currentQuestion.activeStatusId != 3 -->\n                <div id=\"questionText\" class=\"questionText ng-scope\" ng-mouseup=\"invokeHighlights('questionText',constants.flashCardType.selection,$event)\" compiledata=\"\" htmlcontent=\"<p>A 32-year-old woman comes to the office for follow-up of asthma.&amp;nbsp; The patient reports that her asthma seems to be getting worse and describes increasingly frequent episodes of chest tightness and wheezing over the past several months.&amp;nbsp; She says, &amp;#x22;I find myself reaching for my inhaler more often than I used to, and I wonder if I should try a stronger medication.&amp;#x22;&amp;nbsp; The patient has an 8-year history of intermittent asthma managed with an albuterol inhaler.&amp;nbsp; She smoked marijuana in her 20s for several years and started smoking cigarettes over the past year.&amp;nbsp; When asked about her tobacco use the patient says, &amp;#x22;I&amp;#39;m smoking only half a pack a day or less and I really enjoy it, especially with my morning coffee.&amp;#x22;&amp;nbsp; Which of the following is the most appropriate response to the patient?</p>\"><p class=\"ng-scope\">A 32-year-old woman comes to the office for follow-up of asthma.&nbsp; The patient reports that her asthma seems to be getting worse and describes increasingly frequent episodes of chest tightness and wheezing over the past several months.&nbsp; She says, \"I find myself reaching for my inhaler more often than I used to, and I wonder if I should try a stronger medication.\"&nbsp; The patient has an 8-year history of intermittent asthma managed with an albuterol inhaler.&nbsp; She smoked marijuana in her 20s for several years and started smoking cigarettes over the past year.&nbsp; When asked about her tobacco use the patient says, \"I'm smoking only half a pack a day or less and I really enjoy it, especially with my morning coffee.\"&nbsp; Which of the following is the most appropriate response to the patient?</p></div>\n                <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep && (currentQuestion.formatTypeId == constants.questionFormatType.fillInTheBlank ||currentQuestion.formatTypeId == constants.questionFormatType.fillInTheBlankRange )&& currentQuestion.sectionId == constants.collegeprepSectionIds.math -->\n                <!-- change to mediatype object -->\n                <!-- ngIf: currentQuestion.questionMedia.typeId == constants.questionMediaType.drugad || currentQuestion.questionTypeId == constants.questionType.drugad -->\n                <!-- ngIf: currentQuestion.questionMedia.typeId == constants.questionMediaType.audio || currentQuestion.questionMedia.typeId == constants.questionMediaType.video || currentQuestion.questionTypeId == constants.questionType.media -->\n\n                <div id=\"answerContainer\"><div class=\"options\">\n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>A.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight1\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">\"I am concerned that you do not fully understand the negative effects of smoking on your asthma.\"</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>B.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight2\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">\"I can understand that you may not want to stop completely, but even just decreasing will help.\"</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>C.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight3\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">\"I understand that smoking gives you pleasure, but it is also making your asthma worse.\"</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>D.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight4\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">\"We can consider a stronger medication, but it is unlikely to help much if you continue to smoke.\"</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    \n                        <div class=\"option\">\n                            <input class=\"answer-radio\" type=\"radio\">\n                            <strong>E.</strong>\n                            <span>\n                                \n                                            <span ng-attr-id=\"{{'answerhighlight' + answerChoice.choiceNumber}}\" ng-mouseup=\"invokeAnswerStrikeOut($event,answerChoice.choiceNumber)\" id=\"answerhighlight5\">\n                                                <span ng-bind-html=\"answerChoice.choice | unsafe\" class=\"ng-binding\">\"Would you like to know more about how smoking affects your asthma?\"</span>\n                                            </span>\n                                            <!-- ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) --><!-- end ngIf: (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && (currentQuestion.formatTypeId == constants.questionFormatType.singleBestResponse) && !userPreferences.testPreferences.hideAnswerStats && (currentQuestion.peopleTaken > (currentState == 'assignment' ? 100 : (launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat ? 500 : 100 ))) -->\n                                        \n                            </span>\n                        </div>\n                    </div></div>\n\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) && (userPreferences.testPreferences.theme == clientConstants.themes.collegeprep) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!-- ngIf: launchTestOptions.launchCode == 'nbme' && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search && (testInfo.testModeId == constants.testMode.timed || testInfo.testModeId == constants.testMode.untimed  ) -->\n                <!-- ngIf: (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode != 'collegeprep' && launchTestOptions.launchCode != 'cfa' && launchTestOptions.launchCode != 'legal' && userPreferences.testPreferences.theme != clientConstants.themes.uworld -->\n                <!-- ngIf: !currentQuestion.isSubmitted && (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld) && (testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p>This patient is concerned about her <strong>asthma</strong> and appears to be unaware that <strong>smoking</strong> is likely contributing to her worsening symptoms.&amp;nbsp; Her perspective is that she enjoys smoking and that her habit of less than half a pack a day is not detrimental to her health.&amp;nbsp; In patients who are either unaware of or appear to be <strong>minimizing the harms</strong> of smoking on their health, a helpful approach is the <strong>&amp;#x22;elicit-provide-elicit&amp;#x22; strategy</strong>.&amp;nbsp; This involves a nonjudgmental, collaborative approach to exploring the patient&amp;#39;s perspective, knowledge, and questions about the connection between smoking and their health; <strong>providing targeted information</strong> tailored to the patient (eg, this patient&amp;#39;s asthma); and then eliciting the patient&amp;#39;s understanding of the information provided.</p><p>In this case, the patient&amp;#39;s perspective is that she enjoys smoking and does not think it is harmful.&amp;nbsp; The physician should offer to educate her about what usually happens to patients with asthma who smoke and to elicit her understanding of the information.&amp;nbsp; This can be used as a starting point to help her develop ambivalence about continuing to smoke.&amp;nbsp; <a ng-click=&quot;showexhibit('10543',true);&quot;>Motivational interviewing</a> techniques can then be used to further amplify the discrepancy between the patient&amp;#39;s behavior and health goals.</p><p><strong>(Choice A)</strong>&amp;nbsp; This statement fails to take a patient-centered, collaborative approach and has a judgmental tone that is critical of the patient&amp;#39;s lack of knowledge.</p><p><strong>(Choice B)</strong>&amp;nbsp; Suggesting that the patient should decrease her cigarette consumption before providing information about the harms of smoking is premature and unlikely to elicit change.</p><p><strong>(Choice C)</strong>&amp;nbsp; Although this response provides needed information about the negative impact of smoking on asthma, it does not elicit the patient&amp;#39;s understanding or promote her interest in the discussion.&amp;nbsp; Positive change needs to be internally motivated; therefore, having a respectful, collaborative approach (eg, by asking the patient what she knows or would like to know rather than telling her) is preferable.</p><p><strong>(Choice D)</strong>&amp;nbsp; Although this statement responds to the patient&amp;#39;s interest in trying a stronger medication, it fails to address the underlying issue or educate the patient that addressing her smoking could relieve the worsening symptoms without the need for additional medication.</p><p><strong>Educational objective:</strong><br>Patients who are either unaware of or appear to be minimizing the harms of smoking can benefit from a nonjudgmental, collaborative approach.&amp;nbsp; This involves asking for the patient&amp;#39;s perspective, knowledge, and concerns about smoking; providing relevant information; and then eliciting the patient&amp;#39;s understanding of the information.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\">This patient is concerned about her <strong>asthma</strong> and appears to be unaware that <strong>smoking</strong> is likely contributing to her worsening symptoms.&nbsp; Her perspective is that she enjoys smoking and that her habit of less than half a pack a day is not detrimental to her health.&nbsp; In patients who are either unaware of or appear to be <strong>minimizing the harms</strong> of smoking on their health, a helpful approach is the <strong>\"elicit-provide-elicit\" strategy</strong>.&nbsp; This involves a nonjudgmental, collaborative approach to exploring the patient's perspective, knowledge, and questions about the connection between smoking and their health; <strong>providing targeted information</strong> tailored to the patient (eg, this patient's asthma); and then eliciting the patient's understanding of the information provided.</p><p class=\"ng-scope\">In this case, the patient's perspective is that she enjoys smoking and does not think it is harmful.&nbsp; The physician should offer to educate her about what usually happens to patients with asthma who smoke and to elicit her understanding of the information.&nbsp; This can be used as a starting point to help her develop ambivalence about continuing to smoke.&nbsp; <a ng-click=\"showexhibit('10543',true);\" exhibit-data=\"[{&quot;path&quot;:&quot;image/34759.html&quot;,&quot;title&quot;:&quot;34759&quot;}]\">Motivational interviewing</a> techniques can then be used to further amplify the discrepancy between the patient's behavior and health goals.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; This statement fails to take a patient-centered, collaborative approach and has a judgmental tone that is critical of the patient's lack of knowledge.</p><p class=\"ng-scope\"><strong>(Choice B)</strong>&nbsp; Suggesting that the patient should decrease her cigarette consumption before providing information about the harms of smoking is premature and unlikely to elicit change.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Although this response provides needed information about the negative impact of smoking on asthma, it does not elicit the patient's understanding or promote her interest in the discussion.&nbsp; Positive change needs to be internally motivated; therefore, having a respectful, collaborative approach (eg, by asking the patient what she knows or would like to know rather than telling her) is preferable.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; Although this statement responds to the patient's interest in trying a stronger medication, it fails to address the underlying issue or educate the patient that addressing her smoking could relieve the worsening symptoms without the need for additional medication.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Patients who are either unaware of or appear to be minimizing the harms of smoking can benefit from a nonjudgmental, collaborative approach.&nbsp; This involves asking for the patient's perspective, knowledge, and concerns about smoking; providing relevant information; and then eliciting the patient's understanding of the information.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) --><div ng-if=\"((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) &amp;&amp; currentQuestion.questionMappingReferencesList != null) &amp;&amp; (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal)\" class=\"references ng-scope\">\n                        <div class=\"referencesHeader\" ng-style=\"{'border-top': (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle &amp;&amp; currentQuestion.standards != null) ? '0px':'1px'}\" style=\"border-top: 0px;\">\n                            <b>References</b>\n                        </div>\n                        <div class=\"referencesContent\">\n                            <ul>\n                        