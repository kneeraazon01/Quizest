nts.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) -->\n\n                <!--<div ng-if=\"(testInfo.testModeId == constants.testMode.timedTutor || testInfo.testModeId == constants.testMode.tutor) && launchTestOptions.launchCode == 'mcat' \">\n                <button ng-if=\"!currentQuestion.isSubmitted\" class=\"btn btn-default btn--custom\" style=\"color: black;\" ng-click=\"showanswer(0)\">Submit</button>\n                </div>-->\n                <div id=\"prevQuestionBox\" style=\"display: none;\">\n                    <div id=\"prevQuestionBoxHeader\">\n                        <div class=\"pull-right\">\n                            <!-- ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) --><div class=\"header-close ng-scope\" ng-if=\"!(userPreferences.testPreferences.theme == clientConstants.themes.uworld &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep)\" ng-click=\"closeShowPreviousQuestion();\"></div><!-- end ngIf: !(userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep) -->\n                            <!-- ngIf: userPreferences.testPreferences.theme == clientConstants.themes.uworld && launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep -->\n                            <!--<img src=\"img/X-icon.png\" height=\"14\" style=\"margin: 0px; cursor: pointer;\" ng-click=\"closeShowPreviousQuestion();\" />-->\n                        </div>\n                    </div>\n                    <div id=\"prevQuestionBoxContent\">\n                        <div><span ng-bind-html=\"prevQuestioninSequence.questionText\" class=\"ng-binding\"></span></div>\n                        <!-- ngRepeat: answerChoice in prevQuestioninSequence.answerChoiceList -->\n                    </div>\n                </div>\n            </div>\n            <div class=\"statsBarWrapper\" ng-class=\"{'customstatsBar': (launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal || userPreferences.testPreferences.theme == clientConstants.themes.uworld)  &amp;&amp; (launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search)}\">\n                <div ng-class=\"{maxWidth : currentAbstract != '' || (currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat) , widthWithoutAbstract : currentAbstract == '' &amp;&amp; launchTestOptions.topLevelProductId != constants.topLevelProduct.mcat }\" class=\"widthWithoutAbstract\">\n                    <!-- for correct, incorrect and omitted-->\n                    <!-- ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) --><!-- end ngIf: (launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search ) -->\n                    <!-- search -->\n                    <!-- ngIf: testInfo.testModeId == constants.testMode.search -->\n\n                    <div id=\"possibleAnswersForMath\" style=\"display: none;\">\n                        <div id=\"possibleAnswersForMathHeader\">\n                            <div class=\"pull-left\">\n                                <b>Other Possible Answers</b>\n                            </div>\n                            <div class=\"pull-right\">\n                                <button type=\"button\" class=\"close\" ng-click=\"hidePossibleAnswersForMath();\">\u00d7</button>\n                            </div>\n                            <div class=\"clearfix\"></div>\n                        </div>\n                        <div id=\"possibleAnswersDiv\" class=\"ng-binding\">\n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div id=\"questionExplanationPlaceHolder\" style=\"display:none;\"></div>\n            \n\n            <!-- show this for collegeprep and nclex med math set sequence questions to indicate users to answer the next question -->\n            <!-- ngIf: testInfo.testModeId != constants.testMode.search && ((launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || currentQuestion.qbankId == clientConstants.qbankIds.med_math) && (!launchTestOptions.canShowAnswer) && currentQuestion.isSubmitted) -->\n\n            <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.legal && !launchTestOptions.canShowAnswer && testInfo.testModeId != constants.testMode.search -->\n\n        </div>",
        "exp": "<div class=\"explanation\" id=\"questionExplanation\" ng-show=\"launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search\">\n                <div style=\"position:relative\">\n                    \n                    <div style=\"font-size:20px;font-weight:bolder;\">Explanation</div>\n                \n                    <!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.cfa && currentQuestion.questionMappingReferencesList != null && currentQuestion.questionMappingReferencesList.length > 0 -->\n                </div>\n\n                <div id=\"explanation\" ng-mouseup=\"invokeHighlights('explanation',constants.flashCardType.selection,$event)\">\n                    <div class=\"tab-content tab-content-box\">\n                        <!-- ngIf: (launchTestOptions.topLevelProductId == constants.topLevelProduct.collegeprep || launchTestOptions.topLevelProductId == constants.topLevelProduct.mcat ) && (currentQuestion.passageExcerpt !='' && currentQuestion.passageExcerpt != null) --><div id=\"firstExplanation\" class=\"tab-pane active ng-scope\" compiledata=\"\" htmlcontent=\"<div style=&quot;visibility:hidden;height:0px;&quot;><div class=&quot;pull-left&quot;><b>Explanation:</b></div><div class=&quot;pull-right&quot;><b ng-if=&quot;launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards&quot;>User Id: 2955384</b></div><div class=&quot;clearfix&quot;></div></div><br/><p>When discussing serious, potentially life-threatening illness, patients commonly wish to have family and friends present for support.&amp;nbsp; Having others be present may also facilitate better understanding and retention of information when patients are overwhelmed or distracted by ongoing symptoms of illness.&amp;nbsp; However, <strong>patient&amp;nbsp;preferences vary widely</strong>, both among and within patient subpopulations (eg, elderly, minorities), regarding which (if any) companions should be present during discussion of serious illness.</p><p>Therefore, before disclosing any significant information, the physician should first determine <strong>how the patient prefers to receive information</strong> and <strong>which companions</strong>&amp;nbsp;<strong>should be present</strong> during the discussion.&amp;nbsp; The inquiry should be <strong>nondirective</strong>, avoiding assumptions and giving the patient full scope to express preferences.&amp;nbsp; Care should be taken to avoid alienating involved family members, who may be central in supporting the patient&amp;#39;s decision-making and comfort.&amp;nbsp; Only when the patient&amp;#39;s preferences are clear should significant information be disclosed.</p><p><strong>(Choice A)</strong>&amp;nbsp; Although a family meeting can be an appropriate forum to share difficult information and answer questions, the physician should first inquire as to the patient&amp;#39;s preferences before postponing delivery of results.</p><p><strong>(Choice C)</strong>&amp;nbsp; Some patients (eg, elderly) may express the desire for other family members to be informed first about a health condition; however, this preference should not be assumed.&amp;nbsp; The physician must first discuss with the patient how she wishes to receive information.</p><p><strong>(Choice D)</strong>&amp;nbsp; This response conveys empathy but assumes that the patient prefers all her companions to remain in the room.&amp;nbsp; It also reveals the results are upsetting (&amp;#x22;difficult news&amp;#x22;).</p><p><strong>(Choice E)</strong>&amp;nbsp; When discussing test results in the presence of a patient&amp;#39;s family and friends, reviewing what they already know before proceeding to new information is advisable.&amp;nbsp; However, this statement assumes she has already given consent for her companions to be present during the discussion, which she has not.</p><p><strong>Educational objective:</strong><br>Before disclosing any significant information to patients, physicians should determine patient preferences for how to receive information and which companions should be present during the discussions.&amp;nbsp; Only when patients&amp;#39; preferences are clear should significant information be disclosed.</p>\"><div style=\"visibility:hidden;height:0px;\" class=\"ng-scope\"><div class=\"pull-left\"><b>Explanation:</b></div><div class=\"pull-right\"><!-- ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --><b ng-if=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards\" class=\"ng-scope\">User Id: 2955384</b><!-- end ngIf: launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards --></div><div class=\"clearfix\"></div></div><br class=\"ng-scope\"><p class=\"ng-scope\">When discussing serious, potentially life-threatening illness, patients commonly wish to have family and friends present for support.&nbsp; Having others be present may also facilitate better understanding and retention of information when patients are overwhelmed or distracted by ongoing symptoms of illness.&nbsp; However, <strong>patient&nbsp;preferences vary widely</strong>, both among and within patient subpopulations (eg, elderly, minorities), regarding which (if any) companions should be present during discussion of serious illness.</p><p class=\"ng-scope\">Therefore, before disclosing any significant information, the physician should first determine <strong>how the patient prefers to receive information</strong> and <strong>which companions</strong>&nbsp;<strong>should be present</strong> during the discussion.&nbsp; The inquiry should be <strong>nondirective</strong>, avoiding assumptions and giving the patient full scope to express preferences.&nbsp; Care should be taken to avoid alienating involved family members, who may be central in supporting the patient's decision-making and comfort.&nbsp; Only when the patient's preferences are clear should significant information be disclosed.</p><p class=\"ng-scope\"><strong>(Choice A)</strong>&nbsp; Although a family meeting can be an appropriate forum to share difficult information and answer questions, the physician should first inquire as to the patient's preferences before postponing delivery of results.</p><p class=\"ng-scope\"><strong>(Choice C)</strong>&nbsp; Some patients (eg, elderly) may express the desire for other family members to be informed first about a health condition; however, this preference should not be assumed.&nbsp; The physician must first discuss with the patient how she wishes to receive information.</p><p class=\"ng-scope\"><strong>(Choice D)</strong>&nbsp; This response conveys empathy but assumes that the patient prefers all her companions to remain in the room.&nbsp; It also reveals the results are upsetting (\"difficult news\").</p><p class=\"ng-scope\"><strong>(Choice E)</strong>&nbsp; When discussing test results in the presence of a patient's family and friends, reviewing what they already know before proceeding to new information is advisable.&nbsp; However, this statement assumes she has already given consent for her companions to be present during the discussion, which she has not.</p><p class=\"ng-scope\"><strong>Educational objective:</strong><br>Before disclosing any significant information to patients, physicians should determine patient preferences for how to receive information and which companions should be present during the discussions.&nbsp; Only when patients' preferences are clear should significant information be disclosed.</p></div><!-- ngIf: currentQuestion.additionalText -->\n                    </div>\n                </div>\n                <div>\n                    <!-- ngIf: ((launchTestOptions.canShowAnswer || testInfo.testModeId == constants.testMode.search) && currentQuestion.questionMappingReferencesList != null) && (launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || launchTestOptions.topLevelProductId == constants.topLevelProduct.boards || launchTestOptions.topLevelProductId == constants.topLevelProduct.legal) -->\n                    <!-- ngIf: currentQuestion.standards != null && currentQuestion.standards != '' --><div ng-if=\"currentQuestion.standards != null &amp;&amp; currentQuestion.standards != ''\" class=\"standards ng-scope\">\n                        <div class=\"standardsHeader\">\n                            <b ng-hide=\"launchTestOptions.topLevelProductId == constants.topLevelProduct.usmle || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.fnp || $scope.clientConfig.qbankId == $scope.clientConstants.qbankIds.mcat\" style=\"font-size:11pt !important\" class=\"ng-hide\">Additional Information</b>\n                        </div>\n                        <div class=\"standardsContent flexbox-container\">\n                            <div class=\"inner-flex\">\n                                <!-- ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Behavioral science</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Subject</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Social Sciences (Ethics/Legal/Professional)</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">System</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards --><div ng-repeat=\"standard in currentQuestion.standards\" style=\"padding-right:50px;padding-bottom:15px;\" class=\"ng-scope\">\n                                    <div style=\"font-size:11pt !important\" class=\"ng-binding\">Physician patient communication</div>\n                                    <div style=\"font-size:9pt !important;line-height:10px\" class=\"standardscontent ng-binding\">Topic</div>\n                                </div><!-- end ngRepeat: standard in currentQuestion.standards -->\n                            </div>\n                        </div>\n                    </div><!-- end ngIf: currentQuestion.standards != null && currentQuestion.standards != '' -->\n                    <div>\n                        \n                        <div class=\"clearfix\"></div>\n                    </div>\n                    <!-- ngIf: currentQuestion.isHelpNeeded != undefined && currentQuestion.isHelpNeeded != null -->\n                </div>\n            </div>",
